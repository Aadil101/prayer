{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0642\u064e\u062f\u06e1 \u0631\u064e\u0623\u064e\u0649\u0670 \u0645\u0650\u0646\u06e1 \u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650 \u0631\u064e\u0628\u0651\u0650\u0647\u0650 \u0671\u0644\u06e1\u0643\u064f\u0628\u06e1\u0631\u064e\u0649\u0670\u0653"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "Certainly", "word_transliteration": "laqad"}, {"word_arabic_uthmani": "\u0631\u064e\u0623\u064e\u0649\u0670", "word_translation": "he saw", "word_transliteration": "raa"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650", "word_translation": "(the) Signs", "word_transliteration": "ayati"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650", "word_translation": "(of) his Lord", "word_transliteration": "rabbihi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064f\u0628\u06e1\u0631\u064e\u0649\u0670\u0653", "word_translation": "the Greatest", "word_transliteration": "al-kub'ra"}]}