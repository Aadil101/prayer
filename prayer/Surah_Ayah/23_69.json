{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1 \u0644\u064e\u0645\u06e1 \u064a\u064e\u0639\u06e1\u0631\u0650\u0641\u064f\u0648\u0627\u0652 \u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u0645\u06e1 \u0641\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0647\u064f\u06e5 \u0645\u064f\u0646\u0643\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "(do) not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0631\u0650\u0641\u064f\u0648\u0627\u0652", "word_translation": "they recognize", "word_transliteration": "ya'rifu"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their Messenger", "word_transliteration": "rasulahum"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so they", "word_transliteration": "fahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "(are) rejecting him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u0646\u0643\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(are) rejecting him", "word_transliteration": "munkiruna"}]}