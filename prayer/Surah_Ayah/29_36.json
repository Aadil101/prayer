{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u0649\u0670 \u0645\u064e\u062f\u06e1\u064a\u064e\u0646\u064e \u0623\u064e\u062e\u064e\u0627\u0647\u064f\u0645\u06e1 \u0634\u064f\u0639\u064e\u064a\u06e1\u0628\u0657\u0627 \u0641\u064e\u0642\u064e\u0627\u0644\u064e \u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0671\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0671\u0631\u06e1\u062c\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0639\u06e1\u062b\u064e\u0648\u06e1\u0627\u0652 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "And to", "word_transliteration": "wa-ila"}, {"word_arabic_uthmani": "\u0645\u064e\u062f\u06e1\u064a\u064e\u0646\u064e", "word_translation": "Madyan", "word_transliteration": "madyana"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u064e\u0627\u0647\u064f\u0645\u06e1", "word_translation": "their brother", "word_transliteration": "akhahum"}, {"word_arabic_uthmani": "\u0634\u064f\u0639\u064e\u064a\u06e1\u0628\u0657\u0627", "word_translation": "Shuaib", "word_transliteration": "shu'ayban"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "And he said", "word_transliteration": "faqala"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "O my people!", "word_transliteration": "yaqawmi"}, {"word_arabic_uthmani": "\u0671\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0627\u0652", "word_translation": "Worship", "word_transliteration": "u'budu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0631\u06e1\u062c\u064f\u0648\u0627\u0652", "word_translation": "and expect", "word_transliteration": "wa-ir'ju"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "the Day", "word_transliteration": "al-yawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e", "word_translation": "the Last", "word_transliteration": "al-akhira"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u062b\u064e\u0648\u06e1\u0627\u0652", "word_translation": "commit evil", "word_transliteration": "ta'thaw"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e", "word_translation": "(as) corrupters", "word_transliteration": "muf'sidina"}]}