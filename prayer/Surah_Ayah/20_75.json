{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0646 \u064a\u064e\u0623\u06e1\u062a\u0650\u0647\u0650\u06e6 \u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0657\u0627 \u0642\u064e\u062f\u06e1 \u0639\u064e\u0645\u0650\u0644\u064e \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064e\u0670\u062a\u0650 \u0641\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u062f\u0651\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u064f \u0671\u0644\u06e1\u0639\u064f\u0644\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "But whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650\u0647\u0650\u06e6", "word_translation": "comes to Him", "word_transliteration": "yatihi"}, {"word_arabic_uthmani": "\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0657\u0627", "word_translation": "(as) a believer", "word_transliteration": "mu'minan"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "verily", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0650\u0644\u064e", "word_translation": "he has done", "word_transliteration": "amila"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064e\u0670\u062a\u0650", "word_translation": "the righteous deeds", "word_transliteration": "al-salihati"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "then those", "word_transliteration": "fa-ulaika"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "for them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u064f", "word_translation": "(will be) the ranks", "word_transliteration": "al-darajatu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064f\u0644\u064e\u0649\u0670", "word_translation": "[the] high", "word_transliteration": "al-'ula"}]}