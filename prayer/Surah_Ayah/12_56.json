{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0645\u064e\u0643\u0651\u064e\u0646\u0651\u064e\u0627 \u0644\u0650\u064a\u064f\u0648\u0633\u064f\u0641\u064e \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u064a\u064e\u062a\u064e\u0628\u064e\u0648\u0651\u064e\u0623\u064f \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u062d\u064e\u064a\u06e1\u062b\u064f \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da \u0646\u064f\u0635\u0650\u064a\u0628\u064f \u0628\u0650\u0631\u064e\u062d\u06e1\u0645\u064e\u062a\u0650\u0646\u064e\u0627 \u0645\u064e\u0646 \u0646\u0651\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06d6 \u0648\u064e\u0644\u064e\u0627 \u0646\u064f\u0636\u0650\u064a\u0639\u064f \u0623\u064e\u062c\u06e1\u0631\u064e \u0671\u0644\u06e1\u0645\u064f\u062d\u06e1\u0633\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "And thus", "word_transliteration": "wakadhalika"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u0651\u064e\u0646\u0651\u064e\u0627", "word_translation": "We established", "word_transliteration": "makkanna"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u0648\u0633\u064f\u0641\u064e", "word_translation": "[to] Yusuf", "word_transliteration": "liyusufa"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the land", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0628\u064e\u0648\u0651\u064e\u0623\u064f", "word_translation": "to settle", "word_transliteration": "yatabawwa-u"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u062d\u064e\u064a\u06e1\u062b\u064f", "word_translation": "where ever", "word_transliteration": "haythu"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da", "word_translation": "he willed", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0646\u064f\u0635\u0650\u064a\u0628\u064f", "word_translation": "We bestow", "word_transliteration": "nusibu"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u062d\u06e1\u0645\u064e\u062a\u0650\u0646\u064e\u0627", "word_translation": "Our Mercy", "word_transliteration": "birahmatina"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(on) whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06d6", "word_translation": "We will", "word_transliteration": "nashau"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0646\u064f\u0636\u0650\u064a\u0639\u064f", "word_translation": "We let go waste", "word_transliteration": "nudi'u"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0631\u064e", "word_translation": "(the) reward", "word_transliteration": "ajra"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u062d\u06e1\u0633\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "(of) the good-doers", "word_transliteration": "al-muh'sinina"}]}