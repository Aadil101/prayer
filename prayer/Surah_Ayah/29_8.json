{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0648\u064e\u0635\u0651\u064e\u064a\u06e1\u0646\u064e\u0627 \u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e\u0670\u0646\u064e \u0628\u0650\u0648\u064e\u0670\u0644\u0650\u062f\u064e\u064a\u06e1\u0647\u0650 \u062d\u064f\u0633\u06e1\u0646\u0657\u0627\u06d6 \u0648\u064e\u0625\u0650\u0646 \u062c\u064e\u0670\u0647\u064e\u062f\u064e\u0627\u0643\u064e \u0644\u0650\u062a\u064f\u0634\u06e1\u0631\u0650\u0643\u064e \u0628\u0650\u064a \u0645\u064e\u0627 \u0644\u064e\u064a\u06e1\u0633\u064e \u0644\u064e\u0643\u064e \u0628\u0650\u0647\u0650\u06e6 \u0639\u0650\u0644\u06e1\u0645\u065e \u0641\u064e\u0644\u064e\u0627 \u062a\u064f\u0637\u0650\u0639\u06e1\u0647\u064f\u0645\u064e\u0627\u0653\u06da \u0625\u0650\u0644\u064e\u064a\u0651\u064e \u0645\u064e\u0631\u06e1\u062c\u0650\u0639\u064f\u0643\u064f\u0645\u06e1 \u0641\u064e\u0623\u064f\u0646\u064e\u0628\u0651\u0650\u0626\u064f\u0643\u064f\u0645 \u0628\u0650\u0645\u064e\u0627 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0648\u064e\u0635\u0651\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "And We have enjoined", "word_transliteration": "wawassayna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e\u0670\u0646\u064e", "word_translation": "(on) man", "word_transliteration": "al-insana"}, {"word_arabic_uthmani": "\u0628\u0650\u0648\u064e\u0670\u0644\u0650\u062f\u064e\u064a\u06e1\u0647\u0650", "word_translation": "goodness to his parents", "word_transliteration": "biwalidayhi"}, {"word_arabic_uthmani": "\u062d\u064f\u0633\u06e1\u0646\u0657\u0627\u06d6", "word_translation": "goodness to his parents", "word_transliteration": "hus'nan"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "but if", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u062c\u064e\u0670\u0647\u064e\u062f\u064e\u0627\u0643\u064e", "word_translation": "they both strive against you", "word_transliteration": "jahadaka"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064f\u0634\u06e1\u0631\u0650\u0643\u064e", "word_translation": "to make you associate", "word_transliteration": "litush'rika"}, {"word_arabic_uthmani": "\u0628\u0650\u064a", "word_translation": "with Me", "word_transliteration": "bi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "not", "word_transliteration": "laysa"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e", "word_translation": "you have", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "of it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u065e", "word_translation": "any knowledge", "word_transliteration": "il'mun"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "then (do) not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u062a\u064f\u0637\u0650\u0639\u06e1\u0647\u064f\u0645\u064e\u0627\u0653\u06da", "word_translation": "obey both of them", "word_transliteration": "tuti'huma"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u0651\u064e", "word_translation": "To Me", "word_transliteration": "ilayya"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u06e1\u062c\u0650\u0639\u064f\u0643\u064f\u0645\u06e1", "word_translation": "(is) your return", "word_transliteration": "marji'ukum"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064f\u0646\u064e\u0628\u0651\u0650\u0626\u064f\u0643\u064f\u0645", "word_translation": "and I will inform you", "word_transliteration": "fa-unabbi-ukum"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "about what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you used", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "(to) do", "word_transliteration": "ta'maluna"}]}