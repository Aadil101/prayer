{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652 \u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0633\u064e\u0645\u0650\u0639\u06e1\u0646\u064e\u0627 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0633\u06e1\u0645\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652", "word_translation": "be", "word_transliteration": "takunu"}, {"word_arabic_uthmani": "\u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "like those who", "word_transliteration": "ka-alladhina"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "say", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0633\u064e\u0645\u0650\u0639\u06e1\u0646\u064e\u0627", "word_translation": "We heard", "word_transliteration": "sami'na"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "do not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u0645\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "hear", "word_transliteration": "yasma'una"}]}