{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0647\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u0649\u0670 \u0635\u064e\u0644\u064e\u0648\u064e\u0670\u062a\u0650\u0647\u0650\u0645\u06e1 \u064a\u064f\u062d\u064e\u0627\u0641\u0650\u0638\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "And those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "[they]", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0635\u064e\u0644\u064e\u0648\u064e\u0670\u062a\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their prayers", "word_transliteration": "salawatihim"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u064e\u0627\u0641\u0650\u0638\u064f\u0648\u0646\u064e", "word_translation": "they guard", "word_transliteration": "yuhafizuna"}]}