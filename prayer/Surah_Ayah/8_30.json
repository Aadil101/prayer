{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1 \u064a\u064e\u0645\u06e1\u0643\u064f\u0631\u064f \u0628\u0650\u0643\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0644\u0650\u064a\u064f\u062b\u06e1\u0628\u0650\u062a\u064f\u0648\u0643\u064e \u0623\u064e\u0648\u06e1 \u064a\u064e\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0643\u064e \u0623\u064e\u0648\u06e1 \u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064f\u0648\u0643\u064e\u06da \u0648\u064e\u064a\u064e\u0645\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e \u0648\u064e\u064a\u064e\u0645\u06e1\u0643\u064f\u0631\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06d6 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u062e\u064e\u064a\u06e1\u0631\u064f \u0671\u0644\u06e1\u0645\u064e\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1", "word_translation": "And when", "word_transliteration": "wa-idh"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u06e1\u0643\u064f\u0631\u064f", "word_translation": "plotted", "word_transliteration": "yamkuru"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064e", "word_translation": "against you", "word_transliteration": "bika"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u062b\u06e1\u0628\u0650\u062a\u064f\u0648\u0643\u064e", "word_translation": "that they restrain you", "word_transliteration": "liyuth'bituka"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0643\u064e", "word_translation": "kill you", "word_transliteration": "yaqtuluka"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064f\u0648\u0643\u064e\u06da", "word_translation": "drive you out", "word_transliteration": "yukh'rijuka"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0645\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "And they were planning", "word_transliteration": "wayamkuruna"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0645\u06e1\u0643\u064f\u0631\u064f", "word_translation": "and (also) was planning", "word_transliteration": "wayamkuru"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06d6", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u064f", "word_translation": "is (the) Best", "word_transliteration": "khayru"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(of) the Planners", "word_transliteration": "al-makirina"}]}