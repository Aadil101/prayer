{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064f\u0643\u064f\u0645\u06e1 \u0625\u0650\u0644\u064e\u0670\u0647\u065e \u0648\u064e\u0670\u062d\u0650\u062f\u065e\u06da \u0641\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0644\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u0628\u0650\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650 \u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645 \u0645\u0651\u064f\u0646\u0643\u0650\u0631\u064e\u0629\u065e \u0648\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064f\u0643\u064f\u0645\u06e1", "word_translation": "Your god", "word_transliteration": "ilahukum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u065e", "word_translation": "(is) God", "word_transliteration": "ilahun"}, {"word_arabic_uthmani": "\u0648\u064e\u0670\u062d\u0650\u062f\u065e\u06da", "word_translation": "One", "word_transliteration": "wahidun"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "But those who", "word_transliteration": "fa-alladhina"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "believe", "word_transliteration": "yu'minuna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650", "word_translation": "in the Hereafter", "word_transliteration": "bil-akhirati"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645", "word_translation": "their hearts", "word_transliteration": "qulubuhum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0646\u0643\u0650\u0631\u064e\u0629\u065e", "word_translation": "refuse", "word_transliteration": "munkiratun"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(are) arrogant", "word_transliteration": "mus'takbiruna"}]}