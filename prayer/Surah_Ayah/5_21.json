{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0671\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0671\u0644\u06e1\u0645\u064f\u0642\u064e\u062f\u0651\u064e\u0633\u064e\u0629\u064e \u0671\u0644\u0651\u064e\u062a\u0650\u064a \u0643\u064e\u062a\u064e\u0628\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0631\u06e1\u062a\u064e\u062f\u0651\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u062f\u06e1\u0628\u064e\u0627\u0631\u0650\u0643\u064f\u0645\u06e1 \u0641\u064e\u062a\u064e\u0646\u0642\u064e\u0644\u0650\u0628\u064f\u0648\u0627\u0652 \u062e\u064e\u0670\u0633\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "O my people", "word_transliteration": "yaqawmi"}, {"word_arabic_uthmani": "\u0671\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "Enter", "word_transliteration": "ud'khulu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "the land", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0642\u064e\u062f\u0651\u064e\u0633\u064e\u0629\u064e", "word_translation": "the Holy", "word_transliteration": "al-muqadasata"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u062a\u0650\u064a", "word_translation": "which", "word_transliteration": "allati"}, {"word_arabic_uthmani": "\u0643\u064e\u062a\u064e\u0628\u064e", "word_translation": "(has been) ordained", "word_transliteration": "kataba"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(by) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u06e1\u062a\u064e\u062f\u0651\u064f\u0648\u0627\u0652", "word_translation": "turn", "word_transliteration": "tartaddu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064e\u062f\u06e1\u0628\u064e\u0627\u0631\u0650\u0643\u064f\u0645\u06e1", "word_translation": "your backs", "word_transliteration": "adbarikum"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0646\u0642\u064e\u0644\u0650\u0628\u064f\u0648\u0627\u0652", "word_translation": "then you will turn back", "word_transliteration": "fatanqalibu"}, {"word_arabic_uthmani": "\u062e\u064e\u0670\u0633\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(as) losers", "word_transliteration": "khasirina"}]}