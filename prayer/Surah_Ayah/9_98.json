{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0631\u064e\u0627\u0628\u0650 \u0645\u064e\u0646 \u064a\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u064f \u0645\u064e\u0627 \u064a\u064f\u0646\u0641\u0650\u0642\u064f \u0645\u064e\u063a\u06e1\u0631\u064e\u0645\u0657\u0627 \u0648\u064e\u064a\u064e\u062a\u064e\u0631\u064e\u0628\u0651\u064e\u0635\u064f \u0628\u0650\u0643\u064f\u0645\u064f \u0671\u0644\u062f\u0651\u064e\u0648\u064e\u0627\u0653\u0626\u0650\u0631\u064e\u06da \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u062f\u064e\u0627\u0653\u0626\u0650\u0631\u064e\u0629\u064f \u0671\u0644\u0633\u0651\u064e\u0648\u06e1\u0621\u0650\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0633\u064e\u0645\u0650\u064a\u0639\u064c \u0639\u064e\u0644\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e", "word_translation": "And among", "word_transliteration": "wamina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0631\u064e\u0627\u0628\u0650", "word_translation": "the bedouins", "word_transliteration": "al-a'rabi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(is he) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u064f", "word_translation": "takes", "word_transliteration": "yattakhidhu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064f\u0646\u0641\u0650\u0642\u064f", "word_translation": "he spends", "word_transliteration": "yunfiqu"}, {"word_arabic_uthmani": "\u0645\u064e\u063a\u06e1\u0631\u064e\u0645\u0657\u0627", "word_translation": "(as) a loss", "word_transliteration": "maghraman"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062a\u064e\u0631\u064e\u0628\u0651\u064e\u0635\u064f", "word_translation": "and he awaits", "word_transliteration": "wayatarabbasu"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064f\u0645\u064f", "word_translation": "for you", "word_transliteration": "bikumu"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064e\u0648\u064e\u0627\u0653\u0626\u0650\u0631\u064e\u06da", "word_translation": "the turns (of misfortune)", "word_transliteration": "al-dawaira"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "Upon them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0653\u0626\u0650\u0631\u064e\u0629\u064f", "word_translation": "(will be) the turn", "word_transliteration": "dairatu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0648\u06e1\u0621\u0650\u06d7", "word_translation": "(of) the evil", "word_transliteration": "al-sawi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0633\u064e\u0645\u0650\u064a\u0639\u064c", "word_translation": "(is) All-Hearer", "word_transliteration": "sami'un"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u065e", "word_translation": "All-Knower", "word_transliteration": "alimun"}]}