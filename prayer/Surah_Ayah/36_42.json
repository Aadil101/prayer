{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627 \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u0645\u0651\u0650\u062b\u06e1\u0644\u0650\u0647\u0650\u06e6 \u0645\u064e\u0627 \u064a\u064e\u0631\u06e1\u0643\u064e\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627", "word_translation": "And We created", "word_transliteration": "wakhalaqna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u062b\u06e1\u0644\u0650\u0647\u0650\u06e6", "word_translation": "(the) likes of it", "word_transliteration": "mith'lihi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u0643\u064e\u0628\u064f\u0648\u0646\u064e", "word_translation": "they ride", "word_transliteration": "yarkabuna"}]}