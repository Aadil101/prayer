{"ayahs": {"ayah_uthmani": "\u0645\u064e\u0627 \u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u064e\u0627\u0653", "word_translation": "We created both of them", "word_transliteration": "khalaqnahuma"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "in [the] truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e", "word_translation": "but", "word_transliteration": "walakinna"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "most of them", "word_transliteration": "aktharahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "know", "word_transliteration": "ya'lamuna"}]}