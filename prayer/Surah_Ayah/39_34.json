{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u064e\u0627 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u0648\u0646\u064e \u0639\u0650\u0646\u062f\u064e \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u062c\u064e\u0632\u064e\u0627\u0653\u0621\u064f \u0671\u0644\u06e1\u0645\u064f\u062d\u06e1\u0633\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "For them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "(is) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u0648\u0646\u064e", "word_translation": "they wish", "word_transliteration": "yashauna"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "with", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1\u06da", "word_translation": "their Lord", "word_transliteration": "rabbihim"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u062c\u064e\u0632\u064e\u0627\u0653\u0621\u064f", "word_translation": "(is the) reward", "word_transliteration": "jazau"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u062d\u06e1\u0633\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "(of) the good-doers", "word_transliteration": "al-muh'sinina"}]}