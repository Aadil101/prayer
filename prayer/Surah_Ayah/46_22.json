{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u062c\u0650\u0626\u06e1\u062a\u064e\u0646\u064e\u0627 \u0644\u0650\u062a\u064e\u0623\u06e1\u0641\u0650\u0643\u064e\u0646\u064e\u0627 \u0639\u064e\u0646\u06e1 \u0621\u064e\u0627\u0644\u0650\u0647\u064e\u062a\u0650\u0646\u064e\u0627 \u0641\u064e\u0623\u06e1\u062a\u0650\u0646\u064e\u0627 \u0628\u0650\u0645\u064e\u0627 \u062a\u064e\u0639\u0650\u062f\u064f\u0646\u064e\u0627\u0653 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u062f\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u0650\u0626\u06e1\u062a\u064e\u0646\u064e\u0627", "word_translation": "Have you come to us", "word_transliteration": "aji'tana"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064e\u0623\u06e1\u0641\u0650\u0643\u064e\u0646\u064e\u0627", "word_translation": "to turn us away", "word_transliteration": "litafikana"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1", "word_translation": "from", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u0650\u0647\u064e\u062a\u0650\u0646\u064e\u0627", "word_translation": "our gods", "word_transliteration": "alihatina"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u06e1\u062a\u0650\u0646\u064e\u0627", "word_translation": "Then bring us", "word_transliteration": "fatina"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u0650\u062f\u064f\u0646\u064e\u0627\u0653", "word_translation": "you threaten us", "word_transliteration": "ta'iduna"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064e", "word_translation": "you are", "word_transliteration": "kunta"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u062f\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "the truthful", "word_transliteration": "al-sadiqina"}]}