{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1 \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u062a\u064e\u0642\u064e\u0648\u0651\u064e\u0644\u064e\u0647\u064f\u06e5\u06da \u0628\u064e\u0644 \u0644\u0651\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "(do) they say", "word_transliteration": "yaquluna"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u064e\u0648\u0651\u064e\u0644\u064e\u0647\u064f\u06e5\u06da", "word_translation": "He has made it up", "word_transliteration": "taqawwalahu"}, {"word_arabic_uthmani": "\u0628\u064e\u0644", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "they believe", "word_transliteration": "yu'minuna"}]}