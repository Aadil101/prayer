{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0643\u064e \u0623\u064e\u0646\u062a\u064e \u0648\u064e\u0644\u0650\u064a\u0651\u064f\u0646\u064e\u0627 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650\u0647\u0650\u0645\u06d6 \u0628\u064e\u0644\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e \u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u064e\u06d6 \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645 \u0628\u0650\u0647\u0650\u0645 \u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They will say", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0643\u064e", "word_translation": "Glory be to You", "word_transliteration": "sub'hanaka"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "You", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u0651\u064f\u0646\u064e\u0627", "word_translation": "(are) our Protector", "word_transliteration": "waliyyuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "not them", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650\u0647\u0650\u0645\u06d6", "word_translation": "not them", "word_transliteration": "dunihim"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they used", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e", "word_translation": "(to) worship", "word_transliteration": "ya'buduna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u064e\u06d6", "word_translation": "the jinn", "word_transliteration": "al-jina"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645", "word_translation": "most of them", "word_transliteration": "aktharuhum"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u0645", "word_translation": "in them", "word_transliteration": "bihim"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "(were) believers", "word_transliteration": "mu'minuna"}]}