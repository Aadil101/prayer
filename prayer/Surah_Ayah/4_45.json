{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0628\u0650\u0623\u064e\u0639\u06e1\u062f\u064e\u0627\u0653\u0626\u0650\u0643\u064f\u0645\u06e1\u06da \u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0644\u0650\u064a\u0651\u0657\u0627 \u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0646\u064e\u0635\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "knows better", "word_transliteration": "a'lamu"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0639\u06e1\u062f\u064e\u0627\u0653\u0626\u0650\u0643\u064f\u0645\u06e1\u06da", "word_translation": "about your enemies", "word_transliteration": "bi-a'daikum"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "and (is) sufficient", "word_transliteration": "wakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u0651\u0657\u0627", "word_translation": "(as) a Protector", "word_transliteration": "waliyyan"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "and sufficient", "word_transliteration": "wakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(is) Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u0650\u064a\u0631\u0657\u0627", "word_translation": "(as) a Helper", "word_transliteration": "nasiran"}]}