{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0627 \u0645\u064f\u0648\u0633\u064e\u0649 \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e \u0641\u064e\u0671\u062e\u06e1\u062a\u064f\u0644\u0650\u0641\u064e \u0641\u0650\u064a\u0647\u0650\u06da \u0648\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627 \u0643\u064e\u0644\u0650\u0645\u064e\u0629\u065e \u0633\u064e\u0628\u064e\u0642\u064e\u062a\u06e1 \u0645\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064e \u0644\u064e\u0642\u064f\u0636\u0650\u064a\u064e \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1\u06da \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0641\u0650\u064a \u0634\u064e\u0643\u0651\u0656 \u0645\u0651\u0650\u0646\u06e1\u0647\u064f \u0645\u064f\u0631\u0650\u064a\u0628\u0656"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And verily", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "We gave", "word_transliteration": "atayna"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e", "word_translation": "the Book", "word_transliteration": "al-kitaba"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u062e\u06e1\u062a\u064f\u0644\u0650\u0641\u064e", "word_translation": "but differences arose", "word_transliteration": "fa-ukh'tulifa"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650\u06da", "word_translation": "therein", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "And if not", "word_transliteration": "walawla"}, {"word_arabic_uthmani": "\u0643\u064e\u0644\u0650\u0645\u064e\u0629\u065e", "word_translation": "(for) a Word", "word_transliteration": "kalimatun"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u064e\u0642\u064e\u062a\u06e1", "word_translation": "(that) preceded", "word_transliteration": "sabaqat"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0644\u064e\u0642\u064f\u0636\u0650\u064a\u064e", "word_translation": "surely would have been judged", "word_transliteration": "laqudiya"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "between them", "word_transliteration": "baynahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "And indeed they", "word_transliteration": "wa-innahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0641\u0650\u064a", "word_translation": "surely (are) in", "word_transliteration": "lafi"}, {"word_arabic_uthmani": "\u0634\u064e\u0643\u0651\u0656", "word_translation": "doubt", "word_transliteration": "shakkin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f", "word_translation": "concerning it", "word_transliteration": "min'hu"}, {"word_arabic_uthmani": "\u0645\u064f\u0631\u0650\u064a\u0628\u0656", "word_translation": "suspicious", "word_transliteration": "muribin"}]}