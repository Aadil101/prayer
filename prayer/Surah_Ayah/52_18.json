{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0670\u0643\u0650\u0647\u0650\u064a\u0646\u064e \u0628\u0650\u0645\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645\u06e1 \u0631\u064e\u0628\u0651\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0648\u064e\u0642\u064e\u0649\u0670\u0647\u064f\u0645\u06e1 \u0631\u064e\u0628\u0651\u064f\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064e \u0671\u0644\u06e1\u062c\u064e\u062d\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0670\u0643\u0650\u0647\u0650\u064a\u0646\u064e", "word_translation": "Enjoying", "word_transliteration": "fakihina"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627\u0653", "word_translation": "in what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645\u06e1", "word_translation": "has given them", "word_transliteration": "atahum"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their Lord", "word_transliteration": "rabbuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0648\u064e\u0642\u064e\u0649\u0670\u0647\u064f\u0645\u06e1", "word_translation": "and protected them", "word_transliteration": "wawaqahum"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their Lord", "word_transliteration": "rabbuhum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064e", "word_translation": "(from the) punishment", "word_transliteration": "adhaba"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u062d\u0650\u064a\u0645\u0650", "word_translation": "(of) Hellfire", "word_transliteration": "al-jahimi"}]}