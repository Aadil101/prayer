ion": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f", "word_translation": "(is) the All-Hearer", "word_transliteration": "al-sami'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0635\u0650\u064a\u0631\u064f", "word_translation": "the All-Seer", "word_transliteration": "al-basiru"}]}