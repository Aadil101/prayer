{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650 \u0641\u064e\u062a\u064e\u0647\u064e\u062c\u0651\u064e\u062f\u06e1 \u0628\u0650\u0647\u0650\u06e6 \u0646\u064e\u0627\u0641\u0650\u0644\u064e\u0629\u0657 \u0644\u0651\u064e\u0643\u064e \u0639\u064e\u0633\u064e\u0649\u0670\u0653 \u0623\u064e\u0646 \u064a\u064e\u0628\u06e1\u0639\u064e\u062b\u064e\u0643\u064e \u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0645\u064e\u0642\u064e\u0627\u0645\u0657\u0627 \u0645\u0651\u064e\u062d\u06e1\u0645\u064f\u0648\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e", "word_translation": "And from", "word_transliteration": "wamina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650", "word_translation": "the night", "word_transliteration": "al-layli"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0647\u064e\u062c\u0651\u064e\u062f\u06e1", "word_translation": "arise from sleep for prayer", "word_transliteration": "fatahajjad"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u0641\u0650\u0644\u064e\u0629\u0657", "word_translation": "(as) additional", "word_transliteration": "nafilatan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0643\u064e", "word_translation": "for you", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0639\u064e\u0633\u064e\u0649\u0670\u0653", "word_translation": "it may be", "word_transliteration": "asa"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u0628\u06e1\u0639\u064e\u062b\u064e\u0643\u064e", "word_translation": "will raise you", "word_transliteration": "yab'athaka"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbuka"}, {"word_arabic_uthmani": "\u0645\u064e\u0642\u064e\u0627\u0645\u0657\u0627", "word_translation": "(to) a station", "word_transliteration": "maqaman"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u062d\u06e1\u0645\u064f\u0648\u062f\u0657\u0627", "word_translation": "praiseworthy", "word_transliteration": "mahmudan"}]}