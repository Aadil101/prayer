{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0641\u064e\u063a\u064e\u064a\u06e1\u0631\u064e \u062f\u0650\u064a\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u064a\u064e\u0628\u06e1\u063a\u064f\u0648\u0646\u064e \u0648\u064e\u0644\u064e\u0647\u064f\u06e5\u0653 \u0623\u064e\u0633\u06e1\u0644\u064e\u0645\u064e \u0645\u064e\u0646 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0637\u064e\u0648\u06e1\u0639\u0657\u0627 \u0648\u064e\u0643\u064e\u0631\u06e1\u0647\u0657\u0627 \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u064a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u063a\u064e\u064a\u06e1\u0631\u064e", "word_translation": "So is (it) other than", "word_transliteration": "afaghayra"}, {"word_arabic_uthmani": "\u062f\u0650\u064a\u0646\u0650", "word_translation": "(the) religion", "word_transliteration": "dini"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u064a\u064e\u0628\u06e1\u063a\u064f\u0648\u0646\u064e", "word_translation": "they seek", "word_transliteration": "yabghuna"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u06e5\u0653", "word_translation": "While to Him", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u0644\u064e\u0645\u064e", "word_translation": "(have) submitted", "word_transliteration": "aslama"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whatever", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0637\u064e\u0648\u06e1\u0639\u0657\u0627", "word_translation": "willingly", "word_transliteration": "taw'an"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0631\u06e1\u0647\u0657\u0627", "word_translation": "or unwillingly", "word_transliteration": "wakarhan"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "and towards Him", "word_transliteration": "wa-ilayhi"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "they will be returned", "word_transliteration": "yur'ja'una"}]}