{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u062a\u064e\u0639\u06e1\u062c\u064e\u0628\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u06e1 \u0623\u064e\u0645\u06e1\u0631\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6 \u0631\u064e\u062d\u06e1\u0645\u064e\u062a\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0628\u064e\u0631\u064e\u0643\u064e\u0670\u062a\u064f\u0647\u064f\u06e5 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u0623\u064e\u0647\u06e1\u0644\u064e \u0671\u0644\u06e1\u0628\u064e\u064a\u06e1\u062a\u0650\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u062d\u064e\u0645\u0650\u064a\u062f\u065e \u0645\u0651\u064e\u062c\u0650\u064a\u062f\u065e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064e\u0639\u06e1\u062c\u064e\u0628\u0650\u064a\u0646\u064e", "word_translation": "Are you amazed", "word_transliteration": "ata'jabina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "at", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0631\u0650", "word_translation": "(the) decree of Allah", "word_transliteration": "amri"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6", "word_translation": "(the) decree of Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0631\u064e\u062d\u06e1\u0645\u064e\u062a\u064f", "word_translation": "The Mercy of Allah", "word_transliteration": "rahmatu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "The Mercy of Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u0631\u064e\u0643\u064e\u0670\u062a\u064f\u0647\u064f\u06e5", "word_translation": "and His blessings", "word_transliteration": "wabarakatuhu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "(be) upon you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064e", "word_translation": "people", "word_transliteration": "ahla"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u064a\u06e1\u062a\u0650\u06da", "word_translation": "(of) the house", "word_transliteration": "al-bayti"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed, He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u062d\u064e\u0645\u0650\u064a\u062f\u065e", "word_translation": "(is) All-Praiseworthy", "word_transliteration": "hamidun"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u062c\u0650\u064a\u062f\u065e", "word_translation": "All-Glorious", "word_transliteration": "majidun"}]}