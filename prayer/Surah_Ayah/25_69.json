{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0636\u064e\u0670\u0639\u064e\u0641\u06e1 \u0644\u064e\u0647\u064f \u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u064f \u064a\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650 \u0648\u064e\u064a\u064e\u062e\u06e1\u0644\u064f\u062f\u06e1 \u0641\u0650\u064a\u0647\u0650\u06e6 \u0645\u064f\u0647\u064e\u0627\u0646\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0636\u064e\u0670\u0639\u064e\u0641\u06e1", "word_translation": "Will be doubled", "word_transliteration": "yuda'af"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f", "word_translation": "for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u064f", "word_translation": "the punishment", "word_transliteration": "al-'adhabu"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(on the) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650", "word_translation": "(of) Resurrection", "word_transliteration": "al-qiyamati"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062e\u06e1\u0644\u064f\u062f\u06e1", "word_translation": "and he will abide forever", "word_transliteration": "wayakhlud"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650\u06e6", "word_translation": "therein", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u0645\u064f\u0647\u064e\u0627\u0646\u064b\u0627", "word_translation": "humiliated", "word_transliteration": "muhanan"}]}