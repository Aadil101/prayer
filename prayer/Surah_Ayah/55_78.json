{"ayahs": {"ayah_uthmani": "\u062a\u064e\u0628\u064e\u0670\u0631\u064e\u0643\u064e \u0671\u0633\u06e1\u0645\u064f \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0630\u0650\u064a \u0671\u0644\u06e1\u062c\u064e\u0644\u064e\u0670\u0644\u0650 \u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0643\u06e1\u0631\u064e\u0627\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u062a\u064e\u0628\u064e\u0670\u0631\u064e\u0643\u064e", "word_translation": "Blessed is", "word_transliteration": "tabaraka"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u0645\u064f", "word_translation": "(the) name", "word_transliteration": "us'mu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "(of) your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0630\u0650\u064a", "word_translation": "Owner", "word_transliteration": "dhi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0644\u064e\u0670\u0644\u0650", "word_translation": "(of) Majesty", "word_transliteration": "al-jalali"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0643\u06e1\u0631\u064e\u0627\u0645\u0650", "word_translation": "and Honor", "word_transliteration": "wal-ik'rami"}]}