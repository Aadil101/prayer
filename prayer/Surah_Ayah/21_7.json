{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627 \u0642\u064e\u0628\u06e1\u0644\u064e\u0643\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0631\u0650\u062c\u064e\u0627\u0644\u0657\u0627 \u0646\u0651\u064f\u0648\u062d\u0650\u064a\u0653 \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1\u06d6 \u0641\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0647\u06e1\u0644\u064e \u0671\u0644\u0630\u0651\u0650\u0643\u06e1\u0631\u0650 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u0644\u064e\u0627 \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We sent", "word_transliteration": "arsalna"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e\u0643\u064e", "word_translation": "before you", "word_transliteration": "qablaka"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0631\u0650\u062c\u064e\u0627\u0644\u0657\u0627", "word_translation": "men", "word_transliteration": "rijalan"}, {"word_arabic_uthmani": "\u0646\u0651\u064f\u0648\u062d\u0650\u064a\u0653", "word_translation": "We revealed", "word_transliteration": "nuhi"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1\u06d6", "word_translation": "to them", "word_transliteration": "ilayhim"}, {"word_arabic_uthmani": "\u0641\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "So ask", "word_transliteration": "fasalu"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064e", "word_translation": "(the) people", "word_tr