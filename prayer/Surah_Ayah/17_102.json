{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0644\u064e\u0642\u064e\u062f\u06e1 \u0639\u064e\u0644\u0650\u0645\u06e1\u062a\u064e \u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u0632\u064e\u0644\u064e \u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650 \u0625\u0650\u0644\u0651\u064e\u0627 \u0631\u064e\u0628\u0651\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0628\u064e\u0635\u064e\u0627\u0653\u0626\u0650\u0631\u064e \u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a \u0644\u064e\u0623\u064e\u0638\u064f\u0646\u0651\u064f\u0643\u064e \u064a\u064e\u0670\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064f \u0645\u064e\u062b\u06e1\u0628\u064f\u0648\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "Verily", "word_transliteration": "laqad"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u0645\u06e1\u062a\u064e", "word_translation": "you know", "word_transliteration": "alim'ta"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "none", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0632\u064e\u0644\u064e", "word_translation": "has sent down", "word_transliteration": "anzala"}, {"word_arabic_uthmani": "\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650", "word_translation": "these", "word_transliteration": "haulai"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f", "word_translation": "(the) Lord", "word_transliteration": "rabbu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u064e\u0627\u0653\u0626\u0650\u0631\u064e", "word_translation": "(as) evidence", "word_transliteration": "basaira"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "and indeed I", "word_transliteration": "wa-inni"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u064e\u0638\u064f\u0646\u0651\u064f\u0643\u064e", "word_translation": "[I] surely think you", "word_transliteration": "la-azunnuka"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064f", "word_translation": "O Firaun!", "word_transliteration": "yafir'awnu"}, {"word_arabic_uthmani": "\u0645\u064e\u062b\u06e1\u0628\u064f\u0648\u0631\u0657\u0627", "word_translation": "(you are) destroyed", "word_transliteration": "mathburan"}]}