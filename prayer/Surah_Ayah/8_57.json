{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0645\u0651\u064e\u0627 \u062a\u064e\u062b\u06e1\u0642\u064e\u0641\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u062d\u064e\u0631\u06e1\u0628\u0650 \u0641\u064e\u0634\u064e\u0631\u0651\u0650\u062f\u06e1 \u0628\u0650\u0647\u0650\u0645 \u0645\u0651\u064e\u0646\u06e1 \u062e\u064e\u0644\u06e1\u0641\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0630\u0651\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0645\u0651\u064e\u0627", "word_translation": "So if", "word_transliteration": "fa-imma"}, {"word_arabic_uthmani": "\u062a\u064e\u062b\u06e1\u0642\u064e\u0641\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "you gain dominance over them", "word_transliteration": "tathqafannahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0631\u06e1\u0628\u0650", "word_translation": "the war", "word_transliteration": "al-harbi"}, {"word_arabic_uthmani": "\u0641\u064e\u0634\u064e\u0631\u0651\u0650\u062f\u06e1", "word_translation": "disperse", "word_transliteration": "fasharrid"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u0645", "word_translation": "by them", "word_transliteration": "bihim"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646\u06e1", "word_translation": "(those) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "(are) behind them", "word_transliteration": "khalfahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so that they may", "word_transliteration": "la'allahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0630\u0651\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "take heed", "word_transliteration": "yadhakkaruna"}]}