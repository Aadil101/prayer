{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0631\u064e\u0628\u0651\u064f \u0671\u0644\u06e1\u0645\u064e\u0634\u06e1\u0631\u0650\u0642\u0650 \u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u063a\u06e1\u0631\u0650\u0628\u0650 \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u0653\u06d6 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u0639\u06e1\u0642\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f", "word_translation": "Lord", "word_transliteration": "rabbu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0634\u06e1\u0631\u0650\u0642\u0650", "word_translation": "(of) the east", "word_transliteration": "al-mashriqi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u063a\u06e1\u0631\u0650\u0628\u0650", "word_translation": "and the west", "word_transliteration": "wal-maghribi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u0653\u06d6", "word_translation": "(is) between them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you were", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0642\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "(to) reason", "word_transliteration": "ta'qiluna"}]}