{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u0643\u064f\u062a\u064f\u0628\u0656 \u064a\u064e\u062f\u06e1\u0631\u064f\u0633\u064f\u0648\u0646\u064e\u0647\u064e\u0627\u06d6 \u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0642\u064e\u0628\u06e1\u0644\u064e\u0643\u064e \u0645\u0650\u0646 \u0646\u0651\u064e\u0630\u0650\u064a\u0631\u0656"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f\u0645", "word_translation": "We (had) given them", "word_transliteration": "ataynahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0643\u064f\u062a\u064f\u0628\u0656", "word_translation": "Scriptures", "word_transliteration": "kutubin"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u06e1\u0631\u064f\u0633\u064f\u0648\u0646\u064e\u0647\u064e\u0627\u06d6", "word_translation": "which they could study", "word_transliteration": "yadrusunaha"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627\u0653", "word_translation": "We sent", "word_transliteration": "arsalna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "to them", "word_transliteration": "ilayhim"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e\u0643\u064e", "word_translation": "before you", "word_transliteration": "qablaka"}, {"word_arabic_uthmani": "\u0645\u0650\u064