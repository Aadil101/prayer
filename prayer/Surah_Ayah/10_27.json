{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0633\u064e\u0628\u064f\u0648\u0627\u0652 \u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0640\u0654\u064e\u0627\u062a\u0650 \u062c\u064e\u0632\u064e\u0627\u0653\u0621\u064f \u0633\u064e\u064a\u0651\u0650\u0626\u064e\u0629\u0650\u06ed \u0628\u0650\u0645\u0650\u062b\u06e1\u0644\u0650\u0647\u064e\u0627 \u0648\u064e\u062a\u064e\u0631\u06e1\u0647\u064e\u0642\u064f\u0647\u064f\u0645\u06e1 \u0630\u0650\u0644\u0651\u064e\u0629\u065e\u06d6 \u0645\u0651\u064e\u0627 \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0645\u0650\u0646\u06e1 \u0639\u064e\u0627\u0635\u0650\u0645\u0656\u06d6 \u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0645\u064e\u0627\u0653 \u0623\u064f\u063a\u06e1\u0634\u0650\u064a\u064e\u062a\u06e1 \u0648\u064f\u062c\u064f\u0648\u0647\u064f\u0647\u064f\u0645\u06e1 \u0642\u0650\u0637\u064e\u0639\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650 \u0645\u064f\u0638\u06e1\u0644\u0650\u0645\u064b\u0627\u06da \u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064f \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650\u06d6 \u0647\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u064e\u0627 \u062e\u064e\u0670\u0644\u0650\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "And those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0633\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "earned", "word_transliteration": "kasabu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0640\u0654\u064e\u0627\u062a\u0650", "word_translation": "the evil deeds", "word_transliteration": "al-sayiati"}, {"word_arabic_uthmani": "\u062c\u064e\u0632\u064e\u0627\u0653\u0621\u064f", "word_translation": "(the) recompense", "word_transliteration": "jazau"}, {"word_arabic_uthmani": "\u0633\u064e\u064a\u0651\u0650\u0626\u064e\u0629\u0650\u06ed", "word_translation": "(of) an evil deed", "word_transliteration": "sayyi-atin"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u0650\u062b\u06e1\u0644\u0650\u0647\u064e\u0627", "word_translation": "(is) like it", "word_transliteration": "bimith'liha"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0631\u06e1\u0647\u064e\u0642\u064f\u0647\u064f\u0645\u06e1", "word_translation": "and (will) cover them", "word_transliteration": "watarhaquhum"}, {"word_arabic_uthmani": "\u0630\u0650\u0644\u0651\u064e\u0629\u065e\u06d6", "word_translation": "humiliation", "word_transliteration": "dhillatun"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "They will not have", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "They will not have", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u064e\u0627\u0635\u0650\u0645\u0656\u06d6", "word_translation": "defender", "word_transliteration": "asimin"}, {"word_arabic_uthmani": "\u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0645\u064e\u0627\u0653", "word_translation": "As if", "word_transliteration": "ka-annama"}, {"word_arabic_uthmani": "\u0623\u064f\u063a\u06e1\u0634\u0650\u064a\u064e\u062a\u06e1", "word_translation": "had been covered", "word_transliteration": "ugh'shiyat"}, {"word_arabic_uthmani": "\u0648\u064f\u062c\u064f\u0648\u0647\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their faces", "word_transliteration": "wujuhuhum"}, {"word_arabic_uthmani": "\u0642\u0650\u0637\u064e\u0639\u0657\u0627", "word_translation": "(with) pieces", "word_transliteration": "qita'an"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650", "word_translation": "the darkness (of) night", "word_transliteration": "al-layli"}, {"word_arabic_uthmani": "\u0645\u064f\u0638\u06e1\u0644\u0650\u0645\u064b\u0627\u06da", "word_translation": "the darkness (of) night", "word_transliteration": "muz'liman"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064f", "word_translation": "(are the) companions", "word_transliteration": "ashabu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650\u06d6", "word_translation": "(of) the Fire", "word_transliteration": "al-nari"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "in it", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u062e\u064e\u0670\u0644\u0650\u062f\u064f\u0648\u0646\u064e", "word_translation": "(will) abide forever", "word_transliteration": "khaliduna"}]}