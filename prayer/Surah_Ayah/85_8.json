{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0646\u064e\u0642\u064e\u0645\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0625\u0650\u0644\u0651\u064e\u0627\u0653 \u0623\u064e\u0646 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0627\u0652 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u0650 \u0671\u0644\u06e1\u062d\u064e\u0645\u0650\u064a\u062f\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0646\u064e\u0642\u064e\u0645\u064f\u0648\u0627\u0652", "word_translation": "they resented", "word_transliteration": "naqamu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "[of] them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627\u0653", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0627\u0652", "word_translation": "they believed", "word_transliteration": "yu'minu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "in Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u0650", "word_translation": "the All-Mighty", "word_transliteration": "al-'azizi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0645\u0650\u064a\u062f\u0650", "word_translation": "the Praiseworthy", "word_transliteration": "al-hamidi"}]}