{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0625\u0650\u0646\u0651\u0650\u064a \u0645\u064f\u0646\u064e\u0632\u0651\u0650\u0644\u064f\u0647\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1\u06d6 \u0641\u064e\u0645\u064e\u0646 \u064a\u064e\u0643\u06e1\u0641\u064f\u0631\u06e1 \u0628\u064e\u0639\u06e1\u062f\u064f \u0645\u0650\u0646\u0643\u064f\u0645\u06e1 \u0641\u064e\u0625\u0650\u0646\u0651\u0650\u064a\u0653 \u0623\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0647\u064f\u06e5 \u0639\u064e\u0630\u064e\u0627\u0628\u0657\u0627 \u0644\u0651\u064e\u0627\u0653 \u0623\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0647\u064f\u06e5\u0653 \u0623\u064e\u062d\u064e\u062f\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0645\u064f\u0646\u064e\u0632\u0651\u0650\u0644\u064f\u0647\u064e\u0627", "word_translation": "(will) send it down", "word_transliteration": "munazziluha"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "to you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0646", "word_translation": "then whoever", "word_transliteration": "faman"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u06e1\u0641\u064f\u0631\u06e1", "word_translation": "disbelieves", "word_transliteration": "yakfur"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u064f", "word_translation": "after (that)", "word_transliteration": "ba'du"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0643\u064f\u0645\u06e1", "word_translation": "among you", "word_transliteration": "minkum"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "then indeed I", "word_transliteration": "fa-inni"}, {"word_arabic_uthmani": "\u0623\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0647\u064f\u06e5", "word_translation": "[I] will punish him", "word_transliteration": "u'adhibuhu"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u0657\u0627", "word_translation": "(with) a punishment", "word_transliteration": "adhaban"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627\u0653", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0623\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0647\u064f\u06e5\u0653", "word_translation": "I have punished", "word_transliteration": "u'adhibuhu"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u0657\u0627", "word_translation": "anyone", "word_transliteration": "ahadan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "among", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "the worlds", "word_transliteration": "al-'alamina"}]}