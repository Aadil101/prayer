{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1 \u0645\u064e\u0643\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u064e\u0643\u06e1\u0631\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0645\u064e\u0643\u06e1\u0631\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0625\u0650\u0646 \u0643\u064e\u0627\u0646\u064e \u0645\u064e\u0643\u06e1\u0631\u064f\u0647\u064f\u0645\u06e1 \u0644\u0650\u062a\u064e\u0632\u064f\u0648\u0644\u064e \u0645\u0650\u0646\u06e1\u0647\u064f \u0671\u0644\u06e1\u062c\u0650\u0628\u064e\u0627\u0644\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And indeed", "word_transliteration": "waqad"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "they planned", "word_transliteration": "makaru"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u06e1\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their plan", "word_transliteration": "makrahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u0650\u0646\u062f\u064e", "word_translation": "but with", "word_transliteration": "wa'inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u06e1\u0631\u064f\u0647\u064f\u0645\u06e1", "word_translation": "(was) their plan", "word_transliteration": "makruhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "even if", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "was", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u06e1\u0631\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their plan", "word_transliteration": "makruhum"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064e\u0632\u064f\u0648\u0644\u064e", "word_translation":