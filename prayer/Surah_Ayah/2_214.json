{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1 \u062d\u064e\u0633\u0650\u0628\u06e1\u062a\u064f\u0645\u06e1 \u0623\u064e\u0646 \u062a\u064e\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e \u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u064a\u064e\u0623\u06e1\u062a\u0650\u0643\u064f\u0645 \u0645\u0651\u064e\u062b\u064e\u0644\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u062e\u064e\u0644\u064e\u0648\u06e1\u0627\u0652 \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0643\u064f\u0645\u06d6 \u0645\u0651\u064e\u0633\u0651\u064e\u062a\u06e1\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0628\u064e\u0623\u06e1\u0633\u064e\u0627\u0653\u0621\u064f \u0648\u064e\u0671\u0644\u0636\u0651\u064e\u0631\u0651\u064e\u0627\u0653\u0621\u064f \u0648\u064e\u0632\u064f\u0644\u06e1\u0632\u0650\u0644\u064f\u0648\u0627\u0652 \u062d\u064e\u062a\u0651\u064e\u0649\u0670 \u064a\u064e\u0642\u064f\u0648\u0644\u064e \u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064f \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0645\u064e\u0639\u064e\u0647\u064f\u06e5 \u0645\u064e\u062a\u064e\u0649\u0670 \u0646\u064e\u0635\u06e1\u0631\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d7 \u0623\u064e\u0644\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e \u0646\u064e\u0635\u06e1\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0642\u064e\u0631\u0650\u064a\u0628\u065e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u062d\u064e\u0633\u0650\u0628\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "(do) you think", "word_transliteration": "hasib'tum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "you will enter", "word_transliteration": "tadkhulu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e", "word_translation": "Paradise", "word_transliteration": "al-janata"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "while not", "word_transliteration": "walamma"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650\u0643\u064f\u0645", "word_translation": "(has) come to you", "word_transliteration": "yatikum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u062b\u064e\u0644\u064f", "word_translation": "like (came to)", "word_transliteration": "mathalu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0648\u06e1\u0627\u0652", "word_translation": "passed away", "word_transliteration": "khalaw"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0643\u064f\u0645\u06d6", "word_translation": "before you", "word_transliteration": "qablikum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0633\u0651\u064e\u062a\u06e1\u0647\u064f\u0645\u064f", "word_translation": "Touched them", "word_transliteration": "massathumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0623\u06e1\u0633\u064e\u0627\u0653\u0621\u064f", "word_translation": "[the] adversity", "word_transliteration": "al-basau"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0636\u0651\u064e\u0631\u0651\u064e\u0627\u0653\u0621\u064f", "word_translation": "and [the] hardship", "word_transliteration": "wal-darau"}, {"word_arabic_uthmani": "\u0648\u064e\u0632\u064f\u0644\u06e1\u0632\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "and they were shaken", "word_transliteration": "wazul'zilu"}, {"word_arabic_uthmani": "\u062d\u064e\u062a\u0651\u064e\u0649\u0670", "word_translation": "until", "word_transliteration": "hatta"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064e", "word_translation": "said", "word_transliteration": "yaqula"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064f", "word_translation": "the Messenger", "word_transliteration": "al-rasulu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believed", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e\u0647\u064f\u06e5", "word_translation": "with him", "word_transliteration": "ma'ahu"}, {"word_arabic_uthmani": "\u0645\u064e\u062a\u064e\u0649\u0670", "word_translation": "When", "word_transliteration": "mata"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u06e1\u0631\u064f", "word_translation": "[will] (the) help", "word_transliteration": "nasru"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d7", "word_translation": "(of) Allah (come)", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627\u0653", "word_translation": "Unquestionably", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "[Indeed]", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u06e1\u0631\u064e", "word_translation": "help", "word_transliteration": "nasra"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u0650\u064a\u0628\u065e", "word_translation": "(is) near", "word_transliteration": "qaribun"}]}