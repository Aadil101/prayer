{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0631\u0650\u062b\u064f\u0646\u0650\u064a \u0648\u064e\u064a\u064e\u0631\u0650\u062b\u064f \u0645\u0650\u0646\u06e1 \u0621\u064e\u0627\u0644\u0650 \u064a\u064e\u0639\u06e1\u0642\u064f\u0648\u0628\u064e\u06d6 \u0648\u064e\u0671\u062c\u06e1\u0639\u064e\u0644\u06e1\u0647\u064f \u0631\u064e\u0628\u0651\u0650 \u0631\u064e\u0636\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0631\u0650\u062b\u064f\u0646\u0650\u064a", "word_translation": "Who will inherit me", "word_transliteration": "yarithuni"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0631\u0650\u062b\u064f", "word_translation": "and inherit", "word_transliteration": "wayarithu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u0650", "word_translation": "(the) family", "word_transliteration": "ali"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0642\u064f\u0648\u0628\u064e\u06d6", "word_translation": "(of) Yaqub", "word_transliteration": "ya'quba"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u062c\u06e1\u0639\u064e\u0644\u06e1\u0647\u064f", "word_translation": "And make him", "word_transliteration": "wa-ij'alhu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0631\u064e\u0636\u0650\u064a\u0651\u0657\u0627", "word_translation": "pleasing", "word_transliteration": "radiyyan"}]}