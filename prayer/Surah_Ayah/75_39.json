{"ayahs": {"ayah_uthmani": "\u0641\u064e\u062c\u064e\u0639\u064e\u0644\u064e \u0645\u0650\u0646\u06e1\u0647\u064f \u0671\u0644\u0632\u0651\u064e\u0648\u06e1\u062c\u064e\u064a\u06e1\u0646\u0650 \u0671\u0644\u0630\u0651\u064e\u0643\u064e\u0631\u064e \u0648\u064e\u0671\u0644\u06e1\u0623\u064f\u0646\u062b\u064e\u0649\u0670\u0653"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u062c\u064e\u0639\u064e\u0644\u064e", "word_translation": "Then made", "word_transliteration": "faja'ala"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f", "word_translation": "of him", "word_transliteration": "min'hu"}, {"word_arabic_uthmani": "\u0671\u0644\u0632\u0651\u064e\u0648\u06e1\u062c\u064e\u064a\u06e1\u0646\u0650", "word_translation": "two mates", "word_transliteration": "al-zawjayni"}, {"word_arabic_uthmani": "\u0671\u0644\u0630\u0651\u064e\u0643\u064e\u0631\u064e", "word_translation": "(the) male", "word_transliteration": "al-dhakara"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064f\u0646\u062b\u064e\u0649\u0670\u0653", "word_translation": "and the female", "word_transliteration": "wal-untha"}]}