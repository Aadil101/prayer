{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u0646\u0632\u064e\u0644\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650 \u0645\u064e\u0627\u0653\u0621\u0657 \u0641\u064e\u0623\u064e\u062d\u06e1\u064a\u064e\u0627 \u0628\u0650\u0647\u0650 \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0628\u064e\u0639\u06e1\u062f\u064e \u0645\u064e\u0648\u06e1\u062a\u0650\u0647\u064e\u0627\u0653\u06da \u0625\u0650\u0646\u0651\u064e \u0641\u0650\u064a \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0644\u064e\u0623\u0653\u064a\u064e\u0629\u0657 \u0644\u0651\u0650\u0642\u064e\u0648\u06e1\u0645\u0656 \u064a\u064e\u0633\u06e1\u0645\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0632\u064e\u0644\u064e", "word_translation": "sends down", "word_transliteration": "anzala"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650", "word_translation": "the sky", "word_transliteration": "al-samai"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653\u0621\u0657", "word_translation": "water", "word_transliteration": "maan"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u062d\u06e1\u064a\u064e\u0627", "word_translation": "then gives life", "word_transliteration": "fa-ahya"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650", "word_translation": "by it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "(to) the earth", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u064e", "word_translation": "after", "word_transliteration": "ba'da"}, {"word_arabic_uthmani": "\u0645\u064e\u0648\u06e1\u062a\u0650\u0647\u064e\u0627\u0653\u06da", "word_translation": "its death", "word_transliteration": "mawtiha"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u0653\u064a\u064e\u0629\u0657", "word_translation": "(is) surely a Sign", "word_transliteration": "laayatan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0642\u064e\u0648\u06e1\u0645\u0656", "word_translation": "for a people", "word_transliteration": "liqawmin"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u0645\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "who listen", "word_transliteration": "yasma'una"}]}