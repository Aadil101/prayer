{"ayahs": {"ayah_uthmani": "\u0644\u0651\u064e\u0648\u06e1\u0644\u064e\u0627\u0653 \u0623\u064e\u0646 \u062a\u064e\u062f\u064e\u0670\u0631\u064e\u0643\u064e\u0647\u064f\u06e5 \u0646\u0650\u0639\u06e1\u0645\u064e\u0629\u065e \u0645\u0651\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650\u0647\u0650\u06e6 \u0644\u064e\u0646\u064f\u0628\u0650\u0630\u064e \u0628\u0650\u0671\u0644\u06e1\u0639\u064e\u0631\u064e\u0627\u0653\u0621\u0650 \u0648\u064e\u0647\u064f\u0648\u064e \u0645\u064e\u0630\u06e1\u0645\u064f\u0648\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u064e\u0648\u06e1\u0644\u064e\u0627\u0653", "word_translation": "If not", "word_transliteration": "lawla"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u062f\u064e\u0670\u0631\u064e\u0643\u064e\u0647\u064f\u06e5", "word_translation": "overtook him", "word_transliteration": "tadarakahu"}, {"word_arabic_uthmani": "\u0646\u0650\u0639\u06e1\u0645\u064e\u0629\u065e", "word_translation": "a Favor", "word_transliteration": "ni'matun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650\u0647\u0650\u06e6", "word_translation": "his Lord", "word_transliteration": "rabbihi"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064f\u0628\u0650\u0630\u064e", "word_translation": "surely he would have been thrown", "word_transliteration": "lanubidha"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0639\u064e\u0631\u064e\u0627\u0653\u0621\u0650", "word_translation": "onto (the) naked shore", "word_transliteration": "bil-'arai"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "while he", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0645\u064e\u0630\u06e1\u0645\u064f\u0648\u0645\u065e", "word_translation": "(was) blamed", "word_transliteration": "madhmumun"}]}