{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0641\u064e\u0671\u062e\u06e1\u0631\u064f\u062c\u06e1 \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0643\u064e \u0631\u064e\u062c\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u062e\u06e1\u0631\u064f\u062c\u06e1", "word_translation": "Then get out", "word_transliteration": "fa-ukh'ruj"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "of it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "for indeed you", "word_transliteration": "fa-innaka"}, {"word_arabic_uthmani": "\u0631\u064e\u062c\u0650\u064a\u0645\u065e", "word_translation": "(are) expelled", "word_transliteration": "rajimun"}]}