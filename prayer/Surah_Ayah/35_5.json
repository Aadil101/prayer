{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f \u0625\u0650\u0646\u0651\u064e \u0648\u064e\u0639\u06e1\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u062d\u064e\u0642\u0651\u065e\u06d6 \u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u063a\u064f\u0631\u0651\u064e\u0646\u0651\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u064f \u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u063a\u064f\u0631\u0651\u064e\u0646\u0651\u064e\u0643\u064f\u0645 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u063a\u064e\u0631\u064f\u0648\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f", "word_translation": "mankind!", "word_transliteration": "al-nasu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u06e1\u062f\u064e", "word_translation": "(the) promise", "word_transliteration": "wa'da"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u062d\u064e\u0642\u0651\u065e\u06d6", "word_translation": "(is) true", "word_transliteration": "haqqun"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "So (let) not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u062a\u064e\u063a\u064f\u0631\u0651\u064e\u0646\u0651\u064e\u0643\u064f\u0645\u064f", "word_translation": "deceive you", "word_transliteration": "taghurrannakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u064f", "word_translation": "the life", "word_transliteration": "al-hayatu"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627", "word_translation": "(of) the world", "word_transliteration": "al-dun'ya"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (let) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u063a\u064f\u0631\u0651\u064e\u0646\u0651\u064e\u0643\u064f\u0645", "word_translation": "deceive you", "word_transliteration": "yaghurrannakum"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "about Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u0631\u064f\u0648\u0631\u064f", "word_translation": "the Deceiver", "word_transliteration": "al-gharuru"}]}