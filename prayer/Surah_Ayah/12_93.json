{"ayahs": {"ayah_uthmani": "\u0671\u0630\u06e1\u0647\u064e\u0628\u064f\u0648\u0627\u0652 \u0628\u0650\u0642\u064e\u0645\u0650\u064a\u0635\u0650\u064a \u0647\u064e\u0670\u0630\u064e\u0627 \u0641\u064e\u0623\u064e\u0644\u06e1\u0642\u064f\u0648\u0647\u064f \u0639\u064e\u0644\u064e\u0649\u0670 \u0648\u064e\u062c\u06e1\u0647\u0650 \u0623\u064e\u0628\u0650\u064a \u064a\u064e\u0623\u06e1\u062a\u0650 \u0628\u064e\u0635\u0650\u064a\u0631\u0657\u0627 \u0648\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u0650\u064a \u0628\u0650\u0623\u064e\u0647\u06e1\u0644\u0650\u0643\u064f\u0645\u06e1 \u0623\u064e\u062c\u06e1\u0645\u064e\u0639\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0630\u06e1\u0647\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "Go", "word_transliteration": "idh'habu"}, {"word_arabic_uthmani": "\u0628\u0650\u0642\u064e\u0645\u0650\u064a\u0635\u0650\u064a", "word_translation": "with this shirt of mine", "word_transliteration": "biqamisi"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "with this shirt of mine", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0644\u06e1\u0642\u064f\u0648\u0647\u064f", "word_translation": "and cast it", "word_transliteration": "fa-alquhu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u06e1\u0647\u0650", "word_translation": "(the) face", "word_transliteration": "wajhi"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u0650\u064a", "word_translation": "(of) my father", "word_transliteration": "abi"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650", "word_translation": "he will regain sight", "word_transliteration": "yati"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u0650\u064a\u0631\u0657\u0627", "word_translation": "he will regain sight", "word_transliteration": "basiran"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u0650\u064a", "word_translation": "And bring to me", "word_transliteration": "watuni"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0647\u06e1\u0644\u0650\u0643\u064f\u0645\u06e1", "word_translation": "your family", "word_transliteration": "bi-ahlikum"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0645\u064e\u0639\u0650\u064a\u0646\u064e", "word_translation": "all together", "word_transliteration": "ajma'ina"}]}