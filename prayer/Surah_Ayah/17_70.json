{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0643\u064e\u0631\u0651\u064e\u0645\u06e1\u0646\u064e\u0627 \u0628\u064e\u0646\u0650\u064a\u0653 \u0621\u064e\u0627\u062f\u064e\u0645\u064e \u0648\u064e\u062d\u064e\u0645\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u0628\u064e\u0631\u0651\u0650 \u0648\u064e\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650 \u0648\u064e\u0631\u064e\u0632\u064e\u0642\u06e1\u0646\u064e\u0670\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0637\u0651\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650 \u0648\u064e\u0641\u064e\u0636\u0651\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u0649\u0670 \u0643\u064e\u062b\u0650\u064a\u0631\u0656 \u0645\u0651\u0650\u0645\u0651\u064e\u0646\u06e1 \u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627 \u062a\u064e\u0641\u06e1\u0636\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And certainly", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0643\u064e\u0631\u0651\u064e\u0645\u06e1\u0646\u064e\u0627", "word_translation": "We have honored", "word_transliteration": "karramna"}, {"word_arabic_uthmani": "\u0628\u064e\u0646\u0650\u064a\u0653", "word_translation": "(the) children of Adam", "word_transliteration": "bani"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062f\u064e\u0645\u064e", "word_translation": "(the) children of Adam", "word_transliteration": "adama"}, {"word_arabic_uthmani": "\u0648\u064e\u062d\u064e\u0645\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "and We carried them", "word_transliteration": "wahamalnahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "on", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0631\u0651\u0650", "word_translation": "the land", "word_transliteration": "al-bari"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650", "word_translation": "and the sea", "word_transliteration": "wal-bahri"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0632\u064e\u0642\u06e1\u0646\u064e\u0670\u0647\u064f\u0645", "word_translation": "and We have provided them", "word_transliteration": "warazaqnahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0637\u0651\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650", "word_translation": "the good things", "word_transliteration": "al-tayibati"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u064e\u0636\u0651\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "and We preferred them", "word_transliteration": "wafaddalnahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u0656", "word_translation": "many", "word_transliteration": "kathirin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0646\u06e1", "word_translation": "of those whom", "word_transliteration": "mimman"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627", "word_translation": "We have created", "word_transliteration": "khalaqna"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u06e1\u0636\u0650\u064a\u0644\u0657\u0627", "word_translation": "(with) preference", "word_transliteration": "tafdilan"}]}