{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0628\u064f\u0634\u06e1\u0631\u064e\u0649\u0670 \u0641\u0650\u064a \u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u0650 \u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627 \u0648\u064e\u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650\u06da \u0644\u064e\u0627 \u062a\u064e\u0628\u06e1\u062f\u0650\u064a\u0644\u064e \u0644\u0650\u0643\u064e\u0644\u0650\u0645\u064e\u0670\u062a\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0641\u064e\u0648\u06e1\u0632\u064f \u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "For them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064f\u0634\u06e1\u0631\u064e\u0649\u0670", "word_translation": "(are) the glad tidings", "word_transliteration": "al-bush'ra"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u0650", "word_translation": "the life", "word_transliteration": "al-hayati"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627", "word_translation": "(of) the world", "word_transliteration": "al-dun'ya"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u0650\u064a", "word_translation": "and in", "word_transliteration": "wafi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650\u06da", "word_translation": "the Hereafter", "word_transliteration": "al-akhirati"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "No", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0628\u06e1\u062f\u0650\u064a\u0644\u064e", "word_translation": "change", "word_transliteration": "tabdila"}, {"word_arabic_uthmani": "\u0644\u0650\u0643\u064e\u0644\u0650\u0645\u064e\u0670\u062a\u0650", "word_translation": "(is there) in the Words", "word_transliteration": "likalimati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "is", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0648\u06e1\u0632\u064f", "word_translation": "the success", "word_transliteration": "al-fawzu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u064f", "word_translation": "the great", "word_transliteration": "al-'azimu"}]}