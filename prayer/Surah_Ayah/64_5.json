{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0623\u06e1\u062a\u0650\u0643\u064f\u0645\u06e1 \u0646\u064e\u0628\u064e\u0624\u064f\u0627\u0652 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u064f \u0641\u064e\u0630\u064e\u0627\u0642\u064f\u0648\u0627\u0652 \u0648\u064e\u0628\u064e\u0627\u0644\u064e \u0623\u064e\u0645\u06e1\u0631\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064c \u0623\u064e\u0644\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Has not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650\u0643\u064f\u0645\u06e1", "word_translation": "come to you", "word_transliteration": "yatikum"}, {"word_arabic_uthmani": "\u0646\u064e\u0628\u064e\u0624\u064f\u0627\u0652", "word_translation": "(the) news", "word_transliteration": "naba-u"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f", "word_translation": "before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u0641\u064e\u0630\u064e\u0627\u0642\u064f\u0648\u0627\u0652", "word_translation": "So they tasted", "word_transliteration": "fadhaqu"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u0627\u0644\u064e", "word_translation": "(the) bad consequence", "word_transliteration": "wabala"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0631\u0