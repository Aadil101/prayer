{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0642\u06e1\u062f\u064f\u0645\u064f \u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064f\u06e5 \u064a\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650 \u0641\u064e\u0623\u064e\u0648\u06e1\u0631\u064e\u062f\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064e\u06d6 \u0648\u064e\u0628\u0650\u0626\u06e1\u0633\u064e \u0671\u0644\u06e1\u0648\u0650\u0631\u06e1\u062f\u064f \u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u0631\u064f\u0648\u062f\u064f"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0642\u06e1\u062f\u064f\u0645\u064f", "word_translation": "He will precede", "word_transliteration": "yaqdumu"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064f\u06e5", "word_translation": "his people", "word_transliteration": "qawmahu"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(on the) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650", "word_translation": "(of) the Resurrection", "word_transliteration": "al-qiyamati"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0648\u06e1\u0631\u064e\u062f\u064e\u0647\u064f\u0645\u064f", "word_translation": "and lead them", "word_transliteration": "fa-awradahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064e\u06d6", "word_translation": "(into) the Fire", "word_transliteration": "al-nara"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u0650\u0626\u06e1\u0633\u064e", "word_translation": "And wretched", "word_transliteration": "wabi'sa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0648\u0650\u0631\u06e1\u062f\u064f", "word_translation": "(is) the place", "word_transliteration": "al-wir'du"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u0631\u064f\u0648\u062f\u064f", "word_translation": "to which (they are) led", "word_transliteration": "al-mawrudu"}]}