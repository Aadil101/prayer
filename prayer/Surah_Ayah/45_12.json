{"ayahs": {"ayah_uthmani": "\u06de\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0633\u064e\u062e\u0651\u064e\u0631\u064e \u0644\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u064e \u0644\u0650\u062a\u064e\u062c\u06e1\u0631\u0650\u064a\u064e \u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u064f \u0641\u0650\u064a\u0647\u0650 \u0628\u0650\u0623\u064e\u0645\u06e1\u0631\u0650\u0647\u0650\u06e6 \u0648\u064e\u0644\u0650\u062a\u064e\u0628\u06e1\u062a\u064e\u063a\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6 \u0648\u064e\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0643\u064f\u0645\u06e1 \u062a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0633\u064e\u062e\u0651\u064e\u0631\u064e", "word_translation": "subjected", "word_transliteration": "sakhara"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u064f", "word_translation": "to you", "word_transliteration": "lakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u064e", "word_translation": "the sea", "word_transliteration": "al-bahra"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064e\u062c\u06e1\u0631\u0650\u064a\u064e", "word_translation": "that may sail", "word_transliteration": "litajriya"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u064f", "word_translation": "the ships", "word_transliteration": "al-ful'ku"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650", "word_translation": "therein", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0645\u06e1\u0631\u0650\u0647\u0650\u06e6", "word_translation": "by His Command", "word_transliteration": "bi-amrihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u062a\u064e\u0628\u06e1\u062a\u064e\u063a\u064f\u0648\u0627\u0652", "word_translation": "and that you may seek", "word_transliteration": "walitabtaghu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6", "word_translation": "His Bounty", "word_transliteration": "fadlihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "and that you may", "word_transliteration": "wala'allakum"}, {"word_arabic_uthmani": "\u062a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "give thanks", "word_transliteration": "tashkuruna"}]}