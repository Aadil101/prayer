{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0648\u06e1 \u0623\u064e\u0646\u0651\u064e\u0646\u064e\u0627 \u0646\u064e\u0632\u0651\u064e\u0644\u06e1\u0646\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064e \u0648\u064e\u0643\u064e\u0644\u0651\u064e\u0645\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e\u0649\u0670 \u0648\u064e\u062d\u064e\u0634\u064e\u0631\u06e1\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0643\u064f\u0644\u0651\u064e \u0634\u064e\u064a\u06e1\u0621\u0656 \u0642\u064f\u0628\u064f\u0644\u0657\u0627 \u0645\u0651\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0644\u0650\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0644\u0651\u064e\u0627\u0653 \u0623\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u062c\u06e1\u0647\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And (even) if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0646\u064e\u0627", "word_translation": "[that] We (had)", "word_transliteration": "annana"}, {"word_arabic_uthmani": "\u0646\u064e\u0632\u0651\u064e\u0644\u06e1\u0646\u064e\u0627\u0653", "word_translation": "[We] sent down", "word_transliteration": "nazzalna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f", "word_translation": "to them", "word_transliteration": "ilayhimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064e", "word_translation": "the Angels", "word_transliteration": "al-malaikata"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0644\u0651\u064e\u0645\u064e\u0647\u064f\u0645\u064f", "word_translation": "and spoken to them", "word_transliteration": "wakallamahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e\u0649\u0670", "word_translation": "the dead", "word_transliteration": "al-mawta"}, {"word_arabic_uthmani": "\u0648\u064e\u062d\u064e\u0634\u064e\u0631\u06e1\u0646\u064e\u0627", "word_translation": "and We gathered", "word_transliteration": "wahasharna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "before them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064e", "word_translation": "every", "word_transliteration": "kulla"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0642\u064f\u0628\u064f\u0644\u0657\u0627", "word_translation": "face to face", "word_transliteration": "qubulan"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they were", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0653\u0627\u0652", "word_translation": "to believe", "word_transliteration": "liyu'minu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627\u0653", "word_translation": "unless", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "[that]", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064e", "word_translation": "wills", "word_transliteration": "yashaa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e", "word_translation": "But", "word_transliteration": "walakinna"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "most of them", "word_transliteration": "aktharahum"}, {"word_arabic_uthmani": "\u064a\u064e\u062c\u06e1\u0647\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "(are) ignorant", "word_transliteration": "yajhaluna"}]}