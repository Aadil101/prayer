{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1 \u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0631\u064e\u0636\u064f\u0648\u0627\u0652 \u0645\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064f\u0647\u064f\u06e5 \u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u062d\u064e\u0633\u06e1\u0628\u064f\u0646\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0633\u064e\u064a\u064f\u0624\u06e1\u062a\u0650\u064a\u0646\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u0650\u0646 \u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6 \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064f\u0647\u064f\u06e5\u0653 \u0625\u0650\u0646\u0651\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0631\u064e\u0670\u063a\u0650\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "[that] they", "word_transliteration": "annahum"}, {"word_arabic_uthmani": "\u0631\u064e\u0636\u064f\u0648\u0627\u0652", "word_translation": "(were) satisfied", "word_transliteration": "radu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "(with) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645\u064f", "word_translation": "Allah gave them", "word_transliteration": "atahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah gave them", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064f\u0647\u064f\u06e5", "word_translation": "and His Messenger", "word_transliteration": "warasuluhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "and said", "word_transliteration": "waqalu"}, {"word_arabic_uthmani": "\u062d\u064e\u0633\u06e1\u0628\u064f\u0646\u064e\u0627", "word_translation": "Sufficient for us", "word_transliteration": "hasbuna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0633\u064e\u064a\u064f\u0624\u06e1\u062a\u0650\u064a\u0646\u064e\u0627", "word_translation": "Allah will give us", "word_transliteration": "sayu'tina"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah will give us", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6", "word_translation": "His Bounty", "word_transliteration": "fadlihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064f\u0647\u064f\u06e5\u0653", "word_translation": "and His Messenger", "word_transliteration": "warasuluhu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653", "word_translation": "Indeed, we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0631\u064e\u0670\u063a\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "turn our hopes", "word_transliteration": "raghibuna"}]}