ation": "alayhi"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "This (is)", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u064e\u0644\u0650", "word_translation": "(the) deed", "word_transliteration": "amali"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u0650\u06d6", "word_translation": "(of) Shaitaan", "word_transliteration": "al-shaytani"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed, he", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064f\u0648\u0651\u065e", "word_translation": "(is) an enemy", "word_transliteration": "aduwwun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0636\u0650\u0644\u0651\u065e", "word_translation": "one who misleads", "word_transliteration": "mudillun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0628\u0650\u064a\u0646\u065e", "word_translation": "clearly", "word_transliteration": "mubinun"}]}