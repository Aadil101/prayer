{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0670\u0647\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0647\u064f\u0648\u064e \u064a\u064f\u062d\u06e1\u064a\u0650\u06e6 \u0648\u064e\u064a\u064f\u0645\u0650\u064a\u062a\u064f\u06d6 \u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1 \u0648\u064e\u0631\u064e\u0628\u0651\u064f \u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0626\u0650\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0648\u0651\u064e\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0627\u0653", "word_translation": "(There is) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064e", "word_translation": "god", "word_transliteration": "ilaha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "Him", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u06e1\u064a\u0650\u06e6", "word_translation": "He gives life", "word_transliteration": "yuh'yi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0645\u0650\u064a\u062a\u064f\u06d6", "word_translation": "and causes death", "word_transliteration": "wayumitu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1", "word_translation": "your Lord", "word_transliteration": "rabbukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0628\u0651\u064f", "word_translation": "and (the) Lord", "word_transliteration": "warabbu"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0626\u0650\u0643\u064f\u0645\u064f", "word_translation": "(of) your fathers", "word_transliteration": "abaikumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0648\u0651\u064e\u0644\u0650\u064a\u0646\u064e", "word_translation": "the former", "word_transliteration": "al-awalina"}]}