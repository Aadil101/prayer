{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0644\u064e\u0627 \u062a\u064f\u062d\u064e\u0631\u0651\u0650\u0645\u064f\u0648\u0627\u0652 \u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650 \u0645\u064e\u0627\u0653 \u0623\u064e\u062d\u064e\u0644\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0639\u06e1\u062a\u064e\u062f\u064f\u0648\u0653\u0627\u0652\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0627 \u064a\u064f\u062d\u0650\u0628\u0651\u064f \u0671\u0644\u06e1\u0645\u064f\u0639\u06e1\u062a\u064e\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O you", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064f\u062d\u064e\u0631\u0651\u0650\u0645\u064f\u0648\u0627\u0652", "word_translation": "make unlawful", "word_transliteration": "tuharrimu"}, {"word_arabic_uthmani": "\u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650", "word_translation": "(the) good things", "word_transliteration": "tayyibati"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "(of) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u0644\u0651\u064e", "word_translation": "has (been) made lawful", "word_transliteration": "ahalla"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(by) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u062a\u064e\u062f\u064f\u0648\u0653\u0627\u0652\u06da", "word_translation": "transgress", "word_transliteration": "ta'tadu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u0650\u0628\u0651\u064f", "word_translation": "love", "word_transliteration": "yuhibbu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0639\u06e1\u062a\u064e\u062f\u0650\u064a\u0646\u064e", "word_translation": "the transgressors", "word_transliteration": "al-mu'tadina"}]}