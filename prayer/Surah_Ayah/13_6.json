ansliteration": "lashadidu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0642\u064e\u0627\u0628\u0650", "word_translation": "(in) the penalty", "word_transliteration": "al-'iqabi"}]}