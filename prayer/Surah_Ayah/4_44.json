{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1 \u062a\u064e\u0631\u064e \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0623\u064f\u0648\u062a\u064f\u0648\u0627\u0652 \u0646\u064e\u0635\u0650\u064a\u0628\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u064a\u064e\u0634\u06e1\u062a\u064e\u0631\u064f\u0648\u0646\u064e \u0671\u0644\u0636\u0651\u064e\u0644\u064e\u0670\u0644\u064e\u0629\u064e \u0648\u064e\u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e \u0623\u064e\u0646 \u062a\u064e\u0636\u0650\u0644\u0651\u064f\u0648\u0627\u0652 \u0671\u0644\u0633\u0651\u064e\u0628\u0650\u064a\u0644\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Did not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e", "word_translation": "you see", "word_transliteration": "tara"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "[towards]", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u062a\u064f\u0648\u0627\u0652", "word_translation": "were given", "word_transliteration": "utu"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u0650\u064a\u0628\u0657\u0627", "word_translation": "a portion", "word_transliteration": "nasiban"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "the Book", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u062a\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "purchasing", "word_transliteration": "yashtaruna"}, {"word_arabic_uthm