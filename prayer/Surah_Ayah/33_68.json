{"ayahs": {"ayah_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u0650\u0647\u0650\u0645\u06e1 \u0636\u0650\u0639\u06e1\u0641\u064e\u064a\u06e1\u0646\u0650 \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u0650 \u0648\u064e\u0671\u0644\u06e1\u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0644\u064e\u0639\u06e1\u0646\u0657\u0627 \u0643\u064e\u0628\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653", "word_translation": "Our Lord!", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u0650\u0647\u0650\u0645\u06e1", "word_translation": "Give them", "word_transliteration": "atihim"}, {"word_arabic_uthmani": "\u0636\u0650\u0639\u06e1\u0641\u064e\u064a\u06e1\u0646\u0650", "word_translation": "double", "word_transliteration": "di'fayni"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "[of]", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u0650", "word_translation": "punishment", "word_transliteration": "al-'adhabi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "and curse them", "word_transliteration": "wal-'anhum"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u06e1\u0646\u0657\u0627", "word_translation": "(with) a curse", "word_transliteration": "la'nan"}, {"word_arabic_uthmani": "\u0643\u064e\u0628\u0650\u064a\u0631\u0657\u0627", "word_translation": "great", "word_transliteration": "kabiran"}]}