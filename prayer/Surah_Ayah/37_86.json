{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0626\u0650\u0641\u06e1\u0643\u064b\u0627 \u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u0657 \u062f\u064f\u0648\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u062a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0626\u0650\u0641\u06e1\u0643\u064b\u0627", "word_translation": "Is it falsehood", "word_transliteration": "a-if'kan"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u0657", "word_translation": "gods", "word_transliteration": "alihatan"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u064e", "word_translation": "other than", "word_transliteration": "duna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e", "word_translation": "(that) you desire", "word_transliteration": "turiduna"}]}