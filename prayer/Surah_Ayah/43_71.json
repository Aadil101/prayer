{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0637\u064e\u0627\u0641\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645 \u0628\u0650\u0635\u0650\u062d\u064e\u0627\u0641\u0656 \u0645\u0651\u0650\u0646 \u0630\u064e\u0647\u064e\u0628\u0656 \u0648\u064e\u0623\u064e\u0643\u06e1\u0648\u064e\u0627\u0628\u0656\u06d6 \u0648\u064e\u0641\u0650\u064a\u0647\u064e\u0627 \u0645\u064e\u0627 \u062a\u064e\u0634\u06e1\u062a\u064e\u0647\u0650\u064a\u0647\u0650 \u0671\u0644\u06e1\u0623\u064e\u0646\u0641\u064f\u0633\u064f \u0648\u064e\u062a\u064e\u0644\u064e\u0630\u0651\u064f \u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u064a\u064f\u0646\u064f\u06d6 \u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u064e\u0627 \u062e\u064e\u0670\u0644\u0650\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0637\u064e\u0627\u0641\u064f", "word_translation": "Will be circulated", "word_transliteration": "yutafu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645", "word_translation": "for them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0628\u0650\u0635\u0650\u062d\u064e\u0627\u0641\u0656", "word_translation": "plates", "word_transliteration": "bisihafin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0630\u064e\u0647\u064e\u0628\u0656", "word_translation": "gold", "word_transliteration": "dhahabin"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0643\u06e1\u0648\u064e\u0627\u0628\u0656\u06d6", "word_translation": "and cups", "word_transliteration": "wa-akwabin"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "And therein", "word_transliteration": "wafiha"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "(is) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062a\u064e\u0634\u06e1\u062a\u064e\u0647\u0650\u064a\u0647\u0650", "word_translation": "desires", "word_transliteration": "tashtahihi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0646\u0641\u064f\u0633\u064f", "word_translation": "the souls", "word_transliteration": "al-anfusu"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0644\u064e\u0630\u0651\u064f", "word_translation": "and delights", "word_transliteration": "wataladhu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u064a\u064f\u0646\u064f\u06d6", "word_translation": "the eyes", "word_transliteration": "al-a'yunu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "and you", "word_transliteration": "wa-antum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u062e\u064e\u0670\u0644\u0650\u062f\u064f\u0648\u0646\u064e", "word_translation": "will abide forever", "word_transliteration": "khaliduna"}]}