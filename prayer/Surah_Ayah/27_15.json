{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0627 \u062f\u064e\u0627\u0648\u064f\u06e5\u062f\u064e \u0648\u064e\u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e \u0639\u0650\u0644\u06e1\u0645\u0657\u0627\u06d6 \u0648\u064e\u0642\u064e\u0627\u0644\u064e\u0627 \u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0641\u064e\u0636\u0651\u064e\u0644\u064e\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u0649\u0670 \u0643\u064e\u062b\u0650\u064a\u0631\u0656 \u0645\u0651\u0650\u0646\u06e1 \u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650 \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And verily", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "We gave", "word_transliteration": "atayna"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0648\u064f\u06e5\u062f\u064e", "word_translation": "Dawood", "word_transliteration": "dawuda"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e", "word_translation": "and Sulaiman", "word_transliteration": "wasulaymana"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u0657\u0627\u06d6", "word_translation": "knowledge", "word_transliteration": "il'man"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u0627", "word_translation": "and they said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f", "word_translation": "Praise be", "word_transliteration": "al-hamdu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "to Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u0651\u064e\u0644\u064e\u0646\u064e\u0627", "word_translation": "has favored us", "word_transliteration": "faddalana"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u0656", "word_translation": "many", "word_transliteration": "kathirin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650", "word_translation": "His servants", "word_transliteration": "ibadihi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "the believers", "word_transliteration": "al-mu'minina"}]}