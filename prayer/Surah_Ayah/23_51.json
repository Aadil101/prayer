{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0631\u0651\u064f\u0633\u064f\u0644\u064f \u0643\u064f\u0644\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u064e \u0671\u0644\u0637\u0651\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0627\u0652 \u0635\u064e\u0670\u0644\u0650\u062d\u064b\u0627\u06d6 \u0625\u0650\u0646\u0651\u0650\u064a \u0628\u0650\u0645\u064e\u0627 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e \u0639\u064e\u0644\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O Messengers", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064f\u0633\u064f\u0644\u064f", "word_translation": "O Messengers", "word_transliteration": "al-rusulu"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "Eat", "word_transliteration": "kulu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0637\u0651\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650", "word_translation": "the good things", "word_transliteration": "al-tayibati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "and do", "word_transliteration": "wa-i'malu"}, {"word_arabic_uthmani": "\u0635\u064e\u0670\u0644\u0650\u062d\u064b\u0627\u06d6", "word_translation": "righteous (deeds)", "word_transliteration": "salihan"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed, I Am", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "you do", "word_transliteration": "ta'maluna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u065e", "word_translation": "All-Knower", "word_transliteration": "alimun"}]}