{"ayahs": {"ayah_uthmani": "\u0633\u064e\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e \u063a\u064e\u062f\u0657\u0627 \u0645\u0651\u064e\u0646\u0650 \u0671\u0644\u06e1\u0643\u064e\u0630\u0651\u064e\u0627\u0628\u064f \u0671\u0644\u06e1\u0623\u064e\u0634\u0650\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0633\u064e\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "They will know", "word_transliteration": "saya'lamuna"}, {"word_arabic_uthmani": "\u063a\u064e\u062f\u0657\u0627", "word_translation": "tomorrow", "word_transliteration": "ghadan"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646\u0650", "word_translation": "who", "word_transliteration": "mani"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0630\u0651\u064e\u0627\u0628\u064f", "word_translation": "(is) the liar", "word_transliteration": "al-kadhabu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0634\u0650\u0631\u064f", "word_translation": "the insolent one", "word_transliteration": "al-ashiru"}]}