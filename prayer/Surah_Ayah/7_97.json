{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0641\u064e\u0623\u064e\u0645\u0650\u0646\u064e \u0623\u064e\u0647\u06e1\u0644\u064f \u0671\u0644\u06e1\u0642\u064f\u0631\u064e\u0649\u0670\u0653 \u0623\u064e\u0646 \u064a\u064e\u0623\u06e1\u062a\u0650\u064a\u064e\u0647\u064f\u0645 \u0628\u064e\u0623\u06e1\u0633\u064f\u0646\u064e\u0627 \u0628\u064e\u064a\u064e\u0670\u062a\u0657\u0627 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0646\u064e\u0627\u0653\u0626\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0623\u064e\u0645\u0650\u0646\u064e", "word_translation": "Then did feel secure", "word_transliteration": "afa-amina"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064f", "word_translation": "(the) people", "word_transliteration": "ahlu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0631\u064e\u0649\u0670\u0653", "word_translation": "(of) the cities", "word_transliteration": "al-qura"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650\u064a\u064e\u0647\u064f\u0645", "word_translation": "comes to them", "word_transliteration": "yatiyahum"}, {"word_arabic_uthmani": "\u0628\u064e\u0623\u06e1\u0633\u064f\u0646\u064e\u0627", "word_translation": "Our punishment", "word_transliteration": "basuna"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u064e\u0670\u062a\u0657\u0627", "word_translation": "(at) night", "word_transliteration": "bayatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u0653\u0626\u0650\u0645\u064f\u0648\u0646\u064e", "word_translation": "(were) asleep", "word_transliteration": "naimuna"}]}