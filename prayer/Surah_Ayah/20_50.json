{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0631\u064e\u0628\u0651\u064f\u0646\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653 \u0623\u064e\u0639\u06e1\u0637\u064e\u0649\u0670 \u0643\u064f\u0644\u0651\u064e \u0634\u064e\u064a\u06e1\u0621\u064d \u062e\u064e\u0644\u06e1\u0642\u064e\u0647\u064f\u06e5 \u062b\u064f\u0645\u0651\u064e \u0647\u064e\u062f\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0646\u064e\u0627", "word_translation": "Our Lord", "word_transliteration": "rabbuna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0637\u064e\u0649\u0670", "word_translation": "gave", "word_transliteration": "a'ta"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064e", "word_translation": "(to) every", "word_transliteration": "kulla"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u064d", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0642\u064e\u0647\u064f\u06e5", "word_translation": "its form", "word_transliteration": "khalqahu"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0647\u064e\u062f\u064e\u0649\u0670", "word_translation": "He guided (it)", "word_transliteration": "hada"}]}