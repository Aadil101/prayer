{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u0633\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0641\u0650\u064a\u0653 \u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627 \u0645\u064f\u0639\u064e\u0670\u062c\u0650\u0632\u0650\u064a\u0646\u064e \u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0641\u0650\u064a \u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u0650 \u0645\u064f\u062d\u06e1\u0636\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "And those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "strive", "word_transliteration": "yas'awna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0653", "word_translation": "against", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627", "word_translation": "Our Verses", "word_transliteration": "ayatina"}, {"word_arabic_uthmani": "\u0645\u064f\u0639\u064e\u0670\u062c\u0650\u0632\u0650\u064a\u0646\u064e", "word_translation": "(to) cause failure", "word_transliteration": "mu'ajizina"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "into", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u0650", "word_translation": "the punishment", "word_transliteration": "al-'adhabi"}, {"word_arabic_uthmani": "\u0645\u064f\u062d\u06e1\u0636\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "(will be) brought", "word_transliteration": "muh'daruna"}]}