{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0627 \u064a\u064e\u062d\u06e1\u0632\u064f\u0646\u064f\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0641\u064e\u0632\u064e\u0639\u064f \u0671\u0644\u06e1\u0623\u064e\u0643\u06e1\u0628\u064e\u0631\u064f \u0648\u064e\u062a\u064e\u062a\u064e\u0644\u064e\u0642\u0651\u064e\u0649\u0670\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f \u0647\u064e\u0670\u0630\u064e\u0627 \u064a\u064e\u0648\u06e1\u0645\u064f\u0643\u064f\u0645\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u062a\u064f\u0648\u0639\u064e\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0632\u064f\u0646\u064f\u0647\u064f\u0645\u064f", "word_translation": "will grieve them", "word_transliteration": "yahzunuhumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0632\u064e\u0639\u064f", "word_translation": "the terror", "word_transliteration": "al-faza'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0643\u06e1\u0628\u064e\u0631\u064f", "word_translation": "[the] greatest", "word_transliteration": "al-akbaru"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u062a\u064e\u0644\u064e\u0642\u0651\u064e\u0649\u0670\u0647\u064f\u0645\u064f", "word_translation": "and will meet them", "word_transliteration": "watatalaqqahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f", "word_translation": "the Angels", "word_transliteration": "al-malaikatu"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "This", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064f\u0643\u064f\u0645\u064f", "word_translation": "(is) your Day", "word_transliteration": "y