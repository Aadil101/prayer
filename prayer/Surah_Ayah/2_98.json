{"ayahs": {"ayah_uthmani": "\u0645\u064e\u0646 \u0643\u064e\u0627\u0646\u064e \u0639\u064e\u062f\u064f\u0648\u0651\u0657\u0627 \u0644\u0651\u0650\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u062a\u0650\u0647\u0650\u06e6 \u0648\u064e\u0631\u064f\u0633\u064f\u0644\u0650\u0647\u0650\u06e6 \u0648\u064e\u062c\u0650\u0628\u06e1\u0631\u0650\u064a\u0644\u064e \u0648\u064e\u0645\u0650\u064a\u0643\u064e\u0649\u0670\u0644\u064e \u0641\u064e\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0639\u064e\u062f\u064f\u0648\u0651\u065e \u0644\u0651\u0650\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "Whoever", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064f\u0648\u0651\u0657\u0627", "word_translation": "an enemy", "word_transliteration": "aduwwan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "(to) Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u062a\u0650\u0647\u0650\u06e6", "word_translation": "and His Angels", "word_transliteration": "wamalaikatihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064f\u0633\u064f\u0644\u0650\u0647\u0650\u06e6", "word_translation": "and His Messengers", "word_transliteration": "warusulihi"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u0650\u0628\u06e1\u0631\u0650\u064a\u0644\u064e", "word_translation": "and Jibreel", "word_transliteration": "wajib'rila"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u064a\u0643\u064e\u0649\u0670\u0644\u064e", "word_translation": "and Meekael", "word_transliteration": "wamikala"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "then indeed", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064f\u0648\u0651\u065e", "word_translation": "(is) an enemy", "word_transliteration": "aduwwun"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "to the disbelievers", "word_transliteration": "lil'kafirina"}]}