ation": "liyamkuru"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627\u06d6", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "they plot", "word_transliteration": "yamkuruna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1", "word_translation": "against themselves", "word_transliteration": "bi-anfusihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0639\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "they perceive", "word_transliteration": "yash'uruna"}]}