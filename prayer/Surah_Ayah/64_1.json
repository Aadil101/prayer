{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6 \u0644\u064e\u0647\u064f \u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u064f \u0648\u064e\u0644\u064e\u0647\u064f \u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f\u06d6 \u0648\u064e\u0647\u064f\u0648\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0642\u064e\u062f\u0650\u064a\u0631\u064c"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f", "word_translation": "Glorifies", "word_transliteration": "yusabbihu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "[to] Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f", "word_translation": "For Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "(is the) dominion", "word_transliteration": "al-mul'ku"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f", "word_translation": "and for Him", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f\u06d6", "word_translation": "(is) the praise", "word_transliteration": "al-hamdu"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "(is) on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0650\u064a\u0631\u064c", "word_translation": "All-powerful", "word_transliteration": "qadirun"}]}