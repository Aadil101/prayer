{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0647\u064e\u0644\u06e1 \u0639\u064e\u0644\u0650\u0645\u06e1\u062a\u064f\u0645 \u0645\u0651\u064e\u0627 \u0641\u064e\u0639\u064e\u0644\u06e1\u062a\u064f\u0645 \u0628\u0650\u064a\u064f\u0648\u0633\u064f\u0641\u064e \u0648\u064e\u0623\u064e\u062e\u0650\u064a\u0647\u0650 \u0625\u0650\u0630\u06e1 \u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u062c\u064e\u0670\u0647\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0647\u064e\u0644\u06e1", "word_translation": "Do", "word_transliteration": "hal"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u0645\u06e1\u062a\u064f\u0645", "word_translation": "you know", "word_transliteration": "alim'tum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u064e\u0639\u064e\u0644\u06e1\u062a\u064f\u0645", "word_translation": "you did", "word_transliteration": "fa'altum"}, {"word_arabic_uthmani": "\u0628\u0650\u064a\u064f\u0648\u0633\u064f\u0641\u064e", "word_translation": "with Yusuf", "word_transliteration": "biyusufa"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062e\u0650\u064a\u0647\u0650", "word_translation": "and his brother", "word_transliteration": "wa-akhihi"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "when", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you were", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u062c\u064e\u0670\u0647\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "ignorant", "word_transliteration": "jahiluna"}]}