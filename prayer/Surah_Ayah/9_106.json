{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0621\u064e\u0627\u062e\u064e\u0631\u064f\u0648\u0646\u064e \u0645\u064f\u0631\u06e1\u062c\u064e\u0648\u06e1\u0646\u064e \u0644\u0650\u0623\u064e\u0645\u06e1\u0631\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0625\u0650\u0645\u0651\u064e\u0627 \u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0625\u0650\u0645\u0651\u064e\u0627 \u064a\u064e\u062a\u064f\u0648\u0628\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u0650\u064a\u0645\u064c \u062d\u064e\u0643\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0621\u064e\u0627\u062e\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "And others", "word_transliteration": "waakharuna"}, {"word_arabic_uthmani": "\u0645\u064f\u0631\u06e1\u062c\u064e\u0648\u06e1\u0646\u064e", "word_translation": "deferred", "word_transliteration": "mur'jawna"}, {"word_arabic_uthmani": "\u0644\u0650\u0623\u064e\u0645\u06e1\u0631\u0650", "word_translation": "for the Command of Allah", "word_transliteration": "li-amri"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "for the Command of Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0625\u0650\u0645\u0651\u064e\u0627", "word_translation": "whether", "word_transliteration": "imma"}, {"word_arabic_uthmani": "\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0647\u064f\u0645\u06e1", "word_translation": "He will punish them", "word_transliteration": "yu'adhibuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0645\u0651\u064e\u0627", "word_translation": "or", "word_transliteration": "wa-imma"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064f\u0648\u0628\u064f", "word_translation": "He will turn (in mercy)", "word_transliteration": "yatubu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1\u06d7", "word_translation": "to them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u064c", "word_translation": "(is) All-Knower", "word_transliteration": "alimun"}, {"word_arabic_uthmani": "\u062d\u064e\u0643\u0650\u064a\u0645\u065e", "word_translation": "All-Wise", "word_transliteration": "hakimun"}]}