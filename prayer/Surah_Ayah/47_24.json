{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0627 \u064a\u064e\u062a\u064e\u062f\u064e\u0628\u0651\u064e\u0631\u064f\u0648\u0646\u064e \u0671\u0644\u06e1\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064e \u0623\u064e\u0645\u06e1 \u0639\u064e\u0644\u064e\u0649\u0670 \u0642\u064f\u0644\u064f\u0648\u0628\u064d \u0623\u064e\u0642\u06e1\u0641\u064e\u0627\u0644\u064f\u0647\u064e\u0627\u0653"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0627", "word_translation": "Then do not", "word_transliteration": "afala"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u062f\u064e\u0628\u0651\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "they ponder", "word_transliteration": "yatadabbaruna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064e", "word_translation": "(over) the Quran", "word_transliteration": "al-qur'ana"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "upon", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u064d", "word_translation": "(their) hearts", "word_transliteration": "qulubin"}, {"word_arabic_uthmani": "\u0623\u064e\u0642\u06e1\u0641\u064e\u0627\u0644\u064f\u0647\u064e\u0627\u0653", "word_translation": "(are) locks", "word_transliteration": "aqfaluha"}]}