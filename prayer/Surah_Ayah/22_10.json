{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0645\u064e\u0627 \u0642\u064e\u062f\u0651\u064e\u0645\u064e\u062a\u06e1 \u064a\u064e\u062f\u064e\u0627\u0643\u064e \u0648\u064e\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u064a\u06e1\u0633\u064e \u0628\u0650\u0638\u064e\u0644\u0651\u064e\u0640\u0670\u0645\u0656 \u0644\u0651\u0650\u0644\u06e1\u0639\u064e\u0628\u0650\u064a\u062f\u0650"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "(is) for what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0651\u064e\u0645\u064e\u062a\u06e1", "word_translation": "have sent forth", "word_transliteration": "qaddamat"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u064e\u0627\u0643\u064e", "word_translation": "your hands", "word_transliteration": "yadaka"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e", "word_translation": "and that", "word_transliteration": "wa-anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "is not", "word_transliteration": "laysa"}, {"word_arabic_uthmani": "\u0628\u0650\u0638\u064e\u0644\u0651\u064e\u0640\u0670\u0645\u0656", "word_translation": "unjust", "word_transliteration": "bizallamin"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u06e1\u0639\u064e\u0628\u0650\u064a\u062f\u0650", "word_translation": "to His slaves", "word_transliteration": "lil'abidi"}]}