{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646 \u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0643\u064e \u0641\u064e\u0642\u064f\u0644 \u0644\u0651\u0650\u064a \u0639\u064e\u0645\u064e\u0644\u0650\u064a \u0648\u064e\u0644\u064e\u0643\u064f\u0645\u06e1 \u0639\u064e\u0645\u064e\u0644\u064f\u0643\u064f\u0645\u06e1\u06d6 \u0623\u064e\u0646\u062a\u064f\u0645 \u0628\u064e\u0631\u0650\u064a\u0653\u0640\u0654\u064f\u0648\u0646\u064e \u0645\u0650\u0645\u0651\u064e\u0627\u0653 \u0623\u064e\u0639\u06e1\u0645\u064e\u0644\u064f \u0648\u064e\u0623\u064e\u0646\u064e\u0627\u06e0 \u0628\u064e\u0631\u0650\u064a\u0653\u0621\u065e \u0645\u0651\u0650\u0645\u0651\u064e\u0627 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "And if", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0643\u064e", "word_translation": "they deny you", "word_transliteration": "kadhabuka"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064f\u0644", "word_translation": "then say", "word_transliteration": "faqul"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u064a", "word_translation": "For me", "word_transliteration": "li"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u064e\u0644\u0650\u064a", "word_translation": "(are) my deeds", "word_transliteration": "amali"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "and for you", "word_transliteration": "walakum"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u064e\u0644\u064f\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "(are) your deeds", "word_transliteration": "amalukum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645", "word_translation": "You", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u0628\u064e\u0631\u0650\u064a\u0653\u0640\u0654\u064f\u0648\u0646\u064e", "word_translation": "(are) disassociated", "word_transliteration": "bariuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0627\u0653", "word_translation": "from what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0645\u064e\u0644\u064f", "word_translation": "I do", "word_transliteration": "a'malu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "and I am", "word_transliteration": "wa-ana"}, {"word_arabic_uthmani": "\u0628\u064e\u0631\u0650\u064a\u0653\u0621\u065e", "word_translation": "disassociated", "word_transliteration": "barion"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0627", "word_translation": "from what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "you do", "word_transliteration": "ta'maluna"}]}