{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a\u0653 \u0623\u064e\u0646\u064e\u0627\u06e0 \u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0641\u064e\u0671\u062e\u06e1\u0644\u064e\u0639\u06e1 \u0646\u064e\u0639\u06e1\u0644\u064e\u064a\u06e1\u0643\u064e \u0625\u0650\u0646\u0651\u064e\u0643\u064e \u0628\u0650\u0671\u0644\u06e1\u0648\u064e\u0627\u062f\u0650 \u0671\u0644\u06e1\u0645\u064f\u0642\u064e\u062f\u0651\u064e\u0633\u0650 \u0637\u064f\u0648\u0657\u0649"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "Indeed [I]", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "I Am", "word_transliteration": "ana"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbuka"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u062e\u06e1\u0644\u064e\u0639\u06e1", "word_translation": "so remove", "word_transliteration": "fa-ikh'la'"}, {"word_arabic_uthmani": "\u0646\u064e\u0639\u06e1\u0644\u064e\u064a\u06e1\u0643\u064e", "word_translation": "your shoes", "word_transliteration": "na'layka"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "Indeed you", "word_transliteration": "innaka"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0648\u064e\u0627\u062f\u0650", "word_translation": "(are) in the valley", "word_transliteration": "bil-wadi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0642\u064e\u062f\u0651\u064e\u0633\u0650", "word_translation": "the sacred", "word_transliteration": "al-muqadasi"}, {"word_arabic_uthmani": "\u0637\u064f\u0648\u0657\u0649", "word_translation": "(of) Tuwa", "word_transliteration": "tuwan"}]}