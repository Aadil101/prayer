{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f \u0643\u064f\u0644\u064f\u0648\u0627\u0652 \u0645\u0650\u0645\u0651\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u062d\u064e\u0644\u064e\u0670\u0644\u0657\u0627 \u0637\u064e\u064a\u0651\u0650\u0628\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u062a\u0651\u064e\u0628\u0650\u0639\u064f\u0648\u0627\u0652 \u062e\u064f\u0637\u064f\u0648\u064e\u0670\u062a\u0650 \u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u0650\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u0643\u064f\u0645\u06e1 \u0639\u064e\u062f\u064f\u0648\u0651\u065e \u0645\u0651\u064f\u0628\u0650\u064a\u0646\u064c"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f", "word_translation": "mankind", "word_transliteration": "al-nasu"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "Eat", "word_transliteration": "kulu"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u062d\u064e\u0644\u064e\u0670\u0644\u0657\u0627", "word_translation": "lawful", "word_transliteration": "halalan"}, {"word_arabic_uthmani": "\u0637\u064e\u064a\u0651\u0650\u0628\u0657\u0627", "word_translation": "(and) good", "word_transliteration": "tayyiban"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u062a\u0651\u064e\u0628\u0650\u0639\u064f\u0648\u0627\u0652", "word_translation": "follow", "word_transliteration": "tattabi'u"}, {"word_arabic_uthmani": "\u062e\u064f\u0637\u064f\u0648\u064e\u0670\u062a\u0650", "word_translation": "(the) footsteps (of)", "word_transliteration": "khutuwati"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u0650\u06da", "word_translation": "the Shaitaan", "word_transliteration": "al-shaytani"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed, he", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(is) to you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064f\u0648\u0651\u065e", "word_translation": "an enemy", "word_transliteration": "aduwwun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0628\u0650\u064a\u0646\u064c", "word_translation": "clear", "word_transliteration": "mubinun"}]}