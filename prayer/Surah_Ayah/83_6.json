{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e \u064a\u064e\u0642\u064f\u0648\u0645\u064f \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f \u0644\u0650\u0631\u064e\u0628\u0651\u0650 \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(The) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0645\u064f", "word_translation": "will stand", "word_transliteration": "yaqumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f", "word_translation": "mankind", "word_transliteration": "al-nasu"}, {"word_arabic_uthmani": "\u0644\u0650\u0631\u064e\u0628\u0651\u0650", "word_translation": "before (the) Lord", "word_transliteration": "lirabbi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the worlds", "word_transliteration": "al-'alamina"}]}