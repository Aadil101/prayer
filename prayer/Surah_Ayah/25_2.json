{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0644\u064e\u0647\u064f\u06e5 \u0645\u064f\u0644\u06e1\u0643\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u06e1 \u0648\u064e\u0644\u064e\u062f\u0657\u0627 \u0648\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0643\u064f\u0646 \u0644\u0651\u064e\u0647\u064f\u06e5 \u0634\u064e\u0631\u0650\u064a\u0643\u065e \u0641\u0650\u064a \u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u0650 \u0648\u064e\u062e\u064e\u0644\u064e\u0642\u064e \u0643\u064f\u0644\u0651\u064e \u0634\u064e\u064a\u06e1\u0621\u0656 \u0641\u064e\u0642\u064e\u062f\u0651\u064e\u0631\u064e\u0647\u064f\u06e5 \u062a\u064e\u0642\u06e1\u062f\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "The One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "to Him (belongs)", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "(the) dominion", "word_transliteration": "mul'ku"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "and not", "word_transliteration": "walam"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u06e1", "word_translation": "He has taken", "word_transliteration": "yattakhidh"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u062f\u0657\u0627", "word_translation": "a son", "word_transliteration": "waladan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "and not", "word_transliteration": "walam"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0646", "word_translation": "He has", "word_transliteration": "yakun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u06e5", "word_translation": "for Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0634\u064e\u0631\u0650\u064a\u0643\u065e", "word_translation": "a partner", "word_transliteration": "sharikun"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u0650", "word_translation": "the dominion", "word_transliteration": "al-mul'ki"}, {"word_arabic_uthmani": "\u0648\u064e\u062e\u064e\u0644\u064e\u0642\u064e", "word_translation": "and He (has) created", "word_transliteration": "wakhalaqa"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064e", "word_translation": "every", "word_transliteration": "kulla"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u0651\u064e\u0631\u064e\u0647\u064f\u06e5", "word_translation": "and determined it", "word_transliteration": "faqaddarahu"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u06e1\u062f\u0650\u064a\u0631\u0657\u0627", "word_translation": "(with) determination", "word_transliteration": "taqdiran"}]}