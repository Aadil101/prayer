{"ayahs": {"ayah_uthmani": "\u062a\u0650\u0644\u06e1\u0643\u064e \u0671\u0644\u062f\u0651\u064e\u0627\u0631\u064f \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u064f \u0646\u064e\u062c\u06e1\u0639\u064e\u0644\u064f\u0647\u064e\u0627 \u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0644\u064e\u0627 \u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e \u0639\u064f\u0644\u064f\u0648\u0651\u0657\u0627 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0644\u064e\u0627 \u0641\u064e\u0633\u064e\u0627\u062f\u0657\u0627\u06da \u0648\u064e\u0671\u0644\u06e1\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f \u0644\u0650\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u062a\u0650\u0644\u06e1\u0643\u064e", "word_translation": "That", "word_transliteration": "til'ka"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064e\u0627\u0631\u064f", "word_translation": "the Home", "word_transliteration": "al-daru"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u064f", "word_translation": "(of) the Hereafter", "word_transliteration": "al-akhiratu"}, {"word_arabic_uthmani": "\u0646\u064e\u062c\u06e1\u0639\u064e\u0644\u064f\u0647\u064e\u0627", "word_translation": "We assign it", "word_transliteration": "naj'aluha"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "to those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e", "word_translation": "desire", "word_transliteration": "yuriduna"}, {"word_arabic_uthmani": "\u0639\u064f\u0644\u064f\u0648\u0651\u0657\u0627", "word_translation": "exaltedness", "word_transliteration": "uluwwan"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0641\u064e\u0633\u064e\u0627\u062f\u0657\u0627\u06da", "word_translation": "corruption", "word_transliteration": "fasadan"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f", "word_translation": "And the good end", "word_transliteration": "wal-'aqibatu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e", "word_translation": "(is) for the righteous", "word_transliteration": "lil'muttaqina"}]}