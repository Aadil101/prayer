{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0627 \u064a\u064e\u0633\u06e1\u0628\u0650\u0642\u064f\u0648\u0646\u064e\u0647\u064f\u06e5 \u0628\u0650\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u0650 \u0648\u064e\u0647\u064f\u0645 \u0628\u0650\u0623\u064e\u0645\u06e1\u0631\u0650\u0647\u0650\u06e6 \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u0628\u0650\u0642\u064f\u0648\u0646\u064e\u0647\u064f\u06e5", "word_translation": "they (can) precede Him", "word_transliteration": "yasbiqunahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u0650", "word_translation": "in word", "word_transliteration": "bil-qawli"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0645\u06e1\u0631\u0650\u0647\u0650\u06e6", "word_translation": "by His command", "word_transliteration": "bi-amrihi"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "act", "word_transliteration": "ya'maluna"}]}