{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1  \u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627  \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064e  \u0625\u0650\u0646\u064e\u0670\u062b\u0657\u0627  \u0648\u064e\u0647\u064f\u0645\u06e1  \u0634\u064e\u0670\u0647\u0650\u062f\u064f\u0648\u0646\u064e "}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1 ", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627 ", "word_translation": "did We create", "word_transliteration": "khalaqna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064e ", "word_translation": "the Angels", "word_transliteration": "al-malaikata"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u064e\u0670\u062b\u0657\u0627 ", "word_translation": "females", "word_transliteration": "inathan"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1 ", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0634\u064e\u0670\u0647\u0650\u062f\u064f\u0648\u0646\u064e ", "word_translation": "(were) witnesses", "word_transliteration": "shahiduna"}]}