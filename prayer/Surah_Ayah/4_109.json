{"ayahs": {"ayah_uthmani": "\u0647\u064e\u0640\u0670\u0653\u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650 \u062c\u064e\u0670\u062f\u064e\u0644\u06e1\u062a\u064f\u0645\u06e1 \u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u0650 \u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627 \u0641\u064e\u0645\u064e\u0646 \u064a\u064f\u062c\u064e\u0670\u062f\u0650\u0644\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1 \u064a\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650 \u0623\u064e\u0645 \u0645\u0651\u064e\u0646 \u064a\u064e\u0643\u064f\u0648\u0646\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0648\u064e\u0643\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0647\u064e\u0640\u0670\u0653\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "Here you are", "word_transliteration": "haantum"}, {"word_arabic_uthmani": "\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650", "word_translation": "those who", "word_transliteration": "haulai"}, {"word_arabic_uthmani": "\u062c\u064e\u0670\u062f\u064e\u0644\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "[you] argue", "word_transliteration": "jadaltum"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "anhum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u0650", "word_translation": "the life", "word_transliteration": "al-hayati"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627", "word_translation": "(of) the world", "word_transliteration": "al-dun'ya"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0646", "word_translation": "but who", "word_transliteration": "faman"}, {"word_arabic_uthmani": "\u064a\u064f\u062c\u064e\u0670\u062f\u0650\u0644\u064f", "word_translation": "will argue", "word_transliteration": "yujadilu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "(with) Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "anhum"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(on the) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650", "word_translation": "(of) [the] Resurrection", "word_transliteration": "al-qiyamati"}, {"word_arabic_uthmani": "\u0623\u064e\u0645", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646", "word_translation": "who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0648\u0646\u064f", "word_translation": "will be", "word_transliteration": "yakunu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "[over them]", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u0650\u064a\u0644\u0657\u0627", "word_translation": "(their) defender", "word_transliteration": "wakilan"}]}