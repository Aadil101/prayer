{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0645\u0651\u064e\u0627 \u0646\u064e\u0630\u06e1\u0647\u064e\u0628\u064e\u0646\u0651\u064e \u0628\u0650\u0643\u064e \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0645\u0650\u0646\u06e1\u0647\u064f\u0645 \u0645\u0651\u064f\u0646\u062a\u064e\u0642\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0645\u0651\u064e\u0627", "word_translation": "And whether", "word_transliteration": "fa-imma"}, {"word_arabic_uthmani": "\u0646\u064e\u0630\u06e1\u0647\u064e\u0628\u064e\u0646\u0651\u064e", "word_translation": "We take you away", "word_transliteration": "nadhhabanna"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064e", "word_translation": "We take you away", "word_transliteration": "bika"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "then indeed, We", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645", "word_translation": "from them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0646\u062a\u064e\u0642\u0650\u0645\u064f\u0648\u0646\u064e", "word_translation": "(will) take retribution", "word_transliteration": "muntaqimuna"}]}