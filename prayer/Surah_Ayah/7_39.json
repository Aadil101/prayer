{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u06e1 \u0623\u064f\u0648\u0644\u064e\u0649\u0670\u0647\u064f\u0645\u06e1 \u0644\u0650\u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670\u0647\u064f\u0645\u06e1 \u0641\u064e\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e \u0644\u064e\u0643\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646 \u0641\u064e\u0636\u06e1\u0644\u0656 \u0641\u064e\u0630\u064f\u0648\u0642\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u064e \u0628\u0650\u0645\u064e\u0627 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u0643\u06e1\u0633\u0650\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u06e1", "word_translation": "And (will) say", "word_transliteration": "waqalat"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0644\u064e\u0649\u0670\u0647\u064f\u0645\u06e1", "word_translation": "(the) first of them", "word_transliteration": "ulahum"}, {"word_arabic_uthmani": "\u0644\u0650\u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670\u0647\u064f\u0645\u06e1", "word_translation": "to (the) last of them", "word_transliteration": "li-ukh'rahum"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627", "word_translation": "Then not", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "upon us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u0656", "word_translation": "superiority", "word_transliteration": "fadlin"}, {"word_arabic_uthmani": "\u0641\u064e\u0630\u064f\u0648\u0642\u064f\u0648\u0627\u0652", "word_translation": "so taste", "word_transliteration": "fadhuqu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u064e", "word_translation": "the punishment", "word_transliteration": "al-'adhaba"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "for what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you used to", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u06e1\u0633\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "earn", "word_transliteration": "taksibuna"}]}