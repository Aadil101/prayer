{"ayahs": {"ayah_uthmani": "\u0645\u064e\u0627 \u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0648\u064e\u0623\u064e\u062c\u064e\u0644\u0656 \u0645\u0651\u064f\u0633\u064e\u0645\u0651\u0657\u0649\u06da \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0639\u064e\u0645\u0651\u064e\u0627\u0653 \u0623\u064f\u0646\u0630\u0650\u0631\u064f\u0648\u0627\u0652 \u0645\u064f\u0639\u06e1\u0631\u0650\u0636\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627", "word_translation": "We created", "word_transliteration": "khalaqna"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "and the earth", "word_transliteration": "wal-arda"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u0653", "word_translation": "(is) between both of them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "in truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062c\u064e\u0644\u0656", "word_translation": "and (for) a term", "word_transliteration": "wa-ajalin"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0633\u064e\u0645\u0651\u0657\u0649\u06da", "word_translation": "appointed", "word_transliteration": "musamman"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "But those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627\u0653", "word_translation": "from what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u0623\u064f\u0646\u0630\u0650\u0631\u064f\u0648\u0627\u0652", "word_translation": "they are warned", "word_transliteration": "undhiru"}, {"word_arabic_uthmani": "\u0645\u064f\u0639\u06e1\u0631\u0650\u0636\u064f\u0648\u0646\u064e", "word_translation": "(are) turning away", "word_transliteration": "mu'riduna"}]}