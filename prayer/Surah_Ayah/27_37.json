{"ayahs": {"ayah_uthmani": "\u0671\u0631\u06e1\u062c\u0650\u0639\u06e1 \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0641\u064e\u0644\u064e\u0646\u064e\u0623\u06e1\u062a\u0650\u064a\u064e\u0646\u0651\u064e\u0647\u064f\u0645 \u0628\u0650\u062c\u064f\u0646\u064f\u0648\u062f\u0656 \u0644\u0651\u064e\u0627 \u0642\u0650\u0628\u064e\u0644\u064e \u0644\u064e\u0647\u064f\u0645 \u0628\u0650\u0647\u064e\u0627 \u0648\u064e\u0644\u064e\u0646\u064f\u062e\u06e1\u0631\u0650\u062c\u064e\u0646\u0651\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u06e1\u0647\u064e\u0627\u0653 \u0623\u064e\u0630\u0650\u0644\u0651\u064e\u0629\u0657 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0635\u064e\u0670\u063a\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0631\u06e1\u062c\u0650\u0639\u06e1", "word_translation": "Return", "word_transliteration": "ir'ji'"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "to them", "word_transliteration": "ilayhim"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0646\u064e\u0623\u06e1\u062a\u0650\u064a\u064e\u0646\u0651\u064e\u0647\u064f\u0645", "word_translation": "surely we will come to them", "word_transliteration": "falanatiyannahum"}, {"word_arabic_uthmani": "\u0628\u0650\u062c\u064f\u0646\u064f\u0648\u062f\u0656", "word_translation": "with hosts", "word_transliteration": "bijunudin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0642\u0650\u0628\u064e\u0644\u064e", "word_translation": "(is) resistance", "word_transliteration": "qibala"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u064e\u0627", "word_translation": "of it", "word_transliteration": "biha"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0646\u064f\u062e\u06e1\u0631\u0650\u062c\u064e\u0646\u0651\u064e\u0647\u064f\u0645", "word_translation": "and surely we will drive them out", "word_transliteration": "walanukh'rijannahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064e\u0627\u0653", "word_translation": "from there", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0623\u064e\u0630\u0650\u0644\u0651\u064e\u0629\u0657", "word_translation": "(in) humiliation", "word_transliteration": "adhillatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0635\u064e\u0670\u063a\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(will be) abased", "word_transliteration": "saghiruna"}]}