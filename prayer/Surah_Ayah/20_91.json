{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0644\u064e\u0646 \u0646\u0651\u064e\u0628\u06e1\u0631\u064e\u062d\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0639\u064e\u0670\u0643\u0650\u0641\u0650\u064a\u0646\u064e \u062d\u064e\u062a\u0651\u064e\u0649\u0670 \u064a\u064e\u0631\u06e1\u062c\u0650\u0639\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u0645\u064f\u0648\u0633\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0644\u064e\u0646", "word_translation": "Never", "word_transliteration": "lan"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0628\u06e1\u0631\u064e\u062d\u064e", "word_translation": "we will cease", "word_transliteration": "nabraha"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "being devoted to it", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0643\u0650\u0641\u0650\u064a\u0646\u064e", "word_translation": "being devoted to it", "word_transliteration": "akifina"}, {"word_arabic_uthmani": "\u062d\u064e\u062a\u0651\u064e\u0649\u0670", "word_translation": "until", "word_transliteration": "hatta"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u062c\u0650\u0639\u064e", "word_translation": "returns", "word_transliteration": "yarji'a"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "to us", "word_transliteration": "ilayna"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}]}