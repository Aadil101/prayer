{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0645\u064e\u0646 \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0621\u064e\u0627\u0645\u064e\u0646\u0651\u064e\u0627 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0628\u0650\u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u0650 \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u0650 \u0648\u064e\u0645\u064e\u0627 \u0647\u064f\u0645 \u0628\u0650\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e", "word_translation": "And of", "word_transliteration": "wamina"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "the people", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(are some) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "say", "word_transliteration": "yaqulu"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u0651\u064e\u0627", "word_translation": "We believed", "word_transliteration": "amanna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "in Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u0650\u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u0650", "word_translation": "and in the Day", "word_transliteration": "wabil-yawmi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u0650", "word_translation": "[the] Last", "word_transliteration": "al-akhiri"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "but not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0647\u064f\u0645", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "(are) believers (at all)", "word_transliteration": "bimu'minina"}]}