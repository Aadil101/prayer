{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646\u06e2 \u0628\u064e\u064a\u06e1\u0646\u0650 \u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1 \u0633\u064e\u062f\u0651\u0657\u0627 \u0648\u064e\u0645\u0650\u0646\u06e1 \u062e\u064e\u0644\u06e1\u0641\u0650\u0647\u0650\u0645\u06e1 \u0633\u064e\u062f\u0651\u0657\u0627 \u0641\u064e\u0623\u064e\u063a\u06e1\u0634\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0641\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064f\u0628\u06e1\u0635\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "And We have made", "word_transliteration": "waja'alna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e2", "word_translation": "before them", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u0650", "word_translation": "before them", "word_transliteration": "bayni"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "before them", "word_transliteration": "aydihim"}, {"word_arabic_uthmani": "\u0633\u064e\u062f\u0651\u0657\u0627", "word_translation": "a barrier", "word_transliteration": "saddan"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1", "word_translation": "and behind them", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0641\u0650\u0647\u0650\u0645\u06e1", "word_translation": "and behind them", "word_transliteration": "khalfihim"}, {"word_arabic_uthmani": "\u0633\u064e\u062f\u0651\u0657\u0627", "word_translation": "a barrier", "word_transliteration": "saddan"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u063a\u06e1\u0634\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "and We covered them", "word_transliteration": "fa-aghshaynahum"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so they", "word_transliteration": "fahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0628\u06e1\u0635\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "see", "word_transliteration": "yub'siruna"}]}