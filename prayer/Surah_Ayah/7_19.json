{"ayahs": {"ayah_uthmani": "\u0648\u064e\u064a\u064e\u0640\u0670\u0653\u0640\u0654\u064e\u0627\u062f\u064e\u0645\u064f \u0671\u0633\u06e1\u0643\u064f\u0646\u06e1 \u0623\u064e\u0646\u062a\u064e \u0648\u064e\u0632\u064e\u0648\u06e1\u062c\u064f\u0643\u064e \u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e \u0641\u064e\u0643\u064f\u0644\u064e\u0627 \u0645\u0650\u0646\u06e1 \u062d\u064e\u064a\u06e1\u062b\u064f \u0634\u0650\u0626\u06e1\u062a\u064f\u0645\u064e\u0627 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0642\u06e1\u0631\u064e\u0628\u064e\u0627 \u0647\u064e\u0670\u0630\u0650\u0647\u0650 \u0671\u0644\u0634\u0651\u064e\u062c\u064e\u0631\u064e\u0629\u064e \u0641\u064e\u062a\u064e\u0643\u064f\u0648\u0646\u064e\u0627 \u0645\u0650\u0646\u064e \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0640\u0670\u0653\u0640\u0654\u064e\u0627\u062f\u064e\u0645\u064f", "word_translation": "And O Adam!", "word_transliteration": "wayaadamu"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u0643\u064f\u0646\u06e1", "word_translation": "Dwell", "word_transliteration": "us'kun"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "you", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0648\u064e\u0632\u064e\u0648\u06e1\u062c\u064f\u0643\u064e", "word_translation": "and your wife", "word_transliteration": "wazawjuka"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e", "word_translation": "(in) the Garden", "word_transliteration": "al-janata"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064f\u0644\u064e\u0627", "word_translation": "and you both eat", "word_transliteration": "fakula"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062d\u064e\u064a\u06e1\u062b\u064f", "word_translation": "wherever", "word_transliteration": "haythu"}, {"word_arabic_uthmani": "\u0634\u0650\u0626\u06e1\u062a\u064f\u0645\u064e\u0627", "word_translation": "you both wish", "word_transliteration": "shi'tuma"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "but (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u06e1\u0631\u064e\u0628\u064e\u0627", "word_translation": "approach [you both]", "word_transliteration": "taqraba"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u0650\u0647\u0650", "word_translation": "this", "word_transliteration": "hadhihi"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u062c\u064e\u0631\u064e\u0629\u064e", "word_translation": "[the] tree", "word_transliteration": "al-shajarata"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0643\u064f\u0648\u0646\u064e\u0627", "word_translation": "lest you both be", "word_transliteration": "fatakuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "among", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimina"}]}