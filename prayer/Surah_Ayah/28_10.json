{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u0628\u064e\u062d\u064e \u0641\u064f\u0624\u064e\u0627\u062f\u064f \u0623\u064f\u0645\u0651\u0650 \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0641\u064e\u0670\u0631\u0650\u063a\u064b\u0627\u06d6 \u0625\u0650\u0646 \u0643\u064e\u0627\u062f\u064e\u062a\u06e1 \u0644\u064e\u062a\u064f\u0628\u06e1\u062f\u0650\u064a \u0628\u0650\u0647\u0650\u06e6 \u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653 \u0623\u064e\u0646 \u0631\u0651\u064e\u0628\u064e\u0637\u06e1\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u0649\u0670 \u0642\u064e\u0644\u06e1\u0628\u0650\u0647\u064e\u0627 \u0644\u0650\u062a\u064e\u0643\u064f\u0648\u0646\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u0628\u064e\u062d\u064e", "word_translation": "And became", "word_transliteration": "wa-asbaha"}, {"word_arabic_uthmani": "\u0641\u064f\u0624\u064e\u0627\u062f\u064f", "word_translation": "(the) heart", "word_transliteration": "fuadu"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u0650", "word_translation": "(of the) mother", "word_transliteration": "ummi"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "(of) Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0641\u064e\u0670\u0631\u0650\u063a\u064b\u0627\u06d6", "word_translation": "empty", "word_transliteration": "farighan"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "That", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u062f\u064e\u062a\u06e1", "word_translation": "she was near", "word_transliteration": "kadat"}, {"word_arabic_uthmani": "\u0644\u064e\u062a\u064f\u0628\u06e1\u062f\u0650\u064a", "word_translation": "(to) disclosing", "word_transliteration": "latub'di"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "about him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653", "word_translation": "if not", "word_transliteration": "lawla"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u064e\u0637\u06e1\u0646\u064e\u0627", "word_translation": "We strengthened", "word_transliteration": "rabatna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "[over]", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0642\u064e\u0644\u06e1\u0628\u0650\u0647\u064e\u0627", "word_translation": "her heart", "word_transliteration": "qalbiha"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064e\u0643\u064f\u0648\u0646\u064e", "word_translation": "so that she would be", "word_transliteration": "litakuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "the believers", "word_transliteration": "al-mu'minina"}]}