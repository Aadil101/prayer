{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0623\u064e\u0645\u0651\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0648\u064e\u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627\u0652 \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064e\u0670\u062a\u0650 \u0641\u064e\u064a\u064f\u0648\u064e\u0641\u0651\u0650\u064a\u0647\u0650\u0645\u06e1 \u0623\u064f\u062c\u064f\u0648\u0631\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u064a\u064e\u0632\u0650\u064a\u062f\u064f\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6\u06d6 \u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u0633\u06e1\u062a\u064e\u0646\u0643\u064e\u0641\u064f\u0648\u0627\u0652 \u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652 \u0641\u064e\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064b\u0627 \u0623\u064e\u0644\u0650\u064a\u0645\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u062c\u0650\u062f\u064f\u0648\u0646\u064e \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0644\u0650\u064a\u0651\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u0646\u064e\u0635\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0645\u0651\u064e\u0627", "word_translation": "Then as for", "word_transliteration": "fa-amma"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believed", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "and did", "word_transliteration": "wa'amilu"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064e\u0670\u062a\u0650", "word_translation": "the righteous deeds", "word_transliteration": "al-salihati"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064f\u0648\u064e\u0641\u0651\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "then He will give them in full", "word_transliteration": "fayuwaffihim"}, {"word_arabic_uthmani": "\u0623\u064f\u062c\u064f\u0648\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their reward", "word_transliteration": "ujurahum"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0632\u0650\u064a\u062f\u064f\u0647\u064f\u0645", "word_translation": "and give them more", "word_transliteration": "wayaziduhum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6\u06d6", "word_translation": "His Bounty", "word_transliteration": "fadlihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627", "word_translation": "And as for", "word_transliteration": "wa-amma"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0646\u0643\u064e\u0641\u064f\u0648\u0627\u0652", "word_translation": "disdained", "word_transliteration": "is'tankafu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "and were arrogant", "word_transliteration": "wa-is'takbaru"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0647\u064f\u0645\u06e1", "word_translation": "then He will punish them", "word_transliteration": "fayu'adhibuhum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064b\u0627", "word_translation": "(with) a punishment", "word_transliteration": "adhaban"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0650\u064a\u0645\u0657\u0627", "word_translation": "painful", "word_transliteration": "aliman"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u062c\u0650\u062f\u064f\u0648\u0646\u064e", "word_translation": "will they find", "word_transliteration": "yajiduna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "for themselves", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u0651\u0657\u0627", "word_translation": "any protector", "word_transliteration": "waliyyan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u0650\u064a\u0631\u0657\u0627", "word_translation": "any helper", "word_transliteration": "nasiran"}]}