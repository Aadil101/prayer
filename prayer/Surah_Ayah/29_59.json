{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0635\u064e\u0628\u064e\u0631\u064f\u0648\u0627\u0652 \u0648\u064e\u0639\u064e\u0644\u064e\u0649\u0670 \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1 \u064a\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "Those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0635\u064e\u0628\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "(are) patient", "word_transliteration": "sabaru"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "and upon", "word_transliteration": "wa'ala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their Lord", "word_transliteration": "rabbihim"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "put their trust", "word_transliteration": "yatawakkaluna"}]}