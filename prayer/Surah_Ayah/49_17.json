{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0645\u064f\u0646\u0651\u064f\u0648\u0646\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e \u0623\u064e\u0646\u06e1 \u0623\u064e\u0633\u06e1\u0644\u064e\u0645\u064f\u0648\u0627\u0652\u06d6 \u0642\u064f\u0644 \u0644\u0651\u064e\u0627 \u062a\u064e\u0645\u064f\u0646\u0651\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u064a\u0651\u064e \u0625\u0650\u0633\u06e1\u0644\u064e\u0670\u0645\u064e\u0643\u064f\u0645\u06d6 \u0628\u064e\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064e\u0645\u064f\u0646\u0651\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u0623\u064e\u0646\u06e1 \u0647\u064e\u062f\u064e\u0649\u0670\u0643\u064f\u0645\u06e1 \u0644\u0650\u0644\u06e1\u0625\u0650\u064a\u0645\u064e\u0670\u0646\u0650 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u0635\u064e\u0670\u062f\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0645\u064f\u0646\u0651\u064f\u0648\u0646\u064e", "word_translation": "They consider (it) a favor", "word_transliteration": "yamunnuna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e", "word_translation": "to you", "word_transliteration": "alayka"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u0644\u064e\u0645\u064f\u0648\u0627\u0652\u06d6", "word_translation": "they have accepted Islam", "word_transliteration": "aslamu"}, {"word_arabic_uthmani": "\u0642\u064f\u0644", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0645\u064f\u0646\u0651\u064f\u0648\u0627\u0652", "word_translation": "consider a favor", "word_transliteration": "tamunnu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u0651\u064e", "word_translation": "to me", "word_transliteration": "alayya"}, {"word_arabic_uthmani": "\u0625\u0650\u0633\u06e1\u0644\u064e\u0670\u0645\u064e\u0643\u064f\u0645\u06d6", "word_translation": "your Islam", "word_transliteration": "is'lamakum"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u0650", "word_translation": "Nay", "word_transliteration": "bali"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u064f\u0646\u0651\u064f", "word_translation": "has conferred a favor", "word_transliteration": "yamunnu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "upon you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0647\u064e\u062f\u064e\u0649\u0670\u0643\u064f\u0645\u06e1", "word_translation": "He has guided you", "word_transliteration": "hadakum"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0625\u0650\u064a\u0645\u064e\u0670\u0646\u0650", "word_translation": "to the faith", "word_transliteration": "lil'imani"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you are", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0635\u064e\u0670\u062f\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "truthful", "word_transliteration": "sadiqina"}]}