{"ayahs": {"ayah_uthmani": "\u0639\u064e\u0670\u0644\u0650\u0645\u0650 \u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650 \u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0647\u064e\u0670\u062f\u064e\u0629\u0650 \u0641\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649\u0670 \u0639\u064e\u0645\u0651\u064e\u0627 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0639\u064e\u0670\u0644\u0650\u0645\u0650", "word_translation": "Knower", "word_transliteration": "alimi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650", "word_translation": "(of) the unseen", "word_transliteration": "al-ghaybi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0647\u064e\u0670\u062f\u064e\u0629\u0650", "word_translation": "and the witnessed", "word_transliteration": "wal-shahadati"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649\u0670", "word_translation": "exalted is He", "word_transliteration": "fata'ala"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "above what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "they associate", "word_transliteration": "yush'rikuna"}]}