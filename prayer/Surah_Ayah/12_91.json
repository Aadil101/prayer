{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u062a\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0644\u064e\u0642\u064e\u062f\u06e1 \u0621\u064e\u0627\u062b\u064e\u0631\u064e\u0643\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u0648\u064e\u0625\u0650\u0646 \u0643\u064f\u0646\u0651\u064e\u0627 \u0644\u064e\u062e\u064e\u0670\u0637\u0650\u0640\u0654\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u062a\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "By Allah", "word_transliteration": "tal-lahi"}, {"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "certainly", "word_transliteration": "laqad"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062b\u064e\u0631\u064e\u0643\u064e", "word_translation": "Allah has preferred you", "word_transliteration": "atharaka"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah has preferred you", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "over us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "and indeed", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "we have been", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0644\u064e\u062e\u064e\u0670\u0637\u0650\u0640\u0654\u0650\u064a\u0646\u064e", "word_translation": "sinners", "word_transliteration": "lakhatiina"}]}