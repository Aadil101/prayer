{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0670\u0631\u064f\u0648\u0646\u064e \u0648\u064e\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0648\u064e\u0647\u064e\u0670\u0645\u064e\u0670\u0646\u064e\u06d6 \u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0648\u0633\u064e\u0649\u0670 \u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u064a\u0651\u0650\u0646\u064e\u0670\u062a\u0650 \u0641\u064e\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0633\u064e\u0670\u0628\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0670\u0631\u064f\u0648\u0646\u064e", "word_translation": "And Qarun", "word_transliteration": "waqaruna"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "and Firaun", "word_transliteration": "wafir'awna"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064e\u0670\u0645\u064e\u0670\u0646\u064e\u06d6", "word_translation": "and Haman", "word_transliteration": "wahamana"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And certainly", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645", "word_translation": "came to them", "word_transliteration": "jaahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u064a\u0651\u0650\u0646\u064e\u0670\u062a\u0650", "word_translation": "with clear evidences", "word_transliteration": "bil-bayinati"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0633\u06e1\u062a\u06