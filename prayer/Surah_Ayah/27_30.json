{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0645\u0650\u0646 \u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0628\u0650\u0633\u06e1\u0645\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650 \u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed it", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "(is) from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e", "word_translation": "Sulaiman", "word_transliteration": "sulaymana"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "and indeed it (is)", "word_transliteration": "wa-innahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0633\u06e1\u0645\u0650", "word_translation": "In the name", "word_transliteration": "bis'mi"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmani"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u0650", "word_translation": "the Most Merciful", "word_transliteration": "al-rahimi"}]}