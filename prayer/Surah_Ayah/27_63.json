{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u0651\u064e\u0646 \u064a\u064e\u0647\u06e1\u062f\u0650\u064a\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a \u0638\u064f\u0644\u064f\u0645\u064e\u0670\u062a\u0650 \u0671\u0644\u06e1\u0628\u064e\u0631\u0651\u0650 \u0648\u064e\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650 \u0648\u064e\u0645\u064e\u0646 \u064a\u064f\u0631\u06e1\u0633\u0650\u0644\u064f \u0671\u0644\u0631\u0651\u0650\u064a\u064e\u0670\u062d\u064e \u0628\u064f\u0634\u06e1\u0631\u064e\u06e2\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e \u064a\u064e\u062f\u064e\u064a\u06e1 \u0631\u064e\u062d\u06e1\u0645\u064e\u062a\u0650\u0647\u0650\u06e6\u0653\u06d7 \u0623\u064e\u0621\u0650\u0644\u064e\u0670\u0647\u065e \u0645\u0651\u064e\u0639\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0645\u0651\u064e\u0627 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u0651\u064e\u0646", "word_translation": "Or Who", "word_transliteration": "amman"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062f\u0650\u064a\u0643\u064f\u0645\u06e1", "word_translation": "guides you", "word_transliteration": "yahdikum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0638\u064f\u0644\u064f\u0645\u064e\u0670\u062a\u0650", "word_translation": "(the) darkness[es]", "word_transliteration": "zulumati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0631\u0651\u0650", "word_translation": "(of) the land", "word_transliteration": "al-bari"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650", "word_translation": "and the sea", "word_transliteration": "wal-bahri"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "and Who", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u06e1\u0633\u0650\u0644\u064f", "word_translation": "sends", "word_transliteration": "yur'silu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u0650\u064a\u064e\u0670\u062d\u064e", "word_translation": "the winds", "word_transliteration": "al-riyaha"}, {"word_arabic_uthmani": "\u0628\u064f\u0634\u06e1\u0631\u064e\u06e2\u0627", "word_translation": "(as) glad tidings", "word_transliteration": "bush'ran"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e", "word_translation": "before", "word_transliteration": "bayna"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u064e\u064a\u06e1", "word_translation": "before", "word_transliteration": "yaday"}, {"word_arabic_uthmani": "\u0631\u064e\u062d\u06e1\u0645\u064e\u062a\u0650\u0647\u0650\u06e6\u0653\u06d7", "word_translation": "His Mercy", "word_transliteration": "rahmatihi"}, {"word_arabic_uthmani": "\u0623\u064e\u0621\u0650\u0644\u064e\u0670\u0647\u065e", "word_translation": "Is there any god", "word_transliteration": "a-ilahun"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0639\u064e", "word_translation": "with", "word_transliteration": "ma'a"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649", "word_translation": "High is", "word_transliteration": "ta'ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "above what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "they associate (with Him)", "word_transliteration": "yush'rikuna"}]}