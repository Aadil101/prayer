{"ayahs": {"ayah_uthmani": "\u062c\u064e\u0646\u0651\u064e\u0640\u0670\u062a\u0650 \u0639\u064e\u062f\u06e1\u0646\u064d \u0671\u0644\u0651\u064e\u062a\u0650\u064a \u0648\u064e\u0639\u064e\u062f\u064e \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f \u0639\u0650\u0628\u064e\u0627\u062f\u064e\u0647\u064f\u06e5 \u0628\u0650\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0643\u064e\u0627\u0646\u064e \u0648\u064e\u0639\u06e1\u062f\u064f\u0647\u064f\u06e5 \u0645\u064e\u0623\u06e1\u062a\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u062c\u064e\u0646\u0651\u064e\u0640\u0670\u062a\u0650", "word_translation": "Gardens", "word_transliteration": "jannati"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u06e1\u0646\u064d", "word_translation": "(of) Eden", "word_transliteration": "adnin"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u062a\u0650\u064a", "word_translation": "which", "word_transliteration": "allati"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u062f\u064e", "word_translation": "promised", "word_transliteration": "wa'ada"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmanu"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u064e\u0647\u064f\u06e5", "word_translation": "(to) His slaves", "word_transliteration": "ibadahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650\u06da", "word_translation": "in the unseen", "word_transliteration": "bil-ghaybi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed [it]", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u06e1\u062f\u