{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0643\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u064e\u0643\u06e1\u0631\u0657\u0627 \u0648\u064e\u0645\u064e\u0643\u064e\u0631\u06e1\u0646\u064e\u0627 \u0645\u064e\u0643\u06e1\u0631\u0657\u0627 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0634\u06e1\u0639\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0643\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "So they plotted", "word_transliteration": "wamakaru"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u06e1\u0631\u0657\u0627", "word_translation": "a plot", "word_transliteration": "makran"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0643\u064e\u0631\u06e1\u0646\u064e\u0627", "word_translation": "and We planned", "word_transliteration": "wamakarna"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u06e1\u0631\u0657\u0627", "word_translation": "a plan", "word_transliteration": "makran"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(did) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0639\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "perceive", "word_transliteration": "yash'uruna"}]}