{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1 \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u0671\u0641\u06e1\u062a\u064e\u0631\u064e\u0649\u0670\u0647\u064f\u06da \u0628\u064e\u0644\u06e1 \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f \u0645\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064e \u0644\u0650\u062a\u064f\u0646\u0630\u0650\u0631\u064e \u0642\u064e\u0648\u06e1\u0645\u0657\u0627 \u0645\u0651\u064e\u0627\u0653 \u0623\u064e\u062a\u064e\u0649\u0670\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u0646\u0651\u064e\u0630\u0650\u064a\u0631\u0656 \u0645\u0651\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0643\u064e \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0647\u06e1\u062a\u064e\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "(do) they say", "word_transliteration": "yaquluna"}, {"word_arabic_uthmani": "\u0671\u0641\u06e1\u062a\u064e\u0631\u064e\u0649\u0670\u0647\u064f\u06da", "word_translation": "He invented it", "word_transliteration": "if'tarahu"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "it", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f", "word_translation": "(is) the truth", "word_transliteration": "al-haqu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064f\u0646\u0630\u0650\u0631\u064e", "word_translati