{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0644\u064e\u064a\u06e1\u0633\u064e \u0628\u0650\u064a \u0633\u064e\u0641\u064e\u0627\u0647\u064e\u0629\u065e \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u0650\u064a \u0631\u064e\u0633\u064f\u0648\u0644\u065e \u0645\u0651\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650 \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "O my people", "word_transliteration": "yaqawmi"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "(There is) no", "word_transliteration": "laysa"}, {"word_arabic_uthmani": "\u0628\u0650\u064a", "word_translation": "in me", "word_transliteration": "bi"}, {"word_arabic_uthmani": "\u0633\u064e\u0641\u064e\u0627\u0647\u064e\u0629\u065e", "word_translation": "foolishness", "word_transliteration": "safahatun"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u0650\u064a", "word_translation": "but I am", "word_transliteration": "walakinni"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u065e", "word_translation": "a Messenger", "word_transliteration": "rasulun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650", "word_translation": "(the) Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the worlds", "word_transliteration": "al-'alamina"}]}