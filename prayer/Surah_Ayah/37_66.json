{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0623\u0653\u0643\u0650\u0644\u064f\u0648\u0646\u064e \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u0641\u064e\u0645\u064e\u0627\u0644\u0650\u0640\u0654\u064f\u0648\u0646\u064e \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u0671\u0644\u06e1\u0628\u064f\u0637\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "And indeed they", "word_transliteration": "fa-innahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u0653\u0643\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "(will) surely eat", "word_transliteration": "laakiluna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "from it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627\u0644\u0650\u0640\u0654\u064f\u0648\u0646\u064e", "word_translation": "and fill", "word_transliteration": "famaliuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "with it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064f\u0637\u064f\u0648\u0646\u064e", "word_translation": "(their) bellies", "word_transliteration": "al-butuna"}]}