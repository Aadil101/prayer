{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u062e\u064e\u0670\u0645\u0650\u0633\u064e\u0629\u064f \u0623\u064e\u0646\u0651\u064e \u0644\u064e\u0639\u06e1\u0646\u064e\u062a\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0625\u0650\u0646 \u0643\u064e\u0627\u0646\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0643\u064e\u0670\u0630\u0650\u0628\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u062e\u064e\u0670\u0645\u0650\u0633\u064e\u0629\u064f", "word_translation": "And the fifth", "word_transliteration": "wal-khamisatu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e", "word_translation": "that", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u06e1\u0646\u064e\u062a\u064e", "word_translation": "(the) curse of Allah", "word_transliteration": "la'nata"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(the) curse of Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "(be) upon him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "he is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0630\u0650\u0628\u0650\u064a\u0646\u064e", "word_translation": "the liars", "word_transliteration": "al-kadhibina"}]}