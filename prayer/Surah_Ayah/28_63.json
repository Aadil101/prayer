{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u062d\u064e\u0642\u0651\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u064f \u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627 \u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0623\u064e\u063a\u06e1\u0648\u064e\u064a\u06e1\u0646\u064e\u0627\u0653 \u0623\u064e\u063a\u06e1\u0648\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0643\u064e\u0645\u064e\u0627 \u063a\u064e\u0648\u064e\u064a\u06e1\u0646\u064e\u0627\u06d6 \u062a\u064e\u0628\u064e\u0631\u0651\u064e\u0623\u06e1\u0646\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e\u06d6 \u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u064a\u0651\u064e\u0627\u0646\u064e\u0627 \u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "(Will) say", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u062d\u064e\u0642\u0651\u064e", "word_translation": "(has) come true", "word_transliteration": "haqqa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f", "word_translation": "against whom", "word_transliteration": "alayhimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u064f", "word_translation": "the Word", "word_transliteration": "al-qawlu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627", "word_translation": "Our Lord", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650", "word_translation": "These", "word_transliteration": "haulai"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(are) those whom", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0623\u064e\u063a\u06e1\u0648\u064e\u064a\u06e1\u0646\u064e\u0627\u0653", "word_translation": "we led astray", "word_transliteration": "aghwayna"}, {"word_arabic_uthmani": "\u0623\u064e\u063a\u06e1\u0648\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "We led them astray", "word_transliteration": "aghwaynahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u0627", "word_translation": "as", "word_transliteration": "kama"}, {"word_arabic_uthmani": "\u063a\u064e\u0648\u064e\u064a\u06e1\u0646\u064e\u0627\u06d6", "word_translation": "we were astray", "word_transliteration": "ghawayna"}, {"word_arabic_uthmani": "\u062a\u064e\u0628\u064e\u0631\u0651\u064e\u0623\u06e1\u0646\u064e\u0627\u0653", "word_translation": "We declare our innocence", "word_transliteration": "tabarrana"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e\u06d6", "word_translation": "before You", "word_transliteration": "ilayka"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0653\u0627\u0652", "word_translation": "they used (to)", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u0625\u0650\u064a\u0651\u064e\u0627\u0646\u064e\u0627", "word_translation": "worship us", "word_transliteration": "iyyana"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e", "word_translation": "worship us", "word_transliteration": "ya'buduna"}]}