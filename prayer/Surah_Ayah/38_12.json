{"ayahs": {"ayah_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064e\u062a\u06e1 \u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1 \u0642\u064e\u0648\u06e1\u0645\u064f \u0646\u064f\u0648\u062d\u0656 \u0648\u064e\u0639\u064e\u0627\u062f\u065e \u0648\u064e\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064f \u0630\u064f\u0648 \u0671\u0644\u06e1\u0623\u064e\u0648\u06e1\u062a\u064e\u0627\u062f\u0650"}, "words": [{"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064e\u062a\u06e1", "word_translation": "Denied", "word_transliteration": "kadhabat"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "before them", "word_transliteration": "qablahum"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064f", "word_translation": "(the) people", "word_transliteration": "qawmu"}, {"word_arabic_uthmani": "\u0646\u064f\u0648\u062d\u0656", "word_translation": "(of) Nuh", "word_transliteration": "nuhin"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0627\u062f\u065e", "word_translation": "and Aad", "word_transliteration": "wa'adun"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064f", "word_translation": "and Firaun", "word_transliteration": "wafir'awnu"}, {"word_arabic_uthmani": "\u0630\u064f\u0648", "word_translation": "(the) owner", "word_transliteration": "dhu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0648\u06e1\u062a\u064e\u0627\u062f\u0650", "word_translation": "(of) the stakes", "word_transliteration": "al-awtadi"}]}