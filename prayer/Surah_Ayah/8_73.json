{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0628\u064e\u0639\u06e1\u0636\u064f\u0647\u064f\u0645\u06e1 \u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064f \u0628\u064e\u0639\u06e1\u0636\u064d\u06da \u0625\u0650\u0644\u0651\u064e\u0627 \u062a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0647\u064f \u062a\u064e\u0643\u064f\u0646 \u0641\u0650\u062a\u06e1\u0646\u064e\u0629\u065e \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0641\u064e\u0633\u064e\u0627\u062f\u065e \u0643\u064e\u0628\u0650\u064a\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "And those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064f\u0647\u064f\u0645\u06e1", "word_translation": "some of them", "word_transliteration": "ba'duhum"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064f", "word_translation": "(are) allies", "word_transliteration": "awliyau"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064d\u06da", "word_translation": "(to) another", "word_transliteration": "ba'din"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "If not", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0647\u064f", "word_translation": "you do it", "word_transliteration": "taf 'aluhu"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u064f\u0646", "word_translation": "(there) will be", "word_transliteration": "takun"}, {"word_arabic_uthmani": "\u0641\u0650\u062a\u06e1\u0646\u064e\u0629\u065e", "word_translation": "oppression", "word_transliteration": "fit'natun"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u064e\u0633\u064e\u0627\u062f\u065e", "word_translation": "and corruption", "word_transliteration": "wafasadun"}, {"word_arabic_uthmani": "\u0643\u064e\u0628\u0650\u064a\u0631\u065e", "word_translation": "great", "word_transliteration": "kabirun"}]}