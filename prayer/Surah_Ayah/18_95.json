{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0645\u064e\u0627 \u0645\u064e\u0643\u0651\u064e\u0646\u0651\u0650\u064a \u0641\u0650\u064a\u0647\u0650 \u0631\u064e\u0628\u0651\u0650\u064a \u062e\u064e\u064a\u06e1\u0631\u065e \u0641\u064e\u0623\u064e\u0639\u0650\u064a\u0646\u064f\u0648\u0646\u0650\u064a \u0628\u0650\u0642\u064f\u0648\u0651\u064e\u0629\u064d \u0623\u064e\u062c\u06e1\u0639\u064e\u0644\u06e1 \u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1 \u0631\u064e\u062f\u06e1\u0645\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "What", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u0651\u064e\u0646\u0651\u0650\u064a", "word_translation": "has established me", "word_transliteration": "makkanni"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650", "word_translation": "[in it]", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a", "word_translation": "my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u065e", "word_translation": "(is) better", "word_transliteration": "khayrun"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0639\u0650\u064a\u0646\u064f\u0648\u0646\u0650\u064a", "word_translation": "but assist me", "word_transliteration": "fa-a'inuni"}, {"word_arabic_uthmani": "\u0628\u0650\u0642\u064f\u0648\u0651\u064e\u0629\u064d", "word_translation": "with strength", "word_transliteration": "biquwwatin"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0639\u064e\u0644\u06e1", "word_translation": "I will make", "word_transliteration": "aj'al"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1", "word_translation": "between you", "word_transliteration": "baynakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and between them", "word_transliteration": "wabaynahum"}, {"word_arabic_uthmani": "\u0631\u064e\u062f\u06e1\u0645\u064b\u0627", "word_translation": "a barrier", "word_transliteration": "radman"}]}