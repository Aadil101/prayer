{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u0636\u06e1\u0631\u0650\u0628\u064f\u0648\u0627\u0652 \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u062b\u064e\u0627\u0644\u064e\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u0644\u064e\u0627 \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "So (do) not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u062a\u064e\u0636\u06e1\u0631\u0650\u0628\u064f\u0648\u0627\u0652", "word_translation": "put forth", "word_transliteration": "tadribu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "for Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u062b\u064e\u0627\u0644\u064e\u06da", "word_translation": "the similitude", "word_transliteration": "al-amthala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "knows", "word_transliteration": "ya'lamu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "and you", "word_transliteration": "wa-antum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "know", "word_transliteration": "ta'lamuna"}]}