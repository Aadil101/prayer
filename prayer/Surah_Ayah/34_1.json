{"ayahs": {"ayah_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0644\u064e\u0647\u064f\u06e5 \u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0644\u064e\u0647\u064f \u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650\u06da \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f \u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f", "word_translation": "All praises", "word_transliteration": "al-hamdu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "(be) to Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "the One to Whom belongs", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "the One to Whom belongs", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f", "word_translation": "and for Him", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f", "word_translation": "(are) all praises", "word_transliteration": "al-hamdu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650\u06da", "word_translation": "the Hereafter", "word_transliteration": "al-akhirati"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f", "word_translation": "(is) the All-Wise", "word_transliteration": "al-hakimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u0631\u064f", "word_translation": "the All-Aware", "word_transliteration": "al-khabiru"}]}