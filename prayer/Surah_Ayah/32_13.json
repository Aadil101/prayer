{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1 \u0634\u0650\u0626\u06e1\u0646\u064e\u0627 \u0644\u064e\u0623\u0653\u062a\u064e\u064a\u06e1\u0646\u064e\u0627 \u0643\u064f\u0644\u0651\u064e \u0646\u064e\u0641\u06e1\u0633\u064d \u0647\u064f\u062f\u064e\u0649\u0670\u0647\u064e\u0627 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u06e1 \u062d\u064e\u0642\u0651\u064e \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u064f \u0645\u0650\u0646\u0651\u0650\u064a \u0644\u064e\u0623\u064e\u0645\u06e1\u0644\u064e\u0623\u064e\u0646\u0651\u064e \u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u064e\u0629\u0650 \u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0623\u064e\u062c\u06e1\u0645\u064e\u0639\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0634\u0650\u0626\u06e1\u0646\u064e\u0627", "word_translation": "We (had) willed", "word_transliteration": "shi'na"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u0653\u062a\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "surely We (would) have given", "word_transliteration": "laatayna"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064e", "word_translation": "every", "word_transliteration": "kulla"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0633\u064d", "word_translation": "soul", "word_transliteration": "nafsin"}, {"word_arabic_uthmani": "\u0647\u064f\u062f\u064e\u0649\u0670\u0647\u064e\u0627", "word_translation": "its guidance", "word_transliteration": "hudaha"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u06e1", "word_translation": "but", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u062d\u064e\u0642\u0651\u064e", "word_translation": "(is) true", "word_transliteration": "haqqa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u064f", "word_translation": "the Word", "word_transliteration": "al-qawlu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0651\u0650\u064a", "word_translation": "from Me", "word_transliteration": "minni"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u064e\u0645\u06e1\u0644\u064e\u0623\u064e\u0646\u0651\u064e", "word_translation": "that I will surely fill", "word_transliteration": "la-amla-anna"}, {"word_arabic_uthmani": "\u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e", "word_translation": "Hell", "word_transliteration": "jahannama"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "with", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u064e\u0629\u0650", "word_translation": "the jinn", "word_transliteration": "al-jinati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "and the men", "word_transliteration": "wal-nasi"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0645\u064e\u0639\u0650\u064a\u0646\u064e", "word_translation": "together", "word_transliteration": "ajma'ina"}]}