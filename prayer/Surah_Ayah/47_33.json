{"ayahs": {"ayah_uthmani": "\u06de\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0623\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0627\u0652 \u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064e \u0648\u064e\u0644\u064e\u0627 \u062a\u064f\u0628\u06e1\u0637\u0650\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064e\u0643\u064f\u0645\u06e1"}, "words": [{"word_arabic_uthmani": "\u06de\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O (you who)", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(O) you who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0653\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0623\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0627\u0652", "word_translation": "Obey", "word_transliteration": "ati'u"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0627\u0652", "word_translation": "and obey", "word_transliteration": "wa-ati'u"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064e", "word_translation": "the Messenger", "word_transliteration": "al-rasula"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064f\u0628\u06e1\u0637\u0650\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "make vain", "word_transliteration": "tub'tilu"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your deeds", "word_transliteration": "a'malakum"}]}