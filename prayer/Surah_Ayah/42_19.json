{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0637\u0650\u064a\u0641\u064f\u06e2 \u0628\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6 \u064a\u064e\u0631\u06e1\u0632\u064f\u0642\u064f \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06d6 \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0642\u064e\u0648\u0650\u064a\u0651\u064f \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0637\u0650\u064a\u0641\u064f\u06e2", "word_translation": "(is) Subtle", "word_transliteration": "latifun"}, {"word_arabic_uthmani": "\u0628\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6", "word_translation": "with His slaves", "word_transliteration": "bi'ibadihi"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u0632\u064f\u0642\u064f", "word_translation": "He gives provision", "word_transliteration": "yarzuqu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(to) whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06d6", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u0650\u064a\u0651\u064f", "word_translation": "(is) the All-Strong", "word_transliteration": "al-qawiyu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f", "word_translation": "the All-Mighty", "word_transliteration": "al-'azizu"}]}