{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u0650 \u0671\u0644\u06e1\u064a\u064e\u0647\u064f\u0648\u062f\u064f \u0639\u064f\u0632\u064e\u064a\u06e1\u0631\u064c \u0671\u0628\u06e1\u0646\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u0650 \u0671\u0644\u0646\u0651\u064e\u0635\u064e\u0670\u0631\u064e\u0649 \u0671\u0644\u06e1\u0645\u064e\u0633\u0650\u064a\u062d\u064f \u0671\u0628\u06e1\u0646\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6 \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0642\u064e\u0648\u06e1\u0644\u064f\u0647\u064f\u0645 \u0628\u0650\u0623\u064e\u0641\u06e1\u0648\u064e\u0670\u0647\u0650\u0647\u0650\u0645\u06e1\u06d6 \u064a\u064f\u0636\u064e\u0670\u0647\u0650\u0640\u0654\u064f\u0648\u0646\u064e \u0642\u064e\u0648\u06e1\u0644\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u064f\u06da \u0642\u064e\u0670\u062a\u064e\u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06d6 \u0623\u064e\u0646\u0651\u064e\u0649\u0670 \u064a\u064f\u0624\u06e1\u0641\u064e\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u0650", "word_translation": "And said", "word_transliteration": "waqalati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u064a\u064e\u0647\u064f\u0648\u062f\u064f", "word_translation": "the Jews", "word_transliteration": "al-yahudu"}, {"word_arabic_uthmani": "\u0639\u064f\u0632\u064e\u064a\u06e1\u0631\u064c", "word_translation": "Uzair", "word_transliteration": "uzayrun"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u0646\u064f", "word_translation": "(is) son", "word_transliteration": "ub'nu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u0650", "word_translation": "And said", "word_transliteration": "waqalati"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0635\u064e\u0670\u0631\u064e\u0649", "word_translation": "the Christians", "word_transliteration": "al-nasara"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0633\u0650\u064a\u062d\u064f", "word_translation": "Messiah", "word_transliteration": "al-masihu"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u0646\u064f", "word_translation": "(is) son", "word_transliteration": "ub'nu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0644\u064f\u0647\u064f\u0645", "word_translation": "(is) their saying", "word_transliteration": "qawluhum"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0641\u06e1\u0648\u064e\u0670\u0647\u0650\u0647\u0650\u0645\u06e1\u06d6", "word_translation": "with their mouths", "word_transliteration": "bi-afwahihim"}, {"word_arabic_uthmani": "\u064a\u064f\u0636\u064e\u0670\u0647\u0650\u0640\u0654\u064f\u0648\u0646\u064e", "word_translation": "they imitate", "word_transliteration": "yudahiuna"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0644\u064e", "word_translation": "the saying", "word_transliteration": "qawla"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f\u06da", "word_translation": "before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u0642\u064e\u0670\u062a\u064e\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "(May) Allah destroy them", "word_transliteration": "qatalahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06d6", "word_translation": "(May) Allah destroy them", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0649\u0670", "word_translation": "How", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0641\u064e\u0643\u064f\u0648\u0646\u064e", "word_translation": "deluded are they", "word_transliteration": "yu'fakuna"}]}