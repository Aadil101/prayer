{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627 \u0647\u064e\u062f\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f \u0671\u0644\u0633\u0651\u064e\u0628\u0650\u064a\u0644\u064e \u0625\u0650\u0645\u0651\u064e\u0627 \u0634\u064e\u0627\u0643\u0650\u0631\u0657\u0627 \u0648\u064e\u0625\u0650\u0645\u0651\u064e\u0627 \u0643\u064e\u0641\u064f\u0648\u0631\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0647\u064e\u062f\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f", "word_translation": "guided him", "word_transliteration": "hadaynahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0628\u0650\u064a\u0644\u064e", "word_translation": "(to) the way", "word_transliteration": "al-sabila"}, {"word_arabic_uthmani": "\u0625\u0650\u0645\u0651\u064e\u0627", "word_translation": "whether", "word_transliteration": "imma"}, {"word_arabic_uthmani": "\u0634\u064e\u0627\u0643\u0650\u0631\u0657\u0627", "word_translation": "(he) be grateful", "word_transliteration": "shakiran"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0645\u0651\u064e\u0627", "word_translation": "and whether", "word_transliteration": "wa-imma"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064f\u0648\u0631\u064b\u0627", "word_translation": "(he) be ungrateful", "word_transliteration": "kafuran"}]}