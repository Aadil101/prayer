{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0642\u064e\u0627\u0647\u0650\u0631\u064f \u0641\u064e\u0648\u06e1\u0642\u064e \u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6\u06da \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f \u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0627\u0647\u0650\u0631\u064f", "word_translation": "(is) the Subjugator", "word_transliteration": "al-qahiru"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u06e1\u0642\u064e", "word_translation": "over", "word_transliteration": "fawqa"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6\u06da", "word_translation": "His slaves", "word_transliteration": "ibadihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f", "word_translation": "(is) the All-Wise", "word_transliteration": "al-hakimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u0631\u064f", "word_translation": "the All-Aware", "word_transliteration": "al-khabiru"}]}