{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0647\u064e\u0670\u0630\u064e\u0627\u0653 \u0623\u064e\u062e\u0650\u064a \u0644\u064e\u0647\u064f\u06e5 \u062a\u0650\u0633\u06e1\u0639\u065e \u0648\u064e\u062a\u0650\u0633\u06e1\u0639\u064f\u0648\u0646\u064e \u0646\u064e\u0639\u06e1\u062c\u064e\u0629\u0657 \u0648\u064e\u0644\u0650\u064a\u064e \u0646\u064e\u0639\u06e1\u062c\u064e\u0629\u065e \u0648\u064e\u0670\u062d\u0650\u062f\u064e\u0629\u065e \u0641\u064e\u0642\u064e\u0627\u0644\u064e \u0623\u064e\u0643\u06e1\u0641\u0650\u0644\u06e1\u0646\u0650\u064a\u0647\u064e\u0627 \u0648\u064e\u0639\u064e\u0632\u0651\u064e\u0646\u0650\u064a \u0641\u0650\u064a \u0671\u0644\u06e1\u062e\u0650\u0637\u064e\u0627\u0628\u0650"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627\u0653", "word_translation": "this", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u0650\u064a", "word_translation": "(is) my brother", "word_transliteration": "akhi"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "he has", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u062a\u0650\u0633\u06e1\u0639\u065e", "word_translation": "ninety-nine", "word_transliteration": "tis'un"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u0650\u0633\u06e1\u0639\u064f\u0648\u0646\u064e", "word_translation": "ninety-nine", "word_transliteration": "watis'una"}, {"word_arabic_uthmani": "\u0646\u064e\u0639\u06e1\u062c\u064e\u0629\u0657", "word_translation": "ewe(s)", "word_transliteration": "na'jatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u064e", "word_translation": "while I have", "word_transliteration": "waliya"}, {"word_arabic_uthmani": "\u0646\u064e\u0639\u06e1\u062c\u064e\u0629\u065e", "word_translation": "ewe", "word_transliteration": "na'jatun"}, {"word_arabic_uthmani": "\u0648\u064e\u0670\u062d\u0650\u062f\u064e\u0629\u065e", "word_translation": "one", "word_transliteration": "wahidatun"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "so he said", "word_transliteration": "faqala"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u0641\u0650\u0644\u06e1\u0646\u0650\u064a\u0647\u064e\u0627", "word_translation": "Entrust her to me", "word_transliteration": "akfil'niha"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0632\u0651\u064e\u0646\u0650\u064a", "word_translation": "and he overpowered me", "word_transliteration": "wa'azzani"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u0650\u0637\u064e\u0627\u0628\u0650", "word_translation": "[the] speech", "word_transliteration": "al-khitabi"}]}