{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064f\u0637\u0650\u0639\u0650 \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e \u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e \u0648\u064e\u062f\u064e\u0639\u06e1 \u0623\u064e\u0630\u064e\u0649\u0670\u0647\u064f\u0645\u06e1 \u0648\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u06e1 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0643\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064f\u0637\u0650\u0639\u0650", "word_translation": "obey", "word_transliteration": "tuti'i"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the disbelievers", "word_transliteration": "al-kafirina"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "and the hypocrites", "word_transliteration": "wal-munafiqina"}, {"word_arabic_uthmani": "\u0648\u064e\u062f\u064e\u0639\u06e1", "word_translation": "and disregard", "word_transliteration": "wada'"}, {"word_arabic_uthmani": "\u0623\u064e\u0630\u064e\u0649\u0670\u0647\u064f\u0645\u06e1", "word_translation": "their harm", "word_transliteration": "adhahum"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u06e1", "word_translation": "and put your trust", "word_transliteration": "watawakkal"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "in", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "And sufficient is Allah", "word_transliteration": "wakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "And sufficient is Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u0650\u064a\u0644\u0657\u0627", "word_translation": "(as) a Trustee", "word_transliteration": "wakilan"}]}