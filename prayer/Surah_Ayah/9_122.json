{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u0644\u0650\u064a\u064e\u0646\u0641\u0650\u0631\u064f\u0648\u0627\u0652 \u0643\u064e\u0627\u0653\u0641\u0651\u064e\u0629\u0657\u06da \u0641\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627 \u0646\u064e\u0641\u064e\u0631\u064e \u0645\u0650\u0646 \u0643\u064f\u0644\u0651\u0650 \u0641\u0650\u0631\u06e1\u0642\u064e\u0629\u0656 \u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0637\u064e\u0627\u0653\u0626\u0650\u0641\u064e\u0629\u065e \u0644\u0651\u0650\u064a\u064e\u062a\u064e\u0641\u064e\u0642\u0651\u064e\u0647\u064f\u0648\u0627\u0652 \u0641\u0650\u064a \u0671\u0644\u062f\u0651\u0650\u064a\u0646\u0650 \u0648\u064e\u0644\u0650\u064a\u064f\u0646\u0630\u0650\u0631\u064f\u0648\u0627\u0652 \u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064f\u0645\u06e1 \u0625\u0650\u0630\u064e\u0627 \u0631\u064e\u062c\u064e\u0639\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u062d\u06e1\u0630\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "(for) the believers", "word_transliteration": "al-mu'minuna"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064e\u0646\u0641\u0650\u0631\u064f\u0648\u0627\u0652", "word_translation": "that they go forth", "word_transliteration": "liyanfiru"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0653\u0641\u0651\u064e\u0629\u0657\u06da", "word_translation": "all together", "word_transliteration": "kaffatan"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "So if not", "word_transliteration": "falawla"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u064e\u0631\u064e", "word_translation": "go forth", "word_transliteration": "nafara"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0642\u064e\u0629\u0656", "word_translation": "group", "word_transliteration": "fir'qatin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "among them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0637\u064e\u0627\u0653\u0626\u0650\u0641\u064e\u0629\u065e", "word_translation": "a party", "word_transliteration": "taifatun"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u064a\u064e\u062a\u064e\u0641\u064e\u0642\u0651\u064e\u0647\u064f\u0648\u0627\u0652", "word_translation": "that they may obtain understanding", "word_transliteration": "liyatafaqqahu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u0650\u064a\u0646\u0650", "word_translation": "the religion", "word_transliteration": "al-dini"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u064f\u0646\u0630\u0650\u0631\u064f\u0648\u0627\u0652", "word_translation": "and that they may warn", "word_transliteration": "waliyundhiru"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their people", "word_transliteration": "qawmahum"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u0631\u064e\u062c\u064e\u0639\u064f\u0648\u0653\u0627\u0652", "word_translation": "they return", "word_transliteration": "raja'u"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "to them", "word_transliteration": "ilayhim"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so that they may", "word_transliteration": "la'allahum"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0630\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "beware", "word_transliteration": "yahdharuna"}]}