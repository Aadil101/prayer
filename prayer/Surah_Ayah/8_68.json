{"ayahs": {"ayah_uthmani": "\u0644\u0651\u064e\u0648\u06e1\u0644\u064e\u0627 \u0643\u0650\u062a\u064e\u0670\u0628\u065e \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0633\u064e\u0628\u064e\u0642\u064e \u0644\u064e\u0645\u064e\u0633\u0651\u064e\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a\u0645\u064e\u0627\u0653 \u0623\u064e\u062e\u064e\u0630\u06e1\u062a\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064c \u0639\u064e\u0638\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "Had not", "word_transliteration": "lawla"}, {"word_arabic_uthmani": "\u0643\u0650\u062a\u064e\u0670\u0628\u065e", "word_translation": "an ordainment", "word_transliteration": "kitabun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u064e\u0642\u064e", "word_translation": "preceded", "word_transliteration": "sabaqa"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u064e\u0633\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "surely (would) have touched you", "word_transliteration": "lamassakum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0645\u064e\u0627\u0653", "word_translation": "for what", "word_transliteration": "fima"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u064e\u0630\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "you took", "word_transliteration": "akhadhtum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064c", "word_translation": "a punishment", "word_transliteration": "adhabun"}, {"word_arabic_uthmani": "\u0639\u064e\u0638\u0650\u064a\u0645\u065e", "word_translation": "great", "word_transliteration": "azimun"}]}