{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0627 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0645\u064e\u0646\u06e1 \u062e\u064e\u0644\u064e\u0642\u064e \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u0644\u0651\u064e\u0637\u0650\u064a\u0641\u064f \u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627", "word_translation": "Does not", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "know", "word_transliteration": "ya'lamu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u06e1", "word_translation": "(the One) Who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064e", "word_translation": "created", "word_transliteration": "khalaqa"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0637\u0650\u064a\u0641\u064f", "word_translation": "(is) the Subtle", "word_transliteration": "al-latifu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u0631\u064f", "word_translation": "the All-Aware", "word_transliteration": "al-khabiru"}]}