{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0634\u064e\u0631\u0651\u064e \u0671\u0644\u062f\u0651\u064e\u0648\u064e\u0627\u0653\u0628\u0651\u0650 \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0641\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0634\u064e\u0631\u0651\u064e", "word_translation": "(the) worst", "word_transliteration": "sharra"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064e\u0648\u064e\u0627\u0653\u0628\u0651\u0650", "word_translation": "(of) the living creatures", "word_transliteration": "al-dawabi"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "near", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(are) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "fahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(will) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "believe", "word_transliteration": "yu'minuna"}]}