{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e \u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1 \u0648\u064e\u0645\u064e\u0627 \u062e\u064e\u0644\u06e1\u0641\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u064a\u064f\u062d\u0650\u064a\u0637\u064f\u0648\u0646\u064e \u0628\u0650\u0647\u0650\u06e6 \u0639\u0650\u0644\u06e1\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "He knows", "word_transliteration": "ya'lamu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e", "word_translation": "(is) before them", "word_transliteration": "bayna"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "(is) before them", "word_transliteration": "aydihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "(is) behind them", "word_transliteration": "khalfahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "while not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u0650\u064a\u0637\u064f\u0648\u0646\u064e", "word_translation": "they encompass", "word_transliteration": "yuhituna"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u0657\u0627", "word_translation": "(in) knowledge", "word_transliteration": "il'man"}]}