{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0648\u064e\u0647\u064e\u0627\u062c\u064e\u0631\u064f\u0648\u0627\u0652 \u0648\u064e\u062c\u064e\u0670\u0647\u064e\u062f\u064f\u0648\u0627\u0652 \u0641\u0650\u064a \u0633\u064e\u0628\u0650\u064a\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0628\u0650\u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1 \u0623\u064e\u0639\u06e1\u0638\u064e\u0645\u064f \u062f\u064e\u0631\u064e\u062c\u064e\u0629\u064b \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0641\u064e\u0627\u0653\u0626\u0650\u0632\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "Those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believed", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064e\u0627\u062c\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "and emigrated", "word_transliteration": "wahajaru"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0670\u0647\u064e\u062f\u064f\u0648\u0627\u0652", "word_translation": "and strove", "word_transliteration": "wajahadu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u0650", "word_translation": "(the) way", "word_transliteration": "sabili"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u0650\u0647\u0650\u0645\u06e1", "word_translation": "with their wealth", "word_transliteration": "bi-amwalihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1", "word_translation": "and their lives", "word_transliteration": "wa-anfusihim"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0638\u064e\u0645\u064f", "word_translation": "(are) greater", "word_transliteration": "a'zamu"}, {"word_arabic_uthmani": "\u062f\u064e\u0631\u064e\u062c\u064e\u0629\u064b", "word_translation": "(in) rank", "word_transliteration": "darajatan"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "near", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "And those", "word_transliteration": "wa-ulaika"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "they", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0627\u0653\u0626\u0650\u0632\u064f\u0648\u0646\u064e", "word_translation": "(are) the successful", "word_transliteration": "al-faizuna"}]}