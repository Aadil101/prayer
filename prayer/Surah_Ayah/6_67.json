{"ayahs": {"ayah_uthmani": "\u0644\u0651\u0650\u0643\u064f\u0644\u0651\u0650 \u0646\u064e\u0628\u064e\u0625\u0656 \u0645\u0651\u064f\u0633\u06e1\u062a\u064e\u0642\u064e\u0631\u0651\u065e\u06da \u0648\u064e\u0633\u064e\u0648\u06e1\u0641\u064e \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u0650\u0643\u064f\u0644\u0651\u0650", "word_translation": "For every", "word_transliteration": "likulli"}, {"word_arabic_uthmani": "\u0646\u064e\u0628\u064e\u0625\u0656", "word_translation": "news", "word_transliteration": "naba-in"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0633\u06e1\u062a\u064e\u0642\u064e\u0631\u0651\u065e\u06da", "word_translation": "(is) a fixed time", "word_transliteration": "mus'taqarrun"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064e\u0648\u06e1\u0641\u064e", "word_translation": "and soon", "word_transliteration": "wasawfa"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "you will know", "word_transliteration": "ta'lamuna"}]}