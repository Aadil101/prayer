{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u06e1 \u0644\u0650\u0623\u064f\u062e\u06e1\u062a\u0650\u0647\u0650\u06e6 \u0642\u064f\u0635\u0651\u0650\u064a\u0647\u0650\u06d6 \u0641\u064e\u0628\u064e\u0635\u064f\u0631\u064e\u062a\u06e1 \u0628\u0650\u0647\u0650\u06e6 \u0639\u064e\u0646 \u062c\u064f\u0646\u064f\u0628\u0656 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0634\u06e1\u0639\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u06e1", "word_translation": "And she said", "word_transliteration": "waqalat"}, {"word_arabic_uthmani": "\u0644\u0650\u0623\u064f\u062e\u06e1\u062a\u0650\u0647\u0650\u06e6", "word_translation": "to his sister", "word_transliteration": "li-ukh'tihi"}, {"word_arabic_uthmani": "\u0642\u064f\u0635\u0651\u0650\u064a\u0647\u0650\u06d6", "word_translation": "Follow him", "word_transliteration": "qussihi"}, {"word_arabic_uthmani": "\u0641\u064e\u0628\u064e\u0635\u064f\u0631\u064e\u062a\u06e1", "word_translation": "So she watched", "word_transliteration": "fabasurat"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0639\u064e\u0646", "word_translation": "from", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062c\u064f\u0646\u064f\u0628\u0656", "word_translation": "a distance", "word_transliteration": "junubin"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(did) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0639\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "perceive", "word_transliteration": "yash'uruna"}]}