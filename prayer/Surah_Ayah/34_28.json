{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0643\u064e\u0627\u0653\u0641\u0651\u064e\u0629\u0657 \u0644\u0651\u0650\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0628\u064e\u0634\u0650\u064a\u0631\u0657\u0627 \u0648\u064e\u0646\u064e\u0630\u0650\u064a\u0631\u0657\u0627 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0644\u064e\u0627 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e", "word_translation": "We have sent you", "word_transliteration": "arsalnaka"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0653\u0641\u0651\u064e\u0629\u0657", "word_translation": "inclusively", "word_transliteration": "kaffatan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "to mankind", "word_transliteration": "lilnnasi"}, {"word_arabic_uthmani": "\u0628\u064e\u0634\u0650\u064a\u0631\u0657\u0627", "word_translation": "(as) a giver of glad tidings", "word_transliteration": "bashiran"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u0630\u0650\u064a\u0631\u0657\u0627", "word_translation": "and (as) a warner", "word_transliteration": "wanadhiran"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e", "word_translation": "But", "word_transliteration": "walakinna"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e", "word_translation": "most", "word_transliteration": "akthara"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "[the] people", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "know", "word_transliteration": "ya'lamuna"}]}