{"ayahs": {"ayah_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0631\u064e\u0627\u0628\u064f \u0623\u064e\u0634\u064e\u062f\u0651\u064f \u0643\u064f\u0641\u06e1\u0631\u0657\u0627 \u0648\u064e\u0646\u0650\u0641\u064e\u0627\u0642\u0657\u0627 \u0648\u064e\u0623\u064e\u062c\u06e1\u062f\u064e\u0631\u064f \u0623\u064e\u0644\u0651\u064e\u0627 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0627\u0652 \u062d\u064f\u062f\u064f\u0648\u062f\u064e \u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u0632\u064e\u0644\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u0649\u0670 \u0631\u064e\u0633\u064f\u0648\u0644\u0650\u0647\u0650\u06e6\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u0650\u064a\u0645\u064c \u062d\u064e\u0643\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0631\u064e\u0627\u0628\u064f", "word_translation": "The bedouins", "word_transliteration": "al-a'rabu"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u064e\u062f\u0651\u064f", "word_translation": "(are) stronger", "word_transliteration": "ashaddu"}, {"word_arabic_uthmani": "\u0643\u064f\u0641\u06e1\u0631\u0657\u0627", "word_translation": "(in) disbelief", "word_transliteration": "kuf'ran"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u0650\u0641\u064e\u0627\u0642\u0657\u0627", "word_translation": "and hypocrisy", "word_transliteration": "wanifaqan"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062c\u06e1\u062f\u064e\u0631\u064f", "word_translation": "and more likely", "word_transliteration": "wa-ajdaru"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0651\u064e\u0627", "word_translation": "that not", "word_transliteration": "alla"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0627\u0652", "word_translation": "they know", "word_transliteration": "ya'lamu"}, {"word_arabic_uthmani": "\u062d\u064f\u062f\u064f\u0648\u062f\u064e", "word_translation": "(the) limits", "word_transliteration": "hududa"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "(of) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0632\u064e\u0644\u064e", "word_translation": "Allah (has) revealed", "word_transliteration": "anzala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah (has) revealed", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u0650\u0647\u0650\u06e6\u06d7", "word_translation": "His Messenger", "word_transliteration": "rasulihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u064c", "word_translation": "(is) All-Knower", "word_transliteration": "alimun"}, {"word_arabic_uthmani": "\u062d\u064e\u0643\u0650\u064a\u0645\u065e", "word_translation": "All-Wise", "word_transliteration": "hakimun"}]}