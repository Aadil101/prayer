{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0648\u06e1 \u064a\u064f\u0632\u064e\u0648\u0651\u0650\u062c\u064f\u0647\u064f\u0645\u06e1 \u0630\u064f\u0643\u06e1\u0631\u064e\u0627\u0646\u0657\u0627 \u0648\u064e\u0625\u0650\u0646\u064e\u0670\u062b\u0657\u0627\u06d6 \u0648\u064e\u064a\u064e\u062c\u06e1\u0639\u064e\u0644\u064f \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0639\u064e\u0642\u0650\u064a\u0645\u064b\u0627\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0639\u064e\u0644\u0650\u064a\u0645\u065e \u0642\u064e\u062f\u0650\u064a\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "Or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u064a\u064f\u0632\u064e\u0648\u0651\u0650\u062c\u064f\u0647\u064f\u0645\u06e1", "word_translation": "He grants them", "word_transliteration": "yuzawwijuhum"}, {"word_arabic_uthmani": "\u0630\u064f\u0643\u06e1\u0631\u064e\u0627\u0646\u0657\u0627", "word_translation": "males", "word_transliteration": "dhuk'ranan"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u064e\u0670\u062b\u0657\u0627\u06d6", "word_translation": "and females", "word_transliteration": "wa-inathan"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062c\u06e1\u0639\u064e\u0644\u064f", "word_translation": "and He makes", "word_transliteration": "wayaj'alu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0639\u064e\u0642\u0650\u064a\u0645\u064b\u0627\u06da", "word_translation": "barren", "word_transliteration": "aqiman"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u065e", "word_translation": "(is) All-Knower", "word_transliteration": "alimun"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0650\u064a\u0631\u065e", "word_translation": "All-Powerful", "word_transliteration": "qadirun"}]}