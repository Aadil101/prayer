{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0646\u064f\u0631\u06e1\u0633\u0650\u0644\u064f \u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u0650\u064a\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064f\u0628\u064e\u0634\u0651\u0650\u0631\u0650\u064a\u0646\u064e \u0648\u064e\u0645\u064f\u0646\u0630\u0650\u0631\u0650\u064a\u0646\u064e\u06d6 \u0641\u064e\u0645\u064e\u0646\u06e1 \u0621\u064e\u0627\u0645\u064e\u0646\u064e \u0648\u064e\u0623\u064e\u0635\u06e1\u0644\u064e\u062d\u064e \u0641\u064e\u0644\u064e\u0627 \u062e\u064e\u0648\u06e1\u0641\u064c \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u0647\u064f\u0645\u06e1 \u064a\u064e\u062d\u06e1\u0632\u064e\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0646\u064f\u0631\u06e1\u0633\u0650\u0644\u064f", "word_translation": "We send", "word_transliteration": "nur'silu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u0650\u064a\u0646\u064e", "word_translation": "the Messengers", "word_transliteration": "al-mur'salina"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u064f\u0628\u064e\u0634\u0651\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(as) bearer of glad tidings", "word_transliteration": "mubashirina"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064f\u0646\u0630\u0650\u0631\u0650\u064a\u0646\u064e\u06d6", "word_translation": "and (as) warners", "word_transliteration": "wamundhirina"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0646\u06e1", "word_translation": "So whoever", "word_transliteration": "faman"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064e", "word_translation": "believed", "word_translite