{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0642\u064f\u062a\u0650\u0644\u064f\u0648\u0627\u0652 \u0641\u0650\u064a \u0633\u064e\u0628\u0650\u064a\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u062a\u064e\u06e2\u0627\u06da \u0628\u064e\u0644\u06e1 \u0623\u064e\u062d\u06e1\u064a\u064e\u0627\u0653\u0621\u064c \u0639\u0650\u0646\u062f\u064e \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1 \u064a\u064f\u0631\u06e1\u0632\u064e\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e", "word_translation": "think", "word_transliteration": "tahsabanna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0642\u064f\u062a\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "are killed", "word_transliteration": "qutilu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u0650", "word_translation": "(the) way", "word_transliteration": "sabili"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u062a\u064e\u06e2\u0627\u06da", "word_translation": "(as) dead", "word_transliteration": "amwatan"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay!", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u06e1\u064a\u064e\u0627\u0653\u0621\u064c", "word_translation": "They are alive", "word_transliteration": "ahyaon"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "near", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their Lord", "word_transliteration": "rabbihim"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u06e1\u0632\u064e\u0642\u064f\u0648\u0646\u064e", "word_translation": "they are given provision", "word_transliteration": "yur'zaquna"}]}