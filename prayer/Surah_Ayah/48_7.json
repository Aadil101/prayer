{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u062c\u064f\u0646\u064f\u0648\u062f\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da \u0648\u064e\u0643\u064e\u0627\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0632\u0650\u064a\u0632\u064b\u0627 \u062d\u064e\u0643\u0650\u064a\u0645\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "And for Allah", "word_transliteration": "walillahi"}, {"word_arabic_uthmani": "\u062c\u064f\u0646\u064f\u0648\u062f\u064f", "word_translation": "(are the) hosts", "word_transliteration": "junudu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "and Allah", "word_transliteration": "wakana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "and Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0632\u0650\u064a\u0632\u064b\u0627", "word_translation": "(is) All-Mighty", "word_transliteration": "azizan"}, {"word_arabic_uthmani": "\u062d\u064e\u0643\u0650\u064a\u0645\u064b\u0627", "word_translation": "All-Wise", "word_transliteration": "hakiman"}]}