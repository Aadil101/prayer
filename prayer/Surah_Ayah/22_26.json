{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1 \u0628\u064e\u0648\u0651\u064e\u0623\u06e1\u0646\u064e\u0627 \u0644\u0650\u0625\u0650\u0628\u06e1\u0631\u064e\u0670\u0647\u0650\u064a\u0645\u064e \u0645\u064e\u0643\u064e\u0627\u0646\u064e \u0671\u0644\u06e1\u0628\u064e\u064a\u06e1\u062a\u0650 \u0623\u064e\u0646 \u0644\u0651\u064e\u0627 \u062a\u064f\u0634\u06e1\u0631\u0650\u0643\u06e1 \u0628\u0650\u064a \u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627 \u0648\u064e\u0637\u064e\u0647\u0651\u0650\u0631\u06e1 \u0628\u064e\u064a\u06e1\u062a\u0650\u064a\u064e \u0644\u0650\u0644\u0637\u0651\u064e\u0627\u0653\u0626\u0650\u0641\u0650\u064a\u0646\u064e \u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0627\u0653\u0626\u0650\u0645\u0650\u064a\u0646\u064e \u0648\u064e\u0671\u0644\u0631\u0651\u064f\u0643\u0651\u064e\u0639\u0650 \u0671\u0644\u0633\u0651\u064f\u062c\u064f\u0648\u062f\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1", "word_translation": "And when", "word_transliteration": "wa-idh"}, {"word_arabic_uthmani": "\u0628\u064e\u0648\u0651\u064e\u0623\u06e1\u0646\u064e\u0627", "word_translation": "We assigned", "word_transliteration": "bawwana"}, {"word_arabic_uthmani": "\u0644\u0650\u0625\u0650\u0628\u06e1\u0631\u064e\u0670\u0647\u0650\u064a\u0645\u064e", "word_translation": "to Ibrahim", "word_transliteration": "li-ib'rahima"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "(the) site", "word_transliteration": "makana"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u064a\u06e1\u062a\u0650", "word_translation": "(of) the House", "word_transliteration": "al-bayti"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "That", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064f\u0634\u06e1\u0631\u0650\u0643\u06e1", "word_translation": "associate", "word_transliteration": "tush'rik"}, {"word_arabic_uthmani": "\u0628\u0650\u064a", "word_translation": "with Me", "word_transliteration": "bi"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627", "word_translation": "anything", "word_transliteration": "shayan"}, {"word_arabic_uthmani": "\u0648\u064e\u0637\u064e\u0647\u0651\u0650\u0631\u06e1", "word_translation": "and purify", "word_transliteration": "watahhir"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u062a\u0650\u064a\u064e", "word_translation": "My House", "word_transliteration": "baytiya"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0637\u0651\u064e\u0627\u0653\u0626\u0650\u0641\u0650\u064a\u0646\u064e", "word_translation": "for those who circumambulate", "word_transliteration": "lilttaifina"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0627\u0653\u0626\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "and those who stand", "word_transliteration": "wal-qaimina"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0631\u0651\u064f\u0643\u0651\u064e\u0639\u0650", "word_translation": "and those who bow", "word_transliteration": "wal-ruka'i"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064f\u062c\u064f\u0648\u062f\u0650", "word_translation": "(and) those who prostrate", "word_transliteration": "al-sujudi"}]}