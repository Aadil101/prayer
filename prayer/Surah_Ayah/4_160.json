{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0628\u0650\u0638\u064f\u0644\u06e1\u0645\u0656 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0647\u064e\u0627\u062f\u064f\u0648\u0627\u0652 \u062d\u064e\u0631\u0651\u064e\u0645\u06e1\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u064d \u0623\u064f\u062d\u0650\u0644\u0651\u064e\u062a\u06e1 \u0644\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0628\u0650\u0635\u064e\u062f\u0651\u0650\u0647\u0650\u0645\u06e1 \u0639\u064e\u0646 \u0633\u064e\u0628\u0650\u064a\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0628\u0650\u0638\u064f\u0644\u06e1\u0645\u0656", "word_translation": "Then for (the) wrongdoing", "word_transliteration": "fabizul'min"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0647\u064e\u0627\u062f\u064f\u0648\u0627\u0652", "word_translation": "were Jews", "word_transliteration": "hadu"}, {"word_arabic_uthmani": "\u062d\u064e\u0631\u0651\u064e\u0645\u06e1\u0646\u064e\u0627", "word_translation": "We made unlawful", "word_transliteration": "harramna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "for them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u064d", "word_translation": "good things", "word_transliteration": "tayyibatin"}, {"word_arabic_uthmani": "\u0623\u064f\u062d\u0650\u0644\u0651\u064e\u062a\u06e1", "word_translation": "which had been lawful", "word_transliteration": "uhillat"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u0650\u0635\u064e\u062f\u0651\u0650\u0647\u0650\u0645\u06e1", "word_translation": "and for their hindering", "word_transliteration": "wabisaddihim"}, {"word_arabic_uthmani": "\u0639\u064e\u0646", "word_translation": "from", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u0650", "word_translation": "(the) way", "word_transliteration": "sabili"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627", "word_translation": "many", "word_transliteration": "kathiran"}]}