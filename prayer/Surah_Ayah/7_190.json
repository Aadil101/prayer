{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645\u064e\u0627 \u0635\u064e\u0670\u0644\u0650\u062d\u0657\u0627 \u062c\u064e\u0639\u064e\u0644\u064e\u0627 \u0644\u064e\u0647\u064f\u06e5 \u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0621\u064e \u0641\u0650\u064a\u0645\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645\u064e\u0627\u06da \u0641\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0645\u0651\u064e\u0627 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653", "word_translation": "But when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645\u064e\u0627", "word_translation": "He gives them", "word_transliteration": "atahuma"}, {"word_arabic_uthmani": "\u0635\u064e\u0670\u0644\u0650\u062d\u0657\u0627", "word_translation": "a good (child)", "word_transliteration": "salihan"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u064e\u0627", "word_translation": "they make", "word_transliteration": "ja'ala"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "for Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0621\u064e", "word_translation": "partners", "word_transliteration": "shurakaa"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0645\u064e\u0627\u0653", "word_translation": "in what", "word_transliteration": "fima"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645\u064e\u0627\u06da", "word_translation": "He has given them", "word_transliteration": "atahuma"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649", "word_translation": "But exalted", "word_transliteration": "fata'ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "above what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "they associate (with Him)", "word_transliteration": "yush'rikuna"}]}