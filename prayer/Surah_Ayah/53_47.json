{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0671\u0644\u0646\u0651\u064e\u0634\u06e1\u0623\u064e\u0629\u064e \u0671\u0644\u06e1\u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e", "word_translation": "And that", "word_transliteration": "wa-anna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "upon Him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0634\u06e1\u0623\u064e\u0629\u064e", "word_translation": "(is) bringing forth", "word_transliteration": "al-nashata"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670", "word_translation": "another", "word_transliteration": "al-ukh'ra"}]}