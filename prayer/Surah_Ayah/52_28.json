{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627 \u0643\u064f\u0646\u0651\u064e\u0627 \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u064f \u0646\u064e\u062f\u06e1\u0639\u064f\u0648\u0647\u064f\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0628\u064e\u0631\u0651\u064f \u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed, we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "[we] used (to)", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f", "word_translation": "before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u0646\u064e\u062f\u06e1\u0639\u064f\u0648\u0647\u064f\u06d6", "word_translation": "call Him", "word_transliteration": "nad'uhu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed, He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "[He]", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0631\u0651\u064f", "word_translation": "(is) the Most Kind", "word_transliteration": "al-baru"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f", "word_translation": "the Most Merciful", "word_transliteration": "al-rahimu"}]}