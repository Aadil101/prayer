{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0671\u0644\u06e1\u0639\u0650\u0644\u06e1\u0645\u064f \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0623\u064f\u0628\u064e\u0644\u0651\u0650\u063a\u064f\u0643\u064f\u0645 \u0645\u0651\u064e\u0627\u0653 \u0623\u064f\u0631\u06e1\u0633\u0650\u0644\u06e1\u062a\u064f \u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u0650\u064a\u0653 \u0623\u064e\u0631\u064e\u0649\u0670\u0643\u064f\u0645\u06e1 \u0642\u064e\u0648\u06e1\u0645\u0657\u0627 \u062a\u064e\u062c\u06e1\u0647\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0644\u06e1\u0645\u064f", "word_translation": "the knowledge", "word_transliteration": "al-'il'mu"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "(is) with Allah", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(is) with Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0628\u064e\u0644\u0651\u0650\u063a\u064f\u0643\u064f\u0645", "word_translation": "and I convey to you", "word_transliteration": "wa-uballighukum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627\u0653", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064f\u0631\u06e1\u0633\u0650\u0644\u06e1\u062a\u064f", "word_translation": "I am sent", "word_transliteration": "ur'sil'tu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "but", "word_transliteration": "walakinni"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u064e\u0649\u0670\u0643\u064f\u0645\u06e1", "word_translation": "I see you", "word_transliteration": "arakum"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0657\u0627", "word_translation": "a people", "word_transliteration": "qawman"}, {"word_arabic_uthmani": "\u062a\u064e\u062c\u06e1\u0647\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "ignorant", "word_transliteration": "tajhaluna"}]}