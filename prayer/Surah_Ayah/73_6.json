{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0646\u064e\u0627\u0634\u0650\u0626\u064e\u0629\u064e \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650 \u0647\u0650\u064a\u064e \u0623\u064e\u0634\u064e\u062f\u0651\u064f \u0648\u064e\u0637\u06e1\u0640\u0654\u0657\u0627 \u0648\u064e\u0623\u064e\u0642\u06e1\u0648\u064e\u0645\u064f \u0642\u0650\u064a\u0644\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u0634\u0650\u0626\u064e\u0629\u064e", "word_translation": "(the) rising", "word_transliteration": "nashi-ata"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650", "word_translation": "(at) the night", "word_transliteration": "al-layli"}, {"word_arabic_uthmani": "\u0647\u0650\u064a\u064e", "word_translation": "it", "word_transliteration": "hiya"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u064e\u062f\u0651\u064f", "word_translation": "(is) very hard", "word_transliteration": "ashaddu"}, {"word_arabic_uthmani": "\u0648\u064e\u0637\u06e1\u0640\u0654\u0657\u0627", "word_translation": "and most potent", "word_transliteration": "watan"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0642\u06e1\u0648\u064e\u0645\u064f", "word_translation": "and more suitable", "word_transliteration": "wa-aqwamu"}, {"word_arabic_uthmani": "\u0642\u0650\u064a\u0644\u064b\u0627", "word_translation": "(for) Word", "word_transliteration": "qilan"}]}