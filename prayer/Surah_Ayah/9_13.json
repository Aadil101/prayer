{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0627 \u062a\u064f\u0642\u064e\u0670\u062a\u0650\u0644\u064f\u0648\u0646\u064e \u0642\u064e\u0648\u06e1\u0645\u0657\u0627 \u0646\u0651\u064e\u0643\u064e\u062b\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0647\u064e\u0645\u0651\u064f\u0648\u0627\u0652 \u0628\u0650\u0625\u0650\u062e\u06e1\u0631\u064e\u0627\u062c\u0650 \u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u0650 \u0648\u064e\u0647\u064f\u0645 \u0628\u064e\u062f\u064e\u0621\u064f\u0648\u0643\u064f\u0645\u06e1 \u0623\u064e\u0648\u0651\u064e\u0644\u064e \u0645\u064e\u0631\u0651\u064e\u0629\u064d\u06da \u0623\u064e\u062a\u064e\u062e\u06e1\u0634\u064e\u0648\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1\u06da \u0641\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u062d\u064e\u0642\u0651\u064f \u0623\u064e\u0646 \u062a\u064e\u062e\u06e1\u0634\u064e\u0648\u06e1\u0647\u064f \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645 \u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627", "word_translation": "Will not", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u062a\u064f\u0642\u064e\u0670\u062a\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "you fight", "word_transliteration": "tuqatiluna"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0657\u0627", "word_translation": "a people", "word_transliteration": "qawman"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0643\u064e\u062b\u064f\u0648\u0653\u0627\u0652", "word_translation": "who broke", "word_transliteration": "nakathu"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their oaths", "word_transliteration": "aymanahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064e\u0645\u0651\u064f\u0648\u0627\u0652", "word_translation": "and determined", "word_transliteration": "wahammu"}, {"word_arabic_uthmani": "\u0628\u0650\u0625\u0650\u062e\u06e1\u0631\u064e\u0627\u062c\u0650", "word_translation": "to drive out", "word_transliteration": "bi-ikh'raji"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u0650", "word_translation": "the Messenger", "word_transliteration": "al-rasuli"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0628\u064e\u062f\u064e\u0621\u064f\u0648\u0643\u064f\u0645\u06e1", "word_translation": "began (to attack) you", "word_transliteration": "badaukum"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u0651\u064e\u0644\u064e", "word_translation": "first", "word_transliteration": "awwala"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u0651\u064e\u0629\u064d\u06da", "word_translation": "time", "word_transliteration": "marratin"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064e\u062e\u06e1\u0634\u064e\u0648\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "Do you fear them", "word_transliteration": "atakhshawnahum"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "But Allah", "word_transliteration": "fal-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u0642\u0651\u064f", "word_translation": "(has) more right", "word_transliteration": "ahaqqu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u062e\u06e1\u0634\u064e\u0648\u06e1\u0647\u064f", "word_translation": "you should fear Him", "word_transliteration": "takhshawhu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645", "word_translation": "you are", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "believers", "word_transliteration": "mu'minina"}]}