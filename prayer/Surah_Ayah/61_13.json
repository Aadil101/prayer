{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670 \u062a\u064f\u062d\u0650\u0628\u0651\u064f\u0648\u0646\u064e\u0647\u064e\u0627\u06d6 \u0646\u064e\u0635\u06e1\u0631\u065e \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0641\u064e\u062a\u06e1\u062d\u065e \u0642\u064e\u0631\u0650\u064a\u0628\u065e\u06d7 \u0648\u064e\u0628\u064e\u0634\u0651\u0650\u0631\u0650 \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670", "word_translation": "And another", "word_transliteration": "wa-ukh'ra"}, {"word_arabic_uthmani": "\u062a\u064f\u062d\u0650\u0628\u0651\u064f\u0648\u0646\u064e\u0647\u064e\u0627\u06d6", "word_translation": "that you love", "word_transliteration": "tuhibbunaha"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u06e1\u0631\u065e", "word_translation": "a help", "word_transliteration": "nasrun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u064e\u062a\u06e1\u062d\u065e", "word_translation": "and a victory", "word_transliteration": "wafathun"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u0650\u064a\u0628\u065e\u06d7", "word_translation": "near;", "word_transliteration": "qaribun"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u0634\u0651\u0650\u0631\u0650", "word_translation": "and give glad tidings", "word_transliteration": "wabashiri"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "(to) the believers", "word_transliteration": "al-mu'minina"}]}