{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u06e1 \u0645\u064e\u0646\u06e1 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0643\u064e \u0645\u0650\u0646 \u0631\u0651\u064f\u0633\u064f\u0644\u0650\u0646\u064e\u0627\u0653 \u0623\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650 \u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u0657 \u064a\u064f\u0639\u06e1\u0628\u064e\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u06e1", "word_translation": "And ask", "word_transliteration": "wasal"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u06e1", "word_translation": "(those) whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We sent", "word_transliteration": "arsalna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before you", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0643\u064e", "word_translation": "before you", "word_transliteration": "qablika"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064f\u0633\u064f\u0644\u0650\u0646\u064e\u0627\u0653", "word_translation": "Our Messengers", "word_transliteration": "rusulina"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "did We make", "word_transliteration": "aja'alna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "besides", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides", "word_transliteration":