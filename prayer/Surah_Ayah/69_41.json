{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0647\u064f\u0648\u064e \u0628\u0650\u0642\u064e\u0648\u06e1\u0644\u0650 \u0634\u064e\u0627\u0639\u0650\u0631\u0656\u06da \u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627 \u0645\u0651\u064e\u0627 \u062a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "it", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0628\u0650\u0642\u064e\u0648\u06e1\u0644\u0650", "word_translation": "(is the) word", "word_transliteration": "biqawli"}, {"word_arabic_uthmani": "\u0634\u064e\u0627\u0639\u0650\u0631\u0656\u06da", "word_translation": "(of) a poet;", "word_transliteration": "sha'irin"}, {"word_arabic_uthmani": "\u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627", "word_translation": "little", "word_transliteration": "qalilan"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "(is) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "you believe!", "word_transliteration": "tu'minuna"}]}