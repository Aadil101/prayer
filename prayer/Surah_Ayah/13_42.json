{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1 \u0645\u064e\u0643\u064e\u0631\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1 \u0641\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u0645\u064e\u0643\u06e1\u0631\u064f \u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627\u06d6 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0645\u064e\u0627 \u062a\u064e\u0643\u06e1\u0633\u0650\u0628\u064f \u0643\u064f\u0644\u0651\u064f \u0646\u064e\u0641\u06e1\u0633\u0656\u06d7 \u0648\u064e\u0633\u064e\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0671\u0644\u06e1\u0643\u064f\u0641\u0651\u064e\u0640\u0670\u0631\u064f \u0644\u0650\u0645\u064e\u0646\u06e1 \u0639\u064f\u0642\u06e1\u0628\u064e\u0649 \u0671\u0644\u062f\u0651\u064e\u0627\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And certainly", "word_transliteration": "waqad"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u064e\u0631\u064e", "word_translation": "plotted", "word_transliteration": "makara"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "(were) from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1", "word_translation": "before them", "word_transliteration": "qablihim"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "but for Allah", "word_transliteration": "falillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0643\u06e1\u0631\u064f", "word_translation": "(is) the plot", "word_transliteration": "al-makru"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627\u06d6", "word_translation": "all", "word_transliteration": "jami'an"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "He knows", "word_transliteration": "ya'lamu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u06e1\u0633\u0650\u0628\u064f", "word_translation": "earns", "word_transliteration": "taksibu"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064f", "word_translation": "every", "word_transliteration": "kullu"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0633\u0656\u06d7", "word_translation": "soul", "word_transliteration": "nafsin"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064e\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "and will know", "word_transliteration": "wasaya'lamu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064f\u0641\u0651\u064e\u0640\u0670\u0631\u064f", "word_translation": "the disbelievers", "word_transliteration": "al-kufaru"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0646\u06e1", "word_translation": "for whom", "word_transliteration": "liman"}, {"word_arabic_uthmani": "\u0639\u064f\u0642\u06e1\u0628\u064e\u0649", "word_translation": "(is) the final", "word_transliteration": "uq'ba"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064e\u0627\u0631\u0650", "word_translation": "the home", "word_transliteration": "al-dari"}]}