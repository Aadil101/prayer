{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0644\u064e\u0646\u064e\u062d\u06e1\u0646\u064f \u0646\u064f\u062d\u06e1\u064a\u0650\u06e6 \u0648\u064e\u0646\u064f\u0645\u0650\u064a\u062a\u064f \u0648\u064e\u0646\u064e\u062d\u06e1\u0646\u064f \u0671\u0644\u06e1\u0648\u064e\u0670\u0631\u0650\u062b\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "And indeed We", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "surely [We]", "word_transliteration": "lanahnu"}, {"word_arabic_uthmani": "\u0646\u064f\u062d\u06e1\u064a\u0650\u06e6", "word_translation": "We give life", "word_transliteration": "nuh'yi"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064f\u0645\u0650\u064a\u062a\u064f", "word_translation": "and We cause death", "word_transliteration": "wanumitu"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "and We", "word_transliteration": "wanahnu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0648\u064e\u0670\u0631\u0650\u062b\u064f\u0648\u0646\u064e", "word_translation": "(are) the Inheritors", "word_transliteration": "al-warithuna"}]}