{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0627 \u0645\u064f\u0648\u0633\u064e\u0649 \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0647\u06e1\u062a\u064e\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And verily", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "We gave", "word_transliteration": "atayna"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e", "word_translation": "the Scripture", "word_transliteration": "al-kitaba"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so that they may", "word_transliteration": "la'allahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062a\u064e\u062f\u064f\u0648\u0646\u064e", "word_translation": "be guided", "word_transliteration": "yahtaduna"}]}