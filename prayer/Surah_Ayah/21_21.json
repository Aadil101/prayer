{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u0650 \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0653\u0627\u0652 \u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u0657 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0647\u064f\u0645\u06e1 \u064a\u064f\u0646\u0634\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u0650", "word_translation": "Or", "word_transliteration": "ami"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0653\u0627\u0652", "word_translation": "(have) they taken", "word_transliteration": "ittakhadhu"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u0657", "word_translation": "gods", "word_transliteration": "alihatan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u064a\u064f\u0646\u0634\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "raise (the dead)", "word_transliteration": "yunshiruna"}]}