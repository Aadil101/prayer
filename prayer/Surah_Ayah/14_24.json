{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1 \u062a\u064e\u0631\u064e \u0643\u064e\u064a\u06e1\u0641\u064e \u0636\u064e\u0631\u064e\u0628\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064e\u062b\u064e\u0644\u0657\u0627 \u0643\u064e\u0644\u0650\u0645\u064e\u0629\u0657 \u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0629\u0657 \u0643\u064e\u0634\u064e\u062c\u064e\u0631\u064e\u0629\u0656 \u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0629\u064d \u0623\u064e\u0635\u06e1\u0644\u064f\u0647\u064e\u0627 \u062b\u064e\u0627\u0628\u0650\u062a\u065e \u0648\u064e\u0641\u064e\u0631\u06e1\u0639\u064f\u0647\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Do not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e", "word_translation": "you see", "word_transliteration": "tara"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "how", "word_transliteration": "kayfa"}, {"word_arabic_uthmani": "\u0636\u064e\u0631\u064e\u0628\u064e", "word_translation": "Allah sets forth", "word_transliteration": "daraba"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah sets forth", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u062b\u064e\u0644\u0657\u0627", "word_translation": "the example", "word_transliteration": "mathalan"}, {"word_arabic_uthmani": "\u0643\u064e\u0644\u0650\u0645\u064e\u0629\u0657", "word_translation": "a word", "word_transliteration": "kalimatan"}, {"word_arabic_uthmani": "\u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0629\u0657", "word_translation": "good", "word_transliteration": "tayyibatan"}, {"word_arabic_uthmani": "\u0643\u064e\u0634\u064e\u062c\u064e\u0631\u064e\u0629\u0656", "word_translation": "(is) like a tree", "word_transliteration": "kashajaratin"}, {"word_arabic_uthmani": "\u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0629\u064d", "word_translation": "good", "word_transliteration": "tayyibatin"}, {"word_arabic_uthmani": "\u0623\u064e\u0635\u06e1\u0644\u064f\u0647\u064e\u0627", "word_translation": "its root", "word_transliteration": "asluha"}, {"word_arabic_uthmani": "\u062b\u064e\u0627\u0628\u0650\u062a\u065e", "word_translation": "(is) firm", "word_transliteration": "thabitun"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u064e\u0631\u06e1\u0639\u064f\u0647\u064e\u0627", "word_translation": "and its branches", "word_transliteration": "wafar'uha"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(are) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650", "word_translation": "the sky", "word_transliteration": "al-samai"}]}