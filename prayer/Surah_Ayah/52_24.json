{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u064a\u064e\u0637\u064f\u0648\u0641\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u063a\u0650\u0644\u06e1\u0645\u064e\u0627\u0646\u065e \u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0644\u064f\u0624\u06e1\u0644\u064f\u0624\u065e \u0645\u0651\u064e\u0643\u06e1\u0646\u064f\u0648\u0646\u065e"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u064a\u064e\u0637\u064f\u0648\u0641\u064f", "word_translation": "And will circulate", "word_transliteration": "wayatufu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "among them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u063a\u0650\u0644\u06e1\u0645\u064e\u0627\u0646\u065e", "word_translation": "boys", "word_transliteration": "ghil'manun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "as if they (were)", "word_transliteration": "ka-annahum"}, {"word_arabic_uthmani": "\u0644\u064f\u0624\u06e1\u0644\u064f\u0624\u065e", "word_translation": "pearls", "word_transliteration": "lu'lu-on"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0643\u06e1\u0646\u064f\u0648\u0646\u065e", "word_translation": "well-protected", "word_transliteration": "maknunun"}]}