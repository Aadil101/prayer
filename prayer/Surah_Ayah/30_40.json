{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u062e\u064e\u0644\u064e\u0642\u064e\u0643\u064f\u0645\u06e1 \u062b\u064f\u0645\u0651\u064e \u0631\u064e\u0632\u064e\u0642\u064e\u0643\u064f\u0645\u06e1 \u062b\u064f\u0645\u0651\u064e \u064a\u064f\u0645\u0650\u064a\u062a\u064f\u0643\u064f\u0645\u06e1 \u062b\u064f\u0645\u0651\u064e \u064a\u064f\u062d\u06e1\u064a\u0650\u064a\u0643\u064f\u0645\u06e1\u06d6 \u0647\u064e\u0644\u06e1 \u0645\u0650\u0646 \u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0626\u0650\u0643\u064f\u0645 \u0645\u0651\u064e\u0646 \u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f \u0645\u0650\u0646 \u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645 \u0645\u0651\u0650\u0646 \u0634\u064e\u064a\u06e1\u0621\u0656\u06da \u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0647\u064f\u06e5 \u0648\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649\u0670 \u0639\u064e\u0645\u0651\u064e\u0627 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064e\u0643\u064f\u0645\u06e1", "word_translation": "created you", "word_transliteration": "khalaqakum"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0631\u064e\u0632\u064e\u0642\u064e\u0643\u064f\u0645\u06e1", "word_translation": "He provided (for) you", "word_transliteration": "razaqakum"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u064a\u064f\u0645\u0650\u064a\u062a\u064f\u0643\u064f\u0645\u06e1", "word_translation": "He will cause you to die", "word_transliteration": "yumitukum"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u06e1\u064a\u0650\u064a\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "He will give you life", "word_transliteration": "yuh'yikum"}, {"word_arabic_uthmani": "\u0647\u064e\u0644\u06e1", "word_translation": "Is (there)", "word_transliteration": "hal"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0626\u0650\u0643\u064f\u0645", "word_translation": "(of) your partners", "word_transliteration": "shurakaikum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646", "word_translation": "who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f", "word_translation": "does", "word_transliteration": "yaf 'alu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645", "word_translation": "that", "word_transliteration": "dhalikum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656\u06da", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0647\u064f\u06e5", "word_translation": "Glory be to Him", "word_transliteration": "sub'hanahu"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649\u0670", "word_translation": "and exalted is He", "word_transliteration": "wata'ala"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "above what", 