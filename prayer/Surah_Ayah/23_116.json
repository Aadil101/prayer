{"ayahs": {"ayah_uthmani": "\u0641\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u06e1\u0645\u064e\u0644\u0650\u0643\u064f \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f\u06d6 \u0644\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0670\u0647\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0647\u064f\u0648\u064e \u0631\u064e\u0628\u0651\u064f \u0671\u0644\u06e1\u0639\u064e\u0631\u06e1\u0634\u0650 \u0671\u0644\u06e1\u0643\u064e\u0631\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649", "word_translation": "So exalted is", "word_transliteration": "fata'ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u0650\u0643\u064f", "word_translation": "the King", "word_transliteration": "al-maliku"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f\u06d6", "word_translation": "the Truth", "word_transliteration": "al-haqu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627\u0653", "word_translation": "(There is) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064e", "word_translation": "god", "word_transliteration": "ilaha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "Him", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f", "word_translation": "(the) Lord", "word_transliteration": "rabbu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0631\u06e1\u0634\u0650", "word_translation": "(of) the Throne", "word_transliteration": "al-'arshi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0631\u0650\u064a\u0645\u0650", "word