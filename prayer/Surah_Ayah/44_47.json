{"ayahs": {"ayah_uthmani": "\u062e\u064f\u0630\u064f\u0648\u0647\u064f \u0641\u064e\u0671\u0639\u06e1\u062a\u0650\u0644\u064f\u0648\u0647\u064f \u0625\u0650\u0644\u064e\u0649\u0670 \u0633\u064e\u0648\u064e\u0627\u0653\u0621\u0650 \u0671\u0644\u06e1\u062c\u064e\u062d\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u062e\u064f\u0630\u064f\u0648\u0647\u064f", "word_translation": "Seize him", "word_transliteration": "khudhuhu"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0639\u06e1\u062a\u0650\u0644\u064f\u0648\u0647\u064f", "word_translation": "and drag him", "word_transliteration": "fa-i'tiluhu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "into", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0633\u064e\u0648\u064e\u0627\u0653\u0621\u0650", "word_translation": "(the) midst", "word_transliteration": "sawai"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u062d\u0650\u064a\u0645\u0650", "word_translation": "(of) the Hellfire", "word_transliteration": "al-jahimi"}]}