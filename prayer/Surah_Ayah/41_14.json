{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0630\u06e1 \u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1\u0647\u064f\u0645\u064f \u0671\u0644\u0631\u0651\u064f\u0633\u064f\u0644\u064f \u0645\u0650\u0646\u06e2 \u0628\u064e\u064a\u06e1\u0646\u0650 \u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1 \u0648\u064e\u0645\u0650\u0646\u06e1 \u062e\u064e\u0644\u06e1\u0641\u0650\u0647\u0650\u0645\u06e1 \u0623\u064e\u0644\u0651\u064e\u0627 \u062a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0644\u0651\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064e\u06d6 \u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0644\u064e\u0648\u06e1 \u0634\u064e\u0627\u0653\u0621\u064e \u0631\u064e\u0628\u0651\u064f\u0646\u064e\u0627 \u0644\u064e\u0623\u064e\u0646\u0632\u064e\u0644\u064e \u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u0657 \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0628\u0650\u0645\u064e\u0627\u0653 \u0623\u064f\u0631\u06e1\u0633\u0650\u0644\u06e1\u062a\u064f\u0645 \u0628\u0650\u0647\u0650\u06e6 \u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "When", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1\u0647\u064f\u0645\u064f", "word_translation": "came to them", "word_transliteration": "jaathumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064f\u0633\u064f\u0644\u064f", "word_translation": "the Messengers", "word_transliteration": "al-rusulu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e2", "word_translation": "from before them", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u0650", "word_translation": "from before them", "word_transliteration": "bayni"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "from before them", "word_transliteration": "aydihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1", "word_translation": "and from", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0641\u0650\u0647\u0650\u0645\u06e1", "word_translation": "behind them", "word_transliteration": "khalfihim"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0651\u064e\u0627", "word_translation": "(saying) Do not", "word_transliteration": "alla"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0653\u0627\u0652", "word_translation": "worship", "word_transliteration": "ta'budu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e\u06d6", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1", "word_translation": "If", "word_transliteration": "law"}, {"word_arabic_uthmani": "\u0634\u064e\u0627\u0653\u0621\u064e", "word_translation": "(had) willed", "word_transliteration": "shaa"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0646\u064e\u0627", "word_translation": "our Lord", "word_transliteration": "rabbuna"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u064e\u0646\u0632\u064e\u0644\u064e", "word_translation": "surely, He (would have) sent down", "word_transliteration": "la-anzala"}, {"word_arabic_uthmani": "\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u0657", "word_translation": "Angels", "word_transliteration": "malaikatan"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "So indeed, we", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627\u0653", "word_translation": "in what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0623\u064f\u0631\u06e1\u0633\u0650\u0644\u06e1\u062a\u064f\u0645", "word_translation": "you have been sent", "word_transliteration": "ur'sil'tum"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(are) disbelievers", "word_transliteration": "kafiruna"}]}