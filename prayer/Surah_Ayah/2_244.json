{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0670\u062a\u0650\u0644\u064f\u0648\u0627\u0652 \u0641\u0650\u064a \u0633\u064e\u0628\u0650\u064a\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0671\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0633\u064e\u0645\u0650\u064a\u0639\u064c \u0639\u064e\u0644\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0670\u062a\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "And fight", "word_transliteration": "waqatilu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u0650", "word_translation": "(the) way", "word_transliteration": "sabili"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652", "word_translation": "and know", "word_transliteration": "wa-i'lamu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e", "word_translation": "that", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0633\u064e\u0645\u0650\u064a\u0639\u064c", "word_translation": "(is) All-Hearing", "word_transliteration": "sami'un"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u065e", "word_translation": "All-Knowing", "word_transliteration": "alimun"}]}