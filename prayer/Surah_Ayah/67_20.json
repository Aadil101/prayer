{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u0651\u064e\u0646\u06e1 \u0647\u064e\u0670\u0630\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0647\u064f\u0648\u064e \u062c\u064f\u0646\u062f\u065e \u0644\u0651\u064e\u0643\u064f\u0645\u06e1 \u064a\u064e\u0646\u0635\u064f\u0631\u064f\u0643\u064f\u0645 \u0645\u0651\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650\u06da \u0625\u0650\u0646\u0650 \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0641\u0650\u064a \u063a\u064f\u0631\u064f\u0648\u0631\u064d"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u0651\u064e\u0646\u06e1", "word_translation": "Who is", "word_transliteration": "amman"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "this", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "the one", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "he", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u062c\u064f\u0646\u062f\u065e", "word_translation": "(is) an army", "word_transliteration": "jundun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u0635\u064f\u0631\u064f\u0643\u064f\u0645", "word_translation": "to help you", "word_transliteration": "yansurukum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650\u06da", "word_translation": "the Most Gracious", "word_transliteration":