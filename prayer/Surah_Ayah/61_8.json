{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e \u0644\u0650\u064a\u064f\u0637\u06e1\u0641\u0650\u0640\u0654\u064f\u0648\u0627\u0652 \u0646\u064f\u0648\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0628\u0650\u0623\u064e\u0641\u06e1\u0648\u064e\u0670\u0647\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064f\u062a\u0650\u0645\u0651\u064f \u0646\u064f\u0648\u0631\u0650\u0647\u0650\u06e6 \u0648\u064e\u0644\u064e\u0648\u06e1 \u0643\u064e\u0631\u0650\u0647\u064e \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e", "word_translation": "They intend", "word_transliteration": "yuriduna"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u0637\u06e1\u0641\u0650\u0640\u0654\u064f\u0648\u0627\u0652", "word_translation": "to put out", "word_transliteration": "liyut'fiu"}, {"word_arabic_uthmani": "\u0646\u064f\u0648\u0631\u064e", "word_translation": "(the) light", "word_transliteration": "nura"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0641\u06e1\u0648\u064e\u0670\u0647\u0650\u0647\u0650\u0645\u06e1", "word_translation": "with their mouths", "word_transliteration": "bi-afwahihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "but Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u062a\u0650\u0645\u0651\u064f", "word_translation": "will perfect", "word_transliteration": "mutimmu"}, {"word_arabic_uthmani": "\u0646\u064f\u0648\u0631\u0650\u0647\u0650\u06e6", "word_translation": "His Light", "word_transliteration": "nurihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "although", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0643\u064e\u0631\u0650\u0647\u064e", "word_translation": "dislike", "word_transliteration": "kariha"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "the disbelievers", "word_transliteration": "al-kafiruna"}]}