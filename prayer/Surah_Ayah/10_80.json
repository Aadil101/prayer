{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u0633\u0651\u064e\u062d\u064e\u0631\u064e\u0629\u064f \u0642\u064e\u0627\u0644\u064e \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0648\u0633\u064e\u0649\u0670\u0653 \u0623\u064e\u0644\u06e1\u0642\u064f\u0648\u0627\u0652 \u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064f\u0645 \u0645\u0651\u064f\u0644\u06e1\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "So when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "came", "word_transliteration": "jaa"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u062d\u064e\u0631\u064e\u0629\u064f", "word_translation": "the magicians", "word_transliteration": "al-saharatu"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "to them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0648\u0633\u064e\u0649\u0670\u0653", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u06e1\u0642\u064f\u0648\u0627\u0652", "word_translation": "Throw", "word_transliteration": "alqu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645", "word_translation": "you", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0644\u06e1\u0642\u064f\u0648\u0646\u064e", "word_translation": "(wish to) throw", "word_transliteration": "mul'quna"}]}