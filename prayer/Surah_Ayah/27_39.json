{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0639\u0650\u0641\u06e1\u0631\u0650\u064a\u062a\u065e \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u0650 \u0623\u064e\u0646\u064e\u0627\u06e0 \u0621\u064e\u0627\u062a\u0650\u064a\u0643\u064e \u0628\u0650\u0647\u0650\u06e6 \u0642\u064e\u0628\u06e1\u0644\u064e \u0623\u064e\u0646 \u062a\u064e\u0642\u064f\u0648\u0645\u064e \u0645\u0650\u0646 \u0645\u0651\u064e\u0642\u064e\u0627\u0645\u0650\u0643\u064e\u06d6 \u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0644\u064e\u0642\u064e\u0648\u0650\u064a\u0651\u064c \u0623\u064e\u0645\u0650\u064a\u0646\u065e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0639\u0650\u0641\u06e1\u0631\u0650\u064a\u062a\u065e", "word_translation": "a strong one", "word_transliteration": "if'ritun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u0650", "word_translation": "the jinn", "word_transliteration": "al-jini"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "I", "word_transliteration": "ana"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u0650\u064a\u0643\u064e", "word_translation": "will bring it to you", "word_transliteration": "atika"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "will bring it to you", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e", "word_translation": "before", "word_transliteration": "qabla"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "[that]", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u064f\u0648\u0645\u064e", "word_translation": "you rise", "word_transliteration": "taquma"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0642\u064e\u0627\u0645\u0650\u0643\u064e\u06d6", "word_translation": "your place", "word_transliteration": "maqamika"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "And indeed, I am", "word_transliteration": "wa-inni"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "for it", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u0648\u0650\u064a\u0651\u064c", "word_translation": "surely, strong", "word_transliteration": "laqawiyyun"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u0650\u064a\u0646\u065e", "word_translation": "trustworthy", "word_transliteration": "aminun"}]}