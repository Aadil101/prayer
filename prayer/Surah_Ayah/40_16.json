{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e \u0647\u064f\u0645 \u0628\u064e\u0670\u0631\u0650\u0632\u064f\u0648\u0646\u064e\u06d6 \u0644\u064e\u0627 \u064a\u064e\u062e\u06e1\u0641\u064e\u0649\u0670 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0634\u064e\u064a\u06e1\u0621\u065e\u06da \u0644\u0651\u0650\u0645\u064e\u0646\u0650 \u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u064f \u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u064e\u06d6 \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u0648\u064e\u0670\u062d\u0650\u062f\u0650 \u0671\u0644\u06e1\u0642\u064e\u0647\u0651\u064e\u0627\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(The) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u0647\u064f\u0645", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0628\u064e\u0670\u0631\u0650\u0632\u064f\u0648\u0646\u064e\u06d6", "word_translation": "come forth", "word_transliteration": "barizuna"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0641\u064e\u0649\u0670", "word_translation": "is hidden", "word_transliteration": "yakhfa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "from", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "about them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u065e\u06da", "word_translation": "anything", "word_transliteration": "shayon"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0645\u064e\u0646\u0650", "word_translation": "For whom", "word_transliteration": "limani"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "(is) the Dominion", "word_transliteration": "al-mul'ku"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u064e\u06d6", "word_translation": "this Day", "word_transliteration": "al-yawma"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "For Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0648\u064e\u0670\u062d\u0650\u062f\u0650", "word_translation": "the One", "word_transliteration": "al-wahidi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0647\u0651\u064e\u0627\u0631\u0650", "word_translation": "the Irresistible", "word_transliteration": "al-qahari"}]}