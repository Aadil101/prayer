{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u062f\u064e  \u0671\u0644\u0644\u0651\u064e\u0647\u064f  \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1  \u0644\u064e\u0643\u064e\u0670\u0630\u0650\u0628\u064f\u0648\u0646\u064e "}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u062f\u064e ", "word_translation": "Allah has begotten", "word_transliteration": "walada"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f ", "word_translation": "Allah has begotten", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 ", "word_translation": "and indeed, they", "word_transliteration": "wa-innahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e\u0670\u0630\u0650\u0628\u064f\u0648\u0646\u064e ", "word_translation": "surely (are) liars", "word_transliteration": "lakadhibuna"}]}