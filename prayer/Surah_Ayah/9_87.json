{"ayahs": {"ayah_uthmani": "\u0631\u064e\u0636\u064f\u0648\u0627\u0652 \u0628\u0650\u0623\u064e\u0646 \u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652 \u0645\u064e\u0639\u064e \u0671\u0644\u06e1\u062e\u064e\u0648\u064e\u0627\u0644\u0650\u0641\u0650 \u0648\u064e\u0637\u064f\u0628\u0650\u0639\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0647\u0650\u0645\u06e1 \u0641\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0641\u06e1\u0642\u064e\u0647\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0631\u064e\u0636\u064f\u0648\u0627\u0652", "word_translation": "They (were) satisfied", "word_transliteration": "radu"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646", "word_translation": "to", "word_transliteration": "bi-an"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652", "word_translation": "be", "word_transliteration": "yakunu"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e", "word_translation": "with", "word_transliteration": "ma'a"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0648\u064e\u0627\u0644\u0650\u0641\u0650", "word_translation": "those who stay behind", "word_transliteration": "al-khawalifi"}, {"word_arabic_uthmani": "\u0648\u064e\u0637\u064f\u0628\u0650\u0639\u064e", "word_translation": "and were sealed", "word_transliteration": "watubi'a"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "[on]", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their hearts", "word_transliteration": "qulubihim"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so they", "word_transliteration": "fahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0641\u06e1\u064