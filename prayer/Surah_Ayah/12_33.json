{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0631\u064e\u0628\u0651\u0650 \u0671\u0644\u0633\u0651\u0650\u062c\u06e1\u0646\u064f \u0623\u064e\u062d\u064e\u0628\u0651\u064f \u0625\u0650\u0644\u064e\u064a\u0651\u064e \u0645\u0650\u0645\u0651\u064e\u0627 \u064a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e\u0646\u0650\u064a\u0653 \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u06d6 \u0648\u064e\u0625\u0650\u0644\u0651\u064e\u0627 \u062a\u064e\u0635\u06e1\u0631\u0650\u0641\u06e1 \u0639\u064e\u0646\u0651\u0650\u064a \u0643\u064e\u064a\u06e1\u062f\u064e\u0647\u064f\u0646\u0651\u064e \u0623\u064e\u0635\u06e1\u0628\u064f \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0646\u0651\u064e \u0648\u064e\u0623\u064e\u0643\u064f\u0646 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u062c\u064e\u0670\u0647\u0650\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "My Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u0650\u062c\u06e1\u0646\u064f", "word_translation": "the prison", "word_transliteration": "al-sij'nu"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u0628\u0651\u064f", "word_translation": "(is) dearer", "word_transliteration": "ahabbu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u0651\u064e", "word_translation": "to me", "word_transliteration": "ilayya"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0627", "word_translation": "than what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e\u0646\u0650\u064a\u0653", "word_translation": "they invite me", "word_transliteration": "yad'unani"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u06d6", "word_translation": "to it", "word_transliteration": "ilayhi"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "And unless", "word_transliteration": "wa-illa"}, {"word_arabic_uthmani": "\u062a\u064e\u0635\u06e1\u0631\u0650\u0641\u06e1", "word_translation": "You turn away", "word_transliteration": "tasrif"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0651\u0650\u064a", "word_translation": "from me", "word_transliteration": "anni"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u062f\u064e\u0647\u064f\u0646\u0651\u064e", "word_translation": "their plot", "word_transliteration": "kaydahunna"}, {"word_arabic_uthmani": "\u0623\u064e\u0635\u06e1\u0628\u064f", "word_translation": "I might incline", "word_transliteration": "asbu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0646\u0651\u064e", "word_translation": "towards them", "word_transliteration": "ilayhinna"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0643\u064f\u0646", "word_translation": "and [I] be", "word_transliteration": "wa-akun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0670\u0647\u0650\u0644\u0650\u064a\u0646\u064e", "word_translation": "the ignorant", "word_transliteration": "al-jahilina"}]}