{"ayahs": {"ayah_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e \u0646\u064e\u062d\u06e1\u0634\u064f\u0631\u064f\u0647\u064f\u0645\u06e1 \u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627 \u062b\u064f\u0645\u0651\u064e \u0646\u064e\u0642\u064f\u0648\u0644\u064f \u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0623\u064e\u0634\u06e1\u0631\u064e\u0643\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u064a\u06e1\u0646\u064e \u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0624\u064f\u0643\u064f\u0645\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u0632\u06e1\u0639\u064f\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "And (the) Day", "word_transliteration": "wayawma"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0634\u064f\u0631\u064f\u0647\u064f\u0645\u06e1", "word_translation": "We will gather them", "word_transliteration": "nahshuruhum"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627", "word_translation": "all", "word_transliteration": "jami'an"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0646\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "We will say", "word_transliteration": "naqulu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "to those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u06e1\u0631\u064e\u0643\u064f\u0648\u0653\u0627\u0652", "word_translation": "associated others with Allah", "word_transliteration": "ashraku"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u0646\u064e", "word_translation": "Where (are)", "word_transliteration": "ayna"}, {"word_arabic_uthmani": "\u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0624\u064f\u0643\u064f\u0645\u064f", "word_translation": "your partners", "word_transliteration": "shurakaukumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those whom", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you used to", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u062a\u064e\u0632\u06e1\u0639\u064f\u0645\u064f\u0648\u0646\u064e", "word_translation": "claim", "word_transliteration": "taz'umuna"}]}