tion": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "and do", "word_transliteration": "wa'amilu"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064e\u0670\u062a\u0650", "word_translation": "righteous deeds", "word_transliteration": "al-salihati"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645", "word_translation": "among them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u063a\u06e1\u0641\u0650\u0631\u064e\u0629\u0657", "word_translation": "forgiveness", "word_transliteration": "maghfiratan"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062c\u06e1\u0631\u064b\u0627", "word_translation": "and a reward", "word_transliteration": "wa-ajran"}, {"word_arabic_uthmani": "\u0639\u064e\u0638\u0650\u064a\u0645\u064e\u06e2\u0627", "word_translation": "great", "word_transliteration": "aziman"}]}