{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0647\u064e\u0670\u0630\u064e\u0627 \u0644\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0642\u064e\u0635\u064e\u0635\u064f \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f\u06da \u0648\u064e\u0645\u064e\u0627 \u0645\u0650\u0646\u06e1 \u0625\u0650\u0644\u064e\u0670\u0647\u064d \u0625\u0650\u0644\u0651\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06da \u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "this", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0648\u064e", "word_translation": "surely it (is)", "word_transliteration": "lahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0635\u064e\u0635\u064f", "word_translation": "the narration", "word_transliteration": "al-qasasu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f\u06da", "word_translation": "[the] true", "word_transliteration": "al-haqu"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And (there is) no", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "(of)", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064d", "word_translation": "god", "word_transliteration": "ilahin"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06da", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0648\u064e", "word_translation": "surely He", "word_transliteration": "lahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f", "word_translation": "(is) the All-Mighty", "word_transliteration": "al-'azizu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f", "word_translation": "the All-Wise", "word_transliteration": "al-hakimu"}]}