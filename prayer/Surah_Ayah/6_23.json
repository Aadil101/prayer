{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u0644\u064e\u0645\u06e1 \u062a\u064e\u0643\u064f\u0646 \u0641\u0650\u062a\u06e1\u0646\u064e\u062a\u064f\u0647\u064f\u0645\u06e1 \u0625\u0650\u0644\u0651\u064e\u0627\u0653 \u0623\u064e\u0646 \u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0631\u064e\u0628\u0651\u0650\u0646\u064e\u0627 \u0645\u064e\u0627 \u0643\u064f\u0646\u0651\u064e\u0627 \u0645\u064f\u0634\u06e1\u0631\u0650\u0643\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u064f\u0646", "word_translation": "will be", "word_transliteration": "takun"}, {"word_arabic_uthmani": "\u0641\u0650\u062a\u06e1\u0646\u064e\u062a\u064f\u0647\u064f\u0645\u06e1", "word_translation": "(for) them a plea", "word_transliteration": "fit'natuhum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627\u0653", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "they say", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "By Allah", "word_transliteration": "wal-lahi"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0646\u064e\u0627", "word_translation": "our Lord", "word_transliteration": "rabbina"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "we were", "word_transliteration": "kunna"}, {"word_arabic_uthm