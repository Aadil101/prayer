{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0631\u064e\u0628\u0651\u0650 \u0625\u0650\u0646\u0651\u0650\u064a \u0638\u064e\u0644\u064e\u0645\u06e1\u062a\u064f \u0646\u064e\u0641\u06e1\u0633\u0650\u064a \u0641\u064e\u0671\u063a\u06e1\u0641\u0650\u0631\u06e1 \u0644\u0650\u064a \u0641\u064e\u063a\u064e\u0641\u064e\u0631\u064e \u0644\u064e\u0647\u064f\u06e5\u0653\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u063a\u064e\u0641\u064f\u0648\u0631\u064f \u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "My Lord!", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0638\u064e\u0644\u064e\u0645\u06e1\u062a\u064f", "word_translation": "[I] have wronged", "word_transliteration": "zalamtu"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0633\u0650\u064a", "word_translation": "my soul", "word_transliteration": "nafsi"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u063a\u06e1\u0641\u0650\u0631\u06e1", "word_translation": "so forgive", "word_transliteration": "fa-igh'fir"}, {"word_arabic_uthmani": "\u0644\u0650\u064a", "word_translation": "[for] me", "word_transliteration": "li"}, {"word_arabic_uthmani": "\u0641\u064e\u063a\u064e\u0641\u064e\u0631\u064e", "word_translation": "Then He forgave", "word_transliteration": "faghafara"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5\u0653\u06da", "word_translation": "[for] him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He (is)", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u0641\u064f\u0648\u0631\u064f", "word_translation": "the Oft-Forgiving", "word_transliteration": "al-ghafuru"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f", "word_translation": "the Most Merciful", "word_transliteration": "al-rahimu"}]}