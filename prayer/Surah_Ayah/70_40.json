{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0627\u0653 \u0623\u064f\u0642\u06e1\u0633\u0650\u0645\u064f \u0628\u0650\u0631\u064e\u0628\u0651\u0650 \u0671\u0644\u06e1\u0645\u064e\u0634\u064e\u0670\u0631\u0650\u0642\u0650 \u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u063a\u064e\u0670\u0631\u0650\u0628\u0650 \u0625\u0650\u0646\u0651\u064e\u0627 \u0644\u064e\u0642\u064e\u0670\u062f\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627\u0653", "word_translation": "But nay!", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u0623\u064f\u0642\u06e1\u0633\u0650\u0645\u064f", "word_translation": "I swear", "word_transliteration": "uq'simu"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u0628\u0651\u0650", "word_translation": "by (the) Lord", "word_transliteration": "birabbi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0634\u064e\u0670\u0631\u0650\u0642\u0650", "word_translation": "(of) the rising", "word_transliteration": "al-mashariqi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u063a\u064e\u0670\u0631\u0650\u0628\u0650", "word_translation": "and the settings", "word_transliteration": "wal-magharibi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "that We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u0670\u062f\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(are) surely Able", "word_transliteration": "laqadiruna"}]}