{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f \u0671\u0644\u06e1\u062c\u064e\u0648\u064e\u0627\u0631\u0650 \u0671\u0644\u06e1\u0645\u064f\u0646\u0634\u064e\u0640\u0654\u064e\u0627\u062a\u064f \u0641\u0650\u064a \u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650 \u0643\u064e\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0644\u064e\u0670\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f", "word_translation": "And for Him", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0648\u064e\u0627\u0631\u0650", "word_translation": "(are) the ships", "word_transliteration": "al-jawari"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0646\u0634\u064e\u0640\u0654\u064e\u0627\u062a\u064f", "word_translation": "elevated", "word_transliteration": "al-munshaatu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650", "word_translation": "the sea", "word_transliteration": "al-bahri"}, {"word_arabic_uthmani": "\u0643\u064e\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0644\u064e\u0670\u0645\u0650", "word_translation": "like mountains", "word_transliteration": "kal-a'lami"}]}