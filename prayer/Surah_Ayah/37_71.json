{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0636\u064e\u0644\u0651\u064e \u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1 \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f \u0671\u0644\u06e1\u0623\u064e\u0648\u0651\u064e\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And verily", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0636\u064e\u0644\u0651\u064e", "word_translation": "went astray", "word_transliteration": "dalla"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "before them", "word_transliteration": "qablahum"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f", "word_translation": "most", "word_transliteration": "aktharu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0648\u0651\u064e\u0644\u0650\u064a\u0646\u064e", "word_translation": "(of) the former (people)", "word_transliteration": "al-awalina"}]}