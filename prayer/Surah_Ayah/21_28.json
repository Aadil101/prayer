{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e \u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1 \u0648\u064e\u0645\u064e\u0627 \u062e\u064e\u0644\u06e1\u0641\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0634\u06e1\u0641\u064e\u0639\u064f\u0648\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0644\u0650\u0645\u064e\u0646\u0650 \u0671\u0631\u06e1\u062a\u064e\u0636\u064e\u0649\u0670 \u0648\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u06e1 \u062e\u064e\u0634\u06e1\u064a\u064e\u062a\u0650\u0647\u0650\u06e6 \u0645\u064f\u0634\u06e1\u0641\u0650\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "He knows", "word_transliteration": "ya'lamu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e", "word_translation": "(is) before them", "word_transliteration": "bayna"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "(is) before them", "word_transliteration": "aydihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "(is) behind them", "word_transliteration": "khalfahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0641\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "they (can) intercede", "word_transliteration": "yashfa'una"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0646\u0650", "word_translation": "for whom", "word_transliteration": "limani"}, {"word_arabic_uthmani": "\u0671\u0631\u06e1\u062a\u064e\u0636\u064e\u0649\u0670", "word_translation": "He approves", "word_transliteration": "ir'tada"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645", "word_translation": "And they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062e\u064e\u0634\u06e1\u064a\u064e\u062a\u0650\u0647\u0650\u06e6", "word_translation": "fear of Him", "word_transliteration": "khashyatihi"}, {"word_arabic_uthmani": "\u0645\u064f\u0634\u06e1\u0641\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "stand in awe", "word_transliteration": "mush'fiquna"}]}