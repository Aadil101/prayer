{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u064f \u0671\u0633\u06e1\u062a\u064e\u062d\u064e\u0628\u0651\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u064e \u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650 \u0648\u064e\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0627 \u064a\u064e\u0647\u06e1\u062f\u0650\u064a \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That (is)", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u064f", "word_translation": "because", "word_transliteration": "bi-annahumu"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u062d\u064e\u0628\u0651\u064f\u0648\u0627\u0652", "word_translation": "they preferred", "word_transliteration": "is'tahabbu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u064e", "word_translation": "the life", "word_transliteration": "al-hayata"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627", "word_translation": "(of) the world", "word_transliteration": "al-dun'ya"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650", "word_translation": "the Hereafter", "word_transliteration": "al-akhirati"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e", "word_translation": "and that", "word_transliteration": "wa-anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062f\u0650\u064a", "word_translation": "guide", "word_transliteration": "yahdi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064e", "word_translation": "the people", "word_transliteration": "al-qawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the disbelievers", "word_transliteration": "al-kafirina"}]}