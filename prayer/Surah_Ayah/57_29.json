{"ayahs": {"ayah_uthmani": "\u0644\u0651\u0650\u0626\u064e\u0644\u0651\u064e\u0627 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064e \u0623\u064e\u0647\u06e1\u0644\u064f \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u0623\u064e\u0644\u0651\u064e\u0627 \u064a\u064e\u0642\u06e1\u062f\u0650\u0631\u064f\u0648\u0646\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0645\u0651\u0650\u0646 \u0641\u064e\u0636\u06e1\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u06e1\u0641\u064e\u0636\u06e1\u0644\u064e \u0628\u0650\u064a\u064e\u062f\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u064a\u064f\u0624\u06e1\u062a\u0650\u064a\u0647\u0650 \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0630\u064f\u0648 \u0671\u0644\u06e1\u0641\u064e\u0636\u06e1\u0644\u0650 \u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u0650\u0626\u064e\u0644\u0651\u064e\u0627", "word_translation": "So that", "word_transliteration": "li-alla"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064e", "word_translation": "may know", "word_transliteration": "ya'lama"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064f", "word_translation": "(the) People", "word_transliteration": "ahlu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "(of) the Book", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0651\u064e\u0627", "word_translation": "that not", "word_transliteration": "alla"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u06e1\u062f\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "they have power", "word_transliteration": "yaqdiruna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "anything", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u0650", "word_translation": "(the) Bounty", "word_transliteration": "fadli"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e", "word_translation": "and that", "word_transliteration": "wa-anna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0636\u06e1\u0644\u064e", "word_translation": "the Bounty", "word_transliteration": "al-fadla"}, {"word_arabic_uthmani": "\u0628\u0650\u064a\u064e\u062f\u0650", "word_translation": "(is) in (Allah's) hand", "word_transliteration": "biyadi"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(is) (in) Allah's (hand)", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u062a\u0650\u064a\u0647\u0650", "word_translation": "He gives it", "word_transliteration": "yu'tihi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0630\u064f\u0648", "word_translation": "(is) the Possessor of Bounty", "word_transliteration": "dhu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0636\u06e1\u0644\u0650", "word_translation": "(is) the Possessor of Bounty", "word_transliteration": "al-fadli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u0650", "word_translation": "the Great", "word_transliteration": "al-'azimi"}]}