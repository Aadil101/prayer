{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u062f\u0650\u0645\u06e1\u0646\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0649\u0670 \u0645\u064e\u0627 \u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u06e1 \u0639\u064e\u0645\u064e\u0644\u0656 \u0641\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f \u0647\u064e\u0628\u064e\u0627\u0653\u0621\u0657 \u0645\u0651\u064e\u0646\u062b\u064f\u0648\u0631\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u0650\u0645\u06e1\u0646\u064e\u0627\u0653", "word_translation": "And We will proceed", "word_transliteration": "waqadim'na"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "they did", "word_transliteration": "amilu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u064e\u0644\u0656", "word_translation": "(the) deed(s)", "word_transliteration": "amalin"}, {"word_arabic_uthmani": "\u0641\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f", "word_translation": "and We will make them", "word_transliteration": "faja'alnahu"}, {"word_arabic_uthmani": "\u0647\u064e\u0628\u064e\u0627\u0653\u0621\u0657", "word_translation": "(as) dust", "word_transliteration": "habaan"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646\u062b\u064f\u0648\u0631\u064b\u0627", "word_translation": "dispersed", "word_transliteration": "manthuran"}]}