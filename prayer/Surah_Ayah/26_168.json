{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0625\u0650\u0646\u0651\u0650\u064a \u0644\u0650\u0639\u064e\u0645\u064e\u0644\u0650\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0642\u064e\u0627\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I am", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0644\u0650\u0639\u064e\u0645\u064e\u0644\u0650\u0643\u064f\u0645", "word_translation": "(of) your deed", "word_transliteration": "li'amalikum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0627\u0644\u0650\u064a\u0646\u064e", "word_translation": "those who detest", "word_transliteration": "al-qalina"}]}