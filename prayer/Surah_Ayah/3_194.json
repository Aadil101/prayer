{"ayahs": {"ayah_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627 \u0648\u064e\u0621\u064e\u0627\u062a\u0650\u0646\u064e\u0627 \u0645\u064e\u0627 \u0648\u064e\u0639\u064e\u062f\u062a\u0651\u064e\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u0649\u0670 \u0631\u064f\u0633\u064f\u0644\u0650\u0643\u064e \u0648\u064e\u0644\u064e\u0627 \u062a\u064f\u062e\u06e1\u0632\u0650\u0646\u064e\u0627 \u064a\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650\u06d6 \u0625\u0650\u0646\u0651\u064e\u0643\u064e \u0644\u064e\u0627 \u062a\u064f\u062e\u06e1\u0644\u0650\u0641\u064f \u0671\u0644\u06e1\u0645\u0650\u064a\u0639\u064e\u0627\u062f\u064e"}, "words": [{"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627", "word_translation": "Our Lord", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0648\u064e\u0621\u064e\u0627\u062a\u0650\u0646\u064e\u0627", "word_translation": "grant us", "word_transliteration": "waatina"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u062f\u062a\u0651\u064e\u0646\u064e\u0627", "word_translation": "You promised us", "word_transliteration": "wa'adttana"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "through", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0631\u064f\u0633\u064f\u0644\u0650\u0643\u064e", "word_translation": "Your Messengers", "word_transliteration": "rusulika"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064f\u062e\u06e1\u0632\u0650\u0646\u064e\u0627", "word_translation": "disgrace us", "word_transliteration": "tukh'zina"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(on the) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650\u06d6", "word_translation": "(of) [the] Resurrection", "word_transliteration": "al-qiyamati"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "Indeed You", "word_transliteration": "innaka"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064f\u062e\u06e1\u0644\u0650\u0641\u064f", "word_translation": "break", "word_transliteration": "tukh'lifu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u0650\u064a\u0639\u064e\u0627\u062f\u064e", "word_translation": "the promise", "word_transliteration": "al-mi'ada"}]}