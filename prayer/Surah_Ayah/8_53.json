{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0645\u06e1 \u064a\u064e\u0643\u064f \u0645\u064f\u063a\u064e\u064a\u0651\u0650\u0631\u0657\u0627 \u0646\u0651\u0650\u0639\u06e1\u0645\u064e\u0629\u064b \u0623\u064e\u0646\u06e1\u0639\u064e\u0645\u064e\u0647\u064e\u0627 \u0639\u064e\u0644\u064e\u0649\u0670 \u0642\u064e\u0648\u06e1\u0645\u064d \u062d\u064e\u062a\u0651\u064e\u0649\u0670 \u064a\u064f\u063a\u064e\u064a\u0651\u0650\u0631\u064f\u0648\u0627\u0652 \u0645\u064e\u0627 \u0628\u0650\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0633\u064e\u0645\u0650\u064a\u0639\u064c \u0639\u064e\u0644\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e", "word_translation": "(is) because", "word_transliteration": "bi-anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f", "word_translation": "is", "word_transliteration": "yaku"}, {"word_arabic_uthmani": "\u0645\u064f\u063a\u064e\u064a\u0651\u0650\u0631\u0657\u0627", "word_translation": "One Who changes", "word_transliteration": "mughayyiran"}, {"word_arabic_uthmani": "\u0646\u0651\u0650\u0639\u06e1\u0645\u064e\u0629\u064b", "word_translation": "a favor", "word_transliteration": "ni'matan"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1\u0639\u064e\u0645\u064e\u0647\u064e\u0627", "word_translation": "which He had bestowed", "word_transliteration": "an'amaha"}, {"word_arab