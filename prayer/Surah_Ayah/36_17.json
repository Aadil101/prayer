{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0671\u0644\u06e1\u0628\u064e\u0644\u064e\u0670\u063a\u064f \u0671\u0644\u06e1\u0645\u064f\u0628\u0650\u064a\u0646\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627\u0653", "word_translation": "(is) on us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0644\u064e\u0670\u063a\u064f", "word_translation": "the conveyance", "word_transliteration": "al-balaghu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0628\u0650\u064a\u0646\u064f", "word_translation": "clear", "word_transliteration": "al-mubinu"}]}