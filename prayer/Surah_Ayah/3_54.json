{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0643\u064e\u0631\u064f\u0648\u0627\u0652 \u0648\u064e\u0645\u064e\u0643\u064e\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06d6 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u062e\u064e\u064a\u06e1\u0631\u064f \u0671\u0644\u06e1\u0645\u064e\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0643\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "And they schemed", "word_transliteration": "wamakaru"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0643\u064e\u0631\u064e", "word_translation": "and planned", "word_transliteration": "wamakara"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06d6", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u064f", "word_translation": "(is the) best", "word_transliteration": "khayru"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(of) the planners", "word_transliteration": "al-makirina"}]}