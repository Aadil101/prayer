{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u064f \u0671\u062f\u06e1\u0639\u064f\u0648\u0646\u0650\u064a\u0653 \u0623\u064e\u0633\u06e1\u062a\u064e\u062c\u0650\u0628\u06e1 \u0644\u064e\u0643\u064f\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e \u0639\u064e\u0646\u06e1 \u0639\u0650\u0628\u064e\u0627\u062f\u064e\u062a\u0650\u064a \u0633\u064e\u064a\u064e\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0646\u064e \u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e \u062f\u064e\u0627\u062e\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "And said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u064f", "word_translation": "your Lord", "word_transliteration": "rabbukumu"}, {"word_arabic_uthmani": "\u0671\u062f\u06e1\u0639\u064f\u0648\u0646\u0650\u064a\u0653", "word_translation": "Call upon Me", "word_transliteration": "id'uni"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u062a\u064e\u062c\u0650\u0628\u06e1", "word_translation": "I will respond", "word_transliteration": "astajib"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1\u06da", "word_translation": "to you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(are too) proud", "word_transliteration": "yastakbiruna"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1", "word_translation": "to", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u064e\u062a\u0650\u064a", "word_translation": "worship Me", "word_transliteration": "ibadati"}, {"word_arabic_uthmani": "\u0633\u064e\u064a\u064e\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0646\u064e", "word_translation": "will enter", "word_transliteration": "sayadkhuluna"}, {"word_arabic_uthmani": "\u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e", "word_translation": "Hell", "word_transliteration": "jahannama"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u062e\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(in) humiliation", "word_transliteration": "dakhirina"}]}