{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0671\u0641\u06e1\u062a\u064e\u062d\u06e1 \u0628\u064e\u064a\u06e1\u0646\u0650\u064a \u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1 \u0641\u064e\u062a\u06e1\u062d\u0657\u0627 \u0648\u064e\u0646\u064e\u062c\u0651\u0650\u0646\u0650\u064a \u0648\u064e\u0645\u064e\u0646 \u0645\u0651\u064e\u0639\u0650\u064a\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0671\u0641\u06e1\u062a\u064e\u062d\u06e1", "word_translation": "So judge", "word_transliteration": "fa-if'tah"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u0650\u064a", "word_translation": "between me", "word_transliteration": "bayni"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and between them", "word_transliteration": "wabaynahum"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u06e1\u062d\u0657\u0627", "word_translation": "(with decisive) judgment", "word_transliteration": "fathan"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u062c\u0651\u0650\u0646\u0650\u064a", "word_translation": "and save me", "word_transliteration": "wanajjini"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "and who", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0639\u0650\u064a\u064e", "word_translation": "(are) with me", "word_transliteration": "ma'iya"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "the believers", "word_transliteration": "al-mu'minina"}]}