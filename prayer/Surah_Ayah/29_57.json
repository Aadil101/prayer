{"ayahs": {"ayah_uthmani": "\u0643\u064f\u0644\u0651\u064f \u0646\u064e\u0641\u06e1\u0633\u0656 \u0630\u064e\u0627\u0653\u0626\u0650\u0642\u064e\u0629\u064f \u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u0650\u06d6 \u062b\u064f\u0645\u0651\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064f", "word_translation": "Every", "word_transliteration": "kullu"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0633\u0656", "word_translation": "soul", "word_transliteration": "nafsin"}, {"word_arabic_uthmani": "\u0630\u064e\u0627\u0653\u0626\u0650\u0642\u064e\u0629\u064f", "word_translation": "(will) taste", "word_transliteration": "dhaiqatu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u0650\u06d6", "word_translation": "the death", "word_transliteration": "al-mawti"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "to Us", "word_transliteration": "ilayna"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "you will be returned", "word_transliteration": "tur'ja'una"}]}