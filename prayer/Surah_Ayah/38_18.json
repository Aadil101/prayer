{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627 \u0633\u064e\u062e\u0651\u064e\u0631\u06e1\u0646\u064e\u0627 \u0671\u0644\u06e1\u062c\u0650\u0628\u064e\u0627\u0644\u064e \u0645\u064e\u0639\u064e\u0647\u064f\u06e5 \u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u06e1\u0646\u064e \u0628\u0650\u0671\u0644\u06e1\u0639\u064e\u0634\u0650\u064a\u0651\u0650 \u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0634\u06e1\u0631\u064e\u0627\u0642\u0650"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0633\u064e\u062e\u0651\u064e\u0631\u06e1\u0646\u064e\u0627", "word_translation": "subjected", "word_transliteration": "sakharna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u0650\u0628\u064e\u0627\u0644\u064e", "word_translation": "the mountains", "word_transliteration": "al-jibala"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e\u0647\u064f\u06e5", "word_translation": "with him", "word_transliteration": "ma'ahu"}, {"word_arabic_uthmani": "\u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u06e1\u0646\u064e", "word_translation": "glorifying", "word_transliteration": "yusabbih'na"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0639\u064e\u0634\u0650\u064a\u0651\u0650", "word_translation": "in the evening", "word_transliteration": "bil-'ashiyi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0634\u06e1\u0631\u064e\u0627\u0642\u0650", "word_translation": "and [the] sunrise", "word_transliteration": "wal-ish'raqi"}]}