{"ayahs": {"ayah_uthmani": "\u0623\u064e\u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e \u0628\u064e\u0639\u06e1\u0644\u0657\u0627 \u0648\u064e\u062a\u064e\u0630\u064e\u0631\u064f\u0648\u0646\u064e \u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064e \u0671\u0644\u06e1\u062e\u064e\u0670\u0644\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e", "word_translation": "Do you call", "word_transliteration": "atad'una"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0644\u0657\u0627", "word_translation": "Baal", "word_transliteration": "ba'lan"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0630\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "and you forsake", "word_transliteration": "watadharuna"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064e", "word_translation": "(the) Best", "word_transliteration": "ahsana"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0670\u0644\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "(of) Creators", "word_transliteration": "al-khaliqina"}]}