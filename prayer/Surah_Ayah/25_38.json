{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0639\u064e\u0627\u062f\u0657\u0627 \u0648\u064e\u062b\u064e\u0645\u064f\u0648\u062f\u064e\u0627\u0652 \u0648\u064e\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064e \u0671\u0644\u0631\u0651\u064e\u0633\u0651\u0650 \u0648\u064e\u0642\u064f\u0631\u064f\u0648\u0646\u064e\u06e2\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0627\u062f\u0657\u0627", "word_translation": "And Ad", "word_transliteration": "wa'adan"}, {"word_arabic_uthmani": "\u0648\u064e\u062b\u064e\u0645\u064f\u0648\u062f\u064e\u0627\u0652", "word_translation": "and Thamud", "word_transliteration": "wathamuda"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064e", "word_translation": "and (the) dwellers", "word_transliteration": "wa-ashaba"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0633\u0651\u0650", "word_translation": "(of) Ar-rass", "word_transliteration": "al-rasi"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064f\u0631\u064f\u0648\u0646\u064e\u06e2\u0627", "word_translation": "and generations", "word_transliteration": "waqurunan"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e", "word_translation": "between", "word_transliteration": "bayna"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627", "word_translation": "many", "word_transliteration": "kathiran"}]}