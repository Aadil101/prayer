{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0671\u062e\u06e1\u0633\u064e\u0640\u0654\u064f\u0648\u0627\u0652 \u0641\u0650\u064a\u0647\u064e\u0627 \u0648\u064e\u0644\u064e\u0627 \u062a\u064f\u0643\u064e\u0644\u0651\u0650\u0645\u064f\u0648\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He (will) say", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0671\u062e\u06e1\u0633\u064e\u0640\u0654\u064f\u0648\u0627\u0652", "word_translation": "Remain despised", "word_transliteration": "ikh'sau"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "in it", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064f\u0643\u064e\u0644\u0651\u0650\u0645\u064f\u0648\u0646\u0650", "word_translation": "speak to Me", "word_transliteration": "tukallimuni"}]}