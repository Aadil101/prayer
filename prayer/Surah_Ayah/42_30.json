{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0635\u064e\u0670\u0628\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646 \u0645\u0651\u064f\u0635\u0650\u064a\u0628\u064e\u0629\u0656 \u0641\u064e\u0628\u0650\u0645\u064e\u0627 \u0643\u064e\u0633\u064e\u0628\u064e\u062a\u06e1 \u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0643\u064f\u0645\u06e1 \u0648\u064e\u064a\u064e\u0639\u06e1\u0641\u064f\u0648\u0627\u0652 \u0639\u064e\u0646 \u0643\u064e\u062b\u0650\u064a\u0631\u0656"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0635\u064e\u0670\u0628\u064e\u0643\u064f\u0645", "word_translation": "befalls you", "word_transliteration": "asabakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0635\u0650\u064a\u0628\u064e\u0629\u0656", "word_translation": "(the) misfortune", "word_transliteration": "musibatin"}, {"word_arabic_uthmani": "\u0641\u064e\u0628\u0650\u0645\u064e\u0627", "word_translation": "(is because) of what", "word_transliteration": "fabima"}, {"word_arabic_uthmani": "\u0643\u064e\u0633\u064e\u0628\u064e\u062a\u06e1", "word_translation": "have earned", "word_transliteration": "kasabat"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0643\u064f\u0645\u06e1", "word_translation": "your hands", "word_transliteration": "aydikum"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0639\u06e1\u0641\u064f\u0648\u0627\u0652", "word_translation": "But He pardons", "word_transliteration": "waya'fu"}, {"word_arabic_uthmani": "\u0639\u064e\u0646", "word_translation": "[from]", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u0656", "word_translation": "much", "word_transliteration": "kathirin"}]}