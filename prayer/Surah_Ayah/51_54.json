{"ayahs": {"ayah_uthmani": "\u0641\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e \u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0641\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064e \u0628\u0650\u0645\u064e\u0644\u064f\u0648\u0645\u0656"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e", "word_translation": "So turn away", "word_transliteration": "fatawalla"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "from them", "word_transliteration": "anhum"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627\u0653", "word_translation": "for not", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "you", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0644\u064f\u0648\u0645\u0656", "word_translation": "(are) to be blamed", "word_transliteration": "bimalumin"}]}