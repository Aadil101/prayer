{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0646\u064e\u0641\u06e1\u0642\u0650\u062f\u064f \u0635\u064f\u0648\u064e\u0627\u0639\u064e \u0671\u0644\u06e1\u0645\u064e\u0644\u0650\u0643\u0650 \u0648\u064e\u0644\u0650\u0645\u064e\u0646 \u062c\u064e\u0627\u0653\u0621\u064e \u0628\u0650\u0647\u0650\u06e6 \u062d\u0650\u0645\u06e1\u0644\u064f \u0628\u064e\u0639\u0650\u064a\u0631\u0656 \u0648\u064e\u0623\u064e\u0646\u064e\u0627\u06e0 \u0628\u0650\u0647\u0650\u06e6 \u0632\u064e\u0639\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0642\u0650\u062f\u064f", "word_translation": "We are missing", "word_transliteration": "nafqidu"}, {"word_arabic_uthmani": "\u0635\u064f\u0648\u064e\u0627\u0639\u064e", "word_translation": "(the) cup", "word_transliteration": "suwa'a"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u0650\u0643\u0650", "word_translation": "(of) the king", "word_transliteration": "al-maliki"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0645\u064e\u0646", "word_translation": "And for (one) who", "word_transliteration": "waliman"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "brings", "word_transliteration": "jaa"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u062d\u0650\u0645\u06e1\u0644\u064f", "word_translation": "(is) a load", "word_transliteration": "him'lu"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u0650\u064a\u0631\u0656", "word_translation": "(of) a camel", "word_transliteration": "ba'irin"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "and I", "word_transliteration": "wa-ana"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "for it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0632\u064e\u0639\u0650\u064a\u0645\u065e", "word_translation": "(is) responsible", "word_transliteration": "za'imun"}]}