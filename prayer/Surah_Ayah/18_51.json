{"ayahs": {"ayah_uthmani": "\u06de\u0645\u0651\u064e\u0627\u0653 \u0623\u064e\u0634\u06e1\u0647\u064e\u062f\u062a\u0651\u064f\u0647\u064f\u0645\u06e1 \u062e\u064e\u0644\u06e1\u0642\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0644\u064e\u0627 \u062e\u064e\u0644\u06e1\u0642\u064e \u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0645\u064e\u0627 \u0643\u064f\u0646\u062a\u064f \u0645\u064f\u062a\u0651\u064e\u062e\u0650\u0630\u064e \u0671\u0644\u06e1\u0645\u064f\u0636\u0650\u0644\u0651\u0650\u064a\u0646\u064e \u0639\u064e\u0636\u064f\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u06de\u0645\u0651\u064e\u0627\u0653", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u06e1\u0647\u064e\u062f\u062a\u0651\u064f\u0647\u064f\u0645\u06e1", "word_translation": "I made them witness", "word_transliteration": "ashhadttuhum"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0642\u064e", "word_translation": "the creation", "word_transliteration": "khalqa"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0642\u064e", "word_translation": "the creation", "word_transliteration": "khalqa"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1", "word_translation": "(of) themselves", "word_transliteration": "anfusihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f", "word_translation": "I Am", "word_transliteration": "kuntu"}, {"word_arabic_uthmani": "\u0645\u064f\u062a\u0651\u064e\u062e\u0650\u0630\u064e", "word_translation": "the One to take", "word_transliteration": "muttakhidha"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0636\u0650\u0644\u0651\u0650\u064a\u0646\u064e", "word_translation": "the misleaders", "word_transliteration": "al-mudilina"}, {"word_arabic_uthmani": "\u0639\u064e\u0636\u064f\u062f\u0657\u0627", "word_translation": "(as) helper(s)", "word_transliteration": "adudan"}]}