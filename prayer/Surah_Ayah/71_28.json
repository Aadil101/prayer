{"ayahs": {"ayah_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650 \u0671\u063a\u06e1\u0641\u0650\u0631\u06e1 \u0644\u0650\u064a \u0648\u064e\u0644\u0650\u0648\u064e\u0670\u0644\u0650\u062f\u064e\u064a\u0651\u064e \u0648\u064e\u0644\u0650\u0645\u064e\u0646 \u062f\u064e\u062e\u064e\u0644\u064e \u0628\u064e\u064a\u06e1\u062a\u0650\u064a\u064e \u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0657\u0627 \u0648\u064e\u0644\u0650\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e \u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064e\u0670\u062a\u0650\u06d6 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0632\u0650\u062f\u0650 \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u062a\u064e\u0628\u064e\u0627\u0631\u064e\u06e2\u0627"}, "words": [{"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650", "word_translation": "My Lord!", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0671\u063a\u06e1\u0641\u0650\u0631\u06e1", "word_translation": "Forgive", "word_transliteration": "igh'fir"}, {"word_arabic_uthmani": "\u0644\u0650\u064a", "word_translation": "me", "word_transliteration": "li"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0648\u064e\u0670\u0644\u0650\u062f\u064e\u064a\u0651\u064e", "word_translation": "and my parents", "word_transliteration": "waliwalidayya"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0645\u064e\u0646", "word_translation": "and whoever", "word_transliteration": "waliman"}, {"word_arabic_uthmani": "\u062f\u064e\u062e\u064e\u0644\u064e", "word_translation": "enters", "word_transliteration": "dakhala"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u062a\u0650\u064a\u064e", "word_translation": "my house", "word_transliteration": "baytiya"}, {"word_arabic_uthmani": "\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0657\u0627", "word_translation": "a believer", "word_transliteration": "mu'minan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "and believing men", "word_transliteration": "walil'mu'minina"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064e\u0670\u062a\u0650\u06d6", "word_translation": "and believing women", "word_transliteration": "wal-mu'minati"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0632\u0650\u062f\u0650", "word_translation": "increase", "word_transliteration": "tazidi"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimina"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u062a\u064e\u0628\u064e\u0627\u0631\u064e\u06e2\u0627", "word_translation": "(in) destruction", "word_transliteration": "tabaran"}]}