{"ayahs": {"ayah_uthmani": "\u0644\u0651\u064e\u064a\u06e1\u0633\u064e \u0628\u0650\u0623\u064e\u0645\u064e\u0627\u0646\u0650\u064a\u0651\u0650\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627\u0653 \u0623\u064e\u0645\u064e\u0627\u0646\u0650\u064a\u0651\u0650 \u0623\u064e\u0647\u06e1\u0644\u0650 \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650\u06d7 \u0645\u064e\u0646 \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u06e1 \u0633\u064f\u0648\u0653\u0621\u0657\u0627 \u064a\u064f\u062c\u06e1\u0632\u064e \u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u062c\u0650\u062f\u06e1 \u0644\u064e\u0647\u064f\u06e5 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0644\u0650\u064a\u0651\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u0646\u064e\u0635\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u064e\u064a\u06e1\u0633\u064e", "word_translation": "Not", "word_transliteration": "laysa"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0645\u064e\u0627\u0646\u0650\u064a\u0651\u0650\u0643\u064f\u0645\u06e1", "word_translation": "by your desire", "word_transliteration": "bi-amaniyyikum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627\u0653", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u064e\u0627\u0646\u0650\u064a\u0651\u0650", "word_translation": "(by the) desire", "word_transliteration": "amaniyyi"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u0650", "word_translation": "(of the) People", "word_transliteration": "ahli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650\u06d7", "word_translation": "(of) the Book", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "Whoever", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u06e1",