{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0636\u06e1\u0631\u0650\u0628\u06e1 \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u064e\u062b\u064e\u0644\u064b\u0627 \u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064e \u0671\u0644\u06e1\u0642\u064e\u0631\u06e1\u064a\u064e\u0629\u0650 \u0625\u0650\u0630\u06e1 \u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064e\u0627 \u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0636\u06e1\u0631\u0650\u0628\u06e1", "word_translation": "And set forth", "word_transliteration": "wa-id'rib"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "to them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u062b\u064e\u0644\u064b\u0627", "word_translation": "an example", "word_transliteration": "mathalan"}, {"word_arabic_uthmani": "\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064e", "word_translation": "(of the) companions", "word_transliteration": "ashaba"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0631\u06e1\u064a\u064e\u0629\u0650", "word_translation": "(of) the city", "word_transliteration": "al-qaryati"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "when", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064e\u0627", "word_translation": "came to it", "word_transliteration": "jaaha"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "the Messengers", "word_transliteration": "al-mur'saluna"}]}