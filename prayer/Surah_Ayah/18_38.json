{"ayahs": {"ayah_uthmani": "\u0644\u0651\u064e\u0640\u0670\u0643\u0650\u0646\u0651\u064e\u0627\u06e0 \u0647\u064f\u0648\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0631\u064e\u0628\u0651\u0650\u064a \u0648\u064e\u0644\u064e\u0627\u0653 \u0623\u064f\u0634\u06e1\u0631\u0650\u0643\u064f \u0628\u0650\u0631\u064e\u0628\u0651\u0650\u064a\u0653 \u0623\u064e\u062d\u064e\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u064e\u0640\u0670\u0643\u0650\u0646\u0651\u064e\u0627\u06e0", "word_translation": "But as for me", "word_transliteration": "lakinna"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a", "word_translation": "my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627\u0653", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0623\u064f\u0634\u06e1\u0631\u0650\u0643\u064f", "word_translation": "I associate", "word_transliteration": "ush'riku"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u0628\u0651\u0650\u064a\u0653", "word_translation": "with my Lord", "word_transliteration": "birabbi"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u0657\u0627", "word_translation": "anyone", "word_transliteration": "ahadan"}]}