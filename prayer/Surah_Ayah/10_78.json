{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u062c\u0650\u0626\u06e1\u062a\u064e\u0646\u064e\u0627 \u0644\u0650\u062a\u064e\u0644\u06e1\u0641\u0650\u062a\u064e\u0646\u064e\u0627 \u0639\u064e\u0645\u0651\u064e\u0627 \u0648\u064e\u062c\u064e\u062f\u06e1\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0621\u064e\u0646\u064e\u0627 \u0648\u064e\u062a\u064e\u0643\u064f\u0648\u0646\u064e \u0644\u064e\u0643\u064f\u0645\u064e\u0627 \u0671\u0644\u06e1\u0643\u0650\u0628\u06e1\u0631\u0650\u064a\u064e\u0627\u0653\u0621\u064f \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0645\u064e\u0627 \u0646\u064e\u062d\u06e1\u0646\u064f \u0644\u064e\u0643\u064f\u0645\u064e\u0627 \u0628\u0650\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u0650\u0626\u06e1\u062a\u064e\u0646\u064e\u0627", "word_translation": "Have you come to us", "word_transliteration": "aji'tana"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064e\u0644\u06e1\u0641\u0650\u062a\u064e\u0646\u064e\u0627", "word_translation": "to turn us away", "word_transliteration": "litalfitana"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "from that", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u062f\u06e1\u0646\u064e\u0627", "word_translation": "we found", "word_transliteration": "wajadna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "on it", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0621\u064e\u0646\u064e\u0627", "word_translation": "our forefathers", "word_transliteration": "abaana"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0643\u064f\u0648\u0646\u064e", "word_translation": "and you two (may) have", "word_transliteration": "watakuna"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u064e\u0627", "word_translation": "and you two (may) have", "word_transliteration": "lakuma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u0628\u06e1\u0631\u0650\u064a\u064e\u0627\u0653\u0621\u064f", "word_translation": "the greatness", "word_transliteration": "al-kib'riyau"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the land", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And we (are) not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "And we (are) not", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u064e\u0627", "word_translation": "(in) you two", "word_transliteration": "lakuma"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "believers", "word_transliteration": "bimu'minina"}]}