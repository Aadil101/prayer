{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653 \u0621\u064e\u0627\u0645\u064e\u0646\u0651\u064e\u0627 \u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0646\u064e\u0627 \u0644\u0650\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064e \u0644\u064e\u0646\u064e\u0627 \u062e\u064e\u0637\u064e\u0670\u064a\u064e\u0670\u0646\u064e\u0627 \u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0643\u06e1\u0631\u064e\u0647\u06e1\u062a\u064e\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0645\u0650\u0646\u064e \u0671\u0644\u0633\u0651\u0650\u062d\u06e1\u0631\u0650\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u062e\u064e\u064a\u06e1\u0631\u065e \u0648\u064e\u0623\u064e\u0628\u06e1\u0642\u064e\u0649\u0670\u0653"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653", "word_translation": "Indeed [we]", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u0651\u064e\u0627", "word_translation": "we believe", "word_transliteration": "amanna"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0646\u064e\u0627", "word_translation": "in our Lord", "word_transliteration": "birabbina"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064e", "word_translation": "that He may forgive", "word_transliteration": "liyaghfira"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0627", "word_translation": "for us", "word_transliteration": "lana"}, {"word_arabic_uthmani": "\u062e\u064e\u0637\u064e\u0670\u064a\u064e\u0670\u0646\u064e\u0627", "word_translation": "our sins", "word_transliteration": "khatayana"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u0631\u064e\u0647\u06e1\u062a\u064e\u0646\u064e\u0627", "word_translation": "you compelled us", "word_transliteration": "akrahtana"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "on it", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u0650\u062d\u06e1\u0631\u0650\u06d7", "word_translation": "the magic", "word_transliteration": "al-sih'ri"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u065e", "word_translation": "(is) Best", "word_transliteration": "khayrun"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0628\u06e1\u0642\u064e\u0649\u0670\u0653", "word_translation": "and Ever Lasting", "word_transliteration": "wa-abqa"}]}