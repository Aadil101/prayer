{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0645\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u0648\u064e\u0648\u064e\u0642\u064e\u0649\u0670\u0646\u064e\u0627 \u0639\u064e\u0630\u064e\u0627\u0628\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064f\u0648\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0646\u0651\u064e", "word_translation": "But Allah conferred favor", "word_transliteration": "famanna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "But Allah conferred favor", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "upon us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0648\u064e\u0648\u064e\u0642\u064e\u0649\u0670\u0646\u064e\u0627", "word_translation": "and protected us", "word_transliteration": "wawaqana"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064e", "word_translation": "(from the) punishment", "word_transliteration": "adhaba"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064f\u0648\u0645\u0650", "word_translation": "(of) the Scorching Fire", "word_transliteration": "al-samumi"}]}