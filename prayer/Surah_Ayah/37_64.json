{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064e\u0627 \u0634\u064e\u062c\u064e\u0631\u064e\u0629\u065e \u062a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f \u0641\u0650\u064a\u0653 \u0623\u064e\u0635\u06e1\u0644\u0650 \u0671\u0644\u06e1\u062c\u064e\u062d\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064e\u0627", "word_translation": "Indeed it", "word_transliteration": "innaha"}, {"word_arabic_uthmani": "\u0634\u064e\u062c\u064e\u0631\u064e\u0629\u065e", "word_translation": "(is) a tree", "word_transliteration": "shajaratun"}, {"word_arabic_uthmani": "\u062a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f", "word_translation": "that grows", "word_transliteration": "takhruju"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0653", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0623\u064e\u0635\u06e1\u0644\u0650", "word_translation": "(the) bottom", "word_transliteration": "asli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u062d\u0650\u064a\u0645\u0650", "word_translation": "(of) the Hellfire", "word_transliteration": "al-jahimi"}]}