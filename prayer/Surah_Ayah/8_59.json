{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0633\u064e\u0628\u064e\u0642\u064f\u0648\u0653\u0627\u0652\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064f\u0639\u06e1\u062c\u0650\u0632\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (let) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e", "word_translation": "think", "word_transliteration": "yahsabanna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u064e\u0642\u064f\u0648\u0653\u0627\u0652\u06da", "word_translation": "they can outstrip", "word_transliteration": "sabaqu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Indeed they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(can) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0639\u06e1\u062c\u0650\u0632\u064f\u0648\u0646\u064e", "word_translation": "escape", "word_transliteration": "yu'jizuna"}]}