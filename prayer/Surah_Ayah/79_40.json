{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627 \u0645\u064e\u0646\u06e1 \u062e\u064e\u0627\u0641\u064e \u0645\u064e\u0642\u064e\u0627\u0645\u064e \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u06e6 \u0648\u064e\u0646\u064e\u0647\u064e\u0649 \u0671\u0644\u0646\u0651\u064e\u0641\u06e1\u0633\u064e \u0639\u064e\u0646\u0650 \u0671\u0644\u06e1\u0647\u064e\u0648\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627", "word_translation": "But as for", "word_transliteration": "wa-amma"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u06e1", "word_translation": "(him) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u062e\u064e\u0627\u0641\u064e", "word_translation": "feared", "word_transliteration": "khafa"}, {"word_arabic_uthmani": "\u0645\u064e\u0642\u064e\u0627\u0645\u064e", "word_translation": "standing", "word_transliteration": "maqama"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u06e6", "word_translation": "(before) his Lord", "word_transliteration": "rabbihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u0647\u064e\u0649", "word_translation": "and restrained", "word_transliteration": "wanaha"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0641\u06e1\u0633\u064e", "word_translation": "his soul", "word_transliteration": "al-nafsa"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0650", "word_translation": "from", "word_transliteration": "ani"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0647\u064e\u0648\u064e\u0649\u0670", "word_translation": "the vain desires", "word_transliteration": "al-hawa"}]}