{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a \u0645\u064f\u0631\u06e1\u0633\u0650\u0644\u064e\u0629\u064c \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645 \u0628\u0650\u0647\u064e\u062f\u0650\u064a\u0651\u064e\u0629\u0656 \u0641\u064e\u0646\u064e\u0627\u0638\u0650\u0631\u064e\u0629\u064f\u06e2 \u0628\u0650\u0645\u064e \u064a\u064e\u0631\u06e1\u062c\u0650\u0639\u064f \u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "But indeed I am", "word_transliteration": "wa-inni"}, {"word_arabic_uthmani": "\u0645\u064f\u0631\u06e1\u0633\u0650\u0644\u064e\u0629\u064c", "word_translation": "going to send", "word_transliteration": "mur'silatun"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645", "word_translation": "to them", "word_transliteration": "ilayhim"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u064e\u062f\u0650\u064a\u0651\u064e\u0629\u0656", "word_translation": "a gift", "word_transliteration": "bihadiyyatin"}, {"word_arabic_uthmani": "\u0641\u064e\u0646\u064e\u0627\u0638\u0650\u0631\u064e\u0629\u064f\u06e2", "word_translation": "and see", "word_transliteration": "fanaziratun"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e", "word_translation": "with what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u062c\u0650\u0639\u064f", "word_translation": "return", "word_transliteration": "yarji'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "the messengers", "word_transliteration": "al-mur'saluna"}]}