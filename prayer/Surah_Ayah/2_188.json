{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u064e\u0643\u064f\u0645 \u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645 \u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u0670\u0637\u0650\u0644\u0650 \u0648\u064e\u062a\u064f\u062f\u06e1\u0644\u064f\u0648\u0627\u0652 \u0628\u0650\u0647\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u06e1\u062d\u064f\u0643\u0651\u064e\u0627\u0645\u0650 \u0644\u0650\u062a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0627\u0652 \u0641\u064e\u0631\u0650\u064a\u0642\u0657\u0627 \u0645\u0651\u0650\u0646\u06e1 \u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u0650 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0628\u0650\u0671\u0644\u06e1\u0625\u0650\u062b\u06e1\u0645\u0650 \u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "eat", "word_transliteration": "takulu"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u064e\u0643\u064f\u0645", "word_translation": "your properties", "word_transliteration": "amwalakum"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645", "word_translation": "among yourselves", "word_transliteration": "baynakum"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u0670\u0637\u0650\u0644\u0650", "word_translation": "wrongfully", "word_transliteration": "bil-batili"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064f\u062f\u06e1\u0644\u064f\u0648\u0627\u0652", "word_translation": "and present", "word_transliteration": "watud'lu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u064e\u0627\u0653", "word_translation": "[with] it", "word_transliteration": "biha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064f\u0643\u0651\u064e\u0627\u0645\u0650", "word_translation": "the authorities", "word_transliteration": "al-hukami"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "so that you may eat", "word_transliteration": "litakulu"}, {"word_arabic_uthmani": "\u0641\u064e\u0631\u0650\u064a\u0642\u0657\u0627", "word_translation": "a portion", "word_transliteration": "fariqan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u0650", "word_translation": "(the) wealth", "word_transliteration": "amwali"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "(of) the people", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0625\u0650\u062b\u06e1\u0645\u0650", "word_translation": "sinfully", "word_transliteration": "bil-ith'mi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "while you", "word_transliteration": "wa-antum"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "know", "word_transliteration": "ta'lamuna"}]}