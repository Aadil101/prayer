{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0671\u0628\u06e1\u0646\u064f\u0648\u0627\u0652 \u0644\u064e\u0647\u064f\u06e5 \u0628\u064f\u0646\u06e1\u064a\u064e\u0670\u0646\u0657\u0627 \u0641\u064e\u0623\u064e\u0644\u06e1\u0642\u064f\u0648\u0647\u064f \u0641\u0650\u064a \u0671\u0644\u06e1\u062c\u064e\u062d\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u0646\u064f\u0648\u0627\u0652", "word_translation": "Build", "word_transliteration": "ib'nu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0628\u064f\u0646\u06e1\u064a\u064e\u0670\u0646\u0657\u0627", "word_translation": "a structure", "word_transliteration": "bun'yanan"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0644\u06e1\u0642\u064f\u0648\u0647\u064f", "word_translation": "and throw him", "word_transliteration": "fa-alquhu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "into", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u062d\u0650\u064a\u0645\u0650", "word_translation": "the blazing Fire", "word_transliteration": "al-jahimi"}]}