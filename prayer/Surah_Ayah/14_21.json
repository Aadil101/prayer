{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0628\u064e\u0631\u064e\u0632\u064f\u0648\u0627\u0652 \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627 \u0641\u064e\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u0636\u0651\u064f\u0639\u064e\u0641\u064e\u0640\u0670\u0653\u0624\u064f\u0627\u0652 \u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u0651\u064e\u0627 \u0643\u064f\u0646\u0651\u064e\u0627 \u0644\u064e\u0643\u064f\u0645\u06e1 \u062a\u064e\u0628\u064e\u0639\u0657\u0627 \u0641\u064e\u0647\u064e\u0644\u06e1 \u0623\u064e\u0646\u062a\u064f\u0645 \u0645\u0651\u064f\u063a\u06e1\u0646\u064f\u0648\u0646\u064e \u0639\u064e\u0646\u0651\u064e\u0627 \u0645\u0650\u0646\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0645\u0650\u0646 \u0634\u064e\u064a\u06e1\u0621\u0656\u06da \u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0644\u064e\u0648\u06e1 \u0647\u064e\u062f\u064e\u0649\u0670\u0646\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0647\u064e\u062f\u064e\u064a\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1\u06d6 \u0633\u064e\u0648\u064e\u0627\u0653\u0621\u064c \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627\u0653 \u0623\u064e\u062c\u064e\u0632\u0650\u0639\u06e1\u0646\u064e\u0627\u0653 \u0623\u064e\u0645\u06e1 \u0635\u064e\u0628\u064e\u0631\u06e1\u0646\u064e\u0627 \u0645\u064e\u0627 \u0644\u064e\u0646\u064e\u0627 \u0645\u0650\u0646 \u0645\u0651\u064e\u062d\u0650\u064a\u0635\u0656"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u0631\u064e\u0632\u064f\u0648\u0627\u0652", "word_translation": "And they will come forth", "word_transliteration": "wabarazu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "before Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627", "word_translation": "all together", "word_transliteration": "jami'an"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "then will say", "word_transliteration": "faqala"}, {"word_arabic_uthmani": "\u0671\u0644\u0636\u0651\u064f\u0639\u064e\u0641\u064e\u0640\u0670\u0653\u0624\u064f\u0627\u0652", "word_translation": "the weak", "word_transliteration": "al-du'afau"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "to those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0653\u0627\u0652", "word_translation": "were arrogant", "word_transliteration": "is'takbaru"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed, we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "we were", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u062a\u064e\u0628\u064e\u0639\u0657\u0627", "word_translation": "followers", "word_transliteration": "taba'an"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064e\u0644\u06e1", "word_translation": "so can", "word_transliteration": "fahal"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645", "word_translation": "you (be)", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u063a\u06e1\u0646\u064f\u0648\u0646\u064e", "word_translation": "the one who avails", "word_transliteration": "mugh'nuna"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0651\u064e\u0627", "word_translation": "us", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u0650", "word_translation": "(the) punishment", "word_transliteration": "adhabi"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "anything", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656\u06da", "word_translation": "anything", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They will say", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1", "word_translation": "If", "word_transliteration": "law"}, {"word_arabic_uthmani": "\u0647\u064e\u062f\u064e\u0649\u0670\u0646\u064e\u0627", "word_translation": "Allah had guided us", "word_transliteration": "hadana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah had guided us", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064e\u062f\u064e\u064a\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "surely we would have guided you", "word_transliteration": "lahadaynakum"}, {"word_arabic_uthmani": "\u0633\u064e\u0648\u064e\u0627\u0653\u0621\u064c", "word_translation": "(It is) same", "word_transliteration": "sawaon"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627\u0653", "word_translation": "for us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u064e\u0632\u0650\u0639\u06e1\u0646\u064e\u0627\u0653", "word_translation": "whether we show intolerance", "word_transliteration": "ajazi'na"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0635\u064e\u0628\u064e\u0631\u06e1\u0646\u064e\u0627", "word_translation": "we are patient", "word_transliteration": "sabarna"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0627", "word_translation": "(is) for us", "word_transliteration": "lana"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u062d\u0650\u064a\u0635\u0656", "word_translation": "place of escape", "word_transliteration": "mahisin"}]}