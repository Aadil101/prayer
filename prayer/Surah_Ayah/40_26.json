{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064f \u0630\u064e\u0631\u064f\u0648\u0646\u0650\u064a\u0653 \u0623\u064e\u0642\u06e1\u062a\u064f\u0644\u06e1 \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0648\u064e\u0644\u06e1\u064a\u064e\u062f\u06e1\u0639\u064f \u0631\u064e\u0628\u0651\u064e\u0647\u064f\u06e5\u0653\u06d6 \u0625\u0650\u0646\u0651\u0650\u064a\u0653 \u0623\u064e\u062e\u064e\u0627\u0641\u064f \u0623\u064e\u0646 \u064a\u064f\u0628\u064e\u062f\u0651\u0650\u0644\u064e \u062f\u0650\u064a\u0646\u064e\u0643\u064f\u0645\u06e1 \u0623\u064e\u0648\u06e1 \u0623\u064e\u0646 \u064a\u064f\u0638\u06e1\u0647\u0650\u0631\u064e \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0671\u0644\u06e1\u0641\u064e\u0633\u064e\u0627\u062f\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "And said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064f", "word_translation": "Firaun", "word_transliteration": "fir'awnu"}, {"word_arabic_uthmani": "\u0630\u064e\u0631\u064f\u0648\u0646\u0650\u064a\u0653", "word_translation": "Leave me", "word_transliteration": "dharuni"}, {"word_arabic_uthmani": "\u0623\u064e\u0642\u06e1\u062a\u064f\u0644\u06e1", "word_translation": "(so that) I kill", "word_transliteration": "aqtul"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u06e1\u064a\u064e\u062f\u06e1\u0639\u064f", "word_translation": "and let him call", "word_transliteration": "walyad'u"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0647\u064f\u06e5\u0653\u06d6", "word_translation": "his Lord", "word_transliteration": "rabbahu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u064e\u0627\u0641\u064f", "word_translation": "[I] fear", "word_transliteration": "akhafu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u0628\u064e\u062f\u0651\u0650\u0644\u064e", "word_translation": "he will change", "word_transliteration": "yubaddila"}, {"word_arabic_uthmani": "\u062f\u0650\u064a\u0646\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your religion", "word_transliteration": "dinakum"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u0638\u06e1\u0647\u0650\u0631\u064e", "word_translation": "he may cause to appear", "word_transliteration": "yuz'hira"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the land", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0633\u064e\u0627\u062f\u064e", "word_translation": "the corruption", "word_transliteration": "al-fasada"}]}