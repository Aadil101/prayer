{"ayahs": {"ayah_uthmani": "\u0647\u064f\u0648\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u064e \u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5 \u0628\u0650\u0671\u0644\u06e1\u0647\u064f\u062f\u064e\u0649\u0670 \u0648\u064e\u062f\u0650\u064a\u0646\u0650 \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0644\u0650\u064a\u064f\u0638\u06e1\u0647\u0650\u0631\u064e\u0647\u064f\u06e5 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u062f\u0651\u0650\u064a\u0646\u0650 \u0643\u064f\u0644\u0651\u0650\u0647\u0650\u06e6\u06da \u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0634\u064e\u0647\u0650\u064a\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u064e", "word_translation": "(has) sent", "word_transliteration": "arsala"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5", "word_translation": "His Messenger", "word_transliteration": "rasulahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0647\u064f\u062f\u064e\u0649\u0670", "word_translation": "with guidance", "word_transliteration": "bil-huda"}, {"word_arabic_uthmani": "\u0648\u064e\u062f\u0650\u064a\u0646\u0650", "word_translation": "and (the) religion", "word_transliteration": "wadini"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "the true", "word_transliteration": "al-haqi"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u0638\u06e1\u0647\u0650\u0631\u064e\u0647\u064f\u06e5", "word_translation": "that He (may) make it prevail", "word_transliteration": "liyuz'hirahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u0650\u064a\u0646\u0650", "word_translation": "the religions", "word_transliteration": "al-dini"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650\u0647\u0650\u06e6\u06da", "word_translation": "all", "word_transliteration": "kullihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "And sufficient is", "word_transliteration": "wakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0634\u064e\u0647\u0650\u064a\u062f\u0657\u0627", "word_translation": "(as) a Witness", "word_transliteration": "shahidan"}]}