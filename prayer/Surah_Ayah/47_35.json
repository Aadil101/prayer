{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u0647\u0650\u0646\u064f\u0648\u0627\u0652 \u0648\u064e\u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0633\u0651\u064e\u0644\u06e1\u0645\u0650 \u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0644\u064e\u0648\u06e1\u0646\u064e \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064e\u0639\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0646 \u064a\u064e\u062a\u0650\u0631\u064e\u0643\u064f\u0645\u06e1 \u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064e\u0643\u064f\u0645\u06e1"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "So (do) not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u062a\u064e\u0647\u0650\u0646\u064f\u0648\u0627\u0652", "word_translation": "weaken", "word_transliteration": "tahinu"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0653\u0627\u0652", "word_translation": "and call", "word_transliteration": "watad'u"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "for", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0644\u06e1\u0645\u0650", "word_translation": "peace", "word_transliteration": "al-salmi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u064f", "word_translation": "while you", "word_transliteration": "wa-antumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0644\u064e\u0648\u06e1\u0646\u064e", "word_translation": "(are) superior", "word_transliteration": "al-a'lawna"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "and Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(is) with you", "word_transliteration": "ma'akum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0646", "word_translation": "and never", "word_transliteration": "walan"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u0650\u0631\u064e\u0643\u064f\u0645\u06e1", "word_translation": "will deprive you", "word_transliteration": "yatirakum"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(of) your deeds", "word_transliteration": "a'malakum"}]}