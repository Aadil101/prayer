{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0645\u064e\u0646\u064e\u0646\u0651\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e \u0645\u064e\u0631\u0651\u064e\u0629\u064b \u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670\u0653"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And indeed", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u064e\u0646\u0651\u064e\u0627", "word_translation": "We conferred a favor", "word_transliteration": "mananna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e", "word_translation": "on you", "word_transliteration": "alayka"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u0651\u064e\u0629\u064b", "word_translation": "another time", "word_transliteration": "marratan"}, {"word_arabic_uthmani": "\u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670\u0653", "word_translation": "another time", "word_transliteration": "ukh'ra"}]}