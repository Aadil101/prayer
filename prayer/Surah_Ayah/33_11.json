{"ayahs": {"ayah_uthmani": "\u0647\u064f\u0646\u064e\u0627\u0644\u0650\u0643\u064e \u0671\u0628\u06e1\u062a\u064f\u0644\u0650\u064a\u064e \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u0648\u064e\u0632\u064f\u0644\u06e1\u0632\u0650\u0644\u064f\u0648\u0627\u0652 \u0632\u0650\u0644\u06e1\u0632\u064e\u0627\u0644\u0657\u0627 \u0634\u064e\u062f\u0650\u064a\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0647\u064f\u0646\u064e\u0627\u0644\u0650\u0643\u064e", "word_translation": "There", "word_transliteration": "hunalika"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u062a\u064f\u0644\u0650\u064a\u064e", "word_translation": "were tried", "word_transliteration": "ub'tuliya"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "the believers", "word_transliteration": "al-mu'minuna"}, {"word_arabic_uthmani": "\u0648\u064e\u0632\u064f\u0644\u06e1\u0632\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "and shaken", "word_transliteration": "wazul'zilu"}, {"word_arabic_uthmani": "\u0632\u0650\u0644\u06e1\u0632\u064e\u0627\u0644\u0657\u0627", "word_translation": "(with a) shake", "word_transliteration": "zil'zalan"}, {"word_arabic_uthmani": "\u0634\u064e\u062f\u0650\u064a\u062f\u0657\u0627", "word_translation": "severe", "word_transliteration": "shadidan"}]}