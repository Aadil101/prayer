{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e \u0647\u064e\u0670\u0630\u064e\u0627 \u0671\u0644\u06e1\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064f \u0623\u064e\u0646 \u064a\u064f\u0641\u06e1\u062a\u064e\u0631\u064e\u0649\u0670 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646 \u062a\u064e\u0635\u06e1\u062f\u0650\u064a\u0642\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0628\u064e\u064a\u06e1\u0646\u064e \u064a\u064e\u062f\u064e\u064a\u06e1\u0647\u0650 \u0648\u064e\u062a\u064e\u0641\u06e1\u0635\u0650\u064a\u0644\u064e \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u0644\u064e\u0627 \u0631\u064e\u064a\u06e1\u0628\u064e \u0641\u0650\u064a\u0647\u0650 \u0645\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650 \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "this", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064f", "word_translation": "the Quran", "word_transliteration": "al-qur'anu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u0641\u06e1\u062a\u064e\u0631\u064e\u0649\u0670", "word_translation": "(it could be) produced", "word_transliteration": "yuf'tara"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "by", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "other than Allah", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "other than Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646", "word_translation": "but", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u062a\u064e\u0635\u06e1\u062f\u0650\u064a\u0642\u064e", "word_translation": "(it is) a confirmation", "word_transliteration": "tasdiqa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(of that) which", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e", "word_translation": "(was) before it", "word_transliteration": "bayna"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u064e\u064a\u06e1\u0647\u0650", "word_translation": "(was) before it", "word_transliteration": "yadayhi"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0641\u06e1\u0635\u0650\u064a\u0644\u064e", "word_translation": "and a detailed explanation", "word_transliteration": "watafsila"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "(of) the Book", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(there is) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0631\u064e\u064a\u06e1\u0628\u064e", "word_translation": "doubt", "word_transliteration": "rayba"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650", "word_translation": "in it", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650", "word_translation": "(the) Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the worlds", "word_transliteration": "al-'alamina"}]}