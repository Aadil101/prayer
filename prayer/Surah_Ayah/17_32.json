{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0642\u06e1\u0631\u064e\u0628\u064f\u0648\u0627\u0652 \u0671\u0644\u0632\u0651\u0650\u0646\u064e\u0649\u0670\u0653\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0643\u064e\u0627\u0646\u064e \u0641\u064e\u0670\u062d\u0650\u0634\u064e\u0629\u0657 \u0648\u064e\u0633\u064e\u0627\u0653\u0621\u064e \u0633\u064e\u0628\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u06e1\u0631\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "go near", "word_transliteration": "taqrabu"}, {"word_arabic_uthmani": "\u0671\u0644\u0632\u0651\u0650\u0646\u064e\u0649\u0670\u0653\u06d6", "word_translation": "adultery", "word_transliteration": "al-zina"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed it", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0641\u064e\u0670\u062d\u0650\u0634\u064e\u0629\u0657", "word_translation": "an immorality", "word_transliteration": "fahishatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064e\u0627\u0653\u0621\u064e", "word_translation": "and (an) evil", "word_transliteration": "wasaa"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u0657\u0627", "word_translation": "way", "word_transliteration": "sabilan"}]}