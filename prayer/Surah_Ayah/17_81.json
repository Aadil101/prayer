{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064f\u0644\u06e1 \u062c\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f \u0648\u064e\u0632\u064e\u0647\u064e\u0642\u064e \u0671\u0644\u06e1\u0628\u064e\u0670\u0637\u0650\u0644\u064f\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u06e1\u0628\u064e\u0670\u0637\u0650\u0644\u064e \u0643\u064e\u0627\u0646\u064e \u0632\u064e\u0647\u064f\u0648\u0642\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064f\u0644\u06e1", "word_translation": "And say", "word_transliteration": "waqul"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "Has come", "word_transliteration": "jaa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f", "word_translation": "the truth", "word_transliteration": "al-haqu"}, {"word_arabic_uthmani": "\u0648\u064e\u0632\u064e\u0647\u064e\u0642\u064e", "word_translation": "and perished", "word_transliteration": "wazahaqa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0670\u0637\u0650\u0644\u064f\u06da", "word_translation": "the falsehood", "word_transliteration": "al-batilu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0670\u0637\u0650\u0644\u064e", "word_translation": "the falsehood", "word_transliteration": "al-batila"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0632\u064e\u0647\u064f\u0648\u0642\u0657\u0627", "word_translation": "(bound) to perish", "word_transliteration": "zahuqan"}]}