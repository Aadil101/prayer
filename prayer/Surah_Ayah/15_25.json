{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0631\u064e\u0628\u0651\u064e\u0643\u064e \u0647\u064f\u0648\u064e \u064a\u064e\u062d\u06e1\u0634\u064f\u0631\u064f\u0647\u064f\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u062d\u064e\u0643\u0650\u064a\u0645\u064c \u0639\u064e\u0644\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbaka"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0634\u064f\u0631\u064f\u0647\u064f\u0645\u06e1\u06da", "word_translation": "will gather them", "word_transliteration": "yahshuruhum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u062d\u064e\u0643\u0650\u064a\u0645\u064c", "word_translation": "(is) All-Wise", "word_transliteration": "hakimun"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u065e", "word_translation": "All-Knowing", "word_transliteration": "alimun"}]}