{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627 \u0644\u0650\u0628\u064e\u0634\u064e\u0631\u0656 \u0645\u0651\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0643\u064e \u0671\u0644\u06e1\u062e\u064f\u0644\u06e1\u062f\u064e\u06d6 \u0623\u064e\u0641\u064e\u0625\u0650\u064a\u0652\u0646 \u0645\u0651\u0650\u062a\u0651\u064e \u0641\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u062e\u064e\u0670\u0644\u0650\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We made", "word_transliteration": "ja'alna"}, {"word_arabic_uthmani": "\u0644\u0650\u0628\u064e\u0634\u064e\u0631\u0656", "word_translation": "for any man", "word_transliteration": "libasharin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "before you", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0643\u064e", "word_translation": "before you", "word_transliteration": "qablika"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064f\u0644\u06e1\u062f\u064e\u06d6", "word_translation": "[the] immortality", "word_transliteration": "al-khul'da"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0625\u0650\u064a\u0652\u0646", "word_translation": "so if", "word_transliteration": "afa-in"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u062a\u0651\u064e", "word_translation": "you die", "word_transliteration": "mitta"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0645\u064f", "word_translation": "then (would) they", "word_transliteration": "fahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0670\u0644\u0650\u062f\u064f\u0648\u0646\u064e", "word_translation": "live forever", "word_transliteration": "al-khaliduna"}]}