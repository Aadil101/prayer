{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0646\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064e\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0623\u064f\u0631\u06e1\u0633\u0650\u0644\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0648\u064e\u0644\u064e\u0646\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064e\u0646\u0651\u064e \u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0646\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064e\u0646\u0651\u064e", "word_translation": "Then surely We will question", "word_transliteration": "falanasalanna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those (to) whom", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0623\u064f\u0631\u06e1\u0633\u0650\u0644\u064e", "word_translation": "were sent", "word_transliteration": "ur'sila"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "to them (Messengers)", "word_transliteration": "ilayhim"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0646\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064e\u0646\u0651\u064e", "word_translation": "and surely We will question", "word_transliteration": "walanasalanna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u0650\u064a\u0646\u064e", "word_translation": "the Messengers", "word_transliteration": "al-mur'salina"}]}