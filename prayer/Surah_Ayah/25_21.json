{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0644\u064e\u0627 \u064a\u064e\u0631\u06e1\u062c\u064f\u0648\u0646\u064e \u0644\u0650\u0642\u064e\u0627\u0653\u0621\u064e\u0646\u064e\u0627 \u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653 \u0623\u064f\u0646\u0632\u0650\u0644\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f \u0623\u064e\u0648\u06e1 \u0646\u064e\u0631\u064e\u0649\u0670 \u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u06d7 \u0644\u064e\u0642\u064e\u062f\u0650 \u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652 \u0641\u0650\u064a\u0653 \u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0639\u064e\u062a\u064e\u0648\u06e1 \u0639\u064f\u062a\u064f\u0648\u0651\u0657\u0627 \u0643\u064e\u0628\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "And said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u062c\u064f\u0648\u0646\u064e", "word_translation": "expect", "word_transliteration": "yarjuna"}, {"word_arabic_uthmani": "\u0644\u0650\u0642\u064e\u0627\u0653\u0621\u064e\u0646\u064e\u0627", "word_translation": "(the) meeting with Us", "word_transliteration": "liqaana"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653", "word_translation": "Why not", "word_transliteration": "lawla"}, {"word_arabic_uthmani": "\u0623\u064f\u0646\u0632\u0650\u0644\u064e", "word_translation": "are sent down", "word_transliteration": "unzila"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "to us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f", "word_translation": "the Angels", "word_transliteration": "al-malaikatu"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0646\u064e\u0631\u064e\u0649\u0670", "word_translation": "we see", "word_transliteration": "nara"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u06d7", "word_translation": "our Lord", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u062f\u0650", "word_translation": "Indeed", "word_transliteration": "laqadi"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "they have become arrogant", "word_transliteration": "is'takbaru"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0653", "word_translation": "within", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1", "word_translation": "themselves", "word_transliteration": "anfusihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u062a\u064e\u0648\u06e1", "word_translation": "and (become) insolent", "word_transliteration": "wa'ataw"}, {"word_arabic_uthmani": "\u0639\u064f\u062a\u064f\u0648\u0651\u0657\u0627", "word_translation": "(with) insolence", "word_transliteration": "utuwwan"}, {"word_arabic_uthmani": "\u0643\u064e\u0628\u0650\u064a\u0631\u0657\u0627", "word_translation": "great", "word_transliteration": "kabiran"}]}