{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e \u0621\u064e\u0627\u0644\u064e \u0644\u064f\u0648\u0637\u064d \u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "And when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "came", "word_transliteration": "jaa"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u064e", "word_translation": "(to the) family", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0644\u064f\u0648\u0637\u064d", "word_translation": "(of) Lut", "word_transliteration": "lutin"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "the messengers", "word_transliteration": "al-mur'saluna"}]}