{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0648\u06e1\u0645\u064e\u0626\u0650\u0630\u0656 \u0641\u0650\u064a \u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u0650 \u0645\u064f\u0634\u06e1\u062a\u064e\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Then indeed they", "word_transliteration": "fa-innahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e\u0626\u0650\u0630\u0656", "word_translation": "that Day", "word_transliteration": "yawma-idhin"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u0650", "word_translation": "the punishment", "word_transliteration": "al-'adhabi"}, {"word_arabic_uthmani": "\u0645\u064f\u0634\u06e1\u062a\u064e\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "(will be) sharers", "word_transliteration": "mush'tarikuna"}]}