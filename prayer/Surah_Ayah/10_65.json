{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u062d\u06e1\u0632\u064f\u0646\u0643\u064e \u0642\u064e\u0648\u06e1\u0644\u064f\u0647\u064f\u0645\u06e1\u06d8 \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u06e1\u0639\u0650\u0632\u0651\u064e\u0629\u064e \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u062c\u064e\u0645\u0650\u064a\u0639\u064b\u0627\u06da \u0647\u064f\u0648\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (let) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0632\u064f\u0646\u0643\u064e", "word_translation": "grieve you", "word_transliteration": "yahzunka"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0644\u064f\u0647\u064f\u0645\u06e1\u06d8", "word_translation": "their speech", "word_transliteration": "qawluhum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0632\u0651\u064e\u0629\u064e", "word_translation": "the honor", "word_transliteration": "al-'izata"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "(belongs) to Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0639\u064b\u0627\u06da", "word_translation": "all", "word_transliteration": "jami'an"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f", "word_translation": "(is) the All-Hearer", "word_transliteration": "al-sami'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "the All-Knower", "word_transliteration": "al-'alimu"}]}