{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0631\u064e\u0628\u0651\u064f\u0643\u064e \u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f \u0645\u064e\u0627 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0648\u064e\u064a\u064e\u062e\u06e1\u062a\u064e\u0627\u0631\u064f\u06d7 \u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u062e\u0650\u064a\u064e\u0631\u064e\u0629\u064f\u06da \u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649\u0670 \u0639\u064e\u0645\u0651\u064e\u0627 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "And your Lord", "word_transliteration": "warabbuka"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f", "word_translation": "creates", "word_transliteration": "yakhluqu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062e\u06e1\u062a\u064e\u0627\u0631\u064f\u06d7", "word_translation": "and chooses", "word_transliteration": "wayakhtaru"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "they have", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "for them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u0650\u064a\u064e\u0631\u064e\u0629\u064f\u06da", "word_translation": "the choice", "word_transliteration": "al-khiyaratu"}, {"word_arabic_uthmani": "\u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e", "word_translation": "Glory be", "word_transliteration": "sub'hana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(to) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649\u0670", "word_translation": "and High is He", "word_transliteration": "wata'ala"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "above what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "they associate (with Him)", "word_transliteration": "yush'rikuna"}]}