{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0647\u064f\u06e5 \u0645\u064e\u0642\u064e\u0627\u0644\u0650\u064a\u062f\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6 \u064a\u064e\u0628\u06e1\u0633\u064f\u0637\u064f \u0671\u0644\u0631\u0651\u0650\u0632\u06e1\u0642\u064e \u0644\u0650\u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0648\u064e\u064a\u064e\u0642\u06e1\u062f\u0650\u0631\u064f\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0628\u0650\u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u064d \u0639\u064e\u0644\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "To Him (belongs)", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0642\u064e\u0627\u0644\u0650\u064a\u062f\u064f", "word_translation": "(the) keys", "word_transliteration": "maqalidu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u064a\u064e\u0628\u06e1\u0633\u064f\u0637\u064f", "word_translation": "He extends", "word_transliteration": "yabsutu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u0650\u0632\u06e1\u0642\u064e", "word_translation": "the provision", "word_transliteration": "al-riz'qa"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0646", "word_translation": "for whom", "word_transliteration": "liman"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0642\u06e1\u062f\u0650\u0631\u064f\u06da", "word_translation": "and restricts", "word_transliteration": "wayaqdiru"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064f\u0644\u0651\u0650", "word_translation": "of every", "word_transliteration": "bikulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u064d", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u065e", "word_translation": "(is) All-Knower", "word_transliteration": "alimun"}]}