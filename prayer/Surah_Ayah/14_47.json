{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0645\u064f\u062e\u06e1\u0644\u0650\u0641\u064e \u0648\u064e\u0639\u06e1\u062f\u0650\u0647\u0650\u06e6 \u0631\u064f\u0633\u064f\u0644\u064e\u0647\u064f\u06e5\u0653\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0639\u064e\u0632\u0650\u064a\u0632\u065e \u0630\u064f\u0648 \u0671\u0646\u062a\u0650\u0642\u064e\u0627\u0645\u0656"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "So (do) not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e", "word_translation": "think", "word_transliteration": "tahsabanna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "(that) Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0645\u064f\u062e\u06e1\u0644\u0650\u0641\u064e", "word_translation": "will fail", "word_transliteration": "mukh'lifa"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u06e1\u062f\u0650\u0647\u0650\u06e6", "word_translation": "(to) keep His Promise", "word_transliteration": "wa'dihi"}, {"word_arabic_uthmani": "\u0631\u064f\u0633\u064f\u0644\u064e\u0647\u064f\u06e5\u0653\u06da", "word_translation": "(to) His Messengers", "word_transliteration": "rusulahu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0639\u064e\u0632\u0650\u064a\u0632\u065e", "word_translation": "(is) All-Mighty", "word_transliteration": "azizun"}, {"word_arabic_uthmani": "\u0630\u064f\u0648", "word_translation": "Owner (of) Retribution", "word_transliteration": "dhu"}, {"word_arabic_uthmani": "\u0671\u0646\u062a\u0650\u0642\u064e\u0627\u0645\u0656", "word_translation": "Owner (of) Retribution", "word_transliteration": "intiqamin"}]}