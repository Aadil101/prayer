{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u062f\u064f\u0648\u0651\u065e \u0644\u0651\u0650\u064a\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0631\u064e\u0628\u0651\u064e \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Indeed they", "word_transliteration": "fa-innahum"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064f\u0648\u0651\u065e", "word_translation": "(are) enemies", "word_transliteration": "aduwwun"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u064a\u0653", "word_translation": "to me", "word_transliteration": "li"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e", "word_translation": "(the) Lord", "word_transliteration": "rabba"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the worlds", "word_transliteration": "al-'alamina"}]}