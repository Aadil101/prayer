{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u064a\u06e1\u0633\u064e \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0642\u064e\u0670\u062f\u0650\u0631\u064d \u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u0646 \u064a\u064f\u062d\u06e1\u0640\u06e7\u0650\u064a\u064e \u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "Is not", "word_transliteration": "alaysa"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "[that]", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0642\u064e\u0670\u062f\u0650\u0631\u064d", "word_translation": "(He) Able", "word_transliteration": "biqadirin"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "[over]", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "to", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u06e1\u0640\u06e7\u0650\u064a\u064e", "word_translation": "give life", "word_transliteration": "yuh'yiya"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e\u0649\u0670", "word_translation": "(to) the dead", "word_transliteration": "al-mawta"}]}