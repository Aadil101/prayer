{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u062e\u06e1\u0631\u064e\u062c\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u06e2 \u0628\u064f\u0637\u064f\u0648\u0646\u0650 \u0623\u064f\u0645\u0651\u064e\u0647\u064e\u0670\u062a\u0650\u0643\u064f\u0645\u06e1 \u0644\u064e\u0627 \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e \u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627 \u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e \u0644\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u06e1\u0639\u064e \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064e \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0641\u06e1\u0640\u0654\u0650\u062f\u064e\u0629\u064e \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0643\u064f\u0645\u06e1 \u062a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u06e1\u0631\u064e\u062c\u064e\u0643\u064f\u0645", "word_translation": "brought you forth", "word_transliteration": "akhrajakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e2", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064f\u0637\u064f\u0648\u0646\u0650", "word_translation": "the wombs", "word_transliteration": "butuni"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u064e\u0647\u064e\u0670\u062a\u0650\u0643\u064f\u0645\u06e1", "word_translation": "(of) your mothers", "word_transliteration": "ummahatikum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "knowing", "word_transliteration": "ta'lamuna"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627", "word_translation": "anything", "word_transliteration": "shayan"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e", "word_translation": "and made", "word_transliteration": "waja'ala"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u064f", "word_translation": "for you", "word_transliteration": "lakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u06e1\u0639\u064e", "word_translation": "the hearing", "word_transliteration": "al-sam'a"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064e", "word_translation": "and the sight", "word_transliteration": "wal-absara"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0641\u06e1\u0640\u0654\u0650\u062f\u064e\u0629\u064e", "word_translation": "and the hearts", "word_transliteration": "wal-afidata"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "so that you may", "word_transliteration": "la'allakum"}, {"word_arabic_uthmani": "\u062a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "give thanks", "word_transliteration": "tashkuruna"}]}