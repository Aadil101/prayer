{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u0650\u0628\u064f\u064a\u064f\u0648\u062a\u0650\u0647\u0650\u0645\u06e1 \u0623\u064e\u0628\u06e1\u0648\u064e\u0670\u0628\u0657\u0627 \u0648\u064e\u0633\u064f\u0631\u064f\u0631\u064b\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u064e\u0627 \u064a\u064e\u062a\u0651\u064e\u0643\u0650\u0640\u0654\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0628\u064f\u064a\u064f\u0648\u062a\u0650\u0647\u0650\u0645\u06e1", "word_translation": "And for their houses", "word_transliteration": "walibuyutihim"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0648\u064e\u0670\u0628\u0657\u0627", "word_translation": "doors", "word_transliteration": "abwaban"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064f\u0631\u064f\u0631\u064b\u0627", "word_translation": "and couches", "word_transliteration": "wasururan"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u064e\u0627", "word_translation": "upon which", "word_transliteration": "alayha"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u0651\u064e\u0643\u0650\u0640\u0654\u064f\u0648\u0646\u064e", "word_translation": "they recline", "word_transliteration": "yattakiuna"}]}