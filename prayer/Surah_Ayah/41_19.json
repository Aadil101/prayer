{"ayahs": {"ayah_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e \u064a\u064f\u062d\u06e1\u0634\u064e\u0631\u064f \u0623\u064e\u0639\u06e1\u062f\u064e\u0627\u0653\u0621\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650 \u0641\u064e\u0647\u064f\u0645\u06e1 \u064a\u064f\u0648\u0632\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "And (the) Day", "word_transliteration": "wayawma"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u06e1\u0634\u064e\u0631\u064f", "word_translation": "will be gathered", "word_transliteration": "yuh'sharu"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u062f\u064e\u0627\u0653\u0621\u064f", "word_translation": "(the) enemies", "word_transliteration": "a'dau"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650", "word_translation": "the Fire", "word_transliteration": "al-nari"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "then they", "word_transliteration": "fahum"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0632\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "will be assembled in rows", "word_transliteration": "yuza'una"}]}