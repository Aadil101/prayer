{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0645\u06e1\u062d\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064e\u0627 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0648\u064e\u064a\u064f\u062b\u06e1\u0628\u0650\u062a\u064f\u06d6 \u0648\u064e\u0639\u0650\u0646\u062f\u064e\u0647\u064f\u06e5\u0653 \u0623\u064f\u0645\u0651\u064f \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0645\u06e1\u062d\u064f\u0648\u0627\u0652", "word_translation": "(Is) eliminated", "word_transliteration": "yamhu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(by) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u062b\u06e1\u0628\u0650\u062a\u064f\u06d6", "word_translation": "and confirms", "word_transliteration": "wayuth'bitu"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u0650\u0646\u062f\u064e\u0647\u064f\u06e5\u0653", "word_translation": "and with Him", "word_transliteration": "wa'indahu"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u064f", "word_translation": "(is) the Mother (of)", "word_transliteration": "ummu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "the Book", "word_transliteration": "al-kitabi"}]}