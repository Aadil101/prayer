{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u0650 \u0671\u0644\u06e1\u064a\u064e\u0647\u064f\u0648\u062f\u064f \u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0635\u064e\u0670\u0631\u064e\u0649\u0670 \u0646\u064e\u062d\u06e1\u0646\u064f \u0623\u064e\u0628\u06e1\u0646\u064e\u0640\u0670\u0653\u0624\u064f\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0623\u064e\u062d\u0650\u0628\u0651\u064e\u0640\u0670\u0653\u0624\u064f\u0647\u064f\u06e5\u06da \u0642\u064f\u0644\u06e1 \u0641\u064e\u0644\u0650\u0645\u064e \u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0643\u064f\u0645 \u0628\u0650\u0630\u064f\u0646\u064f\u0648\u0628\u0650\u0643\u064f\u0645\u06d6 \u0628\u064e\u0644\u06e1 \u0623\u064e\u0646\u062a\u064f\u0645 \u0628\u064e\u0634\u064e\u0631\u065e \u0645\u0651\u0650\u0645\u0651\u064e\u0646\u06e1 \u062e\u064e\u0644\u064e\u0642\u064e\u06da \u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f \u0644\u0650\u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0648\u064e\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da \u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0645\u064f\u0644\u06e1\u0643\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u06d6 \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u0671\u0644\u06e1\u0645\u064e\u0635\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u0650", "word_translation": "And said", "word_transliteration": "waqalati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u064a\u064e\u0647\u064f\u0648\u062f\u064f", "word_translation": "the Jews", "word_transliteration": "al-yahudu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0635\u064e\u0670\u0631\u064e\u0649\u0670", "word_translation": "and the Christians", "word_transliteration": "wal-nasara"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "We (are)", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0646\u064e\u0640\u0670\u0653\u0624\u064f\u0627\u0652", "word_translation": "(the) children", "word_transliteration": "abnau"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062d\u0650\u0628\u0651\u064e\u0640\u0670\u0653\u0624\u064f\u0647\u064f\u06e5\u06da", "word_translation": "and His beloved", "word_transliteration": "wa-ahibbauhu"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u0650\u0645\u064e", "word_translation": "Then why", "word_transliteration": "falima"}, {"word_arabic_uthmani": "\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0643\u064f\u0645", "word_translation": "(does He) punish you", "word_transliteration": "yu'adhibukum"}, {"word_arabic_uthmani": "\u0628\u0650\u0630\u064f\u0646\u064f\u0648\u0628\u0650\u0643\u064f\u0645\u06d6", "word_translation": "for your sins", "word_transliteration": "bidhunubikum"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645", "word_translation": "you (are)", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u0628\u064e\u0634\u064e\u0631\u065e", "word_translation": "human beings", "word_transliteration": "basharun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0646\u06e1", "word_translation": "from among (those)", "word_transliteration": "mimman"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064e\u06da", "word_translation": "He created", "word_transliteration": "khalaqa"}, {"word_arabic_uthmani": "\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f", "word_translation": "He forgives", "word_transliteration": "yaghfiru"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0646", "word_translation": "[for] whom", "word_transliteration": "liman"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f", "word_translation": "and punishes", "word_transliteration": "wayu'adhibu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "And for Allah", "word_transliteration": "walillahi"}, {"word_arabic_uthmani": "\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "(is the) dominion", "word_transliteration": "mul'ku"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u06d6", "word_translation": "(is) between them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "and to Him", "word_transliteration": "wa-ilayhi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0635\u0650\u064a\u0631\u064f", "word_translation": "(is) the final return", "word_transliteration": "al-masiru"}]}