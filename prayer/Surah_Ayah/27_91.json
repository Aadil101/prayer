{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653 \u0623\u064f\u0645\u0650\u0631\u06e1\u062a\u064f \u0623\u064e\u0646\u06e1 \u0623\u064e\u0639\u06e1\u0628\u064f\u062f\u064e \u0631\u064e\u0628\u0651\u064e \u0647\u064e\u0670\u0630\u0650\u0647\u0650 \u0671\u0644\u06e1\u0628\u064e\u0644\u06e1\u062f\u064e\u0629\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u062d\u064e\u0631\u0651\u064e\u0645\u064e\u0647\u064e\u0627 \u0648\u064e\u0644\u064e\u0647\u064f\u06e5 \u0643\u064f\u0644\u0651\u064f \u0634\u064e\u064a\u06e1\u0621\u0656\u06d6 \u0648\u064e\u0623\u064f\u0645\u0650\u0631\u06e1\u062a\u064f \u0623\u064e\u0646\u06e1 \u0623\u064e\u0643\u064f\u0648\u0646\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0633\u06e1\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0650\u0631\u06e1\u062a\u064f", "word_translation": "I am commanded", "word_transliteration": "umir'tu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0628\u064f\u062f\u064e", "word_translation": "I worship", "word_transliteration": "a'buda"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e", "word_translation": "(the) Lord", "word_transliteration": "rabba"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u0650\u0647\u0650", "word_translation": "(of) this", "word_transliteration": "hadhihi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0644\u06e1\u062f\u064e\u0629\u0650", "word_translation": "city", "word_transliteration": "al-baldati"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u062d\u064e\u0631\u0651\u064e\u0645\u064e\u0647\u064e\u0627", "word_translation": "made it sacred", "word_transliteration": "harramaha"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u06e5", "word_translation": "and to Him (belongs)", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064f", "word_translation": "all", "word_transliteration": "kullu"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656\u06d6", "word_translation": "things", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0645\u0650\u0631\u06e1\u062a\u064f", "word_translation": "And I am commanded", "word_transliteration": "wa-umir'tu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u064f\u0648\u0646\u064e", "word_translation": "I be", "word_transliteration": "akuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0633\u06e1\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the Muslims", "word_transliteration": "al-mus'limina"}]}