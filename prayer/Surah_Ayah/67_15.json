{"ayahs": {"ayah_uthmani": "\u0647\u064f\u0648\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u062c\u064e\u0639\u064e\u0644\u064e \u0644\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0630\u064e\u0644\u064f\u0648\u0644\u0657\u0627 \u0641\u064e\u0671\u0645\u06e1\u0634\u064f\u0648\u0627\u0652 \u0641\u0650\u064a \u0645\u064e\u0646\u064e\u0627\u0643\u0650\u0628\u0650\u0647\u064e\u0627 \u0648\u064e\u0643\u064f\u0644\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u0631\u0651\u0650\u0632\u06e1\u0642\u0650\u0647\u0650\u06e6\u06d6 \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u0671\u0644\u0646\u0651\u064f\u0634\u064f\u0648\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u064e", "word_translation": "made", "word_transliteration": "ja'ala"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u064f", "word_translation": "for you", "word_transliteration": "lakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "the earth", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0630\u064e\u0644\u064f\u0648\u0644\u0657\u0627", "word_translation": "subservient", "word_transliteration": "dhalulan"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0645\u06e1\u0634\u064f\u0648\u0627\u0652", "word_translation": "so walk", "word_transliteration": "fa-im'shu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u064e\u0627\u0643\u0650\u0628\u0650\u0647\u064e\u0627", "word_translation": "(the) paths thereof", "word_transliteration": "manakibiha"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "and eat", "word_transliteration": "wakulu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u0650\u0632\u06e1\u0642\u0650\u0647\u0650\u06e6\u06d6", "word_translation": "His provision", "word_transliteration": "riz'qihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "and to Him", "word_transliteration": "wa-ilayhi"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064f\u0634\u064f\u0648\u0631\u064f", "word_translation": "(is) the Resurrection", "word_transliteration": "al-nushuru"}]}