{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0621\u064e\u0627\u062e\u064e\u0631\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0644\u064e\u0645\u0651\u064e\u0627 \u064a\u064e\u0644\u06e1\u062d\u064e\u0642\u064f\u0648\u0627\u0652 \u0628\u0650\u0647\u0650\u0645\u06e1\u06da \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0621\u064e\u0627\u062e\u064e\u0631\u0650\u064a\u0646\u064e", "word_translation": "And others", "word_transliteration": "waakharina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "among them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "who have not yet", "word_transliteration": "lamma"}, {"word_arabic_uthmani": "\u064a\u064e\u0644\u06e1\u062d\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "joined", "word_transliteration": "yalhaqu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u0645\u06e1\u06da", "word_translation": "them", "word_transliteration": "bihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f", "word_translation": "(is) the All-Mighty", "word_transliteration": "al-'azizu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f", "word_translation": "the All-Wise", "word_transliteration": "al-hakimu"}]}