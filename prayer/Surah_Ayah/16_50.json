{"ayahs": {"ayah_uthmani": "\u064a\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0646\u064e \u0631\u064e\u0628\u0651\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u0641\u064e\u0648\u06e1\u0642\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0646\u064e \u0645\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u064e\u0631\u064f\u0648\u0646\u064e\u06e9"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0646\u064e", "word_translation": "They fear", "word_transliteration": "yakhafuna"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0647\u064f\u0645", "word_translation": "their Lord", "word_transliteration": "rabbahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "above them", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u06e1\u0642\u0650\u0647\u0650\u0645\u06e1", "word_translation": "above them", "word_transliteration": "fawqihim"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "and they do", "word_transliteration": "wayaf 'aluna"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u064e\u0631\u064f\u0648\u0646\u064e\u06e9", "word_translation": "they are commanded", "word_transliteration": "yu'maruna"}]}