{"ayahs": {"ayah_uthmani": "\u06de\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f \u0623\u064e\u0646\u062a\u064f\u0645\u064f \u0671\u0644\u06e1\u0641\u064f\u0642\u064e\u0631\u064e\u0627\u0653\u0621\u064f \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u063a\u064e\u0646\u0650\u064a\u0651\u064f \u0671\u0644\u06e1\u062d\u064e\u0645\u0650\u064a\u062f\u064f"}, "words": [{"word_arabic_uthmani": "\u06de\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f", "word_translation": "mankind!", "word_transliteration": "al-nasu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645\u064f", "word_translation": "You", "word_transliteration": "antumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064f\u0642\u064e\u0631\u064e\u0627\u0653\u0621\u064f", "word_translation": "(are) those in need", "word_transliteration": "al-fuqarau"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "of", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "while Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u0646\u0650\u064a\u0651\u064f", "word_translation": "(is) Free of need", "word_transliteration": "al-ghaniyu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0645\u0650\u064a\u062f\u064f", "word