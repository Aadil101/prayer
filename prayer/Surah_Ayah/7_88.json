{"ayahs": {"ayah_uthmani": "\u06de\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0623\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6 \u0644\u064e\u0646\u064f\u062e\u06e1\u0631\u0650\u062c\u064e\u0646\u0651\u064e\u0643\u064e \u064a\u064e\u0670\u0634\u064f\u0639\u064e\u064a\u06e1\u0628\u064f \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0645\u064e\u0639\u064e\u0643\u064e \u0645\u0650\u0646 \u0642\u064e\u0631\u06e1\u064a\u064e\u062a\u0650\u0646\u064e\u0627\u0653 \u0623\u064e\u0648\u06e1 \u0644\u064e\u062a\u064e\u0639\u064f\u0648\u062f\u064f\u0646\u0651\u064e \u0641\u0650\u064a \u0645\u0650\u0644\u0651\u064e\u062a\u0650\u0646\u064e\u0627\u06da \u0642\u064e\u0627\u0644\u064e \u0623\u064e\u0648\u064e\u0644\u064e\u0648\u06e1 \u0643\u064f\u0646\u0651\u064e\u0627 \u0643\u064e\u0670\u0631\u0650\u0647\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0642\u064e\u0627\u0644\u064e", "word_translation": "Said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0623\u064f", "word_translation": "the chiefs", "word_transliteration": "al-mala-u"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "were arrogant", "word_transliteration": "is'takbaru"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "among", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6", "word_translation": "his people", "word_transliteration": "qawmihi"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064f\u062e\u06e1\u0631\u0650\u062c\u064e\u0646\u0651\u064e\u0643\u064e", "word_translation": "We will surely drive you out", "word_transliteration": "lanukh'rijannaka"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0634\u064f\u0639\u064e\u064a\u06e1\u0628\u064f", "word_translation": "O Shuaib!", "word_transliteration": "yashu'aybu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "And those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "(have) believed", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e\u0643\u064e", "word_translation": "with you", "word_transliteration": "ma'aka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u06e1\u064a\u064e\u062a\u0650\u0646\u064e\u0627\u0653", "word_translation": "our city", "word_transliteration": "qaryatina"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0644\u064e\u062a\u064e\u0639\u064f\u0648\u062f\u064f\u0646\u0651\u064e", "word_translation": "you must return", "word_transliteration": "lata'udunna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "to", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0645\u0650\u0644\u0651\u064e\u062a\u0650\u0646\u064e\u0627\u06da", "word_translation": "our religion", "word_transliteration": "millatina"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "Even if", "word_transliteration": "awalaw"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "we are", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0643\u064e\u0670\u0631\u0650\u0647\u0650\u064a\u0646\u064e", "word_translation": "(the) ones who hate (it)", "word_transliteration": "karihina"}]}