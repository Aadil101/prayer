{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1 \u062d\u064e\u064a\u06e1\u062b\u064f \u062e\u064e\u0631\u064e\u062c\u06e1\u062a\u064e \u0641\u064e\u0648\u064e\u0644\u0651\u0650 \u0648\u064e\u062c\u06e1\u0647\u064e\u0643\u064e \u0634\u064e\u0637\u06e1\u0631\u064e \u0671\u0644\u06e1\u0645\u064e\u0633\u06e1\u062c\u0650\u062f\u0650 \u0671\u0644\u06e1\u062d\u064e\u0631\u064e\u0627\u0645\u0650\u06d6 \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u0644\u06e1\u062d\u064e\u0642\u0651\u064f \u0645\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064e\u06d7 \u0648\u064e\u0645\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u0650\u063a\u064e\u0670\u0641\u0650\u0644\u064d \u0639\u064e\u0645\u0651\u064e\u0627 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1", "word_translation": "And from", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u062d\u064e\u064a\u06e1\u062b\u064f", "word_translation": "wherever", "word_transliteration": "haythu"}, {"word_arabic_uthmani": "\u062e\u064e\u0631\u064e\u062c\u06e1\u062a\u064e", "word_translation": "you start forth", "word_transliteration": "kharajta"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u064e\u0644\u0651\u0650", "word_translation": "[so] turn", "word_transliteration": "fawalli"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u06e1\u0647\u064e\u0643\u064e", "word_translation": "your face", "word_transliteration": "wajhaka"}, {"word_arabic_uthmani": "\u0634\u064e\u0637\u06e1\u0631\u064e", "word_translation": "(in the) direction", "word_transliteration": "shatra"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0633\u06e1\u062c\u0650\u062f\u0650", "word_translation": "(of) Al-Masjid", "word_transliteration": "al-masjidi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0631\u064e\u0627\u0645\u0650\u06d6", "word_translation": "Al-Haraam", "word_transliteration": "al-harami"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "And indeed, it", "word_transliteration": "wa-innahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0644\u06e1\u062d\u064e\u0642\u0651\u064f", "word_translation": "(is) surely the truth", "word_transliteration": "lalhaqqu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064e\u06d7", "word_translation": "your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u063a\u064e\u0670\u0641\u0650\u0644\u064d", "word_translation": "unaware", "word_transliteration": "bighafilin"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "you do", "word_transliteration": "ta'maluna"}]}