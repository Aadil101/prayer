{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064e\u0642\u06e1\u0636\u0650\u064a \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650\u06d6 \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6 \u0644\u064e\u0627 \u064a\u064e\u0642\u06e1\u0636\u064f\u0648\u0646\u064e \u0628\u0650\u0634\u064e\u064a\u06e1\u0621\u064d\u06d7 \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0647\u064f\u0648\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f \u0671\u0644\u06e1\u0628\u064e\u0635\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u06e1\u0636\u0650\u064a", "word_translation": "judges", "word_transliteration": "yaqdi"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650\u06d6", "word_translation": "in truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "while those (whom)", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e", "word_translation": "they invoke", "word_transliteration": "yad'una"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "besides Him", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6", "word_translation": "besides Him", "word_transliteration": "dunihi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u06e1\u0636\u064f\u0648\u0646\u064e", "word_translation": "they judge", "word_transliteration": "yaqduna"}, {"word_arabic_uthmani": "\u0628\u0650\u0634\u064e\u064a\u06e1\u0621\u064d\u06d7", "word_translation": "with anything", "word_transliteration": "bishayin"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f", "word_translation": "(is) the All-Hearer", "word_transliteration": "al-sami'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0635\u0650\u064a\u0631\u064f", "word_translation": "the All-Seer", "word_transliteration": "al-basiru"}]}