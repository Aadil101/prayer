{"ayahs": {"ayah_uthmani": "\u0642\u064e\u062f\u06e1 \u0633\u064e\u0645\u0650\u0639\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0642\u064e\u0648\u06e1\u0644\u064e \u0671\u0644\u0651\u064e\u062a\u0650\u064a \u062a\u064f\u062c\u064e\u0670\u062f\u0650\u0644\u064f\u0643\u064e \u0641\u0650\u064a \u0632\u064e\u0648\u06e1\u062c\u0650\u0647\u064e\u0627 \u0648\u064e\u062a\u064e\u0634\u06e1\u062a\u064e\u0643\u0650\u064a\u0653 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064e\u0633\u06e1\u0645\u064e\u0639\u064f \u062a\u064e\u062d\u064e\u0627\u0648\u064f\u0631\u064e\u0643\u064f\u0645\u064e\u0627\u0653\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0633\u064e\u0645\u0650\u064a\u0639\u064f\u06e2 \u0628\u064e\u0635\u0650\u064a\u0631\u064c"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "Indeed", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u0633\u064e\u0645\u0650\u0639\u064e", "word_translation": "has heard", "word_transliteration": "sami'a"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0644\u064e", "word_translation": "(the) speech", "word_transliteration": "qawla"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u062a\u0650\u064a", "word_translation": "(of) one who", "word_transliteration": "allati"}, {"word_arabic_uthmani": "\u062a\u064f\u062c\u064e\u0670\u062f\u0650\u0644\u064f\u0643\u064e", "word_translation": "disputes with you", "word_transliteration": "tujadiluka"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "concerning", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0632\u064e\u0648\u06e1\u062c\u0650\u0647\u064e\u0627", "word_translation": "her husband", "word_transliteration": "zawjiha"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0634\u06e1\u062a\u064e\u0643\u0650\u064a\u0653", "word_translation": "and she directs her complaint", "word_transliteration": "watashtaki"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u0645\u064e\u0639\u064f", "word_translation": "hears", "word_transliteration": "yasma'u"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u064e\u0627\u0648\u064f\u0631\u064e\u0643\u064f\u0645\u064e\u0627\u0653\u06da", "word_translation": "(the) dialogue of both of you", "word_transliteration": "tahawurakuma"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0633\u064e\u0645\u0650\u064a\u0639\u064f\u06e2", "word_translation": "(is) All-Hearer", "word_transliteration": "sami'un"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u0650\u064a\u0631\u064c", "word_translation": "All-Seer", "word_transliteration": "basirun"}]}