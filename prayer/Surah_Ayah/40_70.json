{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652 \u0628\u0650\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u0648\u064e\u0628\u0650\u0645\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627 \u0628\u0650\u0647\u0650\u06e6 \u0631\u064f\u0633\u064f\u0644\u064e\u0646\u064e\u0627\u06d6 \u0641\u064e\u0633\u064e\u0648\u06e1\u0641\u064e \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "Those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "deny", "word_transliteration": "kadhabu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "the Book", "word_transliteration": "bil-kitabi"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u0650\u0645\u064e\u0627\u0653", "word_translation": "and with what", "word_transliteration": "wabima"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We sent", "word_transliteration": "arsalna"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0631\u064f\u0633\u064f\u0644\u064e\u0646\u064e\u0627\u06d6", "word_translation": "Our Messengers", "word_transliteration": "rusulana"}, {"word_arabic_uthmani": "\u0641\u064e\u0633\u064e\u0648\u06e1\u0641\u064e", "word_translation": "but soon", "word_transliteration": "fasawfa"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "they will know", "word_transliteration": "ya'lamuna"}]}