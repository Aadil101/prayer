{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0643\u064f\u0644\u064f\u0648\u0627\u0652 \u0645\u0650\u0645\u0651\u064e\u0627 \u0631\u064e\u0632\u064e\u0642\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u062d\u064e\u0644\u064e\u0670\u0644\u0657\u0627 \u0637\u064e\u064a\u0651\u0650\u0628\u0657\u0627\u06da \u0648\u064e\u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653 \u0623\u064e\u0646\u062a\u064f\u0645 \u0628\u0650\u0647\u0650\u06e6 \u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0643\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "And eat", "word_transliteration": "wakulu"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0631\u064e\u0632\u064e\u0642\u064e\u0643\u064f\u0645\u064f", "word_translation": "has provided you", "word_transliteration": "razaqakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u062d\u064e\u0644\u064e\u0670\u0644\u0657\u0627", "word_translation": "lawful", "word_transliteration": "halalan"}, {"word_arabic_uthmani": "\u0637\u064e\u064a\u0651\u0650\u0628\u0657\u0627\u06da", "word_translation": "good", "word_transliteration": "tayyiban"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "And fear", "word_transliteration": "wa-ittaqu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653", "word_translation": "the One", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f