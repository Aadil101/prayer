{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u0621\u064e\u0627\u0645\u064e\u0646\u062a\u064f\u0645 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0641\u064e\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645 \u0645\u0651\u064f\u0633\u06e1\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "And Musa said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "And Musa said", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "O my people", "word_transliteration": "yaqawmi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "If", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you have", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u062a\u064f\u0645", "word_translation": "believed", "word_transliteration": "amantum"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "in Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0641\u064e\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "then on Him", "word_transliteration": "fa'alayhi"}, {"word_arabic_uthmani": "\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "put your trust", "word_transliteration": "tawakkalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645", "word_translation": "you are", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0633\u06e1\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "Muslims", "word_transliteration": "mus'limina"}]}