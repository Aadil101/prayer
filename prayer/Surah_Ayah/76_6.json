{"ayahs": {"ayah_uthmani": "\u0639\u064e\u064a\u06e1\u0646\u0657\u0627 \u064a\u064e\u0634\u06e1\u0631\u064e\u0628\u064f \u0628\u0650\u0647\u064e\u0627 \u0639\u0650\u0628\u064e\u0627\u062f\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u064a\u064f\u0641\u064e\u062c\u0651\u0650\u0631\u064f\u0648\u0646\u064e\u0647\u064e\u0627 \u062a\u064e\u0641\u06e1\u062c\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0639\u064e\u064a\u06e1\u0646\u0657\u0627", "word_translation": "A spring", "word_transliteration": "aynan"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0631\u064e\u0628\u064f", "word_translation": "will drink", "word_transliteration": "yashrabu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u064e\u0627", "word_translation": "from it", "word_transliteration": "biha"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u064f", "word_translation": "(the) slaves", "word_transliteration": "ibadu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u064a\u064f\u0641\u064e\u062c\u0651\u0650\u0631\u064f\u0648\u0646\u064e\u0647\u064e\u0627", "word_translation": "causing it to gush forth", "word_transliteration": "yufajjirunaha"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u06e1\u062c\u0650\u064a\u0631\u0657\u0627", "word_translation": "abundantly", "word_transliteration": "tafjiran"}]}