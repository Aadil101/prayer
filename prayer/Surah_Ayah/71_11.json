{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0631\u06e1\u0633\u0650\u0644\u0650 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645 \u0645\u0651\u0650\u062f\u06e1\u0631\u064e\u0627\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0631\u06e1\u0633\u0650\u0644\u0650", "word_translation": "He will send down", "word_transliteration": "yur'sili"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064e", "word_translation": "(rain from) the sky", "word_transliteration": "al-samaa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645", "word_translation": "upon you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u062f\u06e1\u0631\u064e\u0627\u0631\u0657\u0627", "word_translation": "(in) abundance", "word_transliteration": "mid'raran"}]}