{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0648\u06e1 \u064a\u064f\u0648\u0628\u0650\u0642\u06e1\u0647\u064f\u0646\u0651\u064e \u0628\u0650\u0645\u064e\u0627 \u0643\u064e\u0633\u064e\u0628\u064f\u0648\u0627\u0652 \u0648\u064e\u064a\u064e\u0639\u06e1\u0641\u064f \u0639\u064e\u0646 \u0643\u064e\u062b\u0650\u064a\u0631\u0656"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "Or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0628\u0650\u0642\u06e1\u0647\u064f\u0646\u0651\u064e", "word_translation": "He could destroy them", "word_transliteration": "yubiq'hunna"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "for what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0643\u064e\u0633\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "they have earned", "word_transliteration": "kasabu"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0639\u06e1\u0641\u064f", "word_translation": "but He pardons", "word_transliteration": "waya'fu"}, {"word_arabic_uthmani": "\u0639\u064e\u0646", "word_translation": "[from]", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u0656", "word_translation": "much", "word_transliteration": "kathirin"}]}