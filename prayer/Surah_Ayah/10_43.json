{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645 \u0645\u0651\u064e\u0646 \u064a\u064e\u0646\u0638\u064f\u0631\u064f \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e\u06da \u0623\u064e\u0641\u064e\u0623\u064e\u0646\u062a\u064e \u062a\u064e\u0647\u06e1\u062f\u0650\u064a \u0671\u0644\u06e1\u0639\u064f\u0645\u06e1\u064a\u064e \u0648\u064e\u0644\u064e\u0648\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0644\u064e\u0627 \u064a\u064f\u0628\u06e1\u0635\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645", "word_translation": "And among them", "word_transliteration": "wamin'hum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646", "word_translation": "(are some) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u0638\u064f\u0631\u064f", "word_translation": "look", "word_transliteration": "yanzuru"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e\u06da", "word_translation": "at you", "word_transliteration": "ilayka"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0623\u064e\u0646\u062a\u064e", "word_translation": "But (can) you", "word_transliteration": "afa-anta"}, {"word_arabic_uthmani": "\u062a\u064e\u0647\u06e1\u062f\u0650\u064a", "word_translation": "guide", "word_transliteration": "tahdi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064f\u0645\u06e1\u064a\u064e", "word_translation": "the blind", "word_transliteration": "al-'um'ya"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "even though", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they [were]", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0628\u06e1\u0635\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "see", "word_transliteration": "yub'siruna"}]}