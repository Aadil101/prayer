{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0644\u064e\u0627 \u062a\u064e\u062e\u06e1\u062a\u064e\u0635\u0650\u0645\u064f\u0648\u0627\u0652 \u0644\u064e\u062f\u064e\u064a\u0651\u064e \u0648\u064e\u0642\u064e\u062f\u06e1 \u0642\u064e\u062f\u0651\u064e\u0645\u06e1\u062a\u064f \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064f\u0645 \u0628\u0650\u0671\u0644\u06e1\u0648\u064e\u0639\u0650\u064a\u062f\u0650"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He will say", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u062e\u06e1\u062a\u064e\u0635\u0650\u0645\u064f\u0648\u0627\u0652", "word_translation": "dispute", "word_transliteration": "takhtasimu"}, {"word_arabic_uthmani": "\u0644\u064e\u062f\u064e\u064a\u0651\u064e", "word_translation": "(in) My presence", "word_transliteration": "ladayya"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1", "word_translation": "and indeed", "word_transliteration": "waqad"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0651\u064e\u0645\u06e1\u062a\u064f", "word_translation": "I sent forth", "word_transliteration": "qaddamtu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064f\u0645", "word_translation": "to you", "word_transliteration": "ilaykum"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0648\u064e\u0639\u0650\u064a\u062f\u0650", "word_translation": "the Warning", "word_transliteration": "bil-wa'idi"}]}