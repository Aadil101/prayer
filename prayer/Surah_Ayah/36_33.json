{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0621\u064e\u0627\u064a\u064e\u0629\u065e \u0644\u0651\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064f \u0671\u0644\u06e1\u0645\u064e\u064a\u06e1\u062a\u064e\u0629\u064f \u0623\u064e\u062d\u06e1\u064a\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064e\u0627 \u0648\u064e\u0623\u064e\u062e\u06e1\u0631\u064e\u062c\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u062d\u064e\u0628\u0651\u0657\u0627 \u0641\u064e\u0645\u0650\u0646\u06e1\u0647\u064f \u064a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0621\u064e\u0627\u064a\u064e\u0629\u065e", "word_translation": "And a Sign", "word_transliteration": "waayatun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u0645\u064f", "word_translation": "for them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064f", "word_translation": "(is) the earth", "word_transliteration": "al-ardu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u064a\u06e1\u062a\u064e\u0629\u064f", "word_translation": "dead", "word_transliteration": "al-maytatu"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u06e1\u064a\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064e\u0627", "word_translation": "We give it life", "word_transliteration": "ahyaynaha"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062e\u06e1\u0631\u064e\u062c\u06e1\u0646\u064e\u0627", "word_translation": "and We bring forth", "word_transliteration": "wa-akhrajna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "from it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u062d\u064e\u0628\u0651\u0657\u0627", "word_translation": "grain", "word_transliteration": "habban"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u0650\u0646\u06e1\u0647\u064f", "word_translation": "and from it", "word_transliteration": "famin'hu"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0646\u064e", "word_translation": "they eat", "word_transliteration": "yakuluna"}]}