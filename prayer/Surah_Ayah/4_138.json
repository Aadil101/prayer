{"ayahs": {"ayah_uthmani": "\u0628\u064e\u0634\u0651\u0650\u0631\u0650 \u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e \u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064b\u0627 \u0623\u064e\u0644\u0650\u064a\u0645\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0628\u064e\u0634\u0651\u0650\u0631\u0650", "word_translation": "Give tidings", "word_transliteration": "bashiri"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "(to) the hypocrites", "word_transliteration": "al-munafiqina"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e", "word_translation": "that", "word_transliteration": "bi-anna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064b\u0627", "word_translation": "(is) a punishment", "word_transliteration": "adhaban"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0650\u064a\u0645\u064b\u0627", "word_translation": "painful", "word_transliteration": "aliman"}]}