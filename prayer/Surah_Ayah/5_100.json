{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644 \u0644\u0651\u064e\u0627 \u064a\u064e\u0633\u06e1\u062a\u064e\u0648\u0650\u064a \u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u062b\u064f \u0648\u064e\u0671\u0644\u0637\u0651\u064e\u064a\u0651\u0650\u0628\u064f \u0648\u064e\u0644\u064e\u0648\u06e1 \u0623\u064e\u0639\u06e1\u062c\u064e\u0628\u064e\u0643\u064e \u0643\u064e\u062b\u06e1\u0631\u064e\u0629\u064f \u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u062b\u0650\u06da \u0641\u064e\u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u064a\u064e\u0640\u0670\u0653\u0623\u064f\u0648\u0652\u0644\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0644\u06e1\u0628\u064e\u0670\u0628\u0650 \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0643\u064f\u0645\u06e1 \u062a\u064f\u0641\u06e1\u0644\u0650\u062d\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u0648\u0650\u064a", "word_translation": "(are) equal", "word_transliteration": "yastawi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u062b\u064f", "word_translation": "the evil", "word_transliteration": "al-khabithu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0637\u0651\u064e\u064a\u0651\u0650\u0628\u064f", "word_translation": "and the good", "word_transliteration": "wal-tayibu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "even if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u062c\u064e\u0628\u064e\u0643\u064e", "word_translation": "impresses you", "word_transliteration": "a'jabaka"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u06e1\u0631\u064e\u0629\u064f", "word_translation": "abundance", "word_transliteration": "kathratu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u062b\u0650\u06da", "word_translation": "(of) the evil", "word_transliteration": "al-khabithi"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "So fear", "word_transliteration": "fa-ittaqu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064f\u0648\u0652\u0644\u0650\u064a", "word_translation": "O men", "word_transliteration": "yauli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0644\u06e1\u0628\u064e\u0670\u0628\u0650", "word_translation": "(of) understanding", "word_transliteration": "al-albabi"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "so that you may", "word_transliteration": "la'allakum"}, {"word_arabic_uthmani": "\u062a\u064f\u0641\u06e1\u0644\u0650\u062d\u064f\u0648\u0646\u064e", "word_translation": "be successful", "word_transliteration": "tuf'lihuna"}]}