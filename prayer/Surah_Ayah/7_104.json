{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u064a\u064e\u0670\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064f \u0625\u0650\u0646\u0651\u0650\u064a \u0631\u064e\u0633\u064f\u0648\u0644\u065e \u0645\u0651\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650 \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "And said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064f", "word_translation": "O Firaun", "word_transliteration": "yafir'awnu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed, I am", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u065e", "word_translation": "a Messenger", "word_transliteration": "rasulun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650", "word_translation": "(the) Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the worlds", "word_transliteration": "al-'alamina"}]}