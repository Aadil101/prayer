{"ayahs": {"ayah_uthmani": "\u06de\u0623\u064e\u0644\u064e\u0645\u06e1 \u0623\u064e\u0639\u06e1\u0647\u064e\u062f\u06e1 \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u064a\u064e\u0670\u0628\u064e\u0646\u0650\u064a\u0653 \u0621\u064e\u0627\u062f\u064e\u0645\u064e \u0623\u064e\u0646 \u0644\u0651\u064e\u0627 \u062a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0627\u0652 \u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064e\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u0643\u064f\u0645\u06e1 \u0639\u064e\u062f\u064f\u0648\u0651\u065e \u0645\u0651\u064f\u0628\u0650\u064a\u0646\u065e"}, "words": [{"word_arabic_uthmani": "\u06de\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Did not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0647\u064e\u062f\u06e1", "word_translation": "I enjoin", "word_transliteration": "a'had"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "upon you", "word_transliteration": "ilaykum"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0628\u064e\u0646\u0650\u064a\u0653", "word_translation": "O Children of Adam", "word_transliteration": "yabani"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062f\u064e\u0645\u064e", "word_translation": "O Children of Adam", "word_transliteration": "adama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "That", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0627\u0652", "word_translation": "worship", "word_transliteration": "ta'budu"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064e\u06d6", "word_translation": "the Shaitaan", "word_transliteration": "al-shaytana"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "indeed, he", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(is) for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064f\u0648\u0651\u065e", "word_translation": "an enemy", "word_transliteration": "aduwwun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0628\u0650\u064a\u0646\u065e", "word_translation": "clear", "word_transliteration": "mubinun"}]}