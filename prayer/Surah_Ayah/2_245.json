{"ayahs": {"ayah_uthmani": "\u0645\u0651\u064e\u0646 \u0630\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u064a\u064f\u0642\u06e1\u0631\u0650\u0636\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0642\u064e\u0631\u06e1\u0636\u064b\u0627 \u062d\u064e\u0633\u064e\u0646\u0657\u0627 \u0641\u064e\u064a\u064f\u0636\u064e\u0670\u0639\u0650\u0641\u064e\u0647\u064f\u06e5 \u0644\u064e\u0647\u064f\u06e5\u0653 \u0623\u064e\u0636\u06e1\u0639\u064e\u0627\u0641\u0657\u0627 \u0643\u064e\u062b\u0650\u064a\u0631\u064e\u0629\u0657\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064e\u0642\u06e1\u0628\u0650\u0636\u064f \u0648\u064e\u064a\u064e\u0628\u06e1\u0635\u06dc\u064f\u0637\u064f \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u0651\u064e\u0646", "word_translation": "Who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0630\u064e\u0627", "word_translation": "(is) the one", "word_transliteration": "dha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u064a\u064f\u0642\u06e1\u0631\u0650\u0636\u064f", "word_translation": "will lend", "word_transliteration": "yuq'ridu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "(to) Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u06e1\u0636\u064b\u0627", "word_translation": "a loan", "word_transliteration": "qardan"}, {"word_arabic_uthmani": "\u062d\u064e\u0633\u064e\u0646\u0657\u0627", "word_translation": "good", "word_transliteration": "hasanan"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064f\u0636\u064e\u0670\u0639\u0650\u0641\u064e\u0647\u064f\u06e5", "word_translation": "so (that) He multiplies it", "word_transliteration": "fayuda'ifahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5\u0653", "word_translation": "for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0636\u06e1\u0639\u064e\u0627\u0641\u0657\u0627", "word_translation": "manifolds", "word_transliteration": "ad'afan"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u064e\u0629\u0657\u06da", "word_translation": "many", "word_transliteration": "kathiratan"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u06e1\u0628\u0650\u0636\u064f", "word_translation": "withholds", "word_transliteration": "yaqbidu"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0628\u06e1\u0635\u06dc\u064f\u0637\u064f", "word_translation": "and grants abundance", "word_transliteration": "wayabsutu"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "and to Him", "word_transliteration": "wa-ilayhi"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "you will be returned", "word_transliteration": "tur'ja'una"}]}