{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u064f\u0648\u0646\u064e \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646 \u0644\u0651\u064e\u0627 \u064a\u064e\u0634\u06e1\u0639\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627\u0653", "word_translation": "Beware", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "indeed they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "themselves", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u064f\u0648\u0646\u064e", "word_translation": "(are) the ones who spread corruption", "word_transliteration": "al-muf'siduna"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646", "word_translation": "[and] but", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0639\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "they realize (it)", "word_transliteration": "yash'uruna"}]}