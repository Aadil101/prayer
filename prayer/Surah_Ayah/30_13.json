{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0643\u064f\u0646 \u0644\u0651\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0626\u0650\u0647\u0650\u0645\u06e1 \u0634\u064f\u0641\u064e\u0639\u064e\u0640\u0670\u0653\u0624\u064f\u0627\u0652 \u0648\u064e\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0628\u0650\u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0626\u0650\u0647\u0650\u0645\u06e1 \u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "And not", "word_transliteration": "walam"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0646", "word_translation": "will be", "word_transliteration": "yakun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u0645", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "among", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0626\u0650\u0647\u0650\u0645\u06e1", "word_translation": "theirs partners", "word_transliteration": "shurakaihim"}, {"word_arabic_uthmani": "\u0634\u064f\u0641\u064e\u0639\u064e\u0640\u0670\u0653\u0624\u064f\u0627\u0652", "word_translation": "any intercessors", "word_transliteration": "shufa'au"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "and they will be", "word_transliteration": "wakanu"}, {"word_arabic_uthmani": "\u0628\u0650\u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0626\u0650\u0647\u0650\u0645\u06e1", "word_translation": "in their partners", "word_transliteration": "bishurakaihim"}, {"word_arabic_uthmani": "\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "disbelievers", "word_transliteration": "kafirina"}]}