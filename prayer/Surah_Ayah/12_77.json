{"ayahs": {"ayah_uthmani": "\u06de\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646 \u064a\u064e\u0633\u06e1\u0631\u0650\u0642\u06e1 \u0641\u064e\u0642\u064e\u062f\u06e1 \u0633\u064e\u0631\u064e\u0642\u064e \u0623\u064e\u062e\u065e \u0644\u0651\u064e\u0647\u064f\u06e5 \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u064f\u06da \u0641\u064e\u0623\u064e\u0633\u064e\u0631\u0651\u064e\u0647\u064e\u0627 \u064a\u064f\u0648\u0633\u064f\u0641\u064f \u0641\u0650\u064a \u0646\u064e\u0641\u06e1\u0633\u0650\u0647\u0650\u06e6 \u0648\u064e\u0644\u064e\u0645\u06e1 \u064a\u064f\u0628\u06e1\u062f\u0650\u0647\u064e\u0627 \u0644\u064e\u0647\u064f\u0645\u06e1\u06da \u0642\u064e\u0627\u0644\u064e \u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u0634\u064e\u0631\u0651\u065e \u0645\u0651\u064e\u0643\u064e\u0627\u0646\u0657\u0627\u06d6 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0628\u0650\u0645\u064e\u0627 \u062a\u064e\u0635\u0650\u0641\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "If", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u0631\u0650\u0642\u06e1", "word_translation": "he steals", "word_transliteration": "yasriq"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u06e1", "word_translation": "then verily", "word_transliteration": "faqad"}, {"word_arabic_uthmani": "\u0633\u064e\u0631\u064e\u0642\u064e", "word_translation": "stole", "word_transliteration": "saraqa"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u065e", "word_translation": "a brother", "word_transliteration": "akhun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u06e5", "word_translation": "of his", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f\u06da", "word_translation": "before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0633\u064e\u0631\u0651\u064e\u0647\u064e\u0627", "word_translation": "But Yusuf kept it secret", "word_transliteration": "fa-asarraha"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0633\u064f\u0641\u064f", "word_translation": "But Yusuf kept it secret", "word_transliteration": "yusufu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "within", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0633\u0650\u0647\u0650\u06e6", "word_translation": "himself", "word_transliteration": "nafsihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "and (did) not", "word_transliteration": "walam"}, {"word_arabic_uthmani": "\u064a\u064f\u0628\u06e1\u062f\u0650\u0647\u064e\u0627", "word_translation": "reveal it", "word_transliteration": "yub'diha"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "to them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "You", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u0634\u064e\u0631\u0651\u065e", "word_translation": "(are the) worse", "word_transliteration": "sharrun"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0643\u064e\u0627\u0646\u0657\u0627\u06d6", "word_translation": "(in) position", "word_transliteration": "makanan"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "and Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "knows best", "word_transliteration": "a'lamu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u062a\u064e\u0635\u0650\u0641\u064f\u0648\u0646\u064e", "word_translation": "you describe", "word_transliteration": "tasifuna"}]}