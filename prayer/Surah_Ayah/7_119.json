{"ayahs": {"ayah_uthmani": "\u0641\u064e\u063a\u064f\u0644\u0650\u0628\u064f\u0648\u0627\u0652 \u0647\u064f\u0646\u064e\u0627\u0644\u0650\u0643\u064e \u0648\u064e\u0671\u0646\u0642\u064e\u0644\u064e\u0628\u064f\u0648\u0627\u0652 \u0635\u064e\u0670\u063a\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u063a\u064f\u0644\u0650\u0628\u064f\u0648\u0627\u0652", "word_translation": "So they were defeated", "word_transliteration": "faghulibu"}, {"word_arabic_uthmani": "\u0647\u064f\u0646\u064e\u0627\u0644\u0650\u0643\u064e", "word_translation": "there", "word_transliteration": "hunalika"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0646\u0642\u064e\u0644\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "and returned", "word_transliteration": "wa-inqalabu"}, {"word_arabic_uthmani": "\u0635\u064e\u0670\u063a\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "humiliated", "word_transliteration": "saghirina"}]}