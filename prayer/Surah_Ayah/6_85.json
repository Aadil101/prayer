{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0632\u064e\u0643\u064e\u0631\u0650\u064a\u0651\u064e\u0627 \u0648\u064e\u064a\u064e\u062d\u06e1\u064a\u064e\u0649\u0670 \u0648\u064e\u0639\u0650\u064a\u0633\u064e\u0649\u0670 \u0648\u064e\u0625\u0650\u0644\u06e1\u064a\u064e\u0627\u0633\u064e\u06d6 \u0643\u064f\u0644\u0651\u065e \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0632\u064e\u0643\u064e\u0631\u0650\u064a\u0651\u064e\u0627", "word_translation": "And Zakariya", "word_transliteration": "wazakariyya"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062d\u06e1\u064a\u064e\u0649\u0670", "word_translation": "and Yahya", "word_transliteration": "wayahya"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u0650\u064a\u0633\u064e\u0649\u0670", "word_translation": "and Isa", "word_transliteration": "wa'isa"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u06e1\u064a\u064e\u0627\u0633\u064e\u06d6", "word_translation": "and Ilyas", "word_transliteration": "wa-il'yasa"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u065e", "word_translation": "all were", "word_transliteration": "kullun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u0650\u064a\u0646\u064e", "word_translation": "the righteous", "word_transliteration": "al-salihina"}]}