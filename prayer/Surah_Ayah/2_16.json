{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u0634\u06e1\u062a\u064e\u0631\u064e\u0648\u064f\u0627\u0652 \u0671\u0644\u0636\u0651\u064e\u0644\u064e\u0670\u0644\u064e\u0629\u064e \u0628\u0650\u0671\u0644\u06e1\u0647\u064f\u062f\u064e\u0649\u0670 \u0641\u064e\u0645\u064e\u0627 \u0631\u064e\u0628\u0650\u062d\u064e\u062a \u062a\u0651\u0650\u062c\u064e\u0670\u0631\u064e\u062a\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0645\u064f\u0647\u06e1\u062a\u064e\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(are) the ones who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0671\u0634\u06e1\u062a\u064e\u0631\u064e\u0648\u064f\u0627\u0652", "word_translation": "bought", "word_transliteration": "ish'tarawu"}, {"word_arabic_uthmani": "\u0671\u0644\u0636\u0651\u064e\u0644\u064e\u0670\u0644\u064e\u0629\u064e", "word_translation": "[the] astraying", "word_transliteration": "al-dalalata"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0647\u064f\u062f\u064e\u0649\u0670", "word_translation": "for [the] guidance", "word_transliteration": "bil-huda"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627", "word_translation": "So not", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0650\u062d\u064e\u062a", "word_translation": "profited", "word_transliteration": "rabihat"}, {"word_arabic_uthmani": "\u062a\u0651\u0650\u062c\u064e\u0670\u0631\u064e\u062a\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their commerce", "word_transliteration": "tijaratuhum"}, {"word_arabic_ut