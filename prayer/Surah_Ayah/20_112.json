{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0646 \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u06e1 \u0645\u0650\u0646\u064e \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064e\u0670\u062a\u0650 \u0648\u064e\u0647\u064f\u0648\u064e \u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u065e \u0641\u064e\u0644\u064e\u0627 \u064a\u064e\u062e\u064e\u0627\u0641\u064f \u0638\u064f\u0644\u06e1\u0645\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u0647\u064e\u0636\u06e1\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "But (he) who", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u06e1", "word_translation": "does", "word_transliteration": "ya'mal"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064e\u0670\u062a\u0650", "word_translation": "the righteous deeds", "word_transliteration": "al-salihati"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "while he", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u065e", "word_translation": "(is) a believer", "word_transliteration": "mu'minun"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "then not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u064e\u0627\u0641\u064f", "word_translation": "he will fear", "word_transliteration": "yakhafu"}, {"word_arabic_uthmani": "\u0638\u064f\u0644\u06e1\u0645\u0657\u0627", "word_translation": "injustice", "word_transliteration": "zul'man"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0647\u064e\u0636\u06e1\u0645\u0657\u0627", "word_translation": "deprivation", "word_transliteration": "hadman"}]}