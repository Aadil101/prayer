{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e \u064a\u064e\u0641\u06e1\u0631\u064e\u062d\u064f\u0648\u0646\u064e \u0628\u0650\u0645\u064e\u0627\u0653 \u0623\u064f\u0646\u0632\u0650\u0644\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e\u06d6 \u0648\u064e\u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u064e\u062d\u06e1\u0632\u064e\u0627\u0628\u0650 \u0645\u064e\u0646 \u064a\u064f\u0646\u0643\u0650\u0631\u064f \u0628\u064e\u0639\u06e1\u0636\u064e\u0647\u064f\u06e5\u06da \u0642\u064f\u0644\u06e1 \u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653 \u0623\u064f\u0645\u0650\u0631\u06e1\u062a\u064f \u0623\u064e\u0646\u06e1 \u0623\u064e\u0639\u06e1\u0628\u064f\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0644\u064e\u0627\u0653 \u0623\u064f\u0634\u06e1\u0631\u0650\u0643\u064e \u0628\u0650\u0647\u0650\u06e6\u0653\u06da \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u0623\u064e\u062f\u06e1\u0639\u064f\u0648\u0627\u0652 \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u0645\u064e\u0640\u0654\u064e\u0627\u0628\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "And those (to) whom", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u064f", "word_translation": "We have given them", "word_transliteration": "ataynahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e", "word_translation": "the Book", "word_transliteration": "al-kitaba"}, {"word_arabic_uthmani": "\u064a\u064e\u0641\u06e1\u0631\u064e\u062d\u064f\u0648\u0646\u064e", "word_translation": "rejoice", "word_transliteration": "yafrahuna"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627\u0653", "word_translation": "at what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0623\u064f\u0646\u0632\u0650\u0644\u064e", "word_translation": "has been revealed", "word_transliteration": "unzila"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e\u06d6", "word_translation": "to you", "word_transliteration": "ilayka"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e", "word_translation": "but among", "word_transliteration": "wamina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u062d\u06e1\u0632\u064e\u0627\u0628\u0650", "word_translation": "the groups", "word_transliteration": "al-ahzabi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(those) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064f\u0646\u0643\u0650\u0631\u064f", "word_translation": "deny", "word_transliteration": "yunkiru"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064e\u0647\u064f\u06e5\u06da", "word_translation": "a part of it", "word_transliteration": "ba'dahu"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0650\u0631\u06e1\u062a\u064f", "word_translation": "I have been commanded", "word_transliteration": "umir'tu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0628\u064f\u062f\u064e", "word_translation": "I worship", "word_transliteration": "a'buda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627\u0653", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0623\u064f\u0634\u06e1\u0631\u0650\u0643\u064e", "word_translation": "I associate partners", "word_transliteration": "ush'rika"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6\u0653\u06da", "word_translation": "with Him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "To Him", "word_transliteration": "ilayhi"}, {"word_arabic_uthmani": "\u0623\u064e\u062f\u06e1\u0639\u064f\u0648\u0627\u0652", "word_translation": "I call", "word_transliteration": "ad'u"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "and to Him", "word_transliteration": "wa-ilayhi"}, {"word_arabic_uthmani": "\u0645\u064e\u0640\u0654\u064e\u0627\u0628\u0650", "word_translation": "(is) my return", "word_transliteration": "maabi"}]}