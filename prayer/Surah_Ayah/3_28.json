{"ayahs": {"ayah_uthmani": "\u0644\u0651\u064e\u0627 \u064a\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u0650 \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e \u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e\u06d6 \u0648\u064e\u0645\u064e\u0646 \u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u06e1 \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0641\u064e\u0644\u064e\u064a\u06e1\u0633\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0641\u0650\u064a \u0634\u064e\u064a\u06e1\u0621\u064d \u0625\u0650\u0644\u0651\u064e\u0627\u0653 \u0623\u064e\u0646 \u062a\u064e\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u062a\u064f\u0642\u064e\u0649\u0670\u0629\u0657\u06d7 \u0648\u064e\u064a\u064f\u062d\u064e\u0630\u0651\u0650\u0631\u064f\u0643\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0646\u064e\u0641\u06e1\u0633\u064e\u0647\u064f\u06e5\u06d7 \u0648\u064e\u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u0645\u064e\u0635\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(Let) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u0650", "word_translation": "take", "word_transliteration": "yattakhidhi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "the believers", "word_transliteration": "al-mu'minuna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the disbelievers", "word_transliteration": "al-kafirina"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e", "word_translation": "(as) allies", "word_transliteration": "awliyaa"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "instead of", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e\u06d6", "word_translation": "the believers", "word_transliteration": "al-mu'minina"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "And whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u06e1", "word_translation": "does", "word_transliteration": "yaf 'al"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "then not he (has)", "word_transliteration": "falaysa"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "[in]", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u064d", "word_translation": "anything", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627\u0653", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "you fear", 