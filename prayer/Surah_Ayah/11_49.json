{"ayahs": {"ayah_uthmani": "\u062a\u0650\u0644\u06e1\u0643\u064e \u0645\u0650\u0646\u06e1 \u0623\u064e\u0646\u06e2\u0628\u064e\u0627\u0653\u0621\u0650 \u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650 \u0646\u064f\u0648\u062d\u0650\u064a\u0647\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e\u06d6 \u0645\u064e\u0627 \u0643\u064f\u0646\u062a\u064e \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0647\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064e \u0648\u064e\u0644\u064e\u0627 \u0642\u064e\u0648\u06e1\u0645\u064f\u0643\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650 \u0647\u064e\u0670\u0630\u064e\u0627\u06d6 \u0641\u064e\u0671\u0635\u06e1\u0628\u0650\u0631\u06e1\u06d6 \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u06e1\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064e \u0644\u0650\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u062a\u0650\u0644\u06e1\u0643\u064e", "word_translation": "This", "word_transliteration": "til'ka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "(is) from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e2\u0628\u064e\u0627\u0653\u0621\u0650", "word_translation": "the news", "word_transliteration": "anbai"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650", "word_translation": "(of) the unseen", "word_transliteration": "al-ghaybi"}, {"word_arabic_uthmani": "\u0646\u064f\u0648\u062d\u0650\u064a\u0647\u064e\u0627\u0653", "word_translation": "(which) We reveal", "word_transliteration": "nuhiha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e\u06d6", "word_translation": "to you", "word_transliteration": "ilayka"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064e", "word_translation": "you were", "word_transliteration": "kunta"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0647\u064e\u0627\u0653", "word_translation": "knowing it", "word_transliteration": "ta'lamuha"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "you", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064f\u0643\u064e", "word_translation": "your people", "word_transliteration": "qawmuka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650", "word_translation": "before", "word_transliteration": "qabli"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627\u06d6", "word_translation": "this", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0635\u06e1\u0628\u0650\u0631\u06e1\u06d6", "word_translation": "So be patient", "word_transliteration": "fa-is'bir"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064e", "word_translation": "the end", "word_transliteration": "al-'aqibata"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e", "word_translation": "(is) for the God fearing", "word_transliteration": "lil'muttaqina"}]}