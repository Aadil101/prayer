{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0644\u064e\u0627 \u062a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0627\u0652 \u0631\u064e\u0670\u0639\u0650\u0646\u064e\u0627 \u0648\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0627\u0652 \u0671\u0646\u0638\u064f\u0631\u06e1\u0646\u064e\u0627 \u0648\u064e\u0671\u0633\u06e1\u0645\u064e\u0639\u064f\u0648\u0627\u0652\u06d7 \u0648\u064e\u0644\u0650\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e \u0639\u064e\u0630\u064e\u0627\u0628\u064c \u0623\u064e\u0644\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O you", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe[d]!", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0627\u0652", "word_translation": "say", "word_transliteration": "taqulu"}, {"word_arabic_uthmani": "\u0631\u064e\u0670\u0639\u0650\u0646\u064e\u0627", "word_translation": "Raina", "word_transliteration": "ra'ina"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0627\u0652", "word_translation": "and say", "word_transliteration": "waqulu"}, {"word_arabic_uthmani": "\u0671\u0646\u0638\u064f\u0631\u06e1\u0646\u064e\u0627", "word_translation": "Unzurna", "word_transliteration": "unzur'na"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0633\u06e1\u0645\u064e\u0639\u064f\u0648\u0627\u0652\u06d7", "word_translation": "and listen", "word_transliteration": "wa-is'ma'u"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "And for the disbelievers", "word_transliteration": "walil'kafirina"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064c", "word_translation": "(is) a punishment", "word_transliteration": "adhabun"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0650\u064a\u0645\u065e", "word_translation": "painful", "word_transliteration": "alimun"}]}