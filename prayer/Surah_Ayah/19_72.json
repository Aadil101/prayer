{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u0646\u064f\u0646\u064e\u062c\u0651\u0650\u064a \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u062a\u0651\u064e\u0642\u064e\u0648\u0627\u0652 \u0648\u0651\u064e\u0646\u064e\u0630\u064e\u0631\u064f \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e \u0641\u0650\u064a\u0647\u064e\u0627 \u062c\u0650\u062b\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0646\u064f\u0646\u064e\u062c\u0651\u0650\u064a", "word_translation": "We will deliver", "word_transliteration": "nunajji"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u0642\u064e\u0648\u0627\u0652", "word_translation": "feared (Allah)", "word_transliteration": "ittaqaw"}, {"word_arabic_uthmani": "\u0648\u0651\u064e\u0646\u064e\u0630\u064e\u0631\u064f", "word_translation": "and We will leave", "word_transliteration": "wanadharu"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimina"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u062c\u0650\u062b\u0650\u064a\u0651\u0657\u0627", "word_translation": "bent (on) knees", "word_transliteration": "jithiyyan"}]}