{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646 \u0645\u0651\u0650\u0646\u06e1 \u0623\u064e\u0647\u06e1\u0644\u0650 \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u0625\u0650\u0644\u0651\u064e\u0627 \u0644\u064e\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064e\u0646\u0651\u064e \u0628\u0650\u0647\u0650\u06e6 \u0642\u064e\u0628\u06e1\u0644\u064e \u0645\u064e\u0648\u06e1\u062a\u0650\u0647\u0650\u06e6\u06d6 \u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650 \u064a\u064e\u0643\u064f\u0648\u0646\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0634\u064e\u0647\u0650\u064a\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "And (there is) not", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u0650", "word_translation": "(the) People", "word_transliteration": "ahli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "(of) the Book", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064e\u0646\u0651\u064e", "word_translation": "surely he believes", "word_transliteration": "layu'minanna"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "in him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e", "word_translation": "before", "word_transliteration": "qabla"}, {"word_arabic_uthmani": "\u0645\u064e\u0648\u06e1\u062a\u0650\u0647\u0650\u06e6\u06d6", "word_translation": "his death", "word_transliteration": "mawtihi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "And (on the) Day", "word_transliteration": "wayawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650", "word_translation": "(of) the Resurrection", "word_transliteration": "al-qiyamati"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0648\u0646\u064f", "word_translation": "he will be", "word_transliteration": "yakunu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "against them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0634\u064e\u0647\u0650\u064a\u062f\u0657\u0627", "word_translation": "a witness", "word_transliteration": "shahidan"}]}