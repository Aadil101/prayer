{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u064a\u064e\u0630\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627\u0653 \u0623\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06da \u0647\u064f\u0648\u064e \u0623\u064e\u0647\u06e1\u0644\u064f \u0671\u0644\u062a\u0651\u064e\u0642\u06e1\u0648\u064e\u0649\u0670 \u0648\u064e\u0623\u064e\u0647\u06e1\u0644\u064f \u0671\u0644\u06e1\u0645\u064e\u063a\u06e1\u0641\u0650\u0631\u064e\u0629\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u064a\u064e\u0630\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "will pay heed", "word_transliteration": "yadhkuruna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627\u0653", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064e", "word_translation": "wills", "word_transliteration": "yashaa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06da", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064f", "word_translation": "(is) worthy", "word_transliteration": "ahlu"}, {"word_arabic_uthmani": "\u0671\u0644\u062a\u0651\u064e\u0642\u06e1\u0648\u064e\u0649\u0670", "word_translation": "to be feared", "word_transliteration": "al-taqwa"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0647\u06e1\u0644\u064f", "word_translation": "and worthy", "word_transliteration": "wa-ahlu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u063a\u06e1\u0641\u0650\u0631\u064e\u0629\u0650", "word_translation": "to forgive", "word_transliteration": "al-maghfirati"}]}