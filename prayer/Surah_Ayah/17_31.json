{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0648\u06e1\u0644\u064e\u0670\u062f\u064e\u0643\u064f\u0645\u06e1 \u062e\u064e\u0634\u06e1\u064a\u064e\u0629\u064e \u0625\u0650\u0645\u06e1\u0644\u064e\u0670\u0642\u0656\u06d6 \u0646\u0651\u064e\u062d\u06e1\u0646\u064f \u0646\u064e\u0631\u06e1\u0632\u064f\u0642\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0625\u0650\u064a\u0651\u064e\u0627\u0643\u064f\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e \u0642\u064e\u062a\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0627\u0646\u064e \u062e\u0650\u0637\u06e1\u0640\u0654\u0657\u0627 \u0643\u064e\u0628\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "kill", "word_transliteration": "taqtulu"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u064e\u0670\u062f\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your children", "word_transliteration": "awladakum"}, {"word_arabic_uthmani": "\u062e\u064e\u0634\u06e1\u064a\u064e\u0629\u064e", "word_translation": "(for) fear", "word_transliteration": "khashyata"}, {"word_arabic_uthmani": "\u0625\u0650\u0645\u06e1\u0644\u064e\u0670\u0642\u0656\u06d6", "word_translation": "(of) poverty", "word_transliteration": "im'laqin"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u062d\u06e1\u0646\u064f", "word_translation": "We", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0646\u064e\u0631\u06e1\u0632\u064f\u0642\u064f\u0647\u064f\u0645\u06e1", "word_translation": "(We) provide for them", "word_transliteration": "narzuquhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u064a\u0651\u064e\u0627\u0643\u064f\u0645\u06e1\u06da", "word_translation": "and for you", "word_transliteration": "wa-iyyakum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0642\u064e\u062a\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their killing", "word_transliteration": "qatlahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u062e\u0650\u0637\u06e1\u0640\u0654\u0657\u0627", "word_translation": "a sin", "word_transliteration": "khit'an"}, {"word_arabic_uthmani": "\u0643\u064e\u0628\u0650\u064a\u0631\u0657\u0627", "word_translation": "great", "word_transliteration": "kabiran"}]}