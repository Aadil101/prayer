{"ayahs": {"ayah_uthmani": "\u0643\u064e\u0623\u064e\u0646 \u0644\u0651\u064e\u0645\u06e1 \u064a\u064e\u063a\u06e1\u0646\u064e\u0648\u06e1\u0627\u0652 \u0641\u0650\u064a\u0647\u064e\u0627\u0653\u06d7 \u0623\u064e\u0644\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e \u062b\u064e\u0645\u064f\u0648\u062f\u064e\u0627\u0652 \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0631\u064e\u0628\u0651\u064e\u0647\u064f\u0645\u06e1\u06d7 \u0623\u064e\u0644\u064e\u0627 \u0628\u064f\u0639\u06e1\u062f\u0657\u0627 \u0644\u0651\u0650\u062b\u064e\u0645\u064f\u0648\u062f\u064e"}, "words": [{"word_arabic_uthmani": "\u0643\u064e\u0623\u064e\u0646", "word_translation": "As if", "word_transliteration": "ka-an"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u063a\u06e1\u0646\u064e\u0648\u06e1\u0627\u0652", "word_translation": "they (had) prospered", "word_transliteration": "yaghnaw"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627\u0653\u06d7", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627\u0653", "word_translation": "No doubt", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u062b\u064e\u0645\u064f\u0648\u062f\u064e\u0627\u0652", "word_translation": "Thamud", "word_transliteration": "thamuda"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0647\u064f\u0645\u06e1\u06d7", "word_translation": "(in) their Lord", "word_transliteration": "rabbahum"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627", "word_translation": "so", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0628\u064f\