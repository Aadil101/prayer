{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u0623\u064e\u062a\u064f\u062d\u064e\u0627\u0653\u062c\u0651\u064f\u0648\u0646\u064e\u0646\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0647\u064f\u0648\u064e \u0631\u064e\u0628\u0651\u064f\u0646\u064e\u0627 \u0648\u064e\u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0646\u064e\u0627\u0653 \u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064f\u0646\u064e\u0627 \u0648\u064e\u0644\u064e\u0643\u064f\u0645\u06e1 \u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064f\u0643\u064f\u0645\u06e1 \u0648\u064e\u0646\u064e\u062d\u06e1\u0646\u064f \u0644\u064e\u0647\u064f\u06e5 \u0645\u064f\u062e\u06e1\u0644\u0650\u0635\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064f\u062d\u064e\u0627\u0653\u062c\u0651\u064f\u0648\u0646\u064e\u0646\u064e\u0627", "word_translation": "Do you argue with us", "word_transliteration": "atuhajjunana"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "about", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "while He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0646\u064e\u0627", "word_translation": "(is) our Lord", "word_transliteration": "rabbuna"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1", "word_translation": "and your Lord", "word_transliteration": "warabbukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0646\u064e\u0627\u0653", "word_translation": "And for us", "word_transliteration": "walana"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064f\u0646\u064e\u0627", "word_translation": "(are) our deeds", "word_transliteration": "a'maluna"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "and for you", "word_transliteration": "walakum"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064f\u0643\u064f\u0645\u06e1", "word_translation": "(are) your deeds", "word_transliteration": "a'malukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "and we", "word_transliteration": "wanahnu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "to Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u062e\u06e1\u0644\u0650\u0635\u064f\u0648\u0646\u064e", "word_translation": "(are) sincere", "word_transliteration": "mukh'lisuna"}]}