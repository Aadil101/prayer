{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0650 \u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652 \u0641\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0639\u0650\u0646\u062f\u064e \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f\u0648\u0646\u064e \u0644\u064e\u0647\u064f\u06e5 \u0628\u0650\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650 \u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u0650 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0633\u06e1\u0640\u0654\u064e\u0645\u064f\u0648\u0646\u064e\u06e9"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0650", "word_translation": "But if", "word_transliteration": "fa-ini"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "they are arrogant", "word_transliteration": "is'takbaru"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "then those who", "word_transliteration": "fa-alladhina"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "(are) near", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f\u0648\u0646\u064e", "word_translation": "glorify", "word_transliteration": "yusabbihuna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650", "word_translation": "by night", "word_transliteration": "bi-al-layli"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u0650", "word_translation": "and day", "word_transliteration": "wal-nahari"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "And they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u0640\u0654\u064e\u0645\u064f\u0648\u0646\u064e\u06e9", "word_translation": "tire", "word_transliteration": "yasamuna"}]}