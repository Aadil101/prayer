{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0647\u0650\u064a\u064e \u0639\u064e\u0635\u064e\u0627\u064a\u064e \u0623\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0624\u064f\u0627\u0652 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u064e\u0627 \u0648\u064e\u0623\u064e\u0647\u064f\u0634\u0651\u064f \u0628\u0650\u0647\u064e\u0627 \u0639\u064e\u0644\u064e\u0649\u0670 \u063a\u064e\u0646\u064e\u0645\u0650\u064a \u0648\u064e\u0644\u0650\u064a\u064e \u0641\u0650\u064a\u0647\u064e\u0627 \u0645\u064e\u0640\u0654\u064e\u0627\u0631\u0650\u0628\u064f \u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0647\u0650\u064a\u064e", "word_translation": "It", "word_transliteration": "hiya"}, {"word_arabic_uthmani": "\u0639\u064e\u0635\u064e\u0627\u064a\u064e", "word_translation": "(is) my staff", "word_transliteration": "asaya"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0624\u064f\u0627\u0652", "word_translation": "I lean", "word_transliteration": "atawakka-u"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u064e\u0627", "word_translation": "upon it", "word_transliteration": "alayha"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0647\u064f\u0634\u0651\u064f", "word_translation": "and I bring down leaves", "word_transliteration": "wa-ahushu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u064e\u0627", "word_translation": "with it", "word_transliteration": "biha"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "for", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u063a\u064e\u0646\u064e\u0645\u0650\u064a", "word_translation": "my sheep", "word_transliteration": "ghanami"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u064e", "word_translation": "and for me", "word_transliteration": "waliya"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "in it", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0645\u064e\u0640\u0654\u064e\u0627\u0631\u0650\u0628\u064f", "word_translation": "(are) uses", "word_transliteration": "maaribu"}, {"word_arabic_uthmani": "\u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670", "word_translation": "other", "word_transliteration": "ukh'ra"}]}