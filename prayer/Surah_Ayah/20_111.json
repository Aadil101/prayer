{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0639\u064e\u0646\u064e\u062a\u0650 \u0671\u0644\u06e1\u0648\u064f\u062c\u064f\u0648\u0647\u064f \u0644\u0650\u0644\u06e1\u062d\u064e\u064a\u0651\u0650 \u0671\u0644\u06e1\u0642\u064e\u064a\u0651\u064f\u0648\u0645\u0650\u06d6 \u0648\u064e\u0642\u064e\u062f\u06e1 \u062e\u064e\u0627\u0628\u064e \u0645\u064e\u0646\u06e1 \u062d\u064e\u0645\u064e\u0644\u064e \u0638\u064f\u0644\u06e1\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0639\u064e\u0646\u064e\u062a\u0650", "word_translation": "And (will be) humbled", "word_transliteration": "wa'anati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0648\u064f\u062c\u064f\u0648\u0647\u064f", "word_translation": "the faces", "word_transliteration": "al-wujuhu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u062d\u064e\u064a\u0651\u0650", "word_translation": "before the Ever-Living", "word_transliteration": "lil'hayyi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u064a\u0651\u064f\u0648\u0645\u0650\u06d6", "word_translation": "the Self-Subsisting", "word_transliteration": "al-qayumi"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And verily", "word_transliteration": "waqad"}, {"word_arabic_uthmani": "\u062e\u064e\u0627\u0628\u064e", "word_translation": "will have failed", "word_transliteration": "khaba"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u06e1", "word_translation": "(he) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u062d\u064e\u0645\u064e\u0644\u064e", "word_translation": "carried", "word_transliteration": "hamala"}, {"word_arabic_uthmani": "\u0638\u064f\u0644\u06e1\u0645\u0657\u0627", "word_translation": "wrongdoing", "word_transliteration": "zul'man"}]}