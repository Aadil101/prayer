{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062a\u064e\u0628\u064e\u0627\u0631\u064e\u0643\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0644\u064e\u0647\u064f\u06e5 \u0645\u064f\u0644\u06e1\u0643\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627 \u0648\u064e\u0639\u0650\u0646\u062f\u064e\u0647\u064f\u06e5 \u0639\u0650\u0644\u06e1\u0645\u064f \u0671\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u0650 \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0628\u064e\u0627\u0631\u064e\u0643\u064e", "word_translation": "And blessed is", "word_transliteration": "watabaraka"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "to Whom", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "(belongs the) dominion", "word_transliteration": "mul'ku"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627", "word_translation": "(is) between both of them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u0650\u0646\u062f\u064e\u0647\u064f\u06e5", "word_translation": "and with Him", "word_transliteration": "wa'indahu"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u064f", "word_translation": "(is the) knowledge", "word_transliteration": "il'mu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u0650", "word_translation": "(of) the Hour", "word_transliteration": "al-sa'ati"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "and to Him", "word_transliteration": "wa-ilayhi"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "you will be returned", "word_transliteration": "tur'ja'una"}]}