{"ayahs": {"ayah_uthmani": "\u0645\u064f\u062a\u0651\u064e\u0643\u0650\u0640\u0654\u0650\u064a\u0646\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0633\u064f\u0631\u064f\u0631\u0656 \u0645\u0651\u064e\u0635\u06e1\u0641\u064f\u0648\u0641\u064e\u0629\u0656\u06d6 \u0648\u064e\u0632\u064e\u0648\u0651\u064e\u062c\u06e1\u0646\u064e\u0670\u0647\u064f\u0645 \u0628\u0650\u062d\u064f\u0648\u0631\u064d \u0639\u0650\u064a\u0646\u0656"}, "words": [{"word_arabic_uthmani": "\u0645\u064f\u062a\u0651\u064e\u0643\u0650\u0640\u0654\u0650\u064a\u0646\u064e", "word_translation": "Reclining", "word_transliteration": "muttakiina"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0633\u064f\u0631\u064f\u0631\u0656", "word_translation": "thrones", "word_transliteration": "sururin"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0635\u06e1\u0641\u064f\u0648\u0641\u064e\u0629\u0656\u06d6", "word_translation": "lined up", "word_transliteration": "masfufatin"}, {"word_arabic_uthmani": "\u0648\u064e\u0632\u064e\u0648\u0651\u064e\u062c\u06e1\u0646\u064e\u0670\u0647\u064f\u0645", "word_translation": "and We will marry them", "word_transliteration": "wazawwajnahum"}, {"word_arabic_uthmani": "\u0628\u0650\u062d\u064f\u0648\u0631\u064d", "word_translation": "to fair ones", "word_transliteration": "bihurin"}, {"word_arabic_uthmani": "\u0639\u0650\u064a\u0646\u0656", "word_translation": "with large eyes", "word_transliteration": "inin"}]}