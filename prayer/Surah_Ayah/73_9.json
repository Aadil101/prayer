{"ayahs": {"ayah_uthmani": "\u0631\u0651\u064e\u0628\u0651\u064f \u0671\u0644\u06e1\u0645\u064e\u0634\u06e1\u0631\u0650\u0642\u0650 \u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u063a\u06e1\u0631\u0650\u0628\u0650 \u0644\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0670\u0647\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0647\u064f\u0648\u064e \u0641\u064e\u0671\u062a\u0651\u064e\u062e\u0650\u0630\u06e1\u0647\u064f \u0648\u064e\u0643\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u064f", "word_translation": "(The) Lord", "word_transliteration": "rabbu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0634\u06e1\u0631\u0650\u0642\u0650", "word_translation": "(of) the east", "word_transliteration": "al-mashriqi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u063a\u06e1\u0631\u0650\u0628\u0650", "word_translation": "and the west;", "word_transliteration": "wal-maghribi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627\u0653", "word_translation": "(there is) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064e", "word_translation": "god", "word_transliteration": "ilaha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "Him", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u062a\u0651\u064e\u062e\u0650\u0630\u06e1\u0647\u064f", "word_translation": "so take Him", "word_transliteration": "fa-ittakhidh'hu"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u0650\u064a\u0644\u0657\u0627", "word_translation": "(as) Disposer of Affairs", "word_transliteration": "wakilan"}]}