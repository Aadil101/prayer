{"ayahs": {"ayah_uthmani": "\u0671\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u064a\u064f\u062d\u06e1\u064a\u0650 \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0628\u064e\u0639\u06e1\u062f\u064e \u0645\u064e\u0648\u06e1\u062a\u0650\u0647\u064e\u0627\u06da \u0642\u064e\u062f\u06e1 \u0628\u064e\u064a\u0651\u064e\u0646\u0651\u064e\u0627 \u0644\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u0653\u064a\u064e\u0670\u062a\u0650 \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0643\u064f\u0645\u06e1 \u062a\u064e\u0639\u06e1\u0642\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652", "word_translation": "Know", "word_transliteration": "i'lamu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e", "word_translation": "that", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u06e1\u064a\u0650", "word_translation": "gives life", "word_transliteration": "yuh'yi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "(to) the earth", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u064e", "word_translation": "after", "word_transliteration": "ba'da"}, {"word_arabic_uthmani": "\u0645\u064e\u0648\u06e1\u062a\u0650\u0647\u064e\u0627\u06da", "word_translation": "its death", "word_transliteration": "mawtiha"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "Indeed", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u0651\u064e\u0646\u0651\u064e\u0627", "word_translation": "We have made clear", "word_transliteration": "bayyanna"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u