{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u06e1 \u0647\u0650\u064a\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u062d\u064e\u064a\u064e\u0627\u062a\u064f\u0646\u064e\u0627 \u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627 \u0648\u064e\u0645\u064e\u0627 \u0646\u064e\u062d\u06e1\u0646\u064f \u0628\u0650\u0645\u064e\u0628\u06e1\u0639\u064f\u0648\u062b\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "And they said", "word_transliteration": "waqalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u06e1", "word_translation": "Not", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0647\u0650\u064a\u064e", "word_translation": "it (is)", "word_transliteration": "hiya"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u062d\u064e\u064a\u064e\u0627\u062a\u064f\u0646\u064e\u0627", "word_translation": "our life", "word_transliteration": "hayatuna"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627", "word_translation": "(of) the world", "word_transliteration": "al-dun'ya"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "we", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0628\u06e1\u0639\u064f\u0648\u062b\u0650\u064a\u0646\u064e", "word_translation": "(will be) resurrected", "word_transliteration": "bimab'uthina"}]}