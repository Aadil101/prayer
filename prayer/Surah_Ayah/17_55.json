{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0628\u0650\u0645\u064e\u0646 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d7 \u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0641\u064e\u0636\u0651\u064e\u0644\u06e1\u0646\u064e\u0627 \u0628\u064e\u0639\u06e1\u0636\u064e \u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u0650\u0640\u06e7\u0646\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0628\u064e\u0639\u06e1\u0636\u0656\u06d6 \u0648\u064e\u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0627 \u062f\u064e\u0627\u0648\u064f\u06e5\u062f\u064e \u0632\u064e\u0628\u064f\u0648\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "And your Lord", "word_transliteration": "warabbuka"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "(is) most knowing", "word_transliteration": "a'lamu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0646", "word_translation": "of whoever", "word_transliteration": "biman"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d7", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And verily", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u0651\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We have preferred", "word_transliteration": "faddalna"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064e", "word_translation": "some", "word_transliteration": "ba'da"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u0650\u0640\u06e7\u0646\u064e", "word_translation": "(of) the Prophets", "word_transliteration": "al-nabiyina"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u0656\u06d6", "word_translation": "others", "word_transliteration": "ba'din"}, {"word_arabic_uthmani": "\u0648\u064e\u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "And We gave", "word_transliteration": "waatayna"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0648\u064f\u06e5\u062f\u064e", "word_translation": "Dawood", "word_transliteration": "dawuda"}, {"word_arabic_uthmani": "\u0632\u064e\u0628\u064f\u0648\u0631\u0657\u0627", "word_translation": "Zaboor", "word_transliteration": "zaburan"}]}