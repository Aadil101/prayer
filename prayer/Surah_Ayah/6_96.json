{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0627\u0644\u0650\u0642\u064f \u0671\u0644\u06e1\u0625\u0650\u0635\u06e1\u0628\u064e\u0627\u062d\u0650 \u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u064e \u0633\u064e\u0643\u064e\u0646\u0657\u0627 \u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u064e \u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064e \u062d\u064f\u0633\u06e1\u0628\u064e\u0627\u0646\u0657\u0627\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u062a\u064e\u0642\u06e1\u062f\u0650\u064a\u0631\u064f \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u0650 \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0627\u0644\u0650\u0642\u064f", "word_translation": "(He is the) Cleaver", "word_transliteration": "faliqu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0625\u0650\u0635\u06e1\u0628\u064e\u0627\u062d\u0650", "word_translation": "(of) the daybreak", "word_transliteration": "al-is'bahi"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e", "word_translation": "and He has made", "word_transliteration": "waja'ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u064e", "word_translation": "the night", "word_transliteration": "al-layla"}, {"word_arabic_uthmani": "\u0633\u064e\u0643\u064e\u0646\u0657\u0627", "word_translation": "(for) rest", "word_transliteration": "sakanan"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u064e", "word_translation": "and the sun", "word_transliteration": "wal-shamsa"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064e", "word_translation": "and the moon", "word_transliteration": "wal-qamara"}, {"word_arabic_uthmani": "\u062d\u064f\u0633\u06e1\u0628\u064e\u0627\u0646\u0657\u0627\u06da", "word_translation": "(for) reckoning", "word_transliteration": "hus'banan"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u06e1\u062f\u0650\u064a\u0631\u064f", "word_translation": "(is the) ordaining", "word_transliteration": "taqdiru"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u0650", "word_translation": "(of) the All-Mighty", "word_transliteration": "al-'azizi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u0650", "word_translation": "the All-Knowing", "word_transliteration": "al-'alimi"}]}