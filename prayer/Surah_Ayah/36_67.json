{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1 \u0646\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0644\u064e\u0645\u064e\u0633\u064e\u062e\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u0649\u0670 \u0645\u064e\u0643\u064e\u0627\u0646\u064e\u062a\u0650\u0647\u0650\u0645\u06e1 \u0641\u064e\u0645\u064e\u0627 \u0671\u0633\u06e1\u062a\u064e\u0637\u064e\u0670\u0639\u064f\u0648\u0627\u0652 \u0645\u064f\u0636\u0650\u064a\u0651\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0631\u06e1\u062c\u0650\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0646\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "We willed", "word_transliteration": "nashau"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u064e\u0633\u064e\u062e\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "surely We (would have) transformed them", "word_transliteration": "lamasakhnahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "in", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u064e\u0627\u0646\u064e\u062a\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their places", "word_transliteration": "makanatihim"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627", "word_translation": "then not", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0637\u064e\u0670\u0639\u064f\u0648\u0627\u0652", "word_translation": "they would have been able", "word_transliteration": "is'tata'u"}, {"word_arabic_uthmani": "\u0645\u064f\u0636\u0650\u064a\u0651\u0657\u0627", "word_translation": "to proceed", "word_transliteration": "mudiyyan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u062c\u0650\u0639\u064f\u0648\u0646\u064e", "word_translation": "return", "word_transliteration": "yarji'una"}]}