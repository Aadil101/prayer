{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u0651\u064e\u0627 \u062a\u064e\u0637\u064e\u064a\u0651\u064e\u0631\u06e1\u0646\u064e\u0627 \u0628\u0650\u0643\u064f\u0645\u06e1\u06d6 \u0644\u064e\u0626\u0650\u0646 \u0644\u0651\u064e\u0645\u06e1 \u062a\u064e\u0646\u062a\u064e\u0647\u064f\u0648\u0627\u0652 \u0644\u064e\u0646\u064e\u0631\u06e1\u062c\u064f\u0645\u064e\u0646\u0651\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u064a\u064e\u0645\u064e\u0633\u0651\u064e\u0646\u0651\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u0651\u064e\u0627 \u0639\u064e\u0630\u064e\u0627\u0628\u064c \u0623\u064e\u0644\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u062a\u064e\u0637\u064e\u064a\u0651\u064e\u0631\u06e1\u0646\u064e\u0627", "word_translation": "[we] see an evil omen", "word_transliteration": "tatayyarna"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "from you", "word_transliteration": "bikum"}, {"word_arabic_uthmani": "\u0644\u064e\u0626\u0650\u0646", "word_translation": "If", "word_transliteration": "la-in"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u062a\u064e\u0646\u062a\u064e\u0647\u064f\u0648\u0627\u0652", "word_translation": "you desist", "word_transliteration": "tantahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0631\u06e1\u062c\u064f\u0645\u064e\u0646\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "surely we will stone you", "word_transliteration": "lanarjumannakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u064a\u064e\u0645\u064e\u0633\u0651\u064e\u0646\u0651\u064e\u0643\u064f\u0645", "word_translation": "and surely will touch you", "word_transliteration": "walayamassannakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u0651\u064e\u0627", "word_translation": "from us", "word_transliteration": "minna"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064c", "word_translation": "a punishment", "word_transliteration": "adhabun"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0650\u064a\u0645\u065e", "word_translation": "painful", "word_transliteration": "alimun"}]}