{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0648\u0644\u0650\u062c\u064f \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u064e \u0641\u0650\u064a \u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u0650 \u0648\u064e\u064a\u064f\u0648\u0644\u0650\u062c\u064f \u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u064e \u0641\u0650\u064a \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650 \u0648\u064e\u0633\u064e\u062e\u0651\u064e\u0631\u064e \u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u064e \u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064e\u06d6 \u0643\u064f\u0644\u0651\u065e \u064a\u064e\u062c\u06e1\u0631\u0650\u064a \u0644\u0650\u0623\u064e\u062c\u064e\u0644\u0656 \u0645\u0651\u064f\u0633\u064e\u0645\u0651\u0657\u0649\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1 \u0644\u064e\u0647\u064f \u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u064f\u06da \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6 \u0645\u064e\u0627 \u064a\u064e\u0645\u06e1\u0644\u0650\u0643\u064f\u0648\u0646\u064e \u0645\u0650\u0646 \u0642\u0650\u0637\u06e1\u0645\u0650\u064a\u0631\u064d"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0648\u0644\u0650\u062c\u064f", "word_translation": "He causes to enter", "word_transliteration": "yuliju"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u064e", "word_translation": "the night", "word_transliteration": "al-layla"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in (to)", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u0650", "word_translation": "the day", "word_transliteration": "al-nahari"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0648\u0644\u0650\u062c\u064f", "word_translation": "and He causes to enter", "word_transliteration": "wayuliju"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u064e", "word_translation": "the day", "word_transliteration": "al-nahara"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in (to)", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650", "word_translation": "the night", "word_transliteration": "al-layli"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064e\u062e\u0651\u064e\u0631\u064e", "word_translation": "and He has subjected", "word_transliteration": "wasakhara"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u064e", "word_translation": "the sun", "word_transliteration": "al-shamsa"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064e\u06d6", "word_translation": "and the moon", "word_transliteration": "wal-qamara"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u065e", "word_translation": "each", "word_transliteration": "kullun"}, {"word_arabic_uthmani": "\u064a\u064e\u062c\u06e1\u0631\u0650\u064a", "word_translation": "running", "word_transliteration": "yajri"}, {"word_arabic_uthmani": "\u0644\u0650\u0623\u064e\u062c\u064e\u0644\u0656", "word_translation": "for a term", "word_transliteration": "li-ajalin"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0633\u064e\u0645\u0651\u0657\u0649\u06da", "word_translation": "appointed", "word_transliteration": "musamman"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f", "word_translation": "That (is)", "word_transliteration": "dhalikumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1", "word_translation": "your Lord", "word_transliteration": "rabbukum"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f", "word_translation": "for Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u064f\u06da", "word_translation": "(is) the Dominion", "word_transliteration": "al-mul'ku"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "And those whom", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e", "word_translation": "you invoke", "word_transliteration": "tad'una"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "besides Him", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6", "word_translation": "besides Him", "word_transliteration": "dunihi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u06e1\u0644\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "they possess", "word_transliteration": "yamlikuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "even", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u0650\u0637\u06e1\u0645\u0650\u064a\u0631\u064d", "word_translation": "(as much as) the membrane of a date-seed", "word_transliteration": "qit'mirin"}]}