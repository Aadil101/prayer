{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u064f \u062d\u064e\u0633\u06e1\u0628\u064f\u0643\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0648\u064e\u0645\u064e\u0646\u0650 \u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e\u0643\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u064f", "word_translation": "Prophet", "word_transliteration": "al-nabiyu"}, {"word_arabic_uthmani": "\u062d\u064e\u0633\u06e1\u0628\u064f\u0643\u064e", "word_translation": "Sufficient for you", "word_transliteration": "hasbuka"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646\u0650", "word_translation": "and whoever", "word_transliteration": "wamani"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e\u0643\u064e", "word_translation": "follows you", "word_transliteration": "ittaba'aka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "the believers", "word_transliteration": "al-mu'minina"}]}