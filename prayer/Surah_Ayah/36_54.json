{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u064e \u0644\u064e\u0627 \u062a\u064f\u0638\u06e1\u0644\u064e\u0645\u064f \u0646\u064e\u0641\u06e1\u0633\u065e \u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u062a\u064f\u062c\u06e1\u0632\u064e\u0648\u06e1\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064e\u0627 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "So this Day", "word_transliteration": "fal-yawma"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064f\u0638\u06e1\u0644\u064e\u0645\u064f", "word_translation": "will be wronged", "word_transliteration": "tuz'lamu"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0633\u065e", "word_translation": "a soul", "word_transliteration": "nafsun"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627", "word_translation": "(in) anything", "word_transliteration": "shayan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064f\u062c\u06e1\u0632\u064e\u0648\u06e1\u0646\u064e", "word_translation": "you will be recompensed", "word_transliteration": "tuj'zawna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "(for) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you used (to)", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "do", "word_transliteration": "ta'maluna"}]}