{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645 \u0645\u0651\u064e\u0646 \u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u0650\u0646\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627 \u062d\u064e\u0633\u064e\u0646\u064e\u0629\u0657 \u0648\u064e\u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650 \u062d\u064e\u0633\u064e\u0646\u064e\u0629\u0657 \u0648\u064e\u0642\u0650\u0646\u064e\u0627 \u0639\u064e\u0630\u064e\u0627\u0628\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645", "word_translation": "And from those", "word_transliteration": "wamin'hum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646", "word_translation": "who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "say", "word_transliteration": "yaqulu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653", "word_translation": "Our Lord!", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u0650\u0646\u064e\u0627", "word_translation": "Grant us", "word_transliteration": "atina"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627", "word_translation": "the world", "word_transliteration": "al-dun'ya"}, {"word_arabic_uthmani": "\u062d\u064e\u0633\u064e\u0646\u064e\u0629\u0657", "word_translation": "good", "word_transliteration": "hasanatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u0650\u064a", "word_translation": "and in", "word_transliteration": "wafi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650", "word_translation": "the Hereafter", "word_transliteration": "al-akhirati"}, {"word_arabic_uthmani": "\u062d\u064e\u0633\u064e\u0646\u064e\u0629\u0657", "word_translation": "good", "word_transliteration": "hasanatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u0650\u0646\u064e\u0627", "word_translation": "and save us", "word_transliteration": "waqina"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064e", "word_translation": "(from the) punishment", "word_transliteration": "adhaba"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650", "word_translation": "(of) the Fire", "word_transliteration": "al-nari"}]}