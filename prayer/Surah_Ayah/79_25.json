{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0623\u064e\u062e\u064e\u0630\u064e\u0647\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0646\u064e\u0643\u064e\u0627\u0644\u064e \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064f\u0648\u0644\u064e\u0649\u0670\u0653"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u062e\u064e\u0630\u064e\u0647\u064f", "word_translation": "So seized him", "word_transliteration": "fa-akhadhahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0646\u064e\u0643\u064e\u0627\u0644\u064e", "word_translation": "(with) an exemplary punishment", "word_transliteration": "nakala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650", "word_translation": "(for) the last", "word_transliteration": "al-akhirati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064f\u0648\u0644\u064e\u0649\u0670\u0653", "word_translation": "and the first", "word_transliteration": "wal-ula"}]}