{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0631\u064e\u0628\u0651\u064e\u0643\u064e \u0644\u064e\u0630\u064f\u0648 \u0641\u064e\u0636\u06e1\u0644\u064d \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbaka"}, {"word_arabic_uthmani": "\u0644\u064e\u0630\u064f\u0648", "word_translation": "(is) full of Bounty", "word_transliteration": "ladhu"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u064d", "word_translation": "(is) full of Bounty", "word_transliteration": "fadlin"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "for", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "the mankind", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e", "word_translation": "but", "word_transliteration": "walakinna"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "most of them", "word_transliteration": "aktharahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(are) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "grateful", "word_transliteration": "yashkuruna"}]}