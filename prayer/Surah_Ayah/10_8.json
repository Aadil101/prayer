{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0645\u064e\u0623\u06e1\u0648\u064e\u0649\u0670\u0647\u064f\u0645\u064f \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064f \u0628\u0650\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064e\u0643\u06e1\u0633\u0650\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0645\u064e\u0623\u06e1\u0648\u064e\u0649\u0670\u0647\u064f\u0645\u064f", "word_translation": "their abode", "word_transliteration": "mawahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064f", "word_translation": "(will be) the Fire", "word_transliteration": "al-naru"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "for what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they used (to)", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u06e1\u0633\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "earn", "word_transliteration": "yaksibuna"}]}