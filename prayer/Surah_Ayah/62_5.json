{"ayahs": {"ayah_uthmani": "\u0645\u064e\u062b\u064e\u0644\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u062d\u064f\u0645\u0651\u0650\u0644\u064f\u0648\u0627\u0652 \u0671\u0644\u062a\u0651\u064e\u0648\u06e1\u0631\u064e\u0649\u0670\u0629\u064e \u062b\u064f\u0645\u0651\u064e \u0644\u064e\u0645\u06e1 \u064a\u064e\u062d\u06e1\u0645\u0650\u0644\u064f\u0648\u0647\u064e\u0627 \u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650 \u0671\u0644\u06e1\u062d\u0650\u0645\u064e\u0627\u0631\u0650 \u064a\u064e\u062d\u06e1\u0645\u0650\u0644\u064f \u0623\u064e\u0633\u06e1\u0641\u064e\u0627\u0631\u064e\u06e2\u0627\u06da \u0628\u0650\u0626\u06e1\u0633\u064e \u0645\u064e\u062b\u064e\u0644\u064f \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652 \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0627 \u064a\u064e\u0647\u06e1\u062f\u0650\u064a \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u064e\u062b\u064e\u0644\u064f", "word_translation": "(The) likeness", "word_transliteration": "mathalu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u062d\u064f\u0645\u0651\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "were entrusted", "word_transliteration": "hummilu"}, {"word_arabic_uthmani": "\u0671\u0644\u062a\u0651\u064e\u0648\u06e1\u0631\u064e\u0649\u0670\u0629\u064e", "word_translation": "(with) the Taurat", "word_transliteration": "al-tawrata"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0645\u0650\u0644\u064f\u0648\u0647\u064e\u0627", "word_translation": "they bore it", "word_transliteration": "yahmiluha"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650", "word_translation": "(is) like", "word_transliteration": "kamathali"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u0650\u0645\u064e\u0627\u0631\u0650", "word_translation": "the donkey", "word_transliteration": "al-himari"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0645\u0650\u0644\u064f", "word_translation": "who carries", "word_transliteration": "yahmilu"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u0641\u064e\u0627\u0631\u064e\u06e2\u0627\u06da", "word_translation": "books", "word_transliteration": "asfaran"}, {"word_arabic_uthmani": "\u0628\u0650\u0626\u06e1\u0633\u064e", "word_translation": "Wretched is", "word_transliteration": "bi'sa"}, {"word_arabic_uthmani": "\u0645\u064e\u062b\u064e\u0644\u064f", "word_translation": "(the) example", "word_transliteration": "mathalu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "(of) the people", "word_transliteration": "al-qawmi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "deny", "word_transliteration": "kadhabu"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650", "word_translation": "(the) Signs", "word_transliteration": "biayati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u06