{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0634\u064e\u0647\u0650\u064a\u062f\u064e\u06e2\u0627 \u0628\u064e\u064a\u06e1\u0646\u0650\u064a \u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0643\u064e\u0627\u0646\u064e \u0628\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6 \u062e\u064e\u0628\u0650\u064a\u0631\u064e\u06e2\u0627 \u0628\u064e\u0635\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "Sufficient is", "word_transliteration": "kafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0634\u064e\u0647\u0650\u064a\u062f\u064e\u06e2\u0627", "word_translation": "(as) a witness", "word_transliteration": "shahidan"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u0650\u064a", "word_translation": "between me", "word_transliteration": "bayni"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1\u06da", "word_translation": "and between you", "word_transliteration": "wabaynakum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0628\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6", "word_translation": "of His slaves", "word_transliteration": "bi'ibadihi"}, {"word_arabic_uthmani": "\u062e\u064e\u0628\u0650\u064a\u0631\u064e\u06e2\u0627", "word_translation": "All-Aware", "word_transliteration": "khabiran"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u0650\u064a\u0631\u0657\u0627", "word_translation": "All-Seer", "word_transliteration": "basiran"}]}