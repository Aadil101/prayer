{"ayahs": {"ayah_uthmani": "\u06de\u0645\u0651\u064e\u062b\u064e\u0644\u064f \u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u0650 \u0671\u0644\u0651\u064e\u062a\u0650\u064a \u0648\u064f\u0639\u0650\u062f\u064e \u0671\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u064f\u0648\u0646\u064e\u06d6 \u062a\u064e\u062c\u06e1\u0631\u0650\u064a \u0645\u0650\u0646 \u062a\u064e\u062d\u06e1\u062a\u0650\u0647\u064e\u0627 \u0671\u0644\u06e1\u0623\u064e\u0646\u06e1\u0647\u064e\u0670\u0631\u064f\u06d6 \u0623\u064f\u0643\u064f\u0644\u064f\u0647\u064e\u0627 \u062f\u064e\u0627\u0653\u0626\u0650\u0645\u065e \u0648\u064e\u0638\u0650\u0644\u0651\u064f\u0647\u064e\u0627\u06da \u062a\u0650\u0644\u06e1\u0643\u064e \u0639\u064f\u0642\u06e1\u0628\u064e\u0649 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u062a\u0651\u064e\u0642\u064e\u0648\u0627\u0652\u06da \u0648\u0651\u064e\u0639\u064f\u0642\u06e1\u0628\u064e\u0649 \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u06de\u0645\u0651\u064e\u062b\u064e\u0644\u064f", "word_translation": "The example", "word_transliteration": "mathalu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u0650", "word_translation": "of Paradise", "word_transliteration": "al-janati"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u062a\u0650\u064a", "word_translation": "which", "word_transliteration": "allati"}, {"word_arabic_uthmani": "\u0648\u064f\u0639\u0650\u062f\u064e", "word_translation": "(is) promised", "word_transliteration": "wu'ida"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u064f\u0648\u0646\u064e\u06d6", "word_translation": "(to) the righteous", "word_transliteration": "al-mutaquna"}, {"word_arabic_uthmani": "\u062a\u064e\u062c\u06e1\u0631\u0650\u064a", "word_translation": "flows", "word_transliteration": "tajri"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u06e1\u062a\u0650\u0647\u064e\u0627", "word_translation": "underneath it", "word_transliteration": "tahtiha"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0646\u06e1\u0647\u064e\u0670\u0631\u064f\u06d6", "word_translation": "the rivers", "word_transliteration": "al-anharu"}, {"word_arabic_uthmani": "\u0623\u064f\u0643\u064f\u0644\u064f\u0647\u064e\u0627", "word_translation": "Its food", "word_transliteration": "ukuluha"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0653\u0626\u0650\u0645\u065e", "word_translation": "(is) everlasting", "word_transliteration": "daimun"}, {"word_arabic_uthmani": "\u0648\u064e\u0638\u0650\u0644\u0651\u064f\u0647\u064e\u0627\u06da", "word_translation": "and its shade", "word_transliteration": "wazilluha"}, {"word_arabic_uthmani": "\u062a\u0650\u0644\u06e1\u0643\u064e", "word_translation": "This", "word_transliteration": "til'ka"}, {"word_arabic_uthmani": "\u0639\u064f\u0642\u06e1\u0628\u064e\u0649", "word_translation": "(is the) end", "word_transliteration": "uq'ba"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "of those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u0642\u064e\u0648\u0627\u0652\u06da", "word_translation": "(are) righteous", "word_transliteration": "ittaqaw"}, {"word_arabic_uthmani": "\u0648\u0651\u064e\u0639\u064f\u0642\u06e1\u0628\u064e\u0649", "word_translation": "and (the) end", "word_transliteration": "wa'uq'ba"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(of) the disbelievers", "word_transliteration": "al-kafirina"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064f", "word_translation": "(is) the Fire", "word_transliteration": "al-naru"}]}