{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0627 \u064a\u064f\u0641\u064e\u062a\u0651\u064e\u0631\u064f \u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u0650 \u0645\u064f\u0628\u06e1\u0644\u0650\u0633\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0641\u064e\u062a\u0651\u064e\u0631\u064f", "word_translation": "will it subside", "word_transliteration": "yufattaru"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "anhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650", "word_translation": "in it", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u0645\u064f\u0628\u06e1\u0644\u0650\u0633\u064f\u0648\u0646\u064e", "word_translation": "(will) despair", "word_transliteration": "mub'lisuna"}]}