{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f \u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652 \u0631\u064e\u0628\u0651\u064e\u0643\u064f\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e \u0632\u064e\u0644\u06e1\u0632\u064e\u0644\u064e\u0629\u064e \u0671\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u0650 \u0634\u064e\u064a\u06e1\u0621\u064c \u0639\u064e\u0638\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O (mankind)", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f", "word_translation": "(O) mankind", "word_transliteration": "al-nasu"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "Fear", "word_transliteration": "ittaqu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0643\u064f\u0645\u06e1\u06da", "word_translation": "your Lord", "word_transliteration": "rabbakum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0632\u064e\u0644\u06e1\u0632\u064e\u0644\u064e\u0629\u064e", "word_translation": "(the) convulsion", "word_transliteration": "zalzalata"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u0650", "word_translation": "(of) the Hour", "word_transliteration": "al-sa'ati"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u064c", "word_translation": "(is) a thing", "word_transliteration": "shayon"}, {"word_arabic_uthmani": "\u0639\u064e\u0638\u0650\u064a\u0645\u065e", "word_translation": "great", "word_transliteration": "azimun"}]}