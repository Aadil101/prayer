{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0627 \u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f \u0623\u064e\u0646 \u064a\u064f\u0634\u06e1\u0631\u064e\u0643\u064e \u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f \u0645\u064e\u0627 \u062f\u064f\u0648\u0646\u064e \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0644\u0650\u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da \u0648\u064e\u0645\u064e\u0646 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u06e1 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0641\u064e\u0642\u064e\u062f\u0650 \u0671\u0641\u06e1\u062a\u064e\u0631\u064e\u0649\u0670\u0653 \u0625\u0650\u062b\u06e1\u0645\u064b\u0627 \u0639\u064e\u0638\u0650\u064a\u0645\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f", "word_translation": "forgive", "word_transliteration": "yaghfiru"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u064e\u0643\u064e", "word_translation": "partners be associated", "word_transliteration": "yush'raka"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with Him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f", "word_translation": "but He forgives", "word_transliteration": "wayaghfiru"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "from", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u064e", "word_translation": "other than", "word_transliteration": "duna"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0646", "word_translation": "for whom", "word_transliteration": "liman"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "And whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u06e1", "word_translation": "associates partners", "word_transliteration": "yush'rik"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "with Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u0650", "word_translation": "then surely", "word_transliteration": "faqadi"}, {"word_arabic_uthmani": "\u0671\u0641\u06e1\u062a\u064e\u0631\u064e\u0649\u0670\u0653", "word_translation": "he has fabricated", "word_transliteration": "if'tara"}, {"word_arabic_uthmani": "\u0625\u0650\u062b\u06e1\u0645\u064b\u0627", "word_translation": "a sin", "word_transliteration": "ith'man"}, {"word_arabic_uthmani": "\u0639\u064e\u0638\u0650\u064a\u0645\u064b\u0627", "word_translation": "tremendous", "word_transliteration": "aziman"}]}