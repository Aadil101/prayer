{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644 \u0644\u0651\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u0634\u0651\u064e\u0641\u064e\u0670\u0639\u064e\u0629\u064f \u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627\u06d6 \u0644\u0651\u064e\u0647\u064f\u06e5 \u0645\u064f\u0644\u06e1\u0643\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6 \u062b\u064f\u0645\u0651\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "To Allah (belongs)", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u0641\u064e\u0670\u0639\u064e\u0629\u064f", "word_translation": "the intercession", "word_transliteration": "al-shafa'atu"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627\u06d6", "word_translation": "all", "word_transliteration": "jami'an"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u06e5", "word_translation": "For Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "(is the) dominion", "word_transliteration": "mul'ku"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "to Him", "word_transliteration": "ilayhi"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "you will be returned", "word_transliteration": "tur'ja'una"}]}