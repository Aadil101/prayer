{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0647\u064f\u06e5 \u0645\u064e\u0627 \u0633\u064e\u0643\u064e\u0646\u064e \u0641\u0650\u064a \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650 \u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u0650\u06da \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0647\u064f\u06e5", "word_translation": "And for Him", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "(is) whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0633\u064e\u0643\u064e\u0646\u064e", "word_translation": "dwells", "word_transliteration": "sakana"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650", "word_translation": "the night", "word_transliteration": "al-layli"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u0650\u06da", "word_translation": "and the day", "word_transliteration": "wal-nahari"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f", "word_translation": "(is) All-Hearing", "word_transliteration": "al-sami'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "All-Knowing", "word_transliteration": "al-'alimu"}]}