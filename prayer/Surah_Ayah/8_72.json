word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0650\u06ed", "word_translation": "a people", "word_transliteration": "qawmin"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1", "word_translation": "between you", "word_transliteration": "baynakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645", "word_translation": "and between them", "word_transliteration": "wabaynahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u064a\u062b\u064e\u0670\u0642\u065e\u06d7", "word_translation": "(is) a treaty", "word_transliteration": "mithaqun"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "you do", "word_transliteration": "ta'maluna"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u0650\u064a\u0631\u065e", "word_translation": "(is) All-Seer", "word_transliteration": "basirun"}]}