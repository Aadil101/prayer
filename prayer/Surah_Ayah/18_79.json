{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u0651\u064e\u0627 \u0671\u0644\u0633\u0651\u064e\u0641\u0650\u064a\u0646\u064e\u0629\u064f \u0641\u064e\u0643\u064e\u0627\u0646\u064e\u062a\u06e1 \u0644\u0650\u0645\u064e\u0633\u064e\u0670\u0643\u0650\u064a\u0646\u064e \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e \u0641\u0650\u064a \u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650 \u0641\u064e\u0623\u064e\u0631\u064e\u062f\u062a\u0651\u064f \u0623\u064e\u0646\u06e1 \u0623\u064e\u0639\u0650\u064a\u0628\u064e\u0647\u064e\u0627 \u0648\u064e\u0643\u064e\u0627\u0646\u064e \u0648\u064e\u0631\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645 \u0645\u0651\u064e\u0644\u0650\u0643\u065e \u064a\u064e\u0623\u06e1\u062e\u064f\u0630\u064f \u0643\u064f\u0644\u0651\u064e \u0633\u064e\u0641\u0650\u064a\u0646\u064e\u0629\u064d \u063a\u064e\u0635\u06e1\u0628\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u0651\u064e\u0627", "word_translation": "As for", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0641\u0650\u064a\u0646\u064e\u0629\u064f", "word_translation": "the ship", "word_transliteration": "al-safinatu"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u0627\u0646\u064e\u062a\u06e1", "word_translation": "it was", "word_transliteration": "fakanat"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0633\u064e\u0670\u0643\u0650\u064a\u0646\u064e", "word_translation": "of (the) poor people", "word_transliteration": "limasakina"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "working", "word_transliteration": "ya'maluna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650", "word_translation": "the sea", "word_transliteration": "al-bahri"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0631\u064e\u062f\u062a\u0651\u064f", "word_translation": "So I intended", "word_transliteration": "fa-aradttu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u0650\u064a\u0628\u064e\u0647\u064e\u0627", "word_translation": "I cause defect (in) it", "word_transliteration": "a'ibaha"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "(as there) was", "word_transliteration": "wakana"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645", "word_translation": "after them", "word_transliteration": "waraahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0644\u0650\u0643\u065e", "word_translation": "a king", "word_transliteration": "malikun"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062e\u064f\u0630\u064f", "word_translation": "who seized", "word_transliteration": "yakhudhu"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064e", "word_translation": "every", "word_transliteration": "kulla"}, {"word_arabic_uthmani": "\u0633\u064e\u0641\u0650\u064a\u0646\u064e\u0629\u064d", "word_translation": "ship", "word_transliteration": "safinatin"}, {"word_arabic_uthmani": "\u063a\u064e\u0635\u06e1\u0628\u0657\u0627", "word_translation": "(by) force", "word_transliteration": "ghasban"}]}