{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e \u0623\u064e\u0646 \u064a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650 \u0648\u064e\u0645\u064e\u0627 \u0647\u064f\u0645 \u0628\u0650\u062e\u064e\u0670\u0631\u0650\u062c\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u06e1\u0647\u064e\u0627\u06d6 \u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u065e \u0645\u0651\u064f\u0642\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e", "word_translation": "They will wish", "word_transliteration": "yuriduna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0627\u0652", "word_translation": "they come out", "word_transliteration": "yakhruju"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650", "word_translation": "the Fire", "word_transliteration": "al-nari"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "but not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0647\u064f\u0645", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0628\u0650\u062e\u064e\u0670\u0631\u0650\u062c\u0650\u064a\u0646\u064e", "word_translation": "will come out", "word_transliteration": "bikharijina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627\u06d6", "word_translation": "of it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "And for them", "word_transliteration": "walahum"}, {"word_arabic_uthmani": "\u0639\u064e\u06