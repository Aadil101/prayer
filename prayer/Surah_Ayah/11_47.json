{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0631\u064e\u0628\u0651\u0650 \u0625\u0650\u0646\u0651\u0650\u064a\u0653 \u0623\u064e\u0639\u064f\u0648\u0630\u064f \u0628\u0650\u0643\u064e \u0623\u064e\u0646\u06e1 \u0623\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064e\u0643\u064e \u0645\u064e\u0627 \u0644\u064e\u064a\u06e1\u0633\u064e \u0644\u0650\u064a \u0628\u0650\u0647\u0650\u06e6 \u0639\u0650\u0644\u06e1\u0645\u065e\u06d6 \u0648\u064e\u0625\u0650\u0644\u0651\u064e\u0627 \u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1 \u0644\u0650\u064a \u0648\u064e\u062a\u064e\u0631\u06e1\u062d\u064e\u0645\u06e1\u0646\u0650\u064a\u0653 \u0623\u064e\u0643\u064f\u0646 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u062e\u064e\u0670\u0633\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "O my Lord!", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u064f\u0648\u0630\u064f", "word_translation": "seek refuge", "word_transliteration": "a'udhu"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064e", "word_translation": "in You", "word_transliteration": "bika"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064e\u0643\u064e", "word_translation": "I (should) ask You", "word_transliteration": "asalaka"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "not", "word_transliteration": "laysa"}, {"word_arabic_uthmani": "\u0644\u0650\u064a", "word_translation": "I have", "word_transliteration": "li"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "of it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u065e\u06d6", "word_translation": "knowledge", "word_transliteration": "il'mun"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "And unless", "word_transliteration": "wa-illa"}, {"word_arabic_uthmani": "\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1", "word_translation": "You forgive", "word_transliteration": "taghfir"}, {"word_arabic_uthmani": "\u0644\u0650\u064a", "word_translation": "me", "word_transliteration": "li"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0631\u06e1\u062d\u064e\u0645\u06e1\u0646\u0650\u064a\u0653", "word_translation": "and You have mercy on me", "word_transliteration": "watarhamni"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u064f\u0646", "word_translation": "I will be", "word_transliteration": "akun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "among", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0670\u0633\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the losers", "word_transliteration": "al-khasirina"}]}