{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u0644\u064e\u063a\u064f\u0648\u0627\u0652 \u0645\u0650\u0639\u06e1\u0634\u064e\u0627\u0631\u064e \u0645\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0641\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652 \u0631\u064f\u0633\u064f\u0644\u0650\u064a\u06d6 \u0641\u064e\u0643\u064e\u064a\u06e1\u0641\u064e \u0643\u064e\u0627\u0646\u064e \u0646\u064e\u0643\u0650\u064a\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064e", "word_translation": "And denied", "word_transliteration": "wakadhaba"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "(were) before them", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1", "word_translation": "(were) before them", "word_transliteration": "qablihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u064e\u063a\u064f\u0648\u0627\u0652", "word_translation": "they have attained", "word_transliteration": "balaghu"}, {"word_arabic_uthmani": "\u0645\u0650\u0639\u06e1\u0634\u064e\u0627\u0631\u064e", "word_translation": "a tenth", "word_transliteration": "mi'shara"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "(of) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "We (had) given them", "word_transliteration": "ataynahum"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "But they denied", "word_transliteration": "fakadhabu"}, {"word_arabic_uthmani": "\u0631\u064f\u0633\u064f\u0644\u0650\u064a\u06d6", "word_translation": "My Messengers", "word_transliteration": "rusuli"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "so how", "word_transliteration": "fakayfa"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "was", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0646\u064e\u0643\u0650\u064a\u0631\u0650", "word_translation": "My rejection", "word_transliteration": "nakiri"}]}