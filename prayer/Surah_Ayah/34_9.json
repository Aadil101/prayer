{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652 \u0625\u0650\u0644\u064e\u0649\u0670 \u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e \u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1 \u0648\u064e\u0645\u064e\u0627 \u062e\u064e\u0644\u06e1\u0641\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da \u0625\u0650\u0646 \u0646\u0651\u064e\u0634\u064e\u0623\u06e1 \u0646\u064e\u062e\u06e1\u0633\u0650\u0641\u06e1 \u0628\u0650\u0647\u0650\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0623\u064e\u0648\u06e1 \u0646\u064f\u0633\u06e1\u0642\u0650\u0637\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0643\u0650\u0633\u064e\u0641\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650\u06da \u0625\u0650\u0646\u0651\u064e \u0641\u0650\u064a \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0644\u064e\u0623\u0653\u064a\u064e\u0629\u0657 \u0644\u0651\u0650\u0643\u064f\u0644\u0651\u0650 \u0639\u064e\u0628\u06e1\u062f\u0656 \u0645\u0651\u064f\u0646\u0650\u064a\u0628\u0656"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Then, do not", "word_transliteration": "afalam"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652", "word_translation": "they see", "word_transliteration": "yaraw"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "towards", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e", "word_translation": "(is) before them", "word_transliteration": "bayna"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "(is) before them", "word_transliteration": "aydihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0641\u064e\u0647\u064f\u0645", "word_translation": "(is) behind them", "word_transliteration": "khalfahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650", "word_translation": "the heaven", "word_transliteration": "al-samai"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "If", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0634\u064e\u0623\u06e1", "word_translation": "We will", "word_transliteration": "nasha"}, {"word_arabic_uthmani": "\u0646\u064e\u062e\u06e1\u0633\u0650\u0641\u06e1", "word_translation": "We (could) cause to swallow them", "word_transliteration": "nakhsif"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u0645\u064f", "word_translation": "We (could) cause to swallow them", "word_transliteration": "bihimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "the earth", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0646\u064f\u0633\u06e1\u0642\u0650\u0637\u06e1", "word_translation": "cause to fall", "word_transliteration": "nus'qit"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "upon them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0643\u0650\u0633\u064e\u0641\u0657\u0627", "word_translation": "fragments", "word_transliteration": "kisafan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650\u06da", "word_translation": "the sky", "word_transliteration": "al-samai"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u0653\u064a\u064e\u0629\u0657", "word_translation": "surely, is a Sign", "word_transliteration": "laayatan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0643\u064f\u0644\u0651\u0650", "word_translation": "for every", "word_transliteration": "likulli"}, {"word_arabic_uthmani": "\u0639\u064e\u0628\u06e1\u062f\u0656", "word_translation": "slave", "word_transliteration": "abdin"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0646\u0650\u064a\u0628\u0656", "word_translation": "who turns (to Allah)", "word_transliteration": "munibin"}]}