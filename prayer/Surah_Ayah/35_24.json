{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0628\u064e\u0634\u0650\u064a\u0631\u0657\u0627 \u0648\u064e\u0646\u064e\u0630\u0650\u064a\u0631\u0657\u0627\u06da \u0648\u064e\u0625\u0650\u0646 \u0645\u0651\u0650\u0646\u06e1 \u0623\u064f\u0645\u0651\u064e\u0629\u064d \u0625\u0650\u0644\u0651\u064e\u0627 \u062e\u064e\u0644\u064e\u0627 \u0641\u0650\u064a\u0647\u064e\u0627 \u0646\u064e\u0630\u0650\u064a\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653", "word_translation": "Indeed We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e", "word_translation": "[We] have sent you", "word_transliteration": "arsalnaka"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "with the truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u0628\u064e\u0634\u0650\u064a\u0631\u0657\u0627", "word_translation": "(as) a bearer of glad tidings", "word_transliteration": "bashiran"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u0630\u0650\u064a\u0631\u0657\u0627\u06da", "word_translation": "and (as) a warner", "word_transliteration": "wanadhiran"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "And not", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "(was) any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u064e\u0629\u064d", "word_translation": "nation", "word_transliteration": "ummatin"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0627", "word_translation": "had passed", "word_transliteration": "khala"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "within it", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0646\u064e\u0630\u0650\u064a\u0631\u065e", "word_translation": "a warner", "word_transliteration": "nadhirun"}]}