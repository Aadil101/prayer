{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0637\u064e\u0648\u0651\u064e\u0639\u064e\u062a\u06e1 \u0644\u064e\u0647\u064f\u06e5 \u0646\u064e\u0641\u06e1\u0633\u064f\u0647\u064f\u06e5 \u0642\u064e\u062a\u06e1\u0644\u064e \u0623\u064e\u062e\u0650\u064a\u0647\u0650 \u0641\u064e\u0642\u064e\u062a\u064e\u0644\u064e\u0647\u064f\u06e5 \u0641\u064e\u0623\u064e\u0635\u06e1\u0628\u064e\u062d\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u062e\u064e\u0670\u0633\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0637\u064e\u0648\u0651\u064e\u0639\u064e\u062a\u06e1", "word_translation": "Then prompted", "word_transliteration": "fatawwa'at"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "to him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0633\u064f\u0647\u064f\u06e5", "word_translation": "his soul", "word_transliteration": "nafsuhu"}, {"word_arabic_uthmani": "\u0642\u064e\u062a\u06e1\u0644\u064e", "word_translation": "(to) kill", "word_transliteration": "qatla"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u0650\u064a\u0647\u0650", "word_translation": "his brother", "word_transliteration": "akhihi"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062a\u064e\u0644\u064e\u0647\u064f\u06e5", "word_translation": "so he killed him", "word_transliteration": "faqatalahu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0635\u06e1\u0628\u064e\u062d\u064e", "word_translation": "and became", "word_transliteration": "fa-asbaha"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0670\u0633\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the losers", "word_transliteration": "al-khasirina"}]}