{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1 \u062e\u064e\u0670\u0644\u0650\u0642\u064f \u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0644\u0651\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0670\u0647\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0647\u064f\u0648\u064e\u06d6 \u0641\u064e\u0623\u064e\u0646\u0651\u064e\u0649\u0670 \u062a\u064f\u0624\u06e1\u0641\u064e\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f", "word_translation": "That", "word_transliteration": "dhalikumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1", "word_translation": "your Lord", "word_transliteration": "rabbukum"}, {"word_arabic_uthmani": "\u062e\u064e\u0670\u0644\u0650\u0642\u064f", "word_translation": "(the) Creator", "word_transliteration": "khaliqu"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "(of) all", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "things", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627\u0653", "word_translation": "(there is) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064e", "word_translation": "god", "word_transliteration": "ilaha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e\u06d6", "word_translation": "Him", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0646\u0651\u064e\u0649\u0670", "word_translation": "So how", "word_transliteration": "fa-anna"}, {"word_arabic_uthmani": "\u062a\u064f\u0624\u06e1\u0641\u064e\u0643\u064f\u0648\u0646\u064e", "word_translation": "are you deluded", "word_transliteration": "tu'fakuna"}]}