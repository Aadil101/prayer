{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0648\u064e\u0635\u0651\u064e\u064a\u06e1\u0646\u064e\u0627 \u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e\u0670\u0646\u064e \u0628\u0650\u0648\u064e\u0670\u0644\u0650\u062f\u064e\u064a\u06e1\u0647\u0650 \u062d\u064e\u0645\u064e\u0644\u064e\u062a\u06e1\u0647\u064f \u0623\u064f\u0645\u0651\u064f\u0647\u064f\u06e5 \u0648\u064e\u0647\u06e1\u0646\u064b\u0627 \u0639\u064e\u0644\u064e\u0649\u0670 \u0648\u064e\u0647\u06e1\u0646\u0656 \u0648\u064e\u0641\u0650\u0635\u064e\u0670\u0644\u064f\u0647\u064f\u06e5 \u0641\u0650\u064a \u0639\u064e\u0627\u0645\u064e\u064a\u06e1\u0646\u0650 \u0623\u064e\u0646\u0650 \u0671\u0634\u06e1\u0643\u064f\u0631\u06e1 \u0644\u0650\u064a \u0648\u064e\u0644\u0650\u0648\u064e\u0670\u0644\u0650\u062f\u064e\u064a\u06e1\u0643\u064e \u0625\u0650\u0644\u064e\u064a\u0651\u064e \u0671\u0644\u06e1\u0645\u064e\u0635\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0648\u064e\u0635\u0651\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "And We have enjoined", "word_transliteration": "wawassayna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e\u0670\u0646\u064e", "word_translation": "(upon) man", "word_transliteration": "al-insana"}, {"word_arabic_uthmani": "\u0628\u0650\u0648\u064e\u0670\u0644\u0650\u062f\u064e\u064a\u06e1\u0647\u0650", "word_translation": "for his parents", "word_transliteration": "biwalidayhi"}, {"word_arabic_uthmani": "\u062d\u064e\u0645\u064e\u0644\u064e\u062a\u06e1\u0647\u064f", "word_translation": "carried him", "word_transliteration": "hamalathu"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u064f\u0647\u064f\u06e5", "word_translation": "his mother", "word_transliteration": "ummuhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u06e1\u0646\u064b\u0627", "word_translation": "(in) weakness", "word_transliteration": "wahnan"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "upon", "word_tran