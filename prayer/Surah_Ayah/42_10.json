{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0671\u062e\u06e1\u062a\u064e\u0644\u064e\u0641\u06e1\u062a\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u0650 \u0645\u0650\u0646 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0641\u064e\u062d\u064f\u0643\u06e1\u0645\u064f\u0647\u064f\u06e5\u0653 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0631\u064e\u0628\u0651\u0650\u064a \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u06e1\u062a\u064f \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u0623\u064f\u0646\u0650\u064a\u0628\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0671\u062e\u06e1\u062a\u064e\u0644\u064e\u0641\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "you differ", "word_transliteration": "ikh'talaftum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650", "word_translation": "in it", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "a thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0641\u064e\u062d\u064f\u0643\u06e1\u0645\u064f\u0647\u064f\u06e5\u0653", "word_translation": "then its ruling", "word_transliteration": "fahuk'muhu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "(is) to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f", "word_translation": "That", "word_transliteration": "dhalikumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a", "word_translation": "my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "upon Him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u06e1\u062a\u064f", "word_translation": "I put my trust", "word_transliteration": "tawakkaltu"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "and to Him", "word_transliteration": "wa-ilayhi"}, {"word_arabic_uthmani": "\u0623\u064f\u0646\u0650\u064a\u0628\u064f", "word_translation": "I turn", "word_transliteration": "unibu"}]}