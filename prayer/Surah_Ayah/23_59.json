{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0647\u064f\u0645 \u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "And those", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0647\u064f\u0645", "word_translation": "[they]", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1", "word_translation": "with their Lord", "word_transliteration": "birabbihim"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "associate partners", "word_transliteration": "yush'rikuna"}]}