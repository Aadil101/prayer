{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1 \u0642\u064e\u0627\u0644\u064e \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0644\u0650\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6 \u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0644\u0650\u0645\u064e \u062a\u064f\u0624\u06e1\u0630\u064f\u0648\u0646\u064e\u0646\u0650\u064a \u0648\u064e\u0642\u064e\u062f \u062a\u0651\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e \u0623\u064e\u0646\u0651\u0650\u064a \u0631\u064e\u0633\u064f\u0648\u0644\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1\u06d6 \u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u0632\u064e\u0627\u063a\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0632\u064e\u0627\u063a\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0642\u064f\u0644\u064f\u0648\u0628\u064e\u0647\u064f\u0645\u06e1\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0627 \u064a\u064e\u0647\u06e1\u062f\u0650\u064a \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1", "word_translation": "And when", "word_transliteration": "wa-idh"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0644\u0650\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6", "word_translation": "to his people", "word_transliteration": "liqawmihi"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "O my people", "word_transliteration": "yaqawmi"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e", "word_translation": "Why", "word_transliteration": "lima"}, {"word_arabic_uthmani": "\u062a\u064f\u0624\u06e1\u0630\u064f\u0648\u0646\u064e\u0646\u0650\u064a", "word_translation": "do you hurt me", "word_transliteration": "tu'dhunani"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f", "word_translation": "while certainly", "word_transliteration": "waqad"}, {"word_arabic_uthmani": "\u062a\u0651\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "you know", "word_transliteration": "ta'lamuna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u0650\u064a", "word_translation": "that I am", "word_transliteration": "anni"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u064f", "word_translation": "(the) Messenger", "word_transliteration": "rasulu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "to you", "word_transliteration": "ilaykum"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "Then when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u0632\u064e\u0627\u063a\u064f\u0648\u0653\u0627\u0652", "word_translation": "they deviated", "word_transliteration": "zaghu"}, {"word_arabic_uthmani": "\u0623\u064e\u0632\u064e\u0627\u063a\u064e", "word_translation": "(was caused to) deviate", "word_transliteration": "azagha"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(by) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "their hearts", "word_transliteration": "qulubahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062f\u0650\u064a", "word_translation": "guide", "word_transliteration": "yahdi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064e", "word_translation": "the people", "word_transliteration": "al-qawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "the defiantly disobedient", "word_transliteration": "al-fasiqina"}]}