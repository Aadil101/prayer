{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0630\u06e1 \u0642\u064e\u0627\u0644\u064e \u064a\u064f\u0648\u0633\u064f\u0641\u064f \u0644\u0650\u0623\u064e\u0628\u0650\u064a\u0647\u0650 \u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0628\u064e\u062a\u0650 \u0625\u0650\u0646\u0651\u0650\u064a \u0631\u064e\u0623\u064e\u064a\u06e1\u062a\u064f \u0623\u064e\u062d\u064e\u062f\u064e \u0639\u064e\u0634\u064e\u0631\u064e \u0643\u064e\u0648\u06e1\u0643\u064e\u0628\u0657\u0627 \u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u064e \u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064e \u0631\u064e\u0623\u064e\u064a\u06e1\u062a\u064f\u0647\u064f\u0645\u06e1 \u0644\u0650\u064a \u0633\u064e\u0670\u062c\u0650\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "When", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0633\u064f\u0641\u064f", "word_translation": "Yusuf", "word_transliteration": "yusufu"}, {"word_arabic_uthmani": "\u0644\u0650\u0623\u064e\u0628\u0650\u064a\u0647\u0650", "word_translation": "to his father", "word_transliteration": "li-abihi"}, {"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0628\u064e\u062a\u0650", "word_translation": "O my father", "word_transliteration": "yaabati"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed, I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0631\u064e\u0623\u064e\u064a\u06e1\u062a\u064f", "word_translation": "I saw", "word_transliteration": "ra-aytu"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u064e", "word_translation": "eleven", "word_transliteration": "ahada"}, {"word_arabic_uthmani": "\u0639\u064e\u0634\u064e\u0631\u064e", "word_translation": "eleven", "word_transliteration": "ashara"}, {"word_arabic_uthmani": "\u0643\u064e\u0648\u06e1\u0643\u064e\u0628\u0657\u0627", "word_translation": "star(s)", "word_transliteration": "kawkaban"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u064e", "word_translation": "and the sun", "word_transliteration": "wal-shamsa"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064e", "word_translation": "and the moon", "word_transliteration": "wal-qamara"}, {"word_arabic_uthmani": "\u0631\u064e\u0623\u064e\u064a\u06e1\u062a\u064f\u0647\u064f\u0645\u06e1", "word_translation": "I saw them", "word_transliteration": "ra-aytuhum"}, {"word_arabic_uthmani": "\u0644\u0650\u064a", "word_translation": "to me", "word_transliteration": "li"}, {"word_arabic_uthmani": "\u0633\u064e\u0670\u062c\u0650\u062f\u0650\u064a\u0646\u064e", "word_translation": "prostrating", "word_transliteration": "sajidina"}]}