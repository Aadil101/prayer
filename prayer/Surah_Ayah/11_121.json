{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064f\u0644 \u0644\u0651\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0644\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u0671\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u0649\u0670 \u0645\u064e\u0643\u064e\u0627\u0646\u064e\u062a\u0650\u0643\u064f\u0645\u06e1 \u0625\u0650\u0646\u0651\u064e\u0627 \u0639\u064e\u0670\u0645\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064f\u0644", "word_translation": "And say", "word_transliteration": "waqul"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "to those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "believe", "word_transliteration": "yu'minuna"}, {"word_arabic_uthmani": "\u0671\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "Work", "word_transliteration": "i'malu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "(according) to", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u064e\u0627\u0646\u064e\u062a\u0650\u0643\u064f\u0645\u06e1", "word_translation": "your position", "word_transliteration": "makanatikum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "indeed we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0645\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "(are also) working", "word_transliteration": "amiluna"}]}