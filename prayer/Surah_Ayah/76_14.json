{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062f\u064e\u0627\u0646\u0650\u064a\u064e\u0629\u064b \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0638\u0650\u0644\u064e\u0670\u0644\u064f\u0647\u064e\u0627 \u0648\u064e\u0630\u064f\u0644\u0651\u0650\u0644\u064e\u062a\u06e1 \u0642\u064f\u0637\u064f\u0648\u0641\u064f\u0647\u064e\u0627 \u062a\u064e\u0630\u06e1\u0644\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062f\u064e\u0627\u0646\u0650\u064a\u064e\u0629\u064b", "word_translation": "And near", "word_transliteration": "wadaniyatan"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "above them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0638\u0650\u0644\u064e\u0670\u0644\u064f\u0647\u064e\u0627", "word_translation": "(are) its shades", "word_transliteration": "zilaluha"}, {"word_arabic_uthmani": "\u0648\u064e\u0630\u064f\u0644\u0651\u0650\u0644\u064e\u062a\u06e1", "word_translation": "and will hang low", "word_transliteration": "wadhullilat"}, {"word_arabic_uthmani": "\u0642\u064f\u0637\u064f\u0648\u0641\u064f\u0647\u064e\u0627", "word_translation": "its cluster of fruits", "word_transliteration": "qutufuha"}, {"word_arabic_uthmani": "\u062a\u064e\u0630\u06e1\u0644\u0650\u064a\u0644\u0657\u0627", "word_translation": "very low", "word_transliteration": "tadhlilan"}]}