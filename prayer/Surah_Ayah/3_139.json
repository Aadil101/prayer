{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0647\u0650\u0646\u064f\u0648\u0627\u0652 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u062d\u06e1\u0632\u064e\u0646\u064f\u0648\u0627\u0652 \u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0644\u064e\u0648\u06e1\u0646\u064e \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645 \u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0647\u0650\u0646\u064f\u0648\u0627\u0652", "word_translation": "weaken", "word_transliteration": "tahinu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u06e1\u0632\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "grieve", "word_transliteration": "tahzanu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u064f", "word_translation": "and you (will be)", "word_transliteration": "wa-antumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0644\u064e\u0648\u06e1\u0646\u064e", "word_translation": "[the] superior", "word_transliteration": "al-a'lawna"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645", "word_translation": "you are", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "believers", "word_transliteration": "mu'minina"}]}