{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0646\u064f\u0645\u064e\u0643\u0651\u0650\u0646\u064e \u0644\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0646\u064f\u0631\u0650\u064a\u064e \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0648\u064e\u0647\u064e\u0670\u0645\u064e\u0670\u0646\u064e \u0648\u064e\u062c\u064f\u0646\u064f\u0648\u062f\u064e\u0647\u064f\u0645\u064e\u0627 \u0645\u0650\u0646\u06e1\u0647\u064f\u0645 \u0645\u0651\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064e\u062d\u06e1\u0630\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0646\u064f\u0645\u064e\u0643\u0651\u0650\u0646\u064e", "word_translation": "And [We] establish", "word_transliteration": "wanumakkina"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the land", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064f\u0631\u0650\u064a\u064e", "word_translation": "and show", "word_transliteration": "wanuriya"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "Firaun", "word_transliteration": "fir'awna"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064e\u0670\u0645\u064e\u0670\u0646\u064e", "word_translation": "and Haman", "word_transliteration": "wahamana"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064f\u0646\u064f\u0648\u062f\u064e\u0647\u064f\u0645\u064e\u0627", "word_translation": "and their hosts", "word_transliteration": "wajunudahuma"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645", "word_translation": "through them", "word_transliteration": "min'h