{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0645\u064f\u0644\u06e1\u0643\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da \u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f \u0644\u0650\u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0648\u064e\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da \u0648\u064e\u0643\u064e\u0627\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u063a\u064e\u0641\u064f\u0648\u0631\u0657\u0627 \u0631\u0651\u064e\u062d\u0650\u064a\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "And for Allah", "word_transliteration": "walillahi"}, {"word_arabic_uthmani": "\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "(is the) kingdom", "word_transliteration": "mul'ku"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f", "word_translation": "He forgives", "word_transliteration": "yaghfiru"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0646", "word_translation": "whom", "word_transliteration": "liman"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f", "word_translation": "and punishes", "word_transliteration": "wayu'adhibu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "And is", "word_transliteration": "wakana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u063a\u064e\u0641\u064f\u0648\u0631\u0657\u0627", "word_translation": "Oft-Forgiving", "word_transliteration": "ghafuran"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u0657\u0627", "word_translation": "Most Merciful", "word_transliteration": "rahiman"}]}