{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0635\u064f\u062f\u0651\u064e\u0646\u0651\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064f\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u0643\u064f\u0645\u06e1 \u0639\u064e\u062f\u064f\u0648\u0651\u065e \u0645\u0651\u064f\u0628\u0650\u064a\u0646\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (let) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0635\u064f\u062f\u0651\u064e\u0646\u0651\u064e\u0643\u064f\u0645\u064f", "word_translation": "avert you", "word_transliteration": "yasuddannakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064f\u06d6", "word_translation": "the Shaitaan", "word_transliteration": "al-shaytanu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed he", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(is) for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064f\u0648\u0651\u065e", "word_translation": "an enemy", "word_transliteration": "aduwwun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0628\u0650\u064a\u0646\u065e", "word_translation": "clear", "word_transliteration": "mubinun"}]}