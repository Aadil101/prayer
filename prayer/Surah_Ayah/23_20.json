{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0634\u064e\u062c\u064e\u0631\u064e\u0629\u0657 \u062a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f \u0645\u0650\u0646 \u0637\u064f\u0648\u0631\u0650 \u0633\u064e\u064a\u06e1\u0646\u064e\u0627\u0653\u0621\u064e \u062a\u064e\u0646\u06e2\u0628\u064f\u062a\u064f \u0628\u0650\u0671\u0644\u062f\u0651\u064f\u0647\u06e1\u0646\u0650 \u0648\u064e\u0635\u0650\u0628\u06e1\u063a\u0656 \u0644\u0651\u0650\u0644\u06e1\u0623\u0653\u0643\u0650\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0634\u064e\u062c\u064e\u0631\u064e\u0629\u0657", "word_translation": "And a tree", "word_transliteration": "washajaratan"}, {"word_arabic_uthmani": "\u062a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f", "word_translation": "(that) springs forth", "word_transliteration": "takhruju"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0637\u064f\u0648\u0631\u0650", "word_translation": "Mount Sinai", "word_transliteration": "turi"}, {"word_arabic_uthmani": "\u0633\u064e\u064a\u06e1\u0646\u064e\u0627\u0653\u0621\u064e", "word_translation": "Mount Sinai", "word_transliteration": "saynaa"}, {"word_arabic_uthmani": "\u062a\u064e\u0646\u06e2\u0628\u064f\u062a\u064f", "word_translation": "(which) produces", "word_transliteration": "tanbutu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u062f\u0651\u064f\u0647\u06e1\u0646\u0650", "word_translation": "oil", "word_transliteration": "bil-duh'ni"}, {"word_arabic_uthmani": "\u0648\u064e\u0635\u0650\u0628\u06e1\u063a\u0656", "word_translation": "and a relish", "word_transliteration": "wasib'ghin"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u06e1\u0623\u0653\u0643\u0650\u0644\u0650\u064a\u0646\u064e", "word_translation": "for those who eat", "word_transliteration": "lil'akilina"}]}