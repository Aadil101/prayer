{"ayahs": {"ayah_uthmani": "\u0644\u0651\u064e\u0640\u0670\u0643\u0650\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064e\u0634\u06e1\u0647\u064e\u062f\u064f \u0628\u0650\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u0632\u064e\u0644\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e\u06d6 \u0623\u064e\u0646\u0632\u064e\u0644\u064e\u0647\u064f\u06e5 \u0628\u0650\u0639\u0650\u0644\u06e1\u0645\u0650\u0647\u0650\u06e6\u06d6 \u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f \u064a\u064e\u0634\u06e1\u0647\u064e\u062f\u064f\u0648\u0646\u064e\u06da \u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0634\u064e\u0647\u0650\u064a\u062f\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u064e\u0640\u0670\u0643\u0650\u0646\u0650", "word_translation": "But", "word_transliteration": "lakini"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0647\u064e\u062f\u064f", "word_translation": "bears witness", "word_transliteration": "yashhadu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627\u0653", "word_translation": "to what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0632\u064e\u0644\u064e", "word_translation": "He (has) revealed", "word_transliteration": "anzala"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e\u06d6", "word_translation": "to you", "word_transliteration": "ilayka"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0632\u064e\u0644\u064e\u0647\u064f\u06e5", "word_translation": "He has sent it down", "word_transliteration": "anzalahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0639\u0650\u0644\u06e1\u0645\u0650\u0647\u0650\u06e6\u06d6", "word_translation": "with His Knowledge", "word_transliteration": "bi'il'mihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f", "word_translation": "and the Angels", "word_transliteration": "wal-malaikatu"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0647\u064e\u062f\u064f\u0648\u0646\u064e\u06da", "word_translation": "bear witness", "word_transliteration": "yashhaduna"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "And is sufficient", "word_transliteration": "wakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0634\u064e\u0647\u0650\u064a\u062f\u064b\u0627", "word_translation": "(as) a Witness", "word_transliteration": "shahidan"}]}