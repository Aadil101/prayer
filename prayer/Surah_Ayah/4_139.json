{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u064f\u0648\u0646\u064e \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e \u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e\u06da \u0623\u064e\u064a\u064e\u0628\u06e1\u062a\u064e\u063a\u064f\u0648\u0646\u064e \u0639\u0650\u0646\u062f\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0639\u0650\u0632\u0651\u064e\u0629\u064e \u0641\u064e\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u06e1\u0639\u0650\u0632\u0651\u064e\u0629\u064e \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "Those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u064f\u0648\u0646\u064e", "word_translation": "take", "word_transliteration": "yattakhidhuna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the disbelievers", "word_transliteration": "al-kafirina"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e", "word_translation": "(as) allies", "word_transliteration": "awliyaa"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "(from)", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "instead of", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e\u06da", "word_translation": "the believers", "word_transliteration": "al-mu'minina"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u064e\u0628\u06e1\u062a\u064e\u063a\u064f\u0648\u0646\u064e", "word_translation": "Do they seek", "word_transliteration": "ayabtaghuna"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e\u0647\u064f\u0645\u064f", "word_translation": "with them", "word_transliteration": "indahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0632\u0651\u064e\u0629\u064e", "word_translation": "the honor", "word_transliteration": "al-'izata"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "But indeed", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0632\u0651\u064e\u0629\u064e", "word_translation": "the honor", "word_transliteration": "al-'izata"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "(is) for Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627", "word_translation": "all", "word_transliteration": "jami'an"}]}