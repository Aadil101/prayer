{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653 \u0623\u064e\u0646 \u062b\u064e\u0628\u0651\u064e\u062a\u06e1\u0646\u064e\u0670\u0643\u064e \u0644\u064e\u0642\u064e\u062f\u06e1 \u0643\u0650\u062f\u062a\u0651\u064e \u062a\u064e\u0631\u06e1\u0643\u064e\u0646\u064f \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627 \u0642\u064e\u0644\u0650\u064a\u0644\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653", "word_translation": "And if not", "word_transliteration": "walawla"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "[that]", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062b\u064e\u0628\u0651\u064e\u062a\u06e1\u0646\u064e\u0670\u0643\u064e", "word_translation": "We (had) strengthened you", "word_transliteration": "thabbatnaka"}, {"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "certainly", "word_transliteration": "laqad"}, {"word_arabic_uthmani": "\u0643\u0650\u062f\u062a\u0651\u064e", "word_translation": "you almost", "word_transliteration": "kidtta"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u06e1\u0643\u064e\u0646\u064f", "word_translation": "(would) have inclined", "word_transliteration": "tarkanu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "to them", "word_transliteration": "ilayhim"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627", "word_translation": "(in) something", "word_transliteration": "shayan"}, {"word_arabic_uthmani": "\u0642\u064e\u0644\u0650\u064a\u0644\u064b\u0627", "word_translation": "a little", "word_transliteration": "qalilan"}]}