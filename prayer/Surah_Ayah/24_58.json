teration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "(have) not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u0628\u06e1\u0644\u064f\u063a\u064f\u0648\u0627\u0652", "word_translation": "reached", "word_transliteration": "yablughu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064f\u0644\u064f\u0645\u064e", "word_translation": "puberty", "word_transliteration": "al-huluma"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0643\u064f\u0645\u06e1", "word_translation": "among you", "word_transliteration": "minkum"}, {"word_arabic_uthmani": "\u062b\u064e\u0644\u064e\u0670\u062b\u064e", "word_translation": "(at) three", "word_transliteration": "thalatha"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u0651\u064e\u0640\u0670\u062a\u0656\u06da", "word_translation": "times", "word_transliteration": "marratin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650", "word_translation": "before", "word_transliteration": "qabli"}, {"word_arabic_uthmani": "\u0635\u064e\u0644\u064e\u0648\u0670\u0629\u0650", "word_translation": "(the) prayer", "word_transliteration": "salati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u062c\u06e1\u0631\u0650", "word_translation": "(of) dawn", "word_transliteration": "al-fajri"}, {"word_arabic_uthmani": "\u0648\u064e\u062d\u0650\u064a\u0646\u064e", "word_translation": "and when", "word_transliteration": "wahina"}, {"word_arabic_uthmani": "\u062a\u064e\u0636\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "you put aside", "word_transliteration": "tada'una"}, {"word_arabic_uthmani": "\u062b\u0650\u064a\u064e\u0627\u0628\u064e\u0643\u064f\u0645", "word_translation": "your garments", "word_transliteration": "thiyabakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "at", "word_transliteration": "m