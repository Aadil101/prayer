{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u064e\u0627 \u0641\u064e\u0670\u0643\u0650\u0647\u064e\u0629\u065e \u0648\u064e\u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u064e\u0627 \u064a\u064e\u062f\u0651\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "For them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0641\u064e\u0670\u0643\u0650\u0647\u064e\u0629\u065e", "word_translation": "(are) fruits", "word_transliteration": "fakihatun"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u0645", "word_translation": "and for them", "word_transliteration": "walahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "(is) whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u0651\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "they call for", "word_transliteration": "yadda'una"}]}