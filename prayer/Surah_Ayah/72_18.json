{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u06e1\u0645\u064e\u0633\u064e\u0670\u062c\u0650\u062f\u064e \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0627\u0652 \u0645\u064e\u0639\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0623\u064e\u062d\u064e\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e", "word_translation": "And that", "word_transliteration": "wa-anna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0633\u064e\u0670\u062c\u0650\u062f\u064e", "word_translation": "the masajid", "word_transliteration": "al-masajida"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "(are) for Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "so (do) not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0627\u0652", "word_translation": "call", "word_transliteration": "tad'u"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e", "word_translation": "with", "word_transliteration": "ma'a"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u0657\u0627", "word_translation": "anyone", "word_transliteration": "ahadan"}]}