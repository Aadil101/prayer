{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u0670\u0647\u064f\u0643\u064f\u0645\u06e1 \u0625\u0650\u0644\u064e\u0670\u0647\u065e \u0648\u064e\u0670\u062d\u0650\u062f\u065e\u06d6 \u0644\u0651\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0670\u0647\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0647\u064f\u0648\u064e \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f \u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u0670\u0647\u064f\u0643\u064f\u0645\u06e1", "word_translation": "And your God", "word_transliteration": "wa-ilahukum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u065e", "word_translation": "(is) God", "word_transliteration": "ilahun"}, {"word_arabic_uthmani": "\u0648\u064e\u0670\u062d\u0650\u062f\u065e\u06d6", "word_translation": "one (only)", "word_transliteration": "wahidun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627\u0653", "word_translation": "(there is) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064e", "word_translation": "god", "word_transliteration": "ilaha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "Him", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmanu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f", "word_translation": "the Most Merciful", "word_transliteration": "al-rahimu"}]}