{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u062e\u06e1\u0641\u0650\u0636\u06e1 \u062c\u064e\u0646\u064e\u0627\u062d\u064e\u0643\u064e \u0644\u0650\u0645\u064e\u0646\u0650 \u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e\u0643\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u062e\u06e1\u0641\u0650\u0636\u06e1", "word_translation": "And lower", "word_transliteration": "wa-ikh'fid"}, {"word_arabic_uthmani": "\u062c\u064e\u0646\u064e\u0627\u062d\u064e\u0643\u064e", "word_translation": "your wing", "word_transliteration": "janahaka"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0646\u0650", "word_translation": "to (those) who", "word_transliteration": "limani"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e\u0643\u064e", "word_translation": "follow you", "word_transliteration": "ittaba'aka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "the believers", "word_transliteration": "al-mu'minina"}]}