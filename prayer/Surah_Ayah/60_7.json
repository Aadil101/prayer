{"ayahs": {"ayah_uthmani": "\u06de\u0639\u064e\u0633\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u0646 \u064a\u064e\u062c\u06e1\u0639\u064e\u0644\u064e \u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0639\u064e\u0627\u062f\u064e\u064a\u06e1\u062a\u064f\u0645 \u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645 \u0645\u0651\u064e\u0648\u064e\u062f\u0651\u064e\u0629\u0657\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0642\u064e\u062f\u0650\u064a\u0631\u065e\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u063a\u064e\u0641\u064f\u0648\u0631\u065e \u0631\u0651\u064e\u062d\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u06de\u0639\u064e\u0633\u064e\u0649", "word_translation": "Perhaps", "word_transliteration": "asa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "[that]", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u062c\u06e1\u0639\u064e\u0644\u064e", "word_translation": "will put", "word_transliteration": "yaj'ala"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1", "word_translation": "between you", "word_transliteration": "baynakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e", "word_translation": "and between", "word_transliteration": "wabayna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those (to) whom", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0639\u064e\u0627\u062f\u064e\u064a\u06e1\u062a\u064f\u0645", "word_translation": "you have been enemies", "word_transliteration": "adaytum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645", "word_translation": "among them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0648\u064e\u062f\u0651\u064e\u0629\u0657\u06da", "word_translation": "love", "word_transliteration": "mawaddatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0650\u064a\u0631\u065e\u06da", "word_translation": "(is) All-Powerful", "word_transliteration": "qadirun"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u063a\u064e\u0641\u064f\u0648\u0631\u065e", "word_translation": "(is) Oft-Forgiving", "word_transliteration": "ghafurun"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u065e", "word_translation": "Most Merciful", "word_transliteration": "rahimun"}]}