{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653 \u0623\u064e\u0646\u0632\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f \u0641\u0650\u064a \u0644\u064e\u064a\u06e1\u0644\u064e\u0629\u0650 \u0671\u0644\u06e1\u0642\u064e\u062f\u06e1\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653", "word_translation": "Indeed We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0632\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f", "word_translation": "revealed it", "word_transliteration": "anzalnahu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0644\u064e\u0629\u0650", "word_translation": "(the) Night", "word_transliteration": "laylati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u062f\u06e1\u0631\u0650", "word_translation": "(of) Power", "word_transliteration": "al-qadri"}]}