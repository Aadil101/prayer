ation": "that will please You", "word_transliteration": "tardahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062f\u06e1\u062e\u0650\u0644\u06e1\u0646\u0650\u064a", "word_translation": "And admit me", "word_transliteration": "wa-adkhil'ni"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u062d\u06e1\u0645\u064e\u062a\u0650\u0643\u064e", "word_translation": "by Your Mercy", "word_transliteration": "birahmatika"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "among", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0643\u064e", "word_translation": "Your slaves", "word_transliteration": "ibadika"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u0650\u064a\u0646\u064e", "word_translation": "righteous", "word_transliteration": "al-salihina"}]}