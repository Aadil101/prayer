{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0646\u06e1 \u0623\u064e\u0636\u064e\u0644\u0651\u064f \u0645\u0650\u0645\u0651\u064e\u0646 \u064a\u064e\u062f\u06e1\u0639\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0645\u064e\u0646 \u0644\u0651\u064e\u0627 \u064a\u064e\u0633\u06e1\u062a\u064e\u062c\u0650\u064a\u0628\u064f \u0644\u064e\u0647\u064f\u06e5\u0653 \u0625\u0650\u0644\u064e\u0649\u0670 \u064a\u064e\u0648\u06e1\u0645\u0650 \u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0646 \u062f\u064f\u0639\u064e\u0627\u0653\u0626\u0650\u0647\u0650\u0645\u06e1 \u063a\u064e\u0670\u0641\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646\u06e1", "word_translation": "And who", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u0623\u064e\u0636\u064e\u0644\u0651\u064f", "word_translation": "(is) more astray", "word_transliteration": "adallu"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0646", "word_translation": "than (he) who", "word_transliteration": "mimman"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u06e1\u0639\u064f\u0648\u0627\u0652", "word_translation": "calls", "word_transliteration": "yad'u"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "besides", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "will not respond", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064