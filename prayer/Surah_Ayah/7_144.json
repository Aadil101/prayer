{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u064a\u064e\u0670\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653 \u0625\u0650\u0646\u0651\u0650\u064a \u0671\u0635\u06e1\u0637\u064e\u0641\u064e\u064a\u06e1\u062a\u064f\u0643\u064e \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0628\u0650\u0631\u0650\u0633\u064e\u0670\u0644\u064e\u0670\u062a\u0650\u064a \u0648\u064e\u0628\u0650\u0643\u064e\u0644\u064e\u0670\u0645\u0650\u064a \u0641\u064e\u062e\u064f\u0630\u06e1 \u0645\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u062a\u064f\u0643\u064e \u0648\u064e\u0643\u064f\u0646 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0634\u0651\u064e\u0640\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653", "word_translation": "O Musa!", "word_transliteration": "yamusa"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0671\u0635\u06e1\u0637\u064e\u0641\u064e\u064a\u06e1\u062a\u064f\u0643\u064e", "word_translation": "have chosen you", "word_transliteration": "is'tafaytuka"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "the people", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u0650\u0633\u064e\u0670\u0644\u064e\u0670\u062a\u0650\u064a", "word_translation": "with My Messages", "word_transliteration": "birisalati"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u0650\u0643\u064e\u0644\u064e\u0670\u0645\u0650\u064a", "word_translation": "and with My words", "word_transliteration": "wabikalami"}, {"word_arabic_uthmani": "\u0641\u064e\u062e\u064f\u0630\u06e1", "word_translation": "So take", "word_transliteration": "fakhudh"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u062a\u064f\u0643\u064e", "word_translation": "I have given you", "word_transliteration": "ataytuka"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064f\u0646", "word_translation": "and be", "word_transliteration": "wakun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "among", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u0640\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the grateful", "word_transliteration": "al-shakirina"}]}