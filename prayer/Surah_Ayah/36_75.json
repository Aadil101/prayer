{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0627 \u064a\u064e\u0633\u06e1\u062a\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0646\u064e \u0646\u064e\u0635\u06e1\u0631\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0647\u064f\u0645\u06e1 \u062c\u064f\u0646\u062f\u065e \u0645\u0651\u064f\u062d\u06e1\u0636\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0646\u064e", "word_translation": "they are able", "word_transliteration": "yastati'una"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u06e1\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "to help them", "word_transliteration": "nasrahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "but they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u062c\u064f\u0646\u062f\u065e", "word_translation": "(are) host(s)", "word_transliteration": "jundun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u062d\u06e1\u0636\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "(who will) be brought", "word_transliteration": "muh'daruna"}]}