{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u0628\u0650\u0642\u064e\u0648\u06e1\u0644\u0650 \u0643\u064e\u0627\u0647\u0650\u0646\u0656\u06da \u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627 \u0645\u0651\u064e\u0627 \u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0628\u0650\u0642\u064e\u0648\u06e1\u0644\u0650", "word_translation": "(it is the) word", "word_transliteration": "biqawli"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0647\u0650\u0646\u0656\u06da", "word_translation": "(of) a soothsayer;", "word_transliteration": "kahinin"}, {"word_arabic_uthmani": "\u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627", "word_translation": "little", "word_transliteration": "qalilan"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "(is) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "you remember", "word_transliteration": "tadhakkaruna"}]}