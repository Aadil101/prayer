{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627 \u0641\u064e\u0648\u06e1\u0642\u064e\u0643\u064f\u0645\u06e1 \u0633\u064e\u0628\u06e1\u0639\u064e \u0637\u064e\u0631\u064e\u0627\u0653\u0626\u0650\u0642\u064e \u0648\u064e\u0645\u064e\u0627 \u0643\u064f\u0646\u0651\u064e\u0627 \u0639\u064e\u0646\u0650 \u0671\u0644\u06e1\u062e\u064e\u0644\u06e1\u0642\u0650 \u063a\u064e\u0670\u0641\u0650\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And indeed", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627", "word_translation": "We (have) created", "word_transliteration": "khalaqna"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u06e1\u0642\u064e\u0643\u064f\u0645\u06e1", "word_translation": "above you", "word_transliteration": "fawqakum"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u06e1\u0639\u064e", "word_translation": "seven", "word_transliteration": "sab'a"}, {"word_arabic_uthmani": "\u0637\u064e\u0631\u064e\u0627\u0653\u0626\u0650\u0642\u064e", "word_translation": "paths", "word_transliteration": "taraiqa"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "We are", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0650", "word_translation": "of", "word_transliteration": "ani"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0644\u06e1\u0642\u0650", "word_translation": "the creation", "word_transliteration": "al-khalqi"}, {"word_arabic_uthmani": "\u063a\u064e\u0670\u0641\u0650\u0644\u0650\u064a\u0646\u064e", "word_translation": "unaware", "word_transliteration": "ghafilina"}]}