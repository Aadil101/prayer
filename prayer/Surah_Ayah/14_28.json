{"ayahs": {"ayah_uthmani": "\u06de\u0623\u064e\u0644\u064e\u0645\u06e1 \u062a\u064e\u0631\u064e \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0628\u064e\u062f\u0651\u064e\u0644\u064f\u0648\u0627\u0652 \u0646\u0650\u0639\u06e1\u0645\u064e\u062a\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0643\u064f\u0641\u06e1\u0631\u0657\u0627 \u0648\u064e\u0623\u064e\u062d\u064e\u0644\u0651\u064f\u0648\u0627\u0652 \u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064f\u0645\u06e1 \u062f\u064e\u0627\u0631\u064e \u0671\u0644\u06e1\u0628\u064e\u0648\u064e\u0627\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u06de\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Have not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e", "word_translation": "you seen", "word_transliteration": "tara"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "[to]", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0628\u064e\u062f\u0651\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "(have) changed", "word_transliteration": "baddalu"}, {"word_arabic_uthmani": "\u0646\u0650\u0639\u06e1\u0645\u064e\u062a\u064e", "word_translation": "(the) Favor", "word_transliteration": "ni'mata"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0643\u064f\u0641\u06e1\u0631\u0657\u0627", "word_translation": "(for) disbelief", "word_transliteration": "kuf'ran"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062d\u064e\u0644\u0651\u064f\u0648\u0627\u0652", "word_translation": "and they led", "word_transliteration": "wa-ahallu"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064f\u0645\u06e1", "wor