_transliteration": "anfaqa"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650", "word_translation": "before", "word_transliteration": "qabli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u062a\u06e1\u062d\u0650", "word_translation": "the victory", "word_transliteration": "al-fathi"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0670\u062a\u064e\u0644\u064e\u06da", "word_translation": "and fought", "word_transliteration": "waqatala"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0638\u064e\u0645\u064f", "word_translation": "(are) greater", "word_transliteration": "a'zamu"}, {"word_arabic_uthmani": "\u062f\u064e\u0631\u064e\u062c\u064e\u0629\u0657", "word_translation": "(in) degree", "word_transliteration": "darajatan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "than", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "spent", "word_transliteration": "anfaqu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e2", "word_translation": "afterwards", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u064f", "word_translation": "afterwards", "word_transliteration": "ba'du"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0670\u062a\u064e\u0644\u064f\u0648\u0627\u0652\u06da", "word_translation": "and fought", "word_transliteration": "waqatalu"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064f\u0644\u0651\u0657\u0627", "word_translation": "But to all", "word_transliteration": "wakullan"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u062f\u064e", "word_translation": "Allah has promised", "word_transliteration": "wa'ada"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah has promised", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064f\u0633\u06e1\u0646\u064e\u0649\u0670\u06da", "word_translation": "the best", "word_transliteration": "al-hus'na"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "you do", "word_transliteration": "ta'maluna"}, {"word_arabic_uthmani": "\u062e\u064e\u0628\u0650\u064a\u0631\u065e", "word_translation": "(is) All-Aware", "word_transliteration": "khabirun"}]}