{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627 \u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627 \u0641\u0650\u064a\u0653 \u0623\u064e\u0639\u06e1\u0646\u064e\u0670\u0642\u0650\u0647\u0650\u0645\u06e1 \u0623\u064e\u063a\u06e1\u0644\u064e\u0670\u0644\u0657\u0627 \u0641\u064e\u0647\u0650\u064a\u064e \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u06e1\u0623\u064e\u0630\u06e1\u0642\u064e\u0627\u0646\u0650 \u0641\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0642\u06e1\u0645\u064e\u062d\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "[We] have placed", "word_transliteration": "ja'alna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0653", "word_translation": "on", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0646\u064e\u0670\u0642\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their necks", "word_transliteration": "a'naqihim"}, {"word_arabic_uthmani": "\u0623\u064e\u063a\u06e1\u0644\u064e\u0670\u0644\u0657\u0627", "word_translation": "iron collars", "word_transliteration": "aghlalan"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u0650\u064a\u064e", "word_translation": "and they", "word_transliteration": "fahiya"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "(are up) to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0630\u06e1\u0642\u064e\u0627\u0646\u0650", "word_translation": "the chins", "word_transliteration": "al-adhqani"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0645", "word_translation": "so they", "word_transliteration": "fahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0642\u06e1\u0645\u064e\u062d\u064f\u0648\u0646\u064e", "word_translation": "(are with) heads aloft", "word_transliteration": "muq'mahuna"}]}