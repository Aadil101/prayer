{"ayahs": {"ayah_uthmani": "\u06de\u0642\u064e\u062f\u06e1 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u06e1\u0645\u064f\u0639\u064e\u0648\u0651\u0650\u0642\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u0643\u064f\u0645\u06e1 \u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0627\u0653\u0626\u0650\u0644\u0650\u064a\u0646\u064e \u0644\u0650\u0625\u0650\u062e\u06e1\u0648\u064e\u0670\u0646\u0650\u0647\u0650\u0645\u06e1 \u0647\u064e\u0644\u064f\u0645\u0651\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0646\u064e\u0627\u06d6 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u064e \u0671\u0644\u06e1\u0628\u064e\u0623\u06e1\u0633\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0642\u064e\u0644\u0650\u064a\u0644\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u06de\u0642\u064e\u062f\u06e1", "word_translation": "Verily", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "Allah knows", "word_transliteration": "ya'lamu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah knows", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0639\u064e\u0648\u0651\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "those who hinder", "word_transliteration": "al-mu'awiqina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0643\u064f\u0645\u06e1", "word_translation": "among you", "word_transliteration": "minkum"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0627\u0653\u0626\u0650\u0644\u0650\u064a\u0646\u064e", "word_translation": "and those who say", "word_transliteration": "wal-qailina"}, {"word_arabic_uthmani": "\u0644\u0650\u0625\u0650\u062e\u06e1\u0648\u064e\u0670\u0646\u0650\u0647\u0650\u0645\u06e1", "word_translation": "to their brothers", "word_transliteration": "li-ikh'wanihim"}, {"word_arabic_uthmani": "\u0647\u064e\u0644\u064f\u0645\u0651\u064e", "word_translation": "Come", "word_transliteration": "halumma"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0646\u064e\u0627\u06d6", "word_translation": "to us", "word_transliteration": "ilayna"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u064e", "word_translation": "they come", "word_transliteration": "yatuna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0623\u06e1\u0633\u064e", "word_translation": "(to) the battle", "word_transliteration": "al-basa"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0642\u064e\u0644\u0650\u064a\u0644\u064b\u0627", "word_translation": "a few", "word_transliteration": "qalilan"}]}