{"ayahs": {"ayah_uthmani": "\u0644\u0651\u064e\u0627 \u064a\u064e\u0645\u06e1\u0644\u0650\u0643\u064f\u0648\u0646\u064e \u0671\u0644\u0634\u0651\u064e\u0641\u064e\u0670\u0639\u064e\u0629\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064e\u0646\u0650 \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650 \u0639\u064e\u0647\u06e1\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u06e1\u0644\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "they will have the power", "word_transliteration": "yamlikuna"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u0641\u064e\u0670\u0639\u064e\u0629\u064e", "word_translation": "(of) the intercession", "word_transliteration": "al-shafa'ata"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u0650", "word_translation": "(he) who", "word_transliteration": "mani"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e", "word_translation": "has taken", "word_transliteration": "ittakhadha"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "from", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmani"}, {"word_arabic_uthmani": "\u0639\u064e\u0647\u06e1\u062f\u0657\u0627", "word_translation": "a covenant", "word_transliteration": "ahdan"}]}