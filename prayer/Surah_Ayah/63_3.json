{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u062b\u064f\u0645\u0651\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0641\u064e\u0637\u064f\u0628\u0650\u0639\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0647\u0650\u0645\u06e1 \u0641\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0641\u06e1\u0642\u064e\u0647\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "(is) because", "word_transliteration": "bi-annahum"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "they believed", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "they disbelieved;", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0641\u064e\u0637\u064f\u0628\u0650\u0639\u064e", "word_translation": "so were sealed", "word_transliteration": "fatubi'a"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "[upon]", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their hearts", "word_transliteration": "qulubihim"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so they", "word_transliteration": "fahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0641\u06e1\u0642\u064e\u0647\u064f\u0648\u0646\u064e", "word_translation": "understand", "word_transliteration": "yafqahuna"}]}