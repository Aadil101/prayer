{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f \u0671\u0644\u0637\u0651\u064f\u0648\u0641\u064e\u0627\u0646\u064e \u0648\u064e\u0671\u0644\u06e1\u062c\u064e\u0631\u064e\u0627\u062f\u064e \u0648\u064e\u0671\u0644\u06e1\u0642\u064f\u0645\u0651\u064e\u0644\u064e \u0648\u064e\u0671\u0644\u0636\u0651\u064e\u0641\u064e\u0627\u062f\u0650\u0639\u064e \u0648\u064e\u0671\u0644\u062f\u0651\u064e\u0645\u064e \u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0656 \u0645\u0651\u064f\u0641\u064e\u0635\u0651\u064e\u0644\u064e\u0670\u062a\u0656 \u0641\u064e\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652 \u0648\u064e\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0642\u064e\u0648\u06e1\u0645\u0657\u0627 \u0645\u0651\u064f\u062c\u06e1\u0631\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "So We sent", "word_transliteration": "fa-arsalna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f", "word_translation": "on them", "word_transliteration": "alayhimu"}, {"word_arabic_uthmani": "\u0671\u0644\u0637\u0651\u064f\u0648\u0641\u064e\u0627\u0646\u064e", "word_translation": "the flood", "word_transliteration": "al-tufana"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u062c\u064e\u0631\u064e\u0627\u062f\u064e", "word_translation": "and the locusts", "word_transliteration": "wal-jarada"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0642\u064f\u0645\u0651\u064e\u0644\u064e", "word_translation": "and the lice", "word_transliteration": "wal-qumala"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0636\u0651\u064e\u0641\u064e\u0627\u062f\u0650\u0639\u064e", "word_translation": "and the frogs", "word_transliteration": "wal-dafadi'a"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u062f\u0651\u064e\u0645\u064e", "word_translation": "and the blood", "word_transliteration": "wal-dama"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0656", "word_translation": "(as) signs", "word_transliteration": "ayatin"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0641\u064e\u0635\u0651\u064e\u0644\u064e\u0670\u062a\u0656", "word_translation": "manifest", "word_transliteration": "mufassalatin"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "but they showed arrogance", "word_transliteration": "fa-is'takbaru"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "and they were", "word_transliteration": "wakanu"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0657\u0627", "word_translation": "a people", "word_transliteration": "qawman"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u062c\u06e1\u0631\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "criminal", "word_transliteration": "muj'rimina"}]}