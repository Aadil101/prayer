{"ayahs": {"ayah_uthmani": "\u0628\u064e\u0642\u0650\u064a\u0651\u064e\u062a\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u062e\u064e\u064a\u06e1\u0631\u065e \u0644\u0651\u064e\u0643\u064f\u0645\u06e1 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645 \u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e\u06da \u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u064e\u0627\u06e0 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645 \u0628\u0650\u062d\u064e\u0641\u0650\u064a\u0638\u0656"}, "words": [{"word_arabic_uthmani": "\u0628\u064e\u0642\u0650\u064a\u0651\u064e\u062a\u064f", "word_translation": "(What) remains", "word_transliteration": "baqiyyatu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(from) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u065e", "word_translation": "(is) best", "word_transliteration": "khayrun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645", "word_translation": "you are", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e\u06da", "word_translation": "believers", "word_transliteration": "mu'minina"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "I am", "word_transliteration": "ana"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645", "word_translation": "over you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0628\u0650\u062d\u064e\u0641\u0650\u064a\u0638\u0656", 