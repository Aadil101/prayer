{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u06e1\u062a\u064f \u0645\u0650\u0644\u0651\u064e\u0629\u064e \u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0621\u0650\u064a\u0653 \u0625\u0650\u0628\u06e1\u0631\u064e\u0670\u0647\u0650\u064a\u0645\u064e \u0648\u064e\u0625\u0650\u0633\u06e1\u062d\u064e\u0670\u0642\u064e \u0648\u064e\u064a\u064e\u0639\u06e1\u0642\u064f\u0648\u0628\u064e\u06da \u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e \u0644\u064e\u0646\u064e\u0627\u0653 \u0623\u064e\u0646 \u0646\u0651\u064f\u0634\u06e1\u0631\u0650\u0643\u064e \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0645\u0650\u0646 \u0634\u064e\u064a\u06e1\u0621\u0656\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0645\u0650\u0646 \u0641\u064e\u0636\u06e1\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u0648\u064e\u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0644\u064e\u0627 \u064a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u06e1\u062a\u064f", "word_translation": "And I follow", "word_transliteration": "wa-ittaba'tu"}, {"word_arabic_uthmani": "\u0645\u0650\u0644\u0651\u064e\u0629\u064e", "word_translation": "(the) religion", "word_transliteration": "millata"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0621\u0650\u064a\u0653", "word_translation": "(of) my forefathers", "word_transliteration": "abai"}, {"word_arabic_uthmani": "\u0625\u0650\u0628\u06e1\u0631\u064e\u0670\u0647\u0650\u064a\u0645\u064e", "word_translation": "Ibrahim", "word_transliteration": "ib'rahima"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0633\u06e1\u062d\u064e\u0670\u0642\u064e", "word_translation": "and Ishaq", "word_transliteration": "wa-is'haqa"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0639\u06e1\u0642\u064f\u0648\u0628\u064e\u06da", "word_translation": "and Yaqub", "word_transliteration": "waya'quba"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "was", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0627\u0653", "word_translation": "for us", "word_transliteration": "lana"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0646\u0651\u064f\u0634\u06e1\u0631\u0650\u0643\u064e", "word_translation": "we associate", "word_transliteration": "nush'rika"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "with Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656\u06da", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "(is) from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u0650", "word_translation": "(the) Grace", "word_transliteration": "fadli"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "upon us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0644\u064e\u0649", "word_translation": "and upon", "word_transliteration": "wa'ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "the mankind", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e", "word_translation": "but", "word_transliteration": "walakinna"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e", "word_translation": "most", "word_transliteration": "akthara"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "(of) the men", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(are) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "grateful", "word_transliteration": "yashkuruna"}]}