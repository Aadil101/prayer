{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0644\u064e\u0639\u064e\u0646\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0641\u064e\u0623\u064e\u0635\u064e\u0645\u0651\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0623\u064e\u0639\u06e1\u0645\u064e\u0649\u0670\u0653 \u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064e\u0647\u064f\u0645\u06e1"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(are) the ones", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0646\u064e\u0647\u064f\u0645\u064f", "word_translation": "Allah has cursed them", "word_transliteration": "la'anahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah has cursed them", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0635\u064e\u0645\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so He made them deaf", "word_transliteration": "fa-asammahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0639\u06e1\u0645\u064e\u0649\u0670\u0653", "word_translation": "and blinded", "word_transliteration": "wa-a'ma"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their vision", "word_transliteration": "absarahum"}]}