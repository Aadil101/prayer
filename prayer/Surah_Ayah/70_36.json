{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0645\u064e\u0627\u0644\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0642\u0650\u0628\u064e\u0644\u064e\u0643\u064e \u0645\u064f\u0647\u06e1\u0637\u0650\u0639\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627\u0644\u0650", "word_translation": "So what is with", "word_transliteration": "famali"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0642\u0650\u0628\u064e\u0644\u064e\u0643\u064e", "word_translation": "before you", "word_transliteration": "qibalaka"}, {"word_arabic_uthmani": "\u0645\u064f\u0647\u06e1\u0637\u0650\u0639\u0650\u064a\u0646\u064e", "word_translation": "(they) hasten", "word_transliteration": "muh'ti'ina"}]}