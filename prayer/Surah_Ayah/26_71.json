{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0646\u064e\u0639\u06e1\u0628\u064f\u062f\u064f \u0623\u064e\u0635\u06e1\u0646\u064e\u0627\u0645\u0657\u0627 \u0641\u064e\u0646\u064e\u0638\u064e\u0644\u0651\u064f \u0644\u064e\u0647\u064e\u0627 \u0639\u064e\u0670\u0643\u0650\u0641\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0646\u064e\u0639\u06e1\u0628\u064f\u062f\u064f", "word_translation": "We worship", "word_transliteration": "na'budu"}, {"word_arabic_uthmani": "\u0623\u064e\u0635\u06e1\u0646\u064e\u0627\u0645\u0657\u0627", "word_translation": "idols", "word_transliteration": "asnaman"}, {"word_arabic_uthmani": "\u0641\u064e\u0646\u064e\u0638\u064e\u0644\u0651\u064f", "word_translation": "so we will remain", "word_transliteration": "fanazallu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064e\u0627", "word_translation": "to them", "word_transliteration": "laha"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0643\u0650\u0641\u0650\u064a\u0646\u064e", "word_translation": "devoted", "word_transliteration": "akifina"}]}