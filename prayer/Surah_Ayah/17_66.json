{"ayahs": {"ayah_uthmani": "\u0631\u0651\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u064a\u064f\u0632\u06e1\u062c\u0650\u064a \u0644\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u064e \u0641\u0650\u064a \u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650 \u0644\u0650\u062a\u064e\u0628\u06e1\u062a\u064e\u063a\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6\u0653\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0643\u064e\u0627\u0646\u064e \u0628\u0650\u0643\u064f\u0645\u06e1 \u0631\u064e\u062d\u0650\u064a\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u064f", "word_translation": "Your Lord", "word_transliteration": "rabbukumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u064a\u064f\u0632\u06e1\u062c\u0650\u064a", "word_translation": "drives", "word_transliteration": "yuz'ji"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u064f", "word_translation": "for you", "word_transliteration": "lakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u064e", "word_translation": "the ship", "word_transliteration": "al-ful'ka"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650", "word_translation": "the sea", "word_transliteration": "al-bahri"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064e\u0628\u06e1\u062a\u064e\u063a\u064f\u0648\u0627\u0652", "word_translation": "that you may seek", "word_transliteration": "litabtaghu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6\u0653\u06da", "word_translation": "His Bounty", "word_transliteration": "fadlihi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064f\u0645\u06e1", "word_translation": "to you", "word_transliteration": "bikum"}, {"word_arabic_uthmani": "\u0631\u064e\u062d\u0650\u064a\u0645\u0657\u0627", "word_translation": "Ever Merciful", "word_transliteration": "rahiman"}]}