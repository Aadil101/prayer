{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0637\u064e\u0628\u064e\u0639\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u0649\u0670 \u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0633\u064e\u0645\u06e1\u0639\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u0650\u0647\u0650\u0645\u06e1\u06d6 \u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u063a\u064e\u0670\u0641\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(are) the ones", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0637\u064e\u0628\u064e\u0639\u064e", "word_translation": "Allah has set a seal", "word_transliteration": "taba'a"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah has set a seal", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their hearts", "word_transliteration": "qulubihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064e\u0645\u06e1\u0639\u0650\u0647\u0650\u0645\u06e1", "word_translation": "and their hearing", "word_transliteration": "wasam'ihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u0650\u0647\u0650\u0645\u06e1\u06d6", "word_translation": "and their sight", "word_transliteration": "wa-absarihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "And those", "word_transliteration": "wa-ulaika"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "they are", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u0670\u0641\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "the heedless", "word_transliteration": "al-ghafiluna"}]}