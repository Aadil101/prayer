{"ayahs": {"ayah_uthmani": "\u06de\u0644\u064e\u062a\u064e\u062c\u0650\u062f\u064e\u0646\u0651\u064e \u0623\u064e\u0634\u064e\u062f\u0651\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0639\u064e\u062f\u064e\u0670\u0648\u064e\u0629\u0657 \u0644\u0651\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u064a\u064e\u0647\u064f\u0648\u062f\u064e \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0623\u064e\u0634\u06e1\u0631\u064e\u0643\u064f\u0648\u0627\u0652\u06d6 \u0648\u064e\u0644\u064e\u062a\u064e\u062c\u0650\u062f\u064e\u0646\u0651\u064e \u0623\u064e\u0642\u06e1\u0631\u064e\u0628\u064e\u0647\u064f\u0645 \u0645\u0651\u064e\u0648\u064e\u062f\u0651\u064e\u0629\u0657 \u0644\u0651\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u0651\u064e\u0627 \u0646\u064e\u0635\u064e\u0670\u0631\u064e\u0649\u0670\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0642\u0650\u0633\u0651\u0650\u064a\u0633\u0650\u064a\u0646\u064e \u0648\u064e\u0631\u064f\u0647\u06e1\u0628\u064e\u0627\u0646\u0657\u0627 \u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0644\u064e\u062a\u064e\u062c\u0650\u062f\u064e\u0646\u0651\u064e", "word_translation": "Surely you will find", "word_transliteration": "latajidanna"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u064e\u062f\u0651\u064e", "word_translation": "strongest", "word_transliteration": "ashadda"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "(of) the people", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064e\u0670\u0648\u064e\u0629\u0657", "word_translation": "(in) enmity", "word_transliteration": "adawatan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "to those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u064a\u064e\u0647\u064f\u0648\u062f\u064e", "word_translation": "the Jews", "word_transliteration": "al-yahuda"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u06e1\u0631\u064e\u0643\u064f\u0648\u0627\u0652\u06d6", "word_translation": "(are) polytheists", "word_transliteration": "ashraku"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u062a\u064e\u062c\u0650\u062f\u064e\u0646\u0651\u064e", "word_translation": "and surely you will find", "word_transliteration": "walatajidanna"}, {"word_arabic_uthmani": "\u0623\u064e\u0642\u06e1\u0631\u064e\u0628\u064e\u0647\u064f\u0645", "word_translation": "nearest of them", "word_transliteration": "aqrabahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0648\u064e\u062f\u0651\u064e\u0629\u0657", "word_translation": "(in) affection", "word_transliteration": "mawaddatan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "to those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "say", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u064e\u0670\u0631\u064e\u0649\u0670\u06da", "word_translation": "(are) Christians", "word_transliteration": "nasara"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That (is)", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e", "word_translation": "because", "word_transliteration": "bi-anna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "among them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0642\u0650\u0633\u0651\u0650\u064a\u0633\u0650\u064a\u0646\u064e", "word_translation": "(are) priests", "word_transliteration": "qissisina"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064f\u0647\u06e1\u0628\u064e\u0627\u0646\u0657\u0627", "word_translation": "and monks", "word_transliteration": "waruh'banan"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and that they", "word_transliteration": "wa-annahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(are) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "arrogant", "word_transliteration": "yastakbiruna"}]}