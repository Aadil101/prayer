{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u062d\u064e\u0642\u0651\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u064f \u0641\u0650\u064a\u0653 \u0623\u064f\u0645\u064e\u0645\u0656 \u0642\u064e\u062f\u06e1 \u062e\u064e\u0644\u064e\u062a\u06e1 \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u0650 \u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u0650\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u062e\u064e\u0670\u0633\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(are) the ones", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u062d\u064e\u0642\u0651\u064e", "word_translation": "(has) proved true", "word_transliteration": "haqqa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f", "word_translation": "against them", "word_transliteration": "alayhimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u064f", "word_translation": "the word", "word_transliteration": "al-qawlu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0653", "word_translation": "among", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u064e\u0645\u0656", "word_translation": "nations", "word_transliteration": "umamin"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "(that) already passed away", "word_transliteration": "qad"}, {"word_arabic_uthma