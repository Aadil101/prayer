{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0648\u06e1 \u0628\u064e\u0633\u064e\u0637\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u0631\u0651\u0650\u0632\u06e1\u0642\u064e \u0644\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6 \u0644\u064e\u0628\u064e\u063a\u064e\u0648\u06e1\u0627\u0652 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646 \u064a\u064f\u0646\u064e\u0632\u0651\u0650\u0644\u064f \u0628\u0650\u0642\u064e\u062f\u064e\u0631\u0656 \u0645\u0651\u064e\u0627 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0628\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6 \u062e\u064e\u0628\u0650\u064a\u0631\u064f\u06e2 \u0628\u064e\u0635\u0650\u064a\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0628\u064e\u0633\u064e\u0637\u064e", "word_translation": "Allah extends", "word_transliteration": "basata"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah extends", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u0650\u0632\u06e1\u0642\u064e", "word_translation": "the provision", "word_transliteration": "al-riz'qa"}, {"word_arabic_uthmani": "\u0644\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6", "word_translation": "for His slaves", "word_transliteration": "li'ibadihi"}, {"word_arabic_uthmani": "\u0644\u064e\u0628\u064e\u063a\u064e\u0648\u06e1\u0627\u0652", "word_translation": "surely they would rebel", "word_transliteration": "labaghaw"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646", "word_translation": "but", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u064a\u064f\u0646\u064e\u0632\u0651\u0650\u0644\u064f", "word_translation": "He sends down", "word_transliteration": "yunazzilu"}, {"word_arabic_uthmani": "\u0628\u0650\u0642\u064e\u062f\u064e\u0631\u0656", "word_translation": "in (due) measure", "word_transliteration": "biqadarin"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed, He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6", "word_translation": "of His slaves", "word_transliteration": "bi'ibadihi"}, {"word_arabic_uthmani": "\u062e\u064e\u0628\u0650\u064a\u0631\u064f\u06e2", "word_translation": "(is) All-Aware", "word_transliteration": "khabirun"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u0650\u064a\u0631\u065e", "word_translation": "All-Seer", "word_transliteration": "basirun"}]}