rd_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u0644\u0650\u062d\u06e1", "word_translation": "and do right", "word_transliteration": "wa-aslih"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u062a\u0651\u064e\u0628\u0650\u0639\u06e1", "word_translation": "follow", "word_transliteration": "tattabi'"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u064e", "word_translation": "(the) way", "word_transliteration": "sabila"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e", "word_translation": "(of) the corrupters", "word_transliteration": "al-muf'sidina"}]}