{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0645\u064e\u0631\u064e\u062c\u064e \u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u064e\u064a\u06e1\u0646\u0650 \u0647\u064e\u0670\u0630\u064e\u0627 \u0639\u064e\u0630\u06e1\u0628\u065e \u0641\u064f\u0631\u064e\u0627\u062a\u065e \u0648\u064e\u0647\u064e\u0670\u0630\u064e\u0627 \u0645\u0650\u0644\u06e1\u062d\u064c \u0623\u064f\u062c\u064e\u0627\u062c\u065e \u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627 \u0628\u064e\u0631\u06e1\u0632\u064e\u062e\u0657\u0627 \u0648\u064e\u062d\u0650\u062c\u06e1\u0631\u0657\u0627 \u0645\u0651\u064e\u062d\u06e1\u062c\u064f\u0648\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u064e\u062c\u064e", "word_translation": "(has) released", "word_transliteration": "maraja"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u064e\u064a\u06e1\u0646\u0650", "word_translation": "the two seas", "word_transliteration": "al-bahrayni"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "[this] (one)", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u06e1\u0628\u065e", "word_translation": "palatable", "word_transliteration": "adhbun"}, {"word_arabic_uthmani": "\u0641\u064f\u0631\u064e\u0627\u062a\u065e", "word_translation": "and sweet", "word_transliteration": "furatun"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "and [this] (one)", "word_transliteration": "wahadha"}, {"word_arabic_uthmani": "\u0645\u0650\u0644\u06e1\u062d\u064c", "word_translation": "salty", "word_transliteration": "mil'hun"}, {"word_arabic_uthmani": "\u0623\u064f\u062c\u064e\u0627\u062c\u065e", "word_translation": "(and) bitter", "word_transliteration": "ujajun"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e", "word_translation": "and He has made", "word_transliteration": "waja'ala"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627", "word_translation": "between them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0628\u064e\u0631\u06e1\u0632\u064e\u062e\u0657\u0627", "word_translation": "a barrier", "word_transliteration": "barzakhan"}, {"word_arabic_uthmani": "\u0648\u064e\u062d\u0650\u062c\u06e1\u0631\u0657\u0627", "word_translation": "and a partition", "word_transliteration": "wahij'ran"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u062d\u06e1\u062c\u064f\u0648\u0631\u0657\u0627", "word_translation": "forbidden", "word_transliteration": "mahjuran"}]}