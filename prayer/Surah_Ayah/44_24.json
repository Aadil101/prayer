{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u062a\u06e1\u0631\u064f\u0643\u0650 \u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u064e \u0631\u064e\u0647\u06e1\u0648\u064b\u0627\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u062c\u064f\u0646\u062f\u065e \u0645\u0651\u064f\u063a\u06e1\u0631\u064e\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u062a\u06e1\u0631\u064f\u0643\u0650", "word_translation": "And leave", "word_transliteration": "wa-ut'ruki"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u064e", "word_translation": "the sea", "word_transliteration": "al-bahra"}, {"word_arabic_uthmani": "\u0631\u064e\u0647\u06e1\u0648\u064b\u0627\u06d6", "word_translation": "at rest", "word_transliteration": "rahwan"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Indeed they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u062c\u064f\u0646\u062f\u065e", "word_translation": "(are) an army", "word_transliteration": "jundun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u063a\u06e1\u0631\u064e\u0642\u064f\u0648\u0646\u064e", "word_translation": "(to be) drowned", "word_transliteration": "mugh'raquna"}]}