{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653 \u0625\u0650\u0630\u06e1 \u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645 \u0628\u064e\u0623\u06e1\u0633\u064f\u0646\u064e\u0627 \u062a\u064e\u0636\u064e\u0631\u0651\u064e\u0639\u064f\u0648\u0627\u0652 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646 \u0642\u064e\u0633\u064e\u062a\u06e1 \u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0632\u064e\u064a\u0651\u064e\u0646\u064e \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064f \u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653", "word_translation": "Then why not", "word_transliteration": "falawla"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "when", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645", "word_translation": "came to them", "word_transliteration": "jaahum"}, {"word_arabic_uthmani": "\u0628\u064e\u0623\u06e1\u0633\u064f\u0646\u064e\u0627", "word_translation": "Our punishment", "word_transliteration": "basuna"}, {"word_arabic_uthmani": "\u062a\u064e\u0636\u064e\u0631\u0651\u064e\u0639\u064f\u0648\u0627\u0652", "word_translation": "they humbled themselves", "word_transliteration": "tadarra'u"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646", "word_translation": "But", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u0642\u064e\u0633\u064e\u062a\u06e1", "word_translation": "became hardened", "word_transliteration": "qasat"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their hearts", "word_transliteration": "qulubuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0632\u064e\u064a\u0651\u064e\u0646\u064e", "word_translation": "and made fair-seeming", "word_transliteration": "wazayyana"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "to them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064f", "word_translation": "the Shaitaan", "word_transliteration": "al-shaytanu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they used to", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "do", "word_transliteration": "ya'maluna"}]}