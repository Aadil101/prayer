{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0628\u064e\u0627\u0646\u064e\u0627 \u0645\u064e\u0627 \u0644\u064e\u0643\u064e \u0644\u064e\u0627 \u062a\u064e\u0623\u06e1\u0645\u064e\u06ec\u0646\u0651\u064e\u0627 \u0639\u064e\u0644\u064e\u0649\u0670 \u064a\u064f\u0648\u0633\u064f\u0641\u064e \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0644\u064e\u0647\u064f\u06e5 \u0644\u064e\u0646\u064e\u0670\u0635\u0650\u062d\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0628\u064e\u0627\u0646\u064e\u0627", "word_translation": "O our father!", "word_transliteration": "yaabana"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Why", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e", "word_translation": "(do) you", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u0645\u064e\u06ec\u0646\u0651\u064e\u0627", "word_translation": "trust us", "word_transliteration": "tamanna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "with", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0633\u064f\u0641\u064e", "word_translation": "Yusuf", "word_transliteration": "yusufa"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "while indeed we", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "(are) for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0670\u0635\u0650\u062d\u064f\u0648\u0646\u064e", "word_translation": "surely well-wishers", "word_transliteration": "lanasihuna"}]}