{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f \u0648\u064e\u0644\u064e\u062f\u0657\u0627\u06d7 \u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0647\u064f\u06e5\u06da \u0628\u064e\u0644\u06e1 \u0639\u0650\u0628\u064e\u0627\u062f\u065e \u0645\u0651\u064f\u0643\u06e1\u0631\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "And they say", "word_transliteration": "waqalu"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e", "word_translation": "Has taken", "word_transliteration": "ittakhadha"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmanu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u062f\u0657\u0627\u06d7", "word_translation": "a son", "word_transliteration": "waladan"}, {"word_arabic_uthmani": "\u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0647\u064f\u06e5\u06da", "word_translation": "Glorified is He!", "word_transliteration": "sub'hanahu"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u065e", "word_translation": "(they are) slaves", "word_transliteration": "ibadun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0643\u06e1\u0631\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "honored", "word_transliteration": "muk'ramuna"}]}