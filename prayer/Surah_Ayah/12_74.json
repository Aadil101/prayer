{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0641\u064e\u0645\u064e\u0627 \u062c\u064e\u0632\u064e\u0640\u0670\u0653\u0624\u064f\u0647\u064f\u06e5\u0653 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u0643\u064e\u0670\u0630\u0650\u0628\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627", "word_translation": "Then what", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u062c\u064e\u0632\u064e\u0640\u0670\u0653\u0624\u064f\u0647\u064f\u06e5\u0653", "word_translation": "(will be the) recompense (of) it", "word_transliteration": "jazauhu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you are", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0643\u064e\u0670\u0630\u0650\u0628\u0650\u064a\u0646\u064e", "word_translation": "liars", "word_transliteration": "kadhibina"}]}