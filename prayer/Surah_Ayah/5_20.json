{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1 \u0642\u064e\u0627\u0644\u064e \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0644\u0650\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6 \u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0671\u0630\u06e1\u0643\u064f\u0631\u064f\u0648\u0627\u0652 \u0646\u0650\u0639\u06e1\u0645\u064e\u0629\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u0625\u0650\u0630\u06e1 \u062c\u064e\u0639\u064e\u0644\u064e \u0641\u0650\u064a\u0643\u064f\u0645\u06e1 \u0623\u064e\u0646\u06e2\u0628\u0650\u064a\u064e\u0627\u0653\u0621\u064e \u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e\u0643\u064f\u0645 \u0645\u0651\u064f\u0644\u064f\u0648\u0643\u0657\u0627 \u0648\u064e\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0643\u064f\u0645 \u0645\u0651\u064e\u0627 \u0644\u064e\u0645\u06e1 \u064a\u064f\u0624\u06e1\u062a\u0650 \u0623\u064e\u062d\u064e\u062f\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1", "word_translation": "And when", "word_transliteration": "wa-idh"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0644\u0650\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6", "word_translation": "to his people", "word_transliteration": "liqawmihi"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "O my people", "word_transliteration": "yaqawmi"}, {"word_arabic_uthmani": "\u0671\u0630\u06e1\u0643\u064f\u0631\u064f\u0648\u0627\u0652", "word_translation": "remember", "word_transliteration": "udh'kuru"}, {"word_arabic_uthmani": "\u0646\u0650\u0639\u06e1\u0645\u064e\u0629\u064e", "word_translation": "(the) Favor", "word_transliteration": "ni'mata"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "upon you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "when", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u064e", "word_translation": "He placed", "word_transliteration": "ja'ala"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0643\u064f\u0645\u06e1", "word_translation": "among you", "word_transliteration": "fikum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e2\u0628\u0650\u064a\u064e\u0627\u0653\u0621\u064e", "word_translation": "Prophets", "word_transliteration": "anbiyaa"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e\u0643\u064f\u0645", "word_translation": "and made you", "word_transliteration": "waja'alakum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0644\u064f\u0648\u0643\u0657\u0627", "word_translation": "kings", "word_transliteration": "mulukan"}, {"word_arabic_uthmani": "\u0648\u064e\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0643\u064f\u0645", "word_translation": "and He gave you", "word_transliteration": "waatakum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u062a\u0650", "word_translation": "He (had) given", "word_transliteration": "yu'ti"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u0657\u0627", "word_translation": "(to) anyone", "word_transliteration": "ahadan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "the worlds", "word_transliteration": "al-'alamina"}]}