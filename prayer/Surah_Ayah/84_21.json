{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u064e\u0627 \u0642\u064f\u0631\u0650\u0626\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f \u0671\u0644\u06e1\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064f \u0644\u064e\u0627 \u064a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0646\u064e\u06e4\u06e9"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u064e\u0627", "word_translation": "And when", "word_transliteration": "wa-idha"}, {"word_arabic_uthmani": "\u0642\u064f\u0631\u0650\u0626\u064e", "word_translation": "is recited", "word_transliteration": "quri-a"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f", "word_translation": "to them", "word_transliteration": "alayhimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064f", "word_translation": "the Quran", "word_transliteration": "al-qur'anu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0646\u064e\u06e4\u06e9", "word_translation": "they prostrate", "word_transliteration": "yasjuduna"}]}