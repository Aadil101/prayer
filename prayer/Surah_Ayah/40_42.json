{"ayahs": {"ayah_uthmani": "\u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e\u0646\u0650\u064a \u0644\u0650\u0623\u064e\u0643\u06e1\u0641\u064f\u0631\u064e \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0623\u064f\u0634\u06e1\u0631\u0650\u0643\u064e \u0628\u0650\u0647\u0650\u06e6 \u0645\u064e\u0627 \u0644\u064e\u064a\u06e1\u0633\u064e \u0644\u0650\u064a \u0628\u0650\u0647\u0650\u06e6 \u0639\u0650\u0644\u06e1\u0645\u065e \u0648\u064e\u0623\u064e\u0646\u064e\u0627\u06e0 \u0623\u064e\u062f\u06e1\u0639\u064f\u0648\u0643\u064f\u0645\u06e1 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u0650 \u0671\u0644\u06e1\u063a\u064e\u0641\u0651\u064e\u0640\u0670\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e\u0646\u0650\u064a", "word_translation": "You call me", "word_transliteration": "tad'unani"}, {"word_arabic_uthmani": "\u0644\u0650\u0623\u064e\u0643\u06e1\u0641\u064f\u0631\u064e", "word_translation": "that I disbelieve", "word_transliteration": "li-akfura"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "in Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0634\u06e1\u0631\u0650\u0643\u064e", "word_translation": "and (to) associate", "word_transliteration": "wa-ush'rika"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with Him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "not", "word_transliteration": "laysa"}, {"word_arabic_uthmani": "\u0644\u0650\u064a", "word_translation": "for me", "word_transliteration": "li"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "of it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u065e", "word_translation": "any knowledge", "word_transliteration": "il'mun"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "and I", "word_transliteration": "wa-ana"}, {"word_arabic_uthmani": "\u0623\u064e\u062f\u06e1\u0639\u064f\u0648\u0643\u064f\u0645\u06e1", "word_translation": "call you", "word_transliteration": "ad'ukum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u0650", "word_translation": "the All-Mighty", "word_transliteration": "al-'azizi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u0641\u0651\u064e\u0640\u0670\u0631\u0650", "word_translation": "the Oft-Forgiving", "word_transliteration": "al-ghafari"}]}