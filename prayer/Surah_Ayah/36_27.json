{"ayahs": {"ayah_uthmani": "\u0628\u0650\u0645\u064e\u0627 \u063a\u064e\u0641\u064e\u0631\u064e \u0644\u0650\u064a \u0631\u064e\u0628\u0651\u0650\u064a \u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e\u0646\u0650\u064a \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0643\u06e1\u0631\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "Of how", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u063a\u064e\u0641\u064e\u0631\u064e", "word_translation": "has forgiven", "word_transliteration": "ghafara"}, {"word_arabic_uthmani": "\u0644\u0650\u064a", "word_translation": "me", "word_transliteration": "li"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a", "word_translation": "my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e\u0646\u0650\u064a", "word_translation": "and placed me", "word_transliteration": "waja'alani"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "among", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0643\u06e1\u0631\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "the honored ones", "word_transliteration": "al-muk'ramina"}]}