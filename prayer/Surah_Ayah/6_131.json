{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0623\u064e\u0646 \u0644\u0651\u064e\u0645\u06e1 \u064a\u064e\u0643\u064f\u0646 \u0631\u0651\u064e\u0628\u0651\u064f\u0643\u064e \u0645\u064f\u0647\u06e1\u0644\u0650\u0643\u064e \u0671\u0644\u06e1\u0642\u064f\u0631\u064e\u0649\u0670 \u0628\u0650\u0638\u064f\u0644\u06e1\u0645\u0656 \u0648\u064e\u0623\u064e\u0647\u06e1\u0644\u064f\u0647\u064e\u0627 \u063a\u064e\u0670\u0641\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That (is because)", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "[that]", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0646", "word_translation": "is", "word_transliteration": "yakun"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbuka"}, {"word_arabic_uthmani": "\u0645\u064f\u0647\u06e1\u0644\u0650\u0643\u064e", "word_translation": "one who destroys", "word_transliteration": "muh'lika"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0631\u064e\u0649\u0670", "word_translation": "the cities", "word_transliteration": "al-qura"}, {"word_arabic_uthmani": "\u0628\u0650\u0638\u064f\u0644\u06e1\u0645\u0656", "word_translation": "for their wrongdoing", "word_transliteration": "bizul'min"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0647\u06e1\u0644\u064f\u0647\u064e\u0627", "word_translation": "while their people", "word_transliteration": "wa-ahluha"}, {"word_arabic_uthmani": "\u063a\u064e\u0670\u0641\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "(are) unaware", "word_transliteration": "ghafiluna"}]}