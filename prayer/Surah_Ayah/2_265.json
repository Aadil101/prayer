{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u062b\u064e\u0644\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064f\u0646\u0641\u0650\u0642\u064f\u0648\u0646\u064e \u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0628\u06e1\u062a\u0650\u063a\u064e\u0627\u0653\u0621\u064e \u0645\u064e\u0631\u06e1\u0636\u064e\u0627\u062a\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u062a\u064e\u062b\u06e1\u0628\u0650\u064a\u062a\u0657\u0627 \u0645\u0651\u0650\u0646\u06e1 \u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1 \u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650 \u062c\u064e\u0646\u0651\u064e\u0629\u0650\u06ed \u0628\u0650\u0631\u064e\u0628\u06e1\u0648\u064e\u0629\u064d \u0623\u064e\u0635\u064e\u0627\u0628\u064e\u0647\u064e\u0627 \u0648\u064e\u0627\u0628\u0650\u0644\u065e \u0641\u064e\u0640\u0654\u064e\u0627\u062a\u064e\u062a\u06e1 \u0623\u064f\u0643\u064f\u0644\u064e\u0647\u064e\u0627 \u0636\u0650\u0639\u06e1\u0641\u064e\u064a\u06e1\u0646\u0650 \u0641\u064e\u0625\u0650\u0646 \u0644\u0651\u064e\u0645\u06e1 \u064a\u064f\u0635\u0650\u0628\u06e1\u0647\u064e\u0627 \u0648\u064e\u0627\u0628\u0650\u0644\u065e \u0641\u064e\u0637\u064e\u0644\u0651\u065e\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u0650\u0645\u064e\u0627 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e \u0628\u064e\u0635\u0650\u064a\u0631\u064c"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u062b\u064e\u0644\u064f", "word_translation": "And (the) example", "word_transliteration": "wamathalu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064f\u0646\u0641\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "spend", "word_transliteration": "yunfiquna"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "their wealth", "word_transliteration": "amwalahumu"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u062a\u0650\u063a\u064e\u0627\u0653\u0621\u064e", "word_translation": "seeking", "word_transliteration": "ib'tighaa"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u06e1\u0636\u064e\u0627\u062a\u0650", "word_translation": "(the) pleasure", "word_transliteration": "mardati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u062b\u06e1\u0628\u0650\u064a\u062a\u0657\u0627", "word_translation": "and certainty", "word_transliteration": "watathbitan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their (inner) souls", "word_transliteration": "anfusihim"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650", "word_translation": "(is) like", "word_transliteration": "kamathali"}, {"word_arabic_uthmani": "\u062c\u064e\u0646\u0651\u064e\u0629\u0650\u06ed", "word_translation": "a garden", "word_transliteration": "jannatin"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u0628\u06e1\u0648\u064e\u0629\u064d", "word_translation": "on a height", "word_transliteration": "birabwatin"}, {"word_arabic_uthmani": "\u0623\u064e\u0635\u064e\u0627\u0628\u064e\u0647\u064e\u0627", "word_translation": "fell on it", "word_transliteration": "asabaha"}, {"word_arabic_uthmani": "\u0648\u064e\u0627\u0628\u0650\u0644\u065e", "word_translation": "heavy rain", "word_transliteration": "wabilun"}, {"word_arabic_uthmani": "\u0641\u064e\u0640\u0654\u064e\u0627\u062a\u064e\u062a\u06e1", "word_translation": "so it yielded", "word_transliteration": "faatat"}, {"word_arabic_uthmani": "\u0623\u064f\u0643\u064f\u0644\u064e\u0647\u064e\u0627", "word_translation": "its harvest", "word_transliteration": "ukulaha"}, {"word_arabic_uthmani": "\u0636\u0650\u0639\u06e1\u0641\u064e\u064a\u06e1\u0646\u0650", "word_translation": "double", "word_transliteration": "di'fayni"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646", "word_translation": "Then if", "word_transliteration": "fa-in"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u06e1", "word_translation": "(does) not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064f\u0635\u0650\u0628\u06e1\u0647\u064e\u0627", "word_translation": "fall (on) it", "word_transliteration": "yusib'ha"}, {"word_arabic_uthmani": "\u0648\u064e\u0627\u0628\u0650\u0644\u065e", "word_translation": "heavy rain", "word_transliteration": "wabilun"}, {"word_arabic_uthmani": "\u0641\u064e\u0637\u064e\u0644\u0651\u065e\u06d7", "word_translation": "then a drizzle", "word_transliteration": "fatallun"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "you do", "word_transliteration": "ta'maluna"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u0650\u064a\u0631\u064c", "word_translation": "(is) All-Seer", "word_transliteration": "basirun"}]}