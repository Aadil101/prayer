{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0644\u06e1\u0628\u0650\u0633\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064e \u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u0670\u0637\u0650\u0644\u0650 \u0648\u064e\u062a\u064e\u0643\u06e1\u062a\u064f\u0645\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064e \u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0644\u06e1\u0628\u0650\u0633\u064f\u0648\u0627\u0652", "word_translation": "mix", "word_transliteration": "talbisu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064e", "word_translation": "the Truth", "word_transliteration": "al-haqa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u0670\u0637\u0650\u0644\u0650", "word_translation": "with [the] falsehood", "word_transliteration": "bil-batili"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0643\u06e1\u062a\u064f\u0645\u064f\u0648\u0627\u0652", "word_translation": "and conceal", "word_transliteration": "wataktumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064e", "word_translation": "the Truth", "word_transliteration": "al-haqa"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "while you", "word_transliteration": "wa-antum"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "[you] know", "word_transliteration": "ta'lamuna"}]}