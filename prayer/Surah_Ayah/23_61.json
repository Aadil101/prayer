{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u064a\u064f\u0633\u064e\u0670\u0631\u0650\u0639\u064f\u0648\u0646\u064e \u0641\u0650\u064a \u0671\u0644\u06e1\u062e\u064e\u064a\u06e1\u0631\u064e\u0670\u062a\u0650 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0647\u064e\u0627 \u0633\u064e\u0670\u0628\u0650\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "(It is) those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u064a\u064f\u0633\u064e\u0670\u0631\u0650\u0639\u064f\u0648\u0646\u064e", "word_translation": "who hasten", "word_transliteration": "yusari'una"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u064a\u06e1\u0631\u064e\u0670\u062a\u0650", "word_translation": "the good (deeds)", "word_transliteration": "al-khayrati"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064e\u0627", "word_translation": "in them", "word_transliteration": "laha"}, {"word_arabic_uthmani": "\u0633\u064e\u0670\u0628\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "(are) foremost", "word_transliteration": "sabiquna"}]}