{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0643\u064e\u0645\u06e1 \u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0627 \u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u0642\u064e\u0631\u06e1\u0646\u064d \u0647\u064f\u0645\u06e1 \u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064f \u0623\u064e\u062b\u064e\u0670\u062b\u0657\u0627 \u0648\u064e\u0631\u0650\u0621\u06e1\u064a\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0645\u06e1", "word_translation": "And how many", "word_transliteration": "wakam"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0627", "word_translation": "We destroyed", "word_transliteration": "ahlakna"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645", "word_translation": "before them", "word_transliteration": "qablahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u06e1\u0646\u064d", "word_translation": "a generation", "word_transliteration": "qarnin"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064f", "word_translation": "(were) better", "word_transliteration": "ahsanu"}, {"word_arabic_uthmani": "\u0623\u064e\u062b\u064e\u0670\u062b\u0657\u0627", "word_translation": "(in) possessions", "word_transliteration": "athathan"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u0650\u0621\u06e1\u064a\u0657\u0627", "word_translation": "and appearance", "word_transliteration": "wari'yan"}]}