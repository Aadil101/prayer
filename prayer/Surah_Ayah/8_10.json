{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u062c\u064e\u0639\u064e\u0644\u064e\u0647\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u064f\u0634\u06e1\u0631\u064e\u0649\u0670 \u0648\u064e\u0644\u0650\u062a\u064e\u0637\u06e1\u0645\u064e\u0626\u0650\u0646\u0651\u064e \u0628\u0650\u0647\u0650\u06e6 \u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0643\u064f\u0645\u06e1\u06da \u0648\u064e\u0645\u064e\u0627 \u0671\u0644\u0646\u0651\u064e\u0635\u06e1\u0631\u064f \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u0650\u0646\u06e1 \u0639\u0650\u0646\u062f\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0639\u064e\u0632\u0650\u064a\u0632\u064c \u062d\u064e\u0643\u0650\u064a\u0645\u064c"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u064e\u0647\u064f", "word_translation": "(it was) made", "word_transliteration": "ja'alahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(by) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u064f\u0634\u06e1\u0631\u064e\u0649\u0670", "word_translation": "good tidings", "word_transliteration": "bush'ra"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u062a\u064e\u0637\u06e1\u0645\u064e\u0626\u0650\u0646\u0651\u064e", "word_translation": "and so that might be at rest", "word_transliteration": "walitatma-inna"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0643\u064f\u0645\u06e1\u06da", "word_translation": "your hearts", "word_transliteration": "qulubukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And (there is) no", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0635\u06e1\u0631\u064f", "word_translation": "[the] victory", "word_transliteration": "al-nasru"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u0650", "word_translation": "[of]", "word_transliteration": "indi"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0639\u064e\u0632\u0650\u064a\u0632\u064c", "word_translation": "(is) All-Mighty", "word_transliteration": "azizun"}, {"word_arabic_uthmani": "\u062d\u064e\u0643\u0650\u064a\u0645\u064c", "word_translation": "All-Wise", "word_transliteration": "hakimun"}]}