{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1 \u064a\u064e\u062d\u06e1\u0633\u064e\u0628\u064f\u0648\u0646\u064e \u0623\u064e\u0646\u0651\u064e\u0627 \u0644\u064e\u0627 \u0646\u064e\u0633\u06e1\u0645\u064e\u0639\u064f \u0633\u0650\u0631\u0651\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0646\u064e\u062c\u06e1\u0648\u064e\u0649\u0670\u0647\u064f\u0645\u06da \u0628\u064e\u0644\u064e\u0649\u0670 \u0648\u064e\u0631\u064f\u0633\u064f\u0644\u064f\u0646\u064e\u0627 \u0644\u064e\u062f\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u064a\u064e\u0643\u06e1\u062a\u064f\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0633\u064e\u0628\u064f\u0648\u0646\u064e", "word_translation": "(do) they think", "word_transliteration": "yahsabuna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0627", "word_translation": "that We", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(can) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0646\u064e\u0633\u06e1\u0645\u064e\u0639\u064f", "word_translation": "hear", "word_transliteration": "nasma'u"}, {"word_arabic_uthmani": "\u0633\u0650\u0631\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their secret(s)", "word_transliteration": "sirrahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u062c\u06e1\u0648\u064e\u0649\u0670\u0647\u064f\u0645\u06da", "word_translation": "and their private counsel(s)", "word_transliteration": "wanajwahum"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u064e\u0649\u0670", "word_translation": "Nay", "word_transliteration": "bala"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064f\u0633\u064f\u0644\u064f\u0646\u064e\u0627", "word_translation": "and Our Messengers", "word_transliteration": "warusuluna"}, {"word_arabic_uthmani": "\u0644\u064e\u062f\u064e\u064a\u06e1\u0647\u065