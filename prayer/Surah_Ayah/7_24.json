{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0671\u0647\u06e1\u0628\u0650\u0637\u064f\u0648\u0627\u0652 \u0628\u064e\u0639\u06e1\u0636\u064f\u0643\u064f\u0645\u06e1 \u0644\u0650\u0628\u064e\u0639\u06e1\u0636\u064d \u0639\u064e\u062f\u064f\u0648\u0651\u065e\u06d6 \u0648\u064e\u0644\u064e\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0645\u064f\u0633\u06e1\u062a\u064e\u0642\u064e\u0631\u0651\u065e \u0648\u064e\u0645\u064e\u062a\u064e\u0670\u0639\u064c \u0625\u0650\u0644\u064e\u0649\u0670 \u062d\u0650\u064a\u0646\u0656"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "(Allah) said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0671\u0647\u06e1\u0628\u0650\u0637\u064f\u0648\u0627\u0652", "word_translation": "Get down", "word_transliteration": "ih'bitu"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064f\u0643\u064f\u0645\u06e1", "word_translation": "some of you", "word_transliteration": "ba'dukum"}, {"word_arabic_uthmani": "\u0644\u0650\u0628\u064e\u0639\u06e1\u0636\u064d", "word_translation": "to some others", "word_transliteration": "liba'din"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064f\u0648\u0651\u065e\u06d6", "word_translation": "(as) enemy", "word_transliteration": "aduwwun"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "And for you", "word_transliteration": "walakum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0645\u064f\u0633\u06e1\u062a\u064e\u0642\u064e\u0631\u0651\u065e", "word_translation": "(is) a dwelling place", "word_transliteration": "mus'taqarrun"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u062a\u064e\u0670\u0639\u064c", "word_translation": "and livelihood", "word_transliteration": "wamata'un"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "for", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u062d\u0650\u064a\u0646\u0656", "word_translation": "a time", "word_transliteration": "hinin"}]}