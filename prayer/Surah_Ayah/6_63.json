{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u0645\u064e\u0646 \u064a\u064f\u0646\u064e\u062c\u0651\u0650\u064a\u0643\u064f\u0645 \u0645\u0651\u0650\u0646 \u0638\u064f\u0644\u064f\u0645\u064e\u0670\u062a\u0650 \u0671\u0644\u06e1\u0628\u064e\u0631\u0651\u0650 \u0648\u064e\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650 \u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e\u0647\u064f\u06e5 \u062a\u064e\u0636\u064e\u0631\u0651\u064f\u0639\u0657\u0627 \u0648\u064e\u062e\u064f\u0641\u06e1\u064a\u064e\u0629\u0657 \u0644\u0651\u064e\u0626\u0650\u0646\u06e1 \u0623\u064e\u0646\u062c\u064e\u0649\u0670\u0646\u064e\u0627 \u0645\u0650\u0646\u06e1 \u0647\u064e\u0670\u0630\u0650\u0647\u0650\u06e6 \u0644\u064e\u0646\u064e\u0643\u064f\u0648\u0646\u064e\u0646\u0651\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u0634\u0651\u064e\u0640\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "Who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064f\u0646\u064e\u062c\u0651\u0650\u064a\u0643\u064f\u0645", "word_translation": "saves you", "word_transliteration": "yunajjikum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0638\u064f\u0644\u064f\u0645\u064e\u0670\u062a\u0650", "word_translation": "darkness[es]", "word_transliteration": "zulumati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0631\u0651\u0650", "word_translation": "(of) the land", "word_transliteration": "al-bari"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650", "word_translation": "and the sea", "word_transliteration": "wal-bahri"}, {"word_arabic_uthmani": "\u062a\u064e\u062f\u06e1\u0639\u064f\u0648\u0646\u064e\u0647\u064f\u06e5", "word_translation": "you call Him", "word_transliteration": "tad'unahu"}, {"word_arabic_uthmani": "\u062a\u064e\u0636\u064e\u0631\u0651\u064f\u0639\u0657\u0627", "word_translation": "humbly", "word_transliteration": "tadarru'an"}, {"word_arabic_uthmani": "\u0648\u064e\u062e\u064f\u0641\u06e1\u064a\u064e\u0629\u0657", "word_translation": "and secretly", "word_transliteration": "wakhuf'yatan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0626\u0650\u0646\u06e1", "word_translation": "If", "word_transliteration": "la-in"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062c\u064e\u0649\u0670\u0646\u064e\u0627", "word_translation": "He saves us", "word_transliteration": "anjana"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u0650\u0647\u0650\u06e6", "word_translation": "this", "word_transliteration": "hadhihi"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0643\u064f\u0648\u0646\u064e\u0646\u0651\u064e", "word_translation": "surely we will be", "word_transliteration": "lanakunanna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u0640\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the grateful ones", "word_transliteration": "al-shakirina"}]}