{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0646 \u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0648\u06e1\u0627\u0652 \u0641\u064e\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0639\u064e\u0644\u0650\u064a\u0645\u064f\u06e2 \u0628\u0650\u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646", "word_translation": "And if", "word_transliteration": "fa-in"}, {"word_arabic_uthmani": "\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0648\u06e1\u0627\u0652", "word_translation": "they turn back", "word_transliteration": "tawallaw"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "then indeed", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u064f\u06e2", "word_translation": "(is) All-Knowing", "word_transliteration": "alimun"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e", "word_translation": "of the corrupters", "word_transliteration": "bil-muf'sidina"}]}