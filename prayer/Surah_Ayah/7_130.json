{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0623\u064e\u062e\u064e\u0630\u06e1\u0646\u064e\u0627\u0653 \u0621\u064e\u0627\u0644\u064e \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0628\u0650\u0671\u0644\u0633\u0651\u0650\u0646\u0650\u064a\u0646\u064e \u0648\u064e\u0646\u064e\u0642\u06e1\u0635\u0656 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u062b\u0651\u064e\u0645\u064e\u0631\u064e\u0670\u062a\u0650 \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0630\u0651\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And certainly", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u064e\u0630\u06e1\u0646\u064e\u0627\u0653", "word_translation": "We seized", "word_transliteration": "akhadhna"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u064e", "word_translation": "(the) people", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "(of) Firaun", "word_transliteration": "fir'awna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0633\u0651\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "with years (of famine)", "word_transliteration": "bil-sinina"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u0642\u06e1\u0635\u0656", "word_translation": "and a deficit", "word_transliteration": "wanaqsin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u062b\u0651\u064e\u0645\u064e\u0631\u064e\u0670\u062a\u0650", "word_translation": "[the] fruits", "word_transliteration": "al-thamarati"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so that they may", "word_transliteration": "la'allahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0630\u0651\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "receive admonition", "word_transliteration": "yadhakkaruna"}]}