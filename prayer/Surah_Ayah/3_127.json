{"ayahs": {"ayah_uthmani": "\u0644\u0650\u064a\u064e\u0642\u06e1\u0637\u064e\u0639\u064e \u0637\u064e\u0631\u064e\u0641\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0648\u06e1 \u064a\u064e\u0643\u06e1\u0628\u0650\u062a\u064e\u0647\u064f\u0645\u06e1 \u0641\u064e\u064a\u064e\u0646\u0642\u064e\u0644\u0650\u0628\u064f\u0648\u0627\u0652 \u062e\u064e\u0627\u0653\u0626\u0650\u0628\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u0650\u064a\u064e\u0642\u06e1\u0637\u064e\u0639\u064e", "word_translation": "That He may cut off", "word_transliteration": "liyaqta'a"}, {"word_arabic_uthmani": "\u0637\u064e\u0631\u064e\u0641\u0657\u0627", "word_translation": "a part", "word_transliteration": "tarafan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0653\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u06e1\u0628\u0650\u062a\u064e\u0647\u064f\u0645\u06e1", "word_translation": "suppress them", "word_transliteration": "yakbitahum"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064e\u0646\u0642\u064e\u0644\u0650\u0628\u064f\u0648\u0627\u0652", "word_translation": "so (that) they turn back", "word_transliteration": "fayanqalibu"}, {"word_arabic_uthmani": "\u062e\u064e\u0627\u0653\u0626\u0650\u0628\u0650\u064a\u0646\u064e", "word_translation": "disappointed", "word_transliteration": "khaibina"}]}