{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0643\u064e\u0645\u06e1 \u0642\u064e\u0635\u064e\u0645\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646 \u0642\u064e\u0631\u06e1\u064a\u064e\u0629\u0656 \u0643\u064e\u0627\u0646\u064e\u062a\u06e1 \u0638\u064e\u0627\u0644\u0650\u0645\u064e\u0629\u0657 \u0648\u064e\u0623\u064e\u0646\u0634\u064e\u0623\u06e1\u0646\u064e\u0627 \u0628\u064e\u0639\u06e1\u062f\u064e\u0647\u064e\u0627 \u0642\u064e\u0648\u06e1\u0645\u064b\u0627 \u0621\u064e\u0627\u062e\u064e\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0645\u06e1", "word_translation": "And how many", "word_transliteration": "wakam"}, {"word_arabic_uthmani": "\u0642\u064e\u0635\u064e\u0645\u06e1\u0646\u064e\u0627", "word_translation": "We (have) shattered", "word_transliteration": "qasamna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u06e1\u064a\u064e\u0629\u0656", "word_translation": "a town", "word_transliteration": "qaryatin"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e\u062a\u06e1", "word_translation": "(that) was", "word_transliteration": "kanat"}, {"word_arabic_uthmani": "\u0638\u064e\u0627\u0644\u0650\u0645\u064e\u0629\u0657", "word_translation": "unjust", "word_transliteration": "zalimatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0634\u064e\u0623\u06e1\u0646\u064e\u0627", "word_translation": "and We produced", "word_transliteration": "wa-anshana"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u064e\u0647\u064e\u0627", "word_translation": "after them", "word_transliteration": "ba'daha"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064b\u0627", "word_translation": "another people", "word_transliteration": "qawman"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062e\u064e\u0631\u0650\u064a\u0646\u064e", "word_translation": "another people", "word_transliteration": "akharina"}]}