{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0642\u0650\u0645\u0650 \u0671\u0644\u0635\u0651\u064e\u0644\u064e\u0648\u0670\u0629\u064e \u0637\u064e\u0631\u064e\u0641\u064e\u064a\u0650 \u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u0650 \u0648\u064e\u0632\u064f\u0644\u064e\u0641\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u06e1\u062d\u064e\u0633\u064e\u0646\u064e\u0670\u062a\u0650 \u064a\u064f\u0630\u06e1\u0647\u0650\u0628\u06e1\u0646\u064e \u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0640\u0654\u064e\u0627\u062a\u0650\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0630\u0650\u0643\u06e1\u0631\u064e\u0649\u0670 \u0644\u0650\u0644\u0630\u0651\u064e\u0640\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0642\u0650\u0645\u0650", "word_translation": "And establish", "word_transliteration": "wa-aqimi"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0644\u064e\u0648\u0670\u0629\u064e", "word_translation": "the prayer", "word_transliteration": "al-salata"}, {"word_arabic_uthmani": "\u0637\u064e\u0631\u064e\u0641\u064e\u064a\u0650", "word_translation": "(at the) two ends", "word_transliteration": "tarafayi"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u0650", "word_translation": "(of) the day", "word_transliteration": "al-nahari"}, {"word_arabic_uthmani": "\u0648\u064e\u0632\u064f\u0644\u064e\u0641\u0657\u0627", "word_translation": "and (at) the approach", "word_transliteration": "wazulafan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650\u06da", "word_translation": "the night", "word_transliteration": "al-layli"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0633\u064e\u0646\u064e\u0670\u062a\u0650", "word_translation": "the good deeds", "word_transliteration": "al-hasanati"}, {"word_arabic_uthmani": "\u064a\u064f\u0630\u06e1\u0647\u0650\u0628\u06e1\u0646\u064e", "word_translation": "remove", "word_transliteration": "yudh'hib'na"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0640\u0654\u064e\u0627\u062a\u0650\u06da", "word_translation": "the evil deeds", "word_transliteration": "al-sayiati"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0630\u0650\u0643\u06e1\u0631\u064e\u0649\u0670", "word_translation": "(is) a reminder", "word_transliteration": "dhik'ra"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0630\u0651\u064e\u0640\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "for those who remember", "word_transliteration": "lildhakirina"}]}