{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u063a\u064e\u064a\u06e1\u0628\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u064a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f \u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0631\u064f \u0643\u064f\u0644\u0651\u064f\u0647\u064f\u06e5 \u0641\u064e\u0671\u0639\u06e1\u0628\u064f\u062f\u06e1\u0647\u064f \u0648\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u06da \u0648\u064e\u0645\u064e\u0627 \u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0628\u0650\u063a\u064e\u0670\u0641\u0650\u0644\u064d \u0639\u064e\u0645\u0651\u064e\u0627 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "And for Allah", "word_transliteration": "walillahi"}, {"word_arabic_uthmani": "\u063a\u064e\u064a\u06e1\u0628\u064f", "word_translation": "(is the) unseen", "word_transliteration": "ghaybu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "and to Him", "word_transliteration": "wa-ilayhi"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f", "word_translation": "will be returned", "word_transliteration": "yur'ja'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0631\u064f", "word_translation": "the matter", "word_transliteration": "al-amru"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064f\u0647\u064f\u06e5", "word_translation": "all (of) it", "word_transliteration": "kulluhu"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0639\u06e1\u0628\u064f\u062f\u06e1\u0647\u064f", "word_translation": "so worship Him", "word_transliteration": "fa-u'bud'hu"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u06e1", "word_translation": "and put your trust", "word_transliteration": "watawakkal"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u06da", "word_translation": "upon Him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And your Lord is not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "And your Lord is not", "word_transliteration": "rabbuka"}, {"word_arabic_uthmani": "\u0628\u0650\u063a\u064e\u0670\u0641\u0650\u0644\u064d", "word_translation": "unaware", "word_transliteration": "bighafilin"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "you do", "word_transliteration": "ta'maluna"}]}