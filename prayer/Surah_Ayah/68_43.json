{"ayahs": {"ayah_uthmani": "\u062e\u064e\u0670\u0634\u0650\u0639\u064e\u0629\u064b \u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064f\u0647\u064f\u0645\u06e1 \u062a\u064e\u0631\u06e1\u0647\u064e\u0642\u064f\u0647\u064f\u0645\u06e1 \u0630\u0650\u0644\u0651\u064e\u0629\u065e\u06d6 \u0648\u064e\u0642\u064e\u062f\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064f\u062f\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0633\u0651\u064f\u062c\u064f\u0648\u062f\u0650 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0633\u064e\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u062e\u064e\u0670\u0634\u0650\u0639\u064e\u0629\u064b", "word_translation": "Humbled", "word_transliteration": "khashi'atan"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their eyes", "word_transliteration": "absaruhum"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u06e1\u0647\u064e\u0642\u064f\u0647\u064f\u0645\u06e1", "word_translation": "will cover them", "word_transliteration": "tarhaquhum"}, {"word_arabic_uthmani": "\u0630\u0650\u0644\u0651\u064e\u0629\u065e\u06d6", "word_translation": "humiliation", "word_transliteration": "dhillatun"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And indeed", "word_transliteration": "waqad"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they were", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064f\u062f\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "called", "word_transliteration": "yud'awna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064f\u062c\u064f\u0648\u062f\u0650", "word_translation": "prostrate", "word_transliteration": "al-sujudi"}, {"wo