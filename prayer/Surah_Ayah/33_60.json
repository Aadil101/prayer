{"ayahs": {"ayah_uthmani": "\u06de\u0644\u0651\u064e\u0626\u0650\u0646 \u0644\u0651\u064e\u0645\u06e1 \u064a\u064e\u0646\u062a\u064e\u0647\u0650 \u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u064f\u0648\u0646\u064e \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0641\u0650\u064a \u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0647\u0650\u0645 \u0645\u0651\u064e\u0631\u064e\u0636\u065e \u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u062c\u0650\u0641\u064f\u0648\u0646\u064e \u0641\u0650\u064a \u0671\u0644\u06e1\u0645\u064e\u062f\u0650\u064a\u0646\u064e\u0629\u0650 \u0644\u064e\u0646\u064f\u063a\u06e1\u0631\u0650\u064a\u064e\u0646\u0651\u064e\u0643\u064e \u0628\u0650\u0647\u0650\u0645\u06e1 \u062b\u064f\u0645\u0651\u064e \u0644\u064e\u0627 \u064a\u064f\u062c\u064e\u0627\u0648\u0650\u0631\u064f\u0648\u0646\u064e\u0643\u064e \u0641\u0650\u064a\u0647\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u06de\u0644\u0651\u064e\u0626\u0650\u0646", "word_translation": "If", "word_transliteration": "la-in"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u06e1", "word_translation": "(do) not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u062a\u064e\u0647\u0650", "word_translation": "cease", "word_transliteration": "yantahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "the hypocrites", "word_transliteration": "al-munafiquna"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0647\u0650\u0645", "word_translation": "their hearts", "word_transliteration": "qulubihim"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0631\u064e\u0636\u065e", "word_translation": "(is) a disease", "word_transliteration": "maradun"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u062c\u0650\u0641\u064f\u0648\u0646\u064e", "word_translation": "and those who spread rumors", "word_transliteration": "wal-mur'jifuna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u062f\u0650\u064a\u0646\u064e\u0629\u0650", "word_translation": "the city", "word_transliteration": "al-madinati"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064f\u063a\u06e1\u0631\u0650\u064a\u064e\u0646\u0651\u064e\u0643\u064e", "word_translation": "We will let you overpower them", "word_transliteration": "lanugh'riyannaka"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u0645\u06e1", "word_translation": "We will let you overpower them", "word_transliteration": "bihim"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u062c\u064e\u0627\u0648\u0650\u0631\u064f\u0648\u0646\u064e\u0643\u064e", "word_translation": "they will remain your neighbors", "word_transliteration": "yujawirunaka"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627\u0653", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627", "word_translation": "(for) a little", "word_transliteration": "qalilan"}]}