{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e \u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e \u0642\u064e\u0627\u0644\u064e \u0623\u064e\u062a\u064f\u0645\u0650\u062f\u0651\u064f\u0648\u0646\u064e\u0646\u0650 \u0628\u0650\u0645\u064e\u0627\u0644\u0656 \u0641\u064e\u0645\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0646\u0650\u06e6\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u062e\u064e\u064a\u06e1\u0631\u065e \u0645\u0651\u0650\u0645\u0651\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0643\u064f\u0645\u06da \u0628\u064e\u0644\u06e1 \u0623\u064e\u0646\u062a\u064f\u0645 \u0628\u0650\u0647\u064e\u062f\u0650\u064a\u0651\u064e\u062a\u0650\u0643\u064f\u0645\u06e1 \u062a\u064e\u0641\u06e1\u0631\u064e\u062d\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "So when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "came", "word_transliteration": "jaa"}, {"word_arabic_uthmani": "\u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e", "word_translation": "(to) Sulaiman", "word_transliteration": "sulaymana"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "he said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064f\u0645\u0650\u062f\u0651\u064f\u0648\u0646\u064e\u0646\u0650", "word_translation": "Will you provide me", "word_transliteration": "atumiddunani"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627\u0644\u0656", "word_translation": "with wealth", "word_transliteration": "bimalin"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627\u0653", "word_translation": "But what", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0646\u0650\u06e6\u064e", "word_translation": "Allah has given me", "word_transliteration": "ataniya"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah has given me", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u065e", "word_translation": "(is) better", "word_transliteration": "khayrun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0627\u0653", "word_translation": "than what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0643\u064f\u0645\u06da", "word_translation": "He has given you", "word_transliteration": "atakum"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645", "word_translation": "you", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u064e\u062f\u0650\u064a\u0651\u064e\u062a\u0650\u0643\u064f\u0645\u06e1", "word_translation": "in your gift", "word_transliteration": "bihadiyyatikum"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u06e1\u0631\u064e\u062d\u064f\u0648\u0646\u064e", "word_translation": "rejoice", "word_transliteration": "tafrahuna"}]}