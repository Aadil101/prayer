{"ayahs": {"ayah_uthmani": "\u06de\u0642\u064f\u0644\u06e1 \u0623\u064e\u0626\u0650\u0646\u0651\u064e\u0643\u064f\u0645\u06e1 \u0644\u064e\u062a\u064e\u0643\u06e1\u0641\u064f\u0631\u064f\u0648\u0646\u064e \u0628\u0650\u0671\u0644\u0651\u064e\u0630\u0650\u064a \u062e\u064e\u0644\u064e\u0642\u064e \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0641\u0650\u064a \u064a\u064e\u0648\u06e1\u0645\u064e\u064a\u06e1\u0646\u0650 \u0648\u064e\u062a\u064e\u062c\u06e1\u0639\u064e\u0644\u064f\u0648\u0646\u064e \u0644\u064e\u0647\u064f\u06e5\u0653 \u0623\u064e\u0646\u062f\u064e\u0627\u062f\u0657\u0627\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0631\u064e\u0628\u0651\u064f \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0623\u064e\u0626\u0650\u0646\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "Do you indeed", "word_transliteration": "a-innakum"}, {"word_arabic_uthmani": "\u0644\u064e\u062a\u064e\u0643\u06e1\u0641\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "[surely] disbelieve", "word_transliteration": "latakfuruna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "in the One Who", "word_transliteration": "bi-alladhi"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064e", "word_translation": "created", "word_transliteration": "khalaqa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "the earth", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e\u064a\u06e1\u0646\u0650", "word_translation": "two periods", "word_transliteration": "yawmayni"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u062c\u06e1\u0639\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "and you set up", "word_transliteration": "wataj'aluna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5\u0653", "word_translation": "with Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062f\u064e\u0627\u062f\u0657\u0627\u06da", "word_translation": "rivals", "word_transliteration": "andadan"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f", "word_translation": "(is the) Lord", "word_transliteration": "rabbu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the worlds", "word_transliteration": "al-'alamina"}]}