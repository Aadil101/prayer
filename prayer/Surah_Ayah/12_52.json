{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0644\u0650\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064e \u0623\u064e\u0646\u0651\u0650\u064a \u0644\u064e\u0645\u06e1 \u0623\u064e\u062e\u064f\u0646\u06e1\u0647\u064f \u0628\u0650\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650 \u0648\u064e\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0627 \u064a\u064e\u0647\u06e1\u062f\u0650\u064a \u0643\u064e\u064a\u06e1\u062f\u064e \u0671\u0644\u06e1\u062e\u064e\u0627\u0653\u0626\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064e", "word_translation": "he may know", "word_transliteration": "liya'lama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u0650\u064a", "word_translation": "that I", "word_transliteration": "anni"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u064f\u0646\u06e1\u0647\u064f", "word_translation": "[I] betray him", "word_transliteration": "akhun'hu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650", "word_translation": "in secret", "word_transliteration": "bil-ghaybi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e", "word_translation": "and that", "word_transliteration": "wa-anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062f\u0650\u064a", "word_translation": "guide", "word_transliteration": "yahdi"}, {"word_arabic_uthmani": "\u0643\u064e\u0