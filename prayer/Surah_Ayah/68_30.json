{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0623\u064e\u0642\u06e1\u0628\u064e\u0644\u064e \u0628\u064e\u0639\u06e1\u0636\u064f\u0647\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u0649\u0670 \u0628\u064e\u0639\u06e1\u0636\u0656 \u064a\u064e\u062a\u064e\u0644\u064e\u0670\u0648\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0642\u06e1\u0628\u064e\u0644\u064e", "word_translation": "Then approached", "word_transliteration": "fa-aqbala"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064f\u0647\u064f\u0645\u06e1", "word_translation": "some of them", "word_transliteration": "ba'duhum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u0656", "word_translation": "others", "word_transliteration": "ba'din"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0644\u064e\u0670\u0648\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "blaming each other", "word_transliteration": "yatalawamuna"}]}