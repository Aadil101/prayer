{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e\u062a\u06e1 \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u06e1\u0645\u064f\u0644\u064f\u0648\u0643\u064e \u0625\u0650\u0630\u064e\u0627 \u062f\u064e\u062e\u064e\u0644\u064f\u0648\u0627\u0652 \u0642\u064e\u0631\u06e1\u064a\u064e\u0629\u064b \u0623\u064e\u0641\u06e1\u0633\u064e\u062f\u064f\u0648\u0647\u064e\u0627 \u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0639\u0650\u0632\u0651\u064e\u0629\u064e \u0623\u064e\u0647\u06e1\u0644\u0650\u0647\u064e\u0627\u0653 \u0623\u064e\u0630\u0650\u0644\u0651\u064e\u0629\u0657\u06da \u0648\u064e\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e\u062a\u06e1", "word_translation": "She said", "word_transliteration": "qalat"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0644\u064f\u0648\u0643\u064e", "word_translation": "the kings", "word_transliteration": "al-muluka"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u062f\u064e\u062e\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "they enter", "word_transliteration": "dakhalu"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u06e1\u064a\u064e\u0629\u064b", "word_translation": "a town", "word_transliteration": "qaryatan"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u06e1\u0633\u064e\u062f\u064f\u0648\u0647\u064e\u0627", "word_translation": "they ruin it", "word_transliteration": "afsaduha"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "and make", "word_transliteration": "waja'alu"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u0650\u0632\u0651\u064e\u0629\u064e", "word_translation": "(the) most honorable", "word_transliteration": "a'izzata"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u0650\u0647\u064e\u0627\u0653", "word_translation": "(of) its people", "word_transliteration": "ahliha"}, {"word_arabic_uthmani": "\u0623\u064e\u0630\u0650\u0644\u0651\u064e\u0629\u0657\u06da", "word_translation": "(the) lowest", "word_transliteration": "adhillatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "And thus", "word_transliteration": "wakadhalika"}, {"word_arabic_uthmani": "\u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "they do", "word_transliteration": "yaf 'aluna"}]}