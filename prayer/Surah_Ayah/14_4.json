{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646 \u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064d \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u0650\u0644\u0650\u0633\u064e\u0627\u0646\u0650 \u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6 \u0644\u0650\u064a\u064f\u0628\u064e\u064a\u0651\u0650\u0646\u064e \u0644\u064e\u0647\u064f\u0645\u06e1\u06d6 \u0641\u064e\u064a\u064f\u0636\u0650\u0644\u0651\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0648\u064e\u064a\u064e\u0647\u06e1\u062f\u0650\u064a \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We sent", "word_transliteration": "arsalna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064d", "word_translation": "Messenger", "word_transliteration": "rasulin"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u0650\u0644\u0650\u0633\u064e\u0627\u0646\u0650", "word_translation": "with the language", "word_transliteration": "bilisani"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6", "word_translation": "(of) his people", "word_transliteration": "qawmihi"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u0628\u064e\u064a\u0651\u0650\u0646\u064e",