{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0646\u06e2\u0628\u0650\u062a\u064f \u0644\u064e\u0643\u064f\u0645 \u0628\u0650\u0647\u0650 \u0671\u0644\u0632\u0651\u064e\u0631\u06e1\u0639\u064e \u0648\u064e\u0671\u0644\u0632\u0651\u064e\u064a\u06e1\u062a\u064f\u0648\u0646\u064e \u0648\u064e\u0671\u0644\u0646\u0651\u064e\u062e\u0650\u064a\u0644\u064e \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0646\u064e\u0670\u0628\u064e \u0648\u064e\u0645\u0650\u0646 \u0643\u064f\u0644\u0651\u0650 \u0671\u0644\u062b\u0651\u064e\u0645\u064e\u0631\u064e\u0670\u062a\u0650\u06da \u0625\u0650\u0646\u0651\u064e \u0641\u0650\u064a \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0644\u064e\u0623\u0653\u064a\u064e\u0629\u0657 \u0644\u0651\u0650\u0642\u064e\u0648\u06e1\u0645\u0656 \u064a\u064e\u062a\u064e\u0641\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0646\u06e2\u0628\u0650\u062a\u064f", "word_translation": "He causes to grow", "word_transliteration": "yunbitu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650", "word_translation": "with it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0671\u0644\u0632\u0651\u064e\u0631\u06e1\u0639\u064e", "word_translation": "the crops", "word_transliteration": "al-zar'a"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0632\u0651\u064e\u064a\u06e1\u062a\u064f\u0648\u0646\u064e", "word_translation": "and the olives", "word_transliteration": "wal-zaytuna"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0646\u0651\u064e\u062e\u0650\u064a\u0644\u064e", "word_translation": "and the date-palms", "word_transliteration": "wal-nakhila"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0646\u064e\u0670\u0628\u064e", "word_translation": "and the grapes", "word_transliteration": "wal-a'naba"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646", "word_translation": "and of", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every kind", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0671\u0644\u062b\u0651\u064e\u0645\u064e\u0631\u064e\u0670\u062a\u0650\u06da", "word_translation": "(of) fruits", "word_transliteration": "al-thamarati"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u0653\u064a\u064e\u0629\u0657", "word_translation": "surely (is) a sign", "word_transliteration": "laayatan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0642\u064e\u0648\u06e1\u0645\u0656", "word_translation": "for a people", "word_transliteration": "liqawmin"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0641\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "who reflect", "word_transliteration": "yatafakkaruna"}]}