{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0642\u0650\u0645\u0650 \u0671\u0644\u0635\u0651\u064e\u0644\u064e\u0648\u0670\u0629\u064e \u0644\u0650\u062f\u064f\u0644\u064f\u0648\u0643\u0650 \u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u0650 \u0625\u0650\u0644\u064e\u0649\u0670 \u063a\u064e\u0633\u064e\u0642\u0650 \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650 \u0648\u064e\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064e \u0671\u0644\u06e1\u0641\u064e\u062c\u06e1\u0631\u0650\u06d6 \u0625\u0650\u0646\u0651\u064e \u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064e \u0671\u0644\u06e1\u0641\u064e\u062c\u06e1\u0631\u0650 \u0643\u064e\u0627\u0646\u064e \u0645\u064e\u0634\u06e1\u0647\u064f\u0648\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0642\u0650\u0645\u0650", "word_translation": "Establish", "word_transliteration": "aqimi"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0644\u064e\u0648\u0670\u0629\u064e", "word_translation": "the prayer", "word_transliteration": "al-salata"}, {"word_arabic_uthmani": "\u0644\u0650\u062f\u064f\u0644\u064f\u0648\u0643\u0650", "word_translation": "at the decline", "word_transliteration": "liduluki"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u0650", "word_translation": "(of) the sun", "word_transliteration": "al-shamsi"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "till", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u063a\u064e\u0633\u064e\u0642\u0650", "word_translation": "(the) darkness", "word_transliteration": "ghasaqi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650", "word_translation": "(of) the night", "word_transliteration": "al-layli"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064e", "word_translation": "and Quran", "word_transliteration": "waqur'ana"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u062c\u06e1\u0631\u0650\u06d6", "word_translation": "at dawn", "word_transliteration": "al-fajri"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064e", "word_translation": "the Quran", "word_transliteration": "qur'ana"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u062c\u06e1\u0631\u0650", "word_translation": "(at) the dawn", "word_transliteration": "al-fajri"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0645\u064e\u0634\u06e1\u0647\u064f\u0648\u062f\u0657\u0627", "word_translation": "ever witnessed", "word_transliteration": "mashhudan"}]}