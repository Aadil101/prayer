{"ayahs": {"ayah_uthmani": "\u062e\u064e\u0670\u0634\u0650\u0639\u064e\u0629\u064b \u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064f\u0647\u064f\u0645\u06e1 \u062a\u064e\u0631\u06e1\u0647\u064e\u0642\u064f\u0647\u064f\u0645\u06e1 \u0630\u0650\u0644\u0651\u064e\u0629\u065e\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064f\u0648\u0639\u064e\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u062e\u064e\u0670\u0634\u0650\u0639\u064e\u0629\u064b", "word_translation": "Humbled", "word_transliteration": "khashi'atan"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their eyes", "word_transliteration": "absaruhum"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u06e1\u0647\u064e\u0642\u064f\u0647\u064f\u0645\u06e1", "word_translation": "will cover them", "word_transliteration": "tarhaquhum"}, {"word_arabic_uthmani": "\u0630\u0650\u0644\u0651\u064e\u0629\u065e\u06da", "word_translation": "humiliation", "word_transliteration": "dhillatun"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u064f", "word_translation": "(is) the Day", "word_transliteration": "al-yawmu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "which", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they were", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0639\u064e\u062f\u064f\u0648\u0646\u064e", "word_translation": "promised", "word_transliteration": "yu'aduna"}]}