{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0630\u06e1 \u0646\u064f\u0633\u064e\u0648\u0651\u0650\u064a\u0643\u064f\u0645 \u0628\u0650\u0631\u064e\u0628\u0651\u0650 \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "When", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0646\u064f\u0633\u064e\u0648\u0651\u0650\u064a\u0643\u064f\u0645", "word_translation": "we equated you", "word_transliteration": "nusawwikum"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u0628\u0651\u0650", "word_translation": "with (the) Lord", "word_transliteration": "birabbi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the worlds", "word_transliteration": "al-'alamina"}]}