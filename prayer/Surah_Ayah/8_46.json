{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0646\u064e\u0670\u0632\u064e\u0639\u064f\u0648\u0627\u0652 \u0641\u064e\u062a\u064e\u0641\u06e1\u0634\u064e\u0644\u064f\u0648\u0627\u0652 \u0648\u064e\u062a\u064e\u0630\u06e1\u0647\u064e\u0628\u064e \u0631\u0650\u064a\u062d\u064f\u0643\u064f\u0645\u06e1\u06d6 \u0648\u064e\u0671\u0635\u06e1\u0628\u0650\u0631\u064f\u0648\u0653\u0627\u0652\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0645\u064e\u0639\u064e \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0628\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0627\u0652", "word_translation": "And obey", "word_transliteration": "wa-ati'u"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5", "word_translation": "and His Messenger", "word_transliteration": "warasulahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0646\u064e\u0670\u0632\u064e\u0639\u064f\u0648\u0627\u0652", "word_translation": "dispute", "word_transliteration": "tanaza'u"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0641\u06e1\u0634\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "lest you lose courage", "word_transliteration": "fatafshalu"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0630\u06e1\u0647\u064e\u0628\u064e", "word_translation": "and (would) depart", "word_transliteration": "watadhhaba"}, {"word_arabic_uthmani": "\u0631\u0650\u064a\u062d\u064f\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "your strength", "word_transliteration": "rihukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0635\u06e1\u0628\u0650\u0631\u064f\u0648\u0653\u0627\u0652\u06da", "word_translation": "and be patient", "word_transliteration": "wa-is'biru"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e", "word_translation": "(is) with", "word_transliteration": "ma'a"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0628\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the patient ones", "word_transliteration": "al-sabirina"}]}