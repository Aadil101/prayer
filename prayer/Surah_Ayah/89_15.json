{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0623\u064e\u0645\u0651\u064e\u0627 \u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e\u0670\u0646\u064f \u0625\u0650\u0630\u064e\u0627 \u0645\u064e\u0627 \u0671\u0628\u06e1\u062a\u064e\u0644\u064e\u0649\u0670\u0647\u064f \u0631\u064e\u0628\u0651\u064f\u0647\u064f\u06e5 \u0641\u064e\u0623\u064e\u0643\u06e1\u0631\u064e\u0645\u064e\u0647\u064f\u06e5 \u0648\u064e\u0646\u064e\u0639\u0651\u064e\u0645\u064e\u0647\u064f\u06e5 \u0641\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0631\u064e\u0628\u0651\u0650\u064a\u0653 \u0623\u064e\u0643\u06e1\u0631\u064e\u0645\u064e\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0645\u0651\u064e\u0627", "word_translation": "And as for", "word_transliteration": "fa-amma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e\u0670\u0646\u064f", "word_translation": "man", "word_transliteration": "al-insanu"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "does", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u062a\u064e\u0644\u064e\u0649\u0670\u0647\u064f", "word_translation": "try him", "word_transliteration": "ib'talahu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0647\u064f\u06e5", "word_translation": "his Lord", "word_transliteration": "rabbuhu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0643\u06e1\u0631\u064e\u0645\u064e\u0647\u064f\u06e5", "word_translation": "and is generous to him", "word_transliteration": "fa-akramahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u0639\u0651\u064e\u0645\u064e\u0647\u064f\u06e5", "word_translation": "and favors him", "word_transliteration": "wana'amahu"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "he says", "word_transliteration": "fayaqulu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a\u0653", "word_translation": "My Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u0631\u064e\u0645\u064e\u0646\u0650", "word_translation": "has honored me", "word_transliteration": "akramani"}]}