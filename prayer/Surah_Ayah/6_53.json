{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0641\u064e\u062a\u064e\u0646\u0651\u064e\u0627 \u0628\u064e\u0639\u06e1\u0636\u064e\u0647\u064f\u0645 \u0628\u0650\u0628\u064e\u0639\u06e1\u0636\u0656 \u0644\u0651\u0650\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650 \u0645\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645 \u0645\u0651\u0650\u0646\u06e2 \u0628\u064e\u064a\u06e1\u0646\u0650\u0646\u064e\u0627\u0653\u06d7 \u0623\u064e\u0644\u064e\u064a\u06e1\u0633\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u0650\u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064e \u0628\u0650\u0671\u0644\u0634\u0651\u064e\u0640\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "And thus", "word_transliteration": "wakadhalika"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0646\u0651\u064e\u0627", "word_translation": "We try", "word_transliteration": "fatanna"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064e\u0647\u064f\u0645", "word_translation": "some of them", "word_transliteration": "ba'dahum"}, {"word_arabic_uthmani": "\u0628\u0650\u0628\u064e\u0639\u06e1\u0636\u0656", "word_translation": "with others", "word_transliteration": "biba'din"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "that they say", "word_transliteration": "liyaqulu"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650", "word_translation": "Are these", "word_transliteration": "ahaulai"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u0651\u064e", "word_translation": "(whom has been) favored", "word_transliteration": "manna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(by) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645", "word_translation": "[upon them]", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e2", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u0650\u0646\u064e\u0627\u0653\u06d7", "word_translation": "among us", "word_transliteration": "baynina"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "is not", "word_transliteration": "alaysa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064e", "word_translation": "most knowing", "word_transliteration": "bi-a'lama"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0634\u0651\u064e\u0640\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "of those who are grateful", "word_transliteration": "bil-shakirina"}]}