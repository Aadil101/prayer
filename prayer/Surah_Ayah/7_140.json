{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0623\u064e\u063a\u064e\u064a\u06e1\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0623\u064e\u0628\u06e1\u063a\u0650\u064a\u0643\u064f\u0645\u06e1 \u0625\u0650\u0644\u064e\u0670\u0647\u0657\u0627 \u0648\u064e\u0647\u064f\u0648\u064e \u0641\u064e\u0636\u0651\u064e\u0644\u064e\u0643\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0623\u064e\u063a\u064e\u064a\u06e1\u0631\u064e", "word_translation": "Should other than", "word_transliteration": "aghayra"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u063a\u0650\u064a\u0643\u064f\u0645\u06e1", "word_translation": "I seek for you", "word_transliteration": "abghikum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u0657\u0627", "word_translation": "a god", "word_transliteration": "ilahan"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "while He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u0651\u064e\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "has preferred you", "word_transliteration": "faddalakum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "the worlds", "word_transliteration": "al-'alamina"}]}