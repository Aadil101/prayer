{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u063a\u06e1\u0641\u0650\u0631\u06e1 \u0644\u0650\u0623\u064e\u0628\u0650\u064a\u0653 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0643\u064e\u0627\u0646\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u0636\u0651\u064e\u0627\u0653\u0644\u0651\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u063a\u06e1\u0641\u0650\u0631\u06e1", "word_translation": "And forgive", "word_transliteration": "wa-igh'fir"}, {"word_arabic_uthmani": "\u0644\u0650\u0623\u064e\u0628\u0650\u064a\u0653", "word_translation": "my father", "word_transliteration": "li-abi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed he", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0636\u0651\u064e\u0627\u0653\u0644\u0651\u0650\u064a\u0646\u064e", "word_translation": "those astray", "word_transliteration": "al-dalina"}]}