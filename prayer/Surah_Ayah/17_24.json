{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u062e\u06e1\u0641\u0650\u0636\u06e1 \u0644\u064e\u0647\u064f\u0645\u064e\u0627 \u062c\u064e\u0646\u064e\u0627\u062d\u064e \u0671\u0644\u0630\u0651\u064f\u0644\u0651\u0650 \u0645\u0650\u0646\u064e \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0629\u0650 \u0648\u064e\u0642\u064f\u0644 \u0631\u0651\u064e\u0628\u0651\u0650 \u0671\u0631\u06e1\u062d\u064e\u0645\u06e1\u0647\u064f\u0645\u064e\u0627 \u0643\u064e\u0645\u064e\u0627 \u0631\u064e\u0628\u0651\u064e\u064a\u064e\u0627\u0646\u0650\u064a \u0635\u064e\u063a\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u062e\u06e1\u0641\u0650\u0636\u06e1", "word_translation": "And lower", "word_transliteration": "wa-ikh'fid"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064e\u0627", "word_translation": "to them", "word_transliteration": "lahuma"}, {"word_arabic_uthmani": "\u062c\u064e\u0646\u064e\u0627\u062d\u064e", "word_translation": "(the) wing", "word_transliteration": "janaha"}, {"word_arabic_uthmani": "\u0671\u0644\u0630\u0651\u064f\u0644\u0651\u0650", "word_translation": "(of) humility", "word_transliteration": "al-dhuli"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "(out) of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0629\u0650", "word_translation": "[the] mercy", "word_transliteration": "al-rahmati"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064f\u0644", "word_translation": "and say", "word_transliteration": "waqul"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650", "word_translation": "My Lord!", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0671\u0631\u06e1\u062d\u064e\u0645\u06e1\u0647\u064f\u0645\u064e\u0627", "word_translation": "Have mercy on both of them", "word_transliteration": "ir'hamhuma"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u0627", "word_translation": "as", "word_transliteration": "kama"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u064a\u064e\u0627\u0646\u0650\u064a", "word_translation": "they brought me up", "word_transliteration": "rabbayani"}, {"word_arabic_uthmani": "\u0635\u064e\u063a\u0650\u064a\u0631\u0657\u0627", "word_translation": "(when I was) small", "word_transliteration": "saghiran"}]}