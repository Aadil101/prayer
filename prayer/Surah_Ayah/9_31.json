{"ayahs": {"ayah_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u062d\u06e1\u0628\u064e\u0627\u0631\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0631\u064f\u0647\u06e1\u0628\u064e\u0670\u0646\u064e\u0647\u064f\u0645\u06e1 \u0623\u064e\u0631\u06e1\u0628\u064e\u0627\u0628\u0657\u0627 \u0645\u0651\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u0633\u0650\u064a\u062d\u064e \u0671\u0628\u06e1\u0646\u064e \u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064e \u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064f\u0645\u0650\u0631\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0644\u0651\u064e\u0627 \u0644\u0650\u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0644\u064e\u0670\u0647\u0657\u0627 \u0648\u064e\u0670\u062d\u0650\u062f\u0657\u0627\u06d6 \u0644\u0651\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0670\u0647\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0647\u064f\u0648\u064e\u06da \u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0647\u064f\u06e5 \u0639\u064e\u0645\u0651\u064e\u0627 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0653\u0627\u0652", "word_translation": "They have taken", "word_transliteration": "ittakhadhu"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u06e1\u0628\u064e\u0627\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their rabbis", "word_transliteration": "ahbarahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064f\u0647\u06e1\u0628\u064e\u0670\u0646\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and their monks", "word_transliteration": "waruh'banahum"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0628\u064e\u0627\u0628\u0657\u0627", "word_translation": "(as) Lords", "word_transliteration": "arbaban"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "besides", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u0633\u0650\u064a\u062d\u064e", "word_translation": "and the Messiah", "word_transliteration": "wal-masiha"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u0646\u064e", "word_translation": "son", "word_transliteration": "ib'na"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064e", "word_translation": "(of) Maryam", "word_transliteration": "maryama"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0650\u0631\u064f\u0648\u0653\u0627\u0652", "word_translation": "they were commanded", "word_transliteration": "umiru"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0653\u0627\u0652", "word_translation": "that they worship", "word_transliteration": "liya'budu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u0657\u0627", "word_translation": "One God", "word_transliteration": "ilahan"}, {"word_arabic_uthmani": "\u0648\u064e\u0670\u062d\u0650\u062f\u0657\u0627\u06d6", "word_translation": "One God", "word_transliteration": "wahidan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627\u0653", "word_translation": "(There) is no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064e", "word_translation": "god", "word_transliteration": "ilaha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e\u06da", "word_translation": "Him", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0647\u064f\u06e5", "word_translation": "Glory be to Him", "word_transliteration": "sub'hanahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "from what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "they associate (with Him)", "word_transliteration": "yush'rikuna"}]}