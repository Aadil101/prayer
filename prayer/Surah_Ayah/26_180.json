{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064f\u0643\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0645\u0650\u0646\u06e1 \u0623\u064e\u062c\u06e1\u0631\u064d\u06d6 \u0625\u0650\u0646\u06e1 \u0623\u064e\u062c\u06e1\u0631\u0650\u064a\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0639\u064e\u0644\u064e\u0649\u0670 \u0631\u064e\u0628\u0651\u0650 \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064f\u0643\u064f\u0645\u06e1", "word_translation": "I ask (of) you", "word_transliteration": "asalukum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "for it", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0631\u064d\u06d6", "word_translation": "payment", "word_transliteration": "ajrin"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u06e1", "word_translation": "Not", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0631\u0650\u064a\u064e", "word_translation": "(is) my payment", "word_transliteration": "ajriya"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "from", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "(the) Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the