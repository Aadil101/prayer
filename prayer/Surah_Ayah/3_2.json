{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0670\u0647\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u062d\u064e\u064a\u0651\u064f \u0671\u0644\u06e1\u0642\u064e\u064a\u0651\u064f\u0648\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627\u0653", "word_translation": "(there is) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064e", "word_translation": "God", "word_transliteration": "ilaha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "Him", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u064a\u0651\u064f", "word_translation": "the Ever-Living", "word_transliteration": "al-hayu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u064a\u0651\u064f\u0648\u0645\u064f", "word_translation": "the Sustainer of all that exists", "word_transliteration": "al-qayumu"}]}