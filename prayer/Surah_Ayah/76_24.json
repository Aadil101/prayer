{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0671\u0635\u06e1\u0628\u0650\u0631\u06e1 \u0644\u0650\u062d\u064f\u0643\u06e1\u0645\u0650 \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0648\u064e\u0644\u064e\u0627 \u062a\u064f\u0637\u0650\u0639\u06e1 \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0621\u064e\u0627\u062b\u0650\u0645\u064b\u0627 \u0623\u064e\u0648\u06e1 \u0643\u064e\u0641\u064f\u0648\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0671\u0635\u06e1\u0628\u0650\u0631\u06e1", "word_translation": "So be patient", "word_transliteration": "fa-is'bir"}, {"word_arabic_uthmani": "\u0644\u0650\u062d\u064f\u0643\u06e1\u0645\u0650", "word_translation": "for (the) Command", "word_transliteration": "lihuk'mi"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "(of) your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064f\u0637\u0650\u0639\u06e1", "word_translation": "obey", "word_transliteration": "tuti'"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "from them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062b\u0650\u0645\u064b\u0627", "word_translation": "any sinner", "word_transliteration": "athiman"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064f\u0648\u0631\u0657\u0627", "word_translation": "disbeliever", "word_transliteration": "kafuran"}]}