{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u064e\u0627 \u0645\u064e\u0646\u064e\u0670\u0641\u0650\u0639\u064f \u0648\u064e\u0645\u064e\u0634\u064e\u0627\u0631\u0650\u0628\u064f\u06da \u0623\u064e\u0641\u064e\u0644\u064e\u0627 \u064a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "And for them", "word_transliteration": "walahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u064e\u0670\u0641\u0650\u0639\u064f", "word_translation": "(are) benefits", "word_transliteration": "manafi'u"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0634\u064e\u0627\u0631\u0650\u0628\u064f\u06da", "word_translation": "and drinks", "word_transliteration": "wamasharibu"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0627", "word_translation": "so (will) not", "word_transliteration": "afala"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "they give thanks", "word_transliteration": "yashkuruna"}]}