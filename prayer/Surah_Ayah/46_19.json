{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u0650\u0643\u064f\u0644\u0651\u0656 \u062f\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u065e \u0645\u0651\u0650\u0645\u0651\u064e\u0627 \u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627\u0652\u06d6 \u0648\u064e\u0644\u0650\u064a\u064f\u0648\u064e\u0641\u0651\u0650\u064a\u064e\u0647\u064f\u0645\u06e1 \u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064f\u0638\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0643\u064f\u0644\u0651\u0656", "word_translation": "And for all", "word_transliteration": "walikullin"}, {"word_arabic_uthmani": "\u062f\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u065e", "word_translation": "(are) degrees", "word_transliteration": "darajatun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0627", "word_translation": "for what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627\u0652\u06d6", "word_translation": "they did", "word_transliteration": "amilu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u064f\u0648\u064e\u0641\u0651\u0650\u064a\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and that He may fully compensate them", "word_transliteration": "waliyuwaffiyahum"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "(for) their deeds", "word_transliteration": "a'malahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "will not be wronged", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0638\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "will not be wronged", "word_transliteration": "yuz'lamuna"}]}