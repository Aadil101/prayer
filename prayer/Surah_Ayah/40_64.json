{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u062c\u064e\u0639\u064e\u0644\u064e \u0644\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0642\u064e\u0631\u064e\u0627\u0631\u0657\u0627 \u0648\u064e\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064e \u0628\u0650\u0646\u064e\u0627\u0653\u0621\u0657 \u0648\u064e\u0635\u064e\u0648\u0651\u064e\u0631\u064e\u0643\u064f\u0645\u06e1 \u0641\u064e\u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064e \u0635\u064f\u0648\u064e\u0631\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0631\u064e\u0632\u064e\u0642\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0637\u0651\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1\u06d6 \u0641\u064e\u062a\u064e\u0628\u064e\u0627\u0631\u064e\u0643\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0631\u064e\u0628\u0651\u064f \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u064e", "word_translation": "made", "word_transliteration": "ja'ala"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u064f", "word_translation": "for you", "word_transliteration": "lakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "the earth", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u064e\u0627\u0631\u0657\u0627", "word_translation": "a place of settlement", "word_transliteration": "qararan"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064e", "word_translation": "and the sky", "word_transliteration": "wal-samaa"}, {"word_arabic_uthmani": "\u0628\u0650\u0646\u064e\u0627\u0653\u0621\u0657", "word_translation": "a canopy", "word_transliteration": "binaan"}, {"word_arabic_uthmani": "\u0648\u064e\u0635\u064e\u0648\u0651\u064e\u0631\u064e\u0643\u064f\u0645\u06e1", "word_translation": "and He formed you", "word_transliteration": "wasawwarakum"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064e", "word_translation": "and perfected", "word_transliteration": "fa-ahsana"}, {"word_arabic_uthmani": "\u0635\u064f\u0648\u064e\u0631\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your forms", "word_transliteration": "suwarakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0632\u064e\u0642\u064e\u0643\u064f\u0645", "word_translation": "and provided you", "word_transliteration": "warazaqakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0637\u0651\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650\u06da", "word_translation": "the good things", "word_transliteration": "al-tayibati"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f", "word_translation": "That", "word_transliteration": "dhalikumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "your Lord", "word_transliteration": "rabbukum"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0628\u064e\u0627\u0631\u064e\u0643\u064e", "word_translation": "Then blessed is", "word_transliteration": "fatabaraka"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f", "word_translation": "(the) Lord", "word_transliteration": "rabbu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the worlds", "word_transliteration": "al-'alamina"}]}