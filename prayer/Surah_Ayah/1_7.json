{"ayahs": {"ayah_uthmani": "\u0635\u0650\u0631\u064e\u0670\u0637\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0623\u064e\u0646\u06e1\u0639\u064e\u0645\u06e1\u062a\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u063a\u064e\u064a\u06e1\u0631\u0650 \u0671\u0644\u06e1\u0645\u064e\u063a\u06e1\u0636\u064f\u0648\u0628\u0650 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u0671\u0644\u0636\u0651\u064e\u0627\u0653\u0644\u0651\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0635\u0650\u0631\u064e\u0670\u0637\u064e", "word_translation": "(The) path", "word_transliteration": "sirata"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1\u0639\u064e\u0645\u06e1\u062a\u064e", "word_translation": "You have bestowed (Your) Favors", "word_transliteration": "an'amta"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "on them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u063a\u064e\u064a\u06e1\u0631\u0650", "word_translation": "not (of)", "word_transliteration": "ghayri"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u063a\u06e1\u0636\u064f\u0648\u0628\u0650", "word_translation": "those who earned (Your) wrath", "word_transliteration": "al-maghdubi"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "on themselves", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0671\u0644\u0636\u0651\u064e\u0627\u0653\u0644\u0651\u0650\u064a\u0646\u064e", "word_translation": "(of) those who go astray", "word_transliteration": "al-dalina"}]}