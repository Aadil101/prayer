{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0643\u064e\u0627\u0646\u064e \u0644\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed, he", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "(did)", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f", "word_translation": "believe", "word_transliteration": "yu'minu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "in Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u0650", "word_translation": "the Most Great", "word_transliteration": "al-'azimi"}]}