{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0641\u064e\u062c\u0651\u064e\u0631\u06e1\u0646\u064e\u0627 \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0639\u064f\u064a\u064f\u0648\u0646\u0657\u0627 \u0641\u064e\u0671\u0644\u06e1\u062a\u064e\u0642\u064e\u0649 \u0671\u0644\u06e1\u0645\u064e\u0627\u0653\u0621\u064f \u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u0645\u06e1\u0631\u0656 \u0642\u064e\u062f\u06e1 \u0642\u064f\u062f\u0650\u0631\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0641\u064e\u062c\u0651\u064e\u0631\u06e1\u0646\u064e\u0627", "word_translation": "And We caused to burst", "word_transliteration": "wafajjarna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "the earth", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0639\u064f\u064a\u064f\u0648\u0646\u0657\u0627", "word_translation": "(with) springs", "word_transliteration": "uyunan"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0644\u06e1\u062a\u064e\u0642\u064e\u0649", "word_translation": "so met", "word_transliteration": "fal-taqa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0627\u0653\u0621\u064f", "word_translation": "the water(s)", "word_transliteration": "al-mau"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "for", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0631\u0656", "word_translation": "a matter", "word_transliteration": "amrin"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "already", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u0642\u064f\u062f\u0650\u0631\u064e", "word_translation": "predestined", "word_transliteration": "qudira"}]}