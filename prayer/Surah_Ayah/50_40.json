{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650 \u0641\u064e\u0633\u064e\u0628\u0651\u0650\u062d\u06e1\u0647\u064f \u0648\u064e\u0623\u064e\u062f\u06e1\u0628\u064e\u0670\u0631\u064e \u0671\u0644\u0633\u0651\u064f\u062c\u064f\u0648\u062f\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e", "word_translation": "And of", "word_transliteration": "wamina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650", "word_translation": "the night", "word_transliteration": "al-layli"}, {"word_arabic_uthmani": "\u0641\u064e\u0633\u064e\u0628\u0651\u0650\u062d\u06e1\u0647\u064f", "word_translation": "glorify Him", "word_transliteration": "fasabbih'hu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062f\u06e1\u0628\u064e\u0670\u0631\u064e", "word_translation": "and after", "word_transliteration": "wa-adbara"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064f\u062c\u064f\u0648\u062f\u0650", "word_translation": "the prostration", "word_transliteration": "al-sujudi"}]}