{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0633\u06e1\u062a\u064e\u0648\u0650\u064a \u0671\u0644\u06e1\u062d\u064e\u0633\u064e\u0646\u064e\u0629\u064f \u0648\u064e\u0644\u064e\u0627 \u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0626\u064e\u0629\u064f\u06da \u0671\u062f\u06e1\u0641\u064e\u0639\u06e1 \u0628\u0650\u0671\u0644\u0651\u064e\u062a\u0650\u064a \u0647\u0650\u064a\u064e \u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064f \u0641\u064e\u0625\u0650\u0630\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064e \u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u06e5 \u0639\u064e\u062f\u064e\u0670\u0648\u064e\u0629\u065e \u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5 \u0648\u064e\u0644\u0650\u064a\u0651\u064c \u062d\u064e\u0645\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0633\u06e1\u062a\u064e\u0648\u0650\u064a", "word_translation": "are equal", "word_transliteration": "tastawi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0633\u064e\u0646\u064e\u0629\u064f", "word_translation": "the good (deed)", "word_transliteration": "al-hasanatu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0626\u064e\u0629\u064f\u06da", "word_translation": "the evil (deed)", "word_transliteration": "al-sayi-atu"}, {"word_arabic_uthmani": "\u0671\u062f\u06e1\u0641\u064e\u0639\u06e1", "word_translation": "Repel", "word_transliteration": "id'fa'"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0651\u064e\u062a\u0650\u064a", "word_translation": "by (that) which", "word_transliteration": "bi-allati"}, {"word_arabic_uthmani": "\u0647\u0650\u064a\u064e", "word_translation": "[it]", "word_transliteration": "hiya"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064f", "word_translation": "(is) better", "word_transliteration": "ahsanu"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0630\u064e\u0627", "word_translation": "then behold!", "word_transliteration": "fa-idha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "One who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064e", "word_translation": "between you", "word_transliteration": "baynaka"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u06e5", "word_translation": "and between him", "word_transliteration": "wabaynahu"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064e\u0670\u0648\u064e\u0629\u065e", "word_translation": "(was) enmity", "word_transliteration": "adawatun"}, {"word_arabic_uthmani": "\u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "as if he", "word_transliteration": "ka-annahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u0651\u064c", "word_translation": "(was) a friend", "word_transliteration": "waliyyun"}, {"word_arabic_uthmani": "\u062d\u064e\u0645\u0650\u064a\u0645\u065e", "word_translation": "intimate", "word_transliteration": "hamimun"}]}