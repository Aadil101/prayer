{"ayahs": {"ayah_uthmani": "\u0644\u0651\u064e\u0627 \u064a\u064e\u0633\u06e1\u0645\u064e\u0639\u064f\u0648\u0646\u064e \u0641\u0650\u064a\u0647\u064e\u0627 \u0644\u064e\u063a\u06e1\u0648\u064b\u0627 \u0625\u0650\u0644\u0651\u064e\u0627 \u0633\u064e\u0644\u064e\u0670\u0645\u0657\u0627\u06d6 \u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1 \u0631\u0650\u0632\u06e1\u0642\u064f\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u064e\u0627 \u0628\u064f\u0643\u06e1\u0631\u064e\u0629\u0657 \u0648\u064e\u0639\u064e\u0634\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u0645\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "they will hear", "word_transliteration": "yasma'una"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0644\u064e\u063a\u06e1\u0648\u064b\u0627", "word_translation": "vain talk", "word_transliteration": "laghwan"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0633\u064e\u0644\u064e\u0670\u0645\u0657\u0627\u06d6", "word_translation": "peace", "word_transliteration": "salaman"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "And for them", "word_transliteration": "walahum"}, {"word_arabic_uthmani": "\u0631\u0650\u0632\u06e1\u0642\u064f\u0647\u064f\u0645\u06e1", "word_translation": "(is) their provision", "word_transliteration": "riz'quhum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0628\u064f\u0643\u06e1\u0631\u064e\u0629\u0657", "word_translation": "morning", "word_transliteration": "buk'ratan"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0634\u0650\u064a\u0651\u0657\u0627", "word_translation": "and evening", "word_transliteration": "wa'ashiyyan"}]}