{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0639\u064e\u0670\u0644\u0650\u0645\u064f \u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650 \u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0647\u064e\u0670\u062f\u064e\u0629\u0650 \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f \u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0644\u0650\u0645\u064f", "word_translation": "(is the) Knower", "word_transliteration": "alimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650", "word_translation": "(of) the hidden", "word_transliteration": "al-ghaybi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0647\u064e\u0670\u062f\u064e\u0629\u0650", "word_translation": "and the witnessed", "word_transliteration": "wal-shahadati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f", "word_translation": "the All-Mighty", "word_transliteration": "al-'azizu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f", "word_translation": "the Most Merciful", "word_transliteration": "al-rahimu"}]}