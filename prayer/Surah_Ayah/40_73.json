{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u0642\u0650\u064a\u0644\u064e \u0644\u064e\u0647\u064f\u0645\u06e1 \u0623\u064e\u064a\u06e1\u0646\u064e \u0645\u064e\u0627 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u062a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0642\u0650\u064a\u0644\u064e", "word_translation": "it will be said", "word_transliteration": "qila"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "to them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u0646\u064e", "word_translation": "Where", "word_transliteration": "ayna"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "(is) that which", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you used (to)", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u062a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "associate", "word_transliteration": "tush'rikuna"}]}