{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0646\u064e\u062c\u064e\u0627 \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u064e\u0627 \u0648\u064e\u0671\u062f\u0651\u064e\u0643\u064e\u0631\u064e \u0628\u064e\u0639\u06e1\u062f\u064e \u0623\u064f\u0645\u0651\u064e\u0629\u064d \u0623\u064e\u0646\u064e\u0627\u06e0 \u0623\u064f\u0646\u064e\u0628\u0651\u0650\u0626\u064f\u0643\u064f\u0645 \u0628\u0650\u062a\u064e\u0623\u06e1\u0648\u0650\u064a\u0644\u0650\u0647\u0650\u06e6 \u0641\u064e\u0623\u064e\u0631\u06e1\u0633\u0650\u0644\u064f\u0648\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "But said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "the one who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0646\u064e\u062c\u064e\u0627", "word_translation": "was saved", "word_transliteration": "naja"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u064e\u0627", "word_translation": "of the two", "word_transliteration": "min'huma"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u062f\u0651\u064e\u0643\u064e\u0631\u064e", "word_translation": "and remembered", "word_transliteration": "wa-iddakara"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u064e", "word_translation": "after", "word_transliteration": "ba'da"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u064e\u0629\u064d", "word_translation": "a period", "word_transliteration": "ummatin"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "I", "word_transliteration": "ana"}, {"word_arabic_uthmani": "\u0623\u064f\u0646\u064e\u0628\u0651\u0650\u0626\u064f\u0643\u064f\u0645", "word_translation": "[I] will inform you", "word_transliteration": "unabbi-ukum"}, {"word_arabic_uthmani": "\u0628\u0650\u062a\u064e\u0623\u06e1\u0648\u0650\u064a\u0