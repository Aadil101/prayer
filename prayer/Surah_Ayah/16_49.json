{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650\u06e4 \u064a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u06e4 \u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0645\u0650\u0646 \u062f\u064e\u0627\u0653\u0628\u0651\u064e\u0629\u0656 \u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650\u06e4", "word_translation": "And to Allah", "word_transliteration": "walillahi"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u06e4", "word_translation": "prostrate", "word_transliteration": "yasjudu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0653\u0628\u0651\u064e\u0629\u0656", "word_translation": "moving creatures", "word_transliteration": "dabbatin"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f", "word_translation": "and the Angels", "word_transliteration": "wal-malaikatu"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(are) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "arrogant", "word_transliteration": "yastakbiruna"}]}