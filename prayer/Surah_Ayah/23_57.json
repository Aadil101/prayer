{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u06e1 \u062e\u064e\u0634\u06e1\u064a\u064e\u0629\u0650 \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645 \u0645\u0651\u064f\u0634\u06e1\u0641\u0650\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0647\u064f\u0645", "word_translation": "[they]", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062e\u064e\u0634\u06e1\u064a\u064e\u0629\u0650", "word_translation": "(the) fear", "word_transliteration": "khashyati"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645", "word_translation": "(of) their Lord", "word_transliteration": "rabbihim"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0634\u06e1\u0641\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "(are) cautious", "word_transliteration": "mush'fiquna"}]}