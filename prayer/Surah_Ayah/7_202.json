{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u062e\u06e1\u0648\u064e\u0670\u0646\u064f\u0647\u064f\u0645\u06e1 \u064a\u064e\u0645\u064f\u062f\u0651\u064f\u0648\u0646\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u063a\u064e\u064a\u0651\u0650 \u062b\u064f\u0645\u0651\u064e \u0644\u064e\u0627 \u064a\u064f\u0642\u06e1\u0635\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u062e\u06e1\u0648\u064e\u0670\u0646\u064f\u0647\u064f\u0645\u06e1", "word_translation": "But their brothers", "word_transliteration": "wa-ikh'wanuhum"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u064f\u062f\u0651\u064f\u0648\u0646\u064e\u0647\u064f\u0645\u06e1", "word_translation": "they plunge them", "word_transliteration": "yamuddunahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u064a\u0651\u0650", "word_translation": "the error", "word_transliteration": "al-ghayi"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0642\u06e1\u0635\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "they cease", "word_transliteration": "yuq'siruna"}]}