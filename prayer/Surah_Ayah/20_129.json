{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627 \u0643\u064e\u0644\u0650\u0645\u064e\u0629\u065e \u0633\u064e\u0628\u064e\u0642\u064e\u062a\u06e1 \u0645\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064e \u0644\u064e\u0643\u064e\u0627\u0646\u064e \u0644\u0650\u0632\u064e\u0627\u0645\u0657\u0627 \u0648\u064e\u0623\u064e\u062c\u064e\u0644\u065e \u0645\u0651\u064f\u0633\u064e\u0645\u0651\u0657\u0649"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "And if not", "word_transliteration": "walawla"}, {"word_arabic_uthmani": "\u0643\u064e\u0644\u0650\u0645\u064e\u0629\u065e", "word_translation": "(for) a Word", "word_transliteration": "kalimatun"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u064e\u0642\u064e\u062a\u06e1", "word_translation": "(that) preceded", "word_transliteration": "sabaqat"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "surely (would) have been", "word_transliteration": "lakana"}, {"word_arabic_uthmani": "\u0644\u0650\u0632\u064e\u0627\u0645\u0657\u0627", "word_translation": "an obligation", "word_transliteration": "lizaman"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062c\u064e\u0644\u065e", "word_translation": "and a term", "word_transliteration": "wa-ajalun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0633\u064e\u0645\u0651\u0657\u0649", "word_translation": "determined", "word_transliteration": "musamman"}]}