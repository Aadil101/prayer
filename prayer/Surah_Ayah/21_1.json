{"ayahs": {"ayah_uthmani": "\u0671\u0642\u06e1\u062a\u064e\u0631\u064e\u0628\u064e \u0644\u0650\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u062d\u0650\u0633\u064e\u0627\u0628\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u063a\u064e\u0641\u06e1\u0644\u064e\u0629\u0656 \u0645\u0651\u064f\u0639\u06e1\u0631\u0650\u0636\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0642\u06e1\u062a\u064e\u0631\u064e\u0628\u064e", "word_translation": "(Has) approached", "word_transliteration": "iq'taraba"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "for [the] mankind", "word_transliteration": "lilnnasi"}, {"word_arabic_uthmani": "\u062d\u0650\u0633\u064e\u0627\u0628\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their account", "word_transliteration": "hisabuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(are) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u063a\u064e\u0641\u06e1\u0644\u064e\u0629\u0656", "word_translation": "heedlessness", "word_transliteration": "ghaflatin"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0639\u06e1\u0631\u0650\u0636\u064f\u0648\u0646\u064e", "word_translation": "turning away", "word_transliteration": "mu'riduna"}]}