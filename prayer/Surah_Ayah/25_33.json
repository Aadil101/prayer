{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u064e\u0643\u064e \u0628\u0650\u0645\u064e\u062b\u064e\u0644\u064d \u0625\u0650\u0644\u0651\u064e\u0627 \u062c\u0650\u0626\u06e1\u0646\u064e\u0670\u0643\u064e \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0648\u064e\u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064e \u062a\u064e\u0641\u06e1\u0633\u0650\u064a\u0631\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u064e\u0643\u064e", "word_translation": "they come to you", "word_transliteration": "yatunaka"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u062b\u064e\u0644\u064d", "word_translation": "with an example", "word_transliteration": "bimathalin"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u062c\u0650\u0626\u06e1\u0646\u064e\u0670\u0643\u064e", "word_translation": "We bring you", "word_transliteration": "ji'naka"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "the truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064e", "word_translation": "and (the) best", "word_transliteration": "wa-ahsana"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u06e1\u0633\u0650\u064a\u0631\u064b\u0627", "word_translation": "explanation", "word_transliteration": "tafsiran"}]}