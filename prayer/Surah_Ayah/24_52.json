{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0646 \u064a\u064f\u0637\u0650\u0639\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5 \u0648\u064e\u064a\u064e\u062e\u06e1\u0634\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u064a\u064e\u062a\u0651\u064e\u0642\u06e1\u0647\u0650 \u0641\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0641\u064e\u0627\u0653\u0626\u0650\u0632\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "And whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u064a\u064f\u0637\u0650\u0639\u0650", "word_translation": "obeys", "word_transliteration": "yuti'i"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5", "word_translation": "and His Messenger", "word_transliteration": "warasulahu"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062e\u06e1\u0634\u064e", "word_translation": "and fears", "word_transliteration": "wayakhsha"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062a\u0651\u064e\u0642\u06e1\u0647\u0650", "word_translation": "and (is) conscious of Him", "word_transliteration": "wayattaqhi"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "then those", "word_transliteration": "fa-ulaika"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "[they]", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0627\u0653\u0626\u0