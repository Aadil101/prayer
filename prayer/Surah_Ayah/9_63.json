{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5 \u0645\u064e\u0646 \u064a\u064f\u062d\u064e\u0627\u062f\u0650\u062f\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5 \u0641\u064e\u0623\u064e\u0646\u0651\u064e \u0644\u064e\u0647\u064f\u06e5 \u0646\u064e\u0627\u0631\u064e \u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e \u062e\u064e\u0670\u0644\u0650\u062f\u0657\u0627 \u0641\u0650\u064a\u0647\u064e\u0627\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0671\u0644\u06e1\u062e\u0650\u0632\u06e1\u064a\u064f \u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Do not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652", "word_translation": "they know", "word_transliteration": "ya'lamu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "that he", "word_transliteration": "annahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u064e\u0627\u062f\u0650\u062f\u0650", "word_translation": "opposes", "word_transliteration": "yuhadidi"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5", "word_translation": "and His Messenger", "word_transliteration": "warasulahu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0646\u0651\u064e", "word_translation": "[then] that", "word_transliteration": "fa-anna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u0631\u064e", "word_translation": "(is the) Fire", "word_transliteration": "nara"}, {"word_arabic_uthmani": "\u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e", "word_translation": "(of) Hell", "word_transliteration": "jahannama"}, {"word_arabic_uthmani": "\u062e\u064e\u0670\u0644\u0650\u062f\u0657\u0627", "word_translation": "(will) abide forever", "word_transliteration": "khalidan"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627\u06da", "word_translation": "in it", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u0650\u0632\u06e1\u064a\u064f", "word_translation": "(is) the disgrace", "word_transliteration": "al-khiz'yu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u064f", "word_translation": "the great", "word_transliteration": "al-'azimu"}]}