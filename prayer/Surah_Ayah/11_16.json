{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0644\u064e\u064a\u06e1\u0633\u064e \u0644\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650 \u0625\u0650\u0644\u0651\u064e\u0627 \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064f\u06d6 \u0648\u064e\u062d\u064e\u0628\u0650\u0637\u064e \u0645\u064e\u0627 \u0635\u064e\u0646\u064e\u0639\u064f\u0648\u0627\u0652 \u0641\u0650\u064a\u0647\u064e\u0627 \u0648\u064e\u0628\u064e\u0670\u0637\u0650\u0644\u065e \u0645\u0651\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(are) the ones who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "(is) not", "word_transliteration": "laysa"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650", "word_translation": "the Hereafter", "word_transliteration": "al-akhirati"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064f\u06d6", "word_translation": "the Fire", "word_transliteration": "al-naru"}, {"word_arabic_uthmani": "\u0648\u064e\u062d\u064e\u0628\u0650\u0637\u064e", "word_translation": "And (has) gone in vain", "word_transliteration": "wahabita"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0635\u064e\u0646\u064e\u0639\u064f\u0648\u0627\u0652", "word_translation": "they did", "word_transliteration": "sana'u"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u0670\u0637\u0650\u0644\u065e", "word_translation": "and (is) worthless", "word_transliteration": "wabatilun"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they used (to)", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "do", "word_transliteration": "ya'maluna"}]}