{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0630\u06e1 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f \u0671\u062b\u06e1\u0646\u064e\u064a\u06e1\u0646\u0650 \u0641\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0647\u064f\u0645\u064e\u0627 \u0641\u064e\u0639\u064e\u0632\u0651\u064e\u0632\u06e1\u0646\u064e\u0627 \u0628\u0650\u062b\u064e\u0627\u0644\u0650\u062b\u0656 \u0641\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u0651\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064f\u0645 \u0645\u0651\u064f\u0631\u06e1\u0633\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "When", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627\u0653", "word_translation": "We sent", "word_transliteration": "arsalna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f", "word_translation": "to them", "word_transliteration": "ilayhimu"}, {"word_arabic_uthmani": "\u0671\u062b\u06e1\u0646\u064e\u064a\u06e1\u0646\u0650", "word_translation": "two (Messengers)", "word_transliteration": "ith'nayni"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0647\u064f\u0645\u064e\u0627", "word_translation": "but they denied both of them", "word_transliteration": "fakadhabuhuma"}, {"word_arabic_uthmani": "\u0641\u064e\u0639\u064e\u0632\u0651\u064e\u0632\u06e1\u0646\u064e\u0627", "word_translation": "so We strengthened them", "word_transliteration": "fa'azzazna"}, {"word_arabic_uthmani": "\u0628\u0650\u062b\u064e\u0627\u0644\u0650\u062b\u0656", "word_translation": "with a third", "word_transliteration": "bithalithin"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "and they said", "word_transliteration": "faqalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653", "word_translation": "Indeed We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064f\u0645", "word_translation": "to you", "word_transliteration": "ilaykum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0631\u06e1\u0633\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "(are) Messengers", "word_transliteration": "mur'saluna"}]}