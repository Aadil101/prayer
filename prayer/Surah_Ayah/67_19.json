{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0637\u0651\u064e\u064a\u06e1\u0631\u0650 \u0641\u064e\u0648\u06e1\u0642\u064e\u0647\u064f\u0645\u06e1 \u0635\u064e\u0640\u0670\u0653\u0641\u0651\u064e\u0640\u0670\u062a\u0656 \u0648\u064e\u064a\u064e\u0642\u06e1\u0628\u0650\u0636\u06e1\u0646\u064e\u06da \u0645\u064e\u0627 \u064a\u064f\u0645\u06e1\u0633\u0650\u0643\u064f\u0647\u064f\u0646\u0651\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0628\u0650\u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u0650\u06ed \u0628\u064e\u0635\u0650\u064a\u0631\u064c"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Do not", "word_transliteration": "awalam"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652", "word_translation": "they see", "word_transliteration": "yaraw"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "[to]", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0637\u0651\u064e\u064a\u06e1\u0631\u0650", "word_translation": "the birds", "word_transliteration": "al-tayri"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u06e1\u0642\u064e\u0647\u064f\u0645\u06e1", "word_translation": "above them", "word_transliteration": "fawqahum"}, {"word_arabic_uthmani": "\u0635\u064e\u0640\u0670\u0653\u0641\u0651\u064e\u0640\u0670\u062a\u0656", "word_translation": "spreading (their wings)", "word_transliteration": "saffatin"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0642\u06e1\u0628\u0650\u0636\u06e1\u0646\u064e\u06da", "word_translation": "and folding", "word_transliteration": "wayaqbid'na"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064f\u0645\u06e1\u0633\u0650\u0643\u064f\u0647\u064f\u0646\u0651\u064e", "word_translation": "holds them", "word_transliteration": "yum'sikuhunna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f\u06da", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmanu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064f\u0644\u0651\u0650", "word_translation": "(is) of every", "word_transliteration": "bikulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0650\u06ed", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u0650\u064a\u0631\u064c", "word_translation": "All-Seer", "word_transliteration": "basirun"}]}