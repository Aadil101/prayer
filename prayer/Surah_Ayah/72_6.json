{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5 \u0643\u064e\u0627\u0646\u064e \u0631\u0650\u062c\u064e\u0627\u0644\u065e \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u0650 \u064a\u064e\u0639\u064f\u0648\u0630\u064f\u0648\u0646\u064e \u0628\u0650\u0631\u0650\u062c\u064e\u0627\u0644\u0656 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u0650 \u0641\u064e\u0632\u064e\u0627\u062f\u064f\u0648\u0647\u064f\u0645\u06e1 \u0631\u064e\u0647\u064e\u0642\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "And that", "word_transliteration": "wa-annahu"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "(there) were", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0631\u0650\u062c\u064e\u0627\u0644\u065e", "word_translation": "men", "word_transliteration": "rijalun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "among", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u0650", "word_translation": "mankind", "word_transliteration": "al-insi"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u064f\u0648\u0630\u064f\u0648\u0646\u064e", "word_translation": "who sought refuge", "word_transliteration": "ya'udhuna"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u0650\u062c\u064e\u0627\u0644\u0656", "word_translation": "in (the) men", "word_transliteration": "birijalin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u0650", "word_translation": "the jinn", "word_transliteration": "al-jini"}, {"word_arabic_uthmani": "\u0641\u064e\u0632\u064e\u0627\u062f\u064f\u0648\u0647\u064f\u0645\u06e1", "word_translation": "so they increased them", "word_transliterati