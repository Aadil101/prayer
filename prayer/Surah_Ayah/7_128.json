{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0644\u0650\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650 \u0671\u0633\u06e1\u062a\u064e\u0639\u0650\u064a\u0646\u064f\u0648\u0627\u0652 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0671\u0635\u06e1\u0628\u0650\u0631\u064f\u0648\u0653\u0627\u0652\u06d6 \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u064a\u064f\u0648\u0631\u0650\u062b\u064f\u0647\u064e\u0627 \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0645\u0650\u0646\u06e1 \u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6\u06d6 \u0648\u064e\u0671\u0644\u06e1\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f \u0644\u0650\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0644\u0650\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650", "word_translation": "to his people", "word_transliteration": "liqawmihi"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0639\u0650\u064a\u0646\u064f\u0648\u0627\u0652", "word_translation": "Seek help", "word_transliteration": "is'ta'inu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "from Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0635\u06e1\u0628\u0650\u0631\u064f\u0648\u0653\u0627\u0652\u06d6", "word_translation": "and be patient", "word_transliteration": "wa-is'biru"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "the earth", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "(belongs) to Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0631\u0650\u062b\u064f\u0647\u064e\u0627", "word_translation": "He causes to inherit it", "word_transliteration": "yurithuha"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6\u06d6", "word_translation": "His servants", "word_transliteration": "ibadihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f", "word_translation": "And the end", "word_transliteration": "wal-'aqibatu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e", "word_translation": "(is) for the righteous", "word_transliteration": "lil'muttaqina"}]}