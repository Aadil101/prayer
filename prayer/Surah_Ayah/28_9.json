{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u0650 \u0671\u0645\u06e1\u0631\u064e\u0623\u064e\u062a\u064f \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0642\u064f\u0631\u0651\u064e\u062a\u064f \u0639\u064e\u064a\u06e1\u0646\u0656 \u0644\u0651\u0650\u064a \u0648\u064e\u0644\u064e\u0643\u064e\u06d6 \u0644\u064e\u0627 \u062a\u064e\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0647\u064f \u0639\u064e\u0633\u064e\u0649\u0670\u0653 \u0623\u064e\u0646 \u064a\u064e\u0646\u0641\u064e\u0639\u064e\u0646\u064e\u0627\u0653 \u0623\u064e\u0648\u06e1 \u0646\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u064e\u0647\u064f\u06e5 \u0648\u064e\u0644\u064e\u062f\u0657\u0627 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0634\u06e1\u0639\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e\u062a\u0650", "word_translation": "And said", "word_transliteration": "waqalati"}, {"word_arabic_uthmani": "\u0671\u0645\u06e1\u0631\u064e\u0623\u064e\u062a\u064f", "word_translation": "(the) wife", "word_transliteration": "im'ra-atu"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "(of) Firaun", "word_transliteration": "fir'awna"}, {"word_arabic_uthmani": "\u0642\u064f\u0631\u0651\u064e\u062a\u064f", "word_translation": "A comfort", "word_transliteration": "qurratu"}, {"word_arabic_uthmani": "\u0639\u064e\u064a\u06e1\u0646\u0656", "word_translation": "(of the) eye", "word_transliteration": "aynin"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u064a", "word_translation": "for me", "word_transliteration": "li"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0643\u064e\u06d6", "word_translation": "and for you", "word_transliteration": "walaka"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0647\u064f", "word_translation": "kill him", "word_transliteration": "taqtuluhu"}, {"word_arabic_uthmani": "\u0639\u064e\u0633\u064e\u0649\u0670\u0653", "word_translation": "perhaps", "word_transliteration": "asa"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "(that)", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u0641\u064e\u0639\u064e\u0646\u064e\u0627\u0653", "word_translation": "he may benefit us", "word_transliteration": "yanfa'ana"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0646\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u064e\u0647\u064f\u06e5", "word_translation": "we may take him", "word_transliteration": "nattakhidhahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u062f\u0657\u0627", "word_translation": "(as) a son", "word_transliteration": "waladan"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "And they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(did) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0639\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "perceive", "word_transliteration": "yash'uruna"}]}