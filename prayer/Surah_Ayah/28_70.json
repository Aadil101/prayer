{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0670\u0647\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0647\u064f\u0648\u064e\u06d6 \u0644\u064e\u0647\u064f \u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064f\u0648\u0644\u064e\u0649\u0670 \u0648\u064e\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650\u06d6 \u0648\u064e\u0644\u064e\u0647\u064f \u0671\u0644\u06e1\u062d\u064f\u0643\u06e1\u0645\u064f \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627\u0653", "word_translation": "(there is) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064e", "word_translation": "god", "word_transliteration": "ilaha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e\u06d6", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f", "word_translation": "To Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f", "word_translation": "(are due) all praises", "word_transliteration": "al-hamdu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064f\u0648\u0644\u064e\u0649\u0670", "word_translation": "the first", "word_transliteration": "al-ula"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650\u06d6", "word_translation": "and the last", "word_transliteration": "wal-akhirati"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f", "word_translation": "And for Him", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064f\u0643\u06e1\u0645\u064f", "word_translation": "(is) the Decision", "word_transliteration": "al-huk'mu"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "and to Him", "word_transliteration": "wa-ilayhi"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "you will be returned", "word_transliteration": "tur'ja'una"}]}