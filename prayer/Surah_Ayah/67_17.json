{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1 \u0623\u064e\u0645\u0650\u0646\u062a\u064f\u0645 \u0645\u0651\u064e\u0646 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650 \u0623\u064e\u0646 \u064a\u064f\u0631\u06e1\u0633\u0650\u0644\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u062d\u064e\u0627\u0635\u0650\u0628\u0657\u0627\u06d6 \u0641\u064e\u0633\u064e\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e \u0643\u064e\u064a\u06e1\u0641\u064e \u0646\u064e\u0630\u0650\u064a\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u0650\u0646\u062a\u064f\u0645", "word_translation": "do you feel secure", "word_transliteration": "amintum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646", "word_translation": "(from Him) Who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650", "word_translation": "the heaven", "word_transliteration": "al-samai"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "not", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u06e1\u0633\u0650\u0644\u064e", "word_translation": "He will send", "word_transliteration": "yur'sila"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "against you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u062d\u064e\u0627\u0635\u0650\u0628\u0657\u0627\u06d6", "word_translation": "a storm of stones", "word_transliteration": "hasiban"}, {"word_arabic_uthmani": "\u0641\u064e\u0633\u064e\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "Then you would know", "word_transliteration": "fasa