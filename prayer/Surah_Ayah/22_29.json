{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u0644\u06e1\u064a\u064e\u0642\u06e1\u0636\u064f\u0648\u0627\u0652 \u062a\u064e\u0641\u064e\u062b\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0644\u06e1\u064a\u064f\u0648\u0641\u064f\u0648\u0627\u0652 \u0646\u064f\u0630\u064f\u0648\u0631\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0644\u06e1\u064a\u064e\u0637\u0651\u064e\u0648\u0651\u064e\u0641\u064f\u0648\u0627\u0652 \u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u064a\u06e1\u062a\u0650 \u0671\u0644\u06e1\u0639\u064e\u062a\u0650\u064a\u0642\u0650"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0644\u06e1\u064a\u064e\u0642\u06e1\u0636\u064f\u0648\u0627\u0652", "word_translation": "let them complete", "word_transliteration": "l'yaqdu"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u064e\u062b\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their prescribed duties", "word_transliteration": "tafathahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u06e1\u064a\u064f\u0648\u0641\u064f\u0648\u0627\u0652", "word_translation": "and fulfil", "word_transliteration": "walyufu"}, {"word_arabic_uthmani": "\u0646\u064f\u0630\u064f\u0648\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their vows", "word_transliteration": "nudhurahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u06e1\u064a\u064e\u0637\u0651\u064e\u0648\u0651\u064e\u0641\u064f\u0648\u0627\u0652", "word_translation": "and circumambulate", "word_transliteration": "walyattawwafu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u064a\u06e1\u062a\u0650", "word_translation": "the House", "word_transliteration": "bil-bayti"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u062a\u0650\u064a\u0642\u0650", "word_translation": "[the] Ancient", "word_transliteration": "al-'atiqi"}]}