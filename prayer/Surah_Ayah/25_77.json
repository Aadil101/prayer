{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u0645\u064e\u0627 \u064a\u064e\u0639\u06e1\u0628\u064e\u0624\u064f\u0627\u0652 \u0628\u0650\u0643\u064f\u0645\u06e1 \u0631\u064e\u0628\u0651\u0650\u064a \u0644\u064e\u0648\u06e1\u0644\u064e\u0627 \u062f\u064f\u0639\u064e\u0627\u0653\u0624\u064f\u0643\u064f\u0645\u06e1\u06d6 \u0641\u064e\u0642\u064e\u062f\u06e1 \u0643\u064e\u0630\u0651\u064e\u0628\u06e1\u062a\u064f\u0645\u06e1 \u0641\u064e\u0633\u064e\u0648\u06e1\u0641\u064e \u064a\u064e\u0643\u064f\u0648\u0646\u064f \u0644\u0650\u0632\u064e\u0627\u0645\u064e\u06e2\u0627"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0628\u064e\u0624\u064f\u0627\u0652", "word_translation": "will care", "word_transliteration": "ya'ba-u"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "bikum"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a", "word_translation": "my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "if not", "word_transliteration": "lawla"}, {"word_arabic_uthmani": "\u062f\u064f\u0639\u064e\u0627\u0653\u0624\u064f\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "your prayer (is to Him)", "word_transliteration": "du'aukum"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u06e1", "word_translation": "But verily", "word_transliteration": "faqad"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "you have denied", "word_transliteration": "kadhabtum"}, {"word_arabic_uthmani": "\u0641\u064e\u0633\u064e\u0648\u06e1\u0641\u064e", "word_translation": "so soon", "word_transliteration": "fasawfa"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0648\u0646\u064f", "word_translation": "will be", "word_transliteration": "yakunu"}, {"word_arabic_uthmani": "\u0644\u0650\u0632\u064e\u0627\u0645\u064e\u06e2\u0627", "word_translation": "the inevitable (punishment)", "word_transliteration": "lizaman"}]}