{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0627\u0652 \u0645\u0650\u0645\u0651\u064e\u0627 \u0644\u064e\u0645\u06e1 \u064a\u064f\u0630\u06e1\u0643\u064e\u0631\u0650 \u0671\u0633\u06e1\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u0641\u0650\u0633\u06e1\u0642\u065e\u06d7 \u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0634\u0651\u064e\u064a\u064e\u0670\u0637\u0650\u064a\u0646\u064e \u0644\u064e\u064a\u064f\u0648\u062d\u064f\u0648\u0646\u064e \u0625\u0650\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0626\u0650\u0647\u0650\u0645\u06e1 \u0644\u0650\u064a\u064f\u062c\u064e\u0670\u062f\u0650\u0644\u064f\u0648\u0643\u064f\u0645\u06e1\u06d6 \u0648\u064e\u0625\u0650\u0646\u06e1 \u0623\u064e\u0637\u064e\u0639\u06e1\u062a\u064f\u0645\u064f\u0648\u0647\u064f\u0645\u06e1 \u0625\u0650\u0646\u0651\u064e\u0643\u064f\u0645\u06e1 \u0644\u064e\u0645\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "eat", "word_transliteration": "takulu"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0627", "word_translation": "of that", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064f\u0630\u06e1\u0643\u064e\u0631\u0650", "word_translation": "has been mentioned", "word_transliteration": "yudh'kari"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u0645\u064f", "word_translation": "(the) name", "word_transliteration": "us'mu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "on it", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "and indeed, it (is)", "word_transliteration": "wa-innahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0641\u0650\u0633\u06e1\u0642\u065e\u06d7", "word_translation": "grave disobedience", "word_transliteration": "lafis'qun"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u064e\u0670\u0637\u0650\u064a\u0646\u064e", "word_translation": "the devils", "word_transliteration": "al-shayatina"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u064f\u0648\u062d\u064f\u0648\u0646\u064e", "word_translation": "inspire", "word_transliteration": "layuhuna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670\u0653", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0626\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their friends", "word_transliteration": "awliyaihim"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u062c\u064e\u0670\u062f\u0650\u0644\u064f\u0648\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "so that they dispute with you", "word_transliteration": "liyujadilukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u06e1", "word_translation": "and if", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0623\u064e\u0637\u064e\u0639\u06e1\u062a\u064f\u0645\u064f\u0648\u0647\u064f\u0645\u06e1", "word_translation": "you obey them", "word_transliteration": "ata'tumuhum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "indeed, you", "word_transliteration": "innakum"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "(would) be the polytheists", "word_transliteration": "lamush'rikuna"}]}