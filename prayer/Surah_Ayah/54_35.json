{"ayahs": {"ayah_uthmani": "\u0646\u0651\u0650\u0639\u06e1\u0645\u064e\u0629\u0657 \u0645\u0651\u0650\u0646\u06e1 \u0639\u0650\u0646\u062f\u0650\u0646\u064e\u0627\u06da \u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0646\u064e\u062c\u06e1\u0632\u0650\u064a \u0645\u064e\u0646 \u0634\u064e\u0643\u064e\u0631\u064e"}, "words": [{"word_arabic_uthmani": "\u0646\u0651\u0650\u0639\u06e1\u0645\u064e\u0629\u0657", "word_translation": "(As) a favor", "word_transliteration": "ni'matan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u0650\u0646\u064e\u0627\u06da", "word_translation": "Us", "word_transliteration": "indina"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "Thus", "word_transliteration": "kadhalika"}, {"word_arabic_uthmani": "\u0646\u064e\u062c\u06e1\u0632\u0650\u064a", "word_translation": "We reward", "word_transliteration": "najzi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(one) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0634\u064e\u0643\u064e\u0631\u064e", "word_translation": "(is) grateful", "word_transliteration": "shakara"}]}