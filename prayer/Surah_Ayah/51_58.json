{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0647\u064f\u0648\u064e \u0671\u0644\u0631\u0651\u064e\u0632\u0651\u064e\u0627\u0642\u064f \u0630\u064f\u0648 \u0671\u0644\u06e1\u0642\u064f\u0648\u0651\u064e\u0629\u0650 \u0671\u0644\u06e1\u0645\u064e\u062a\u0650\u064a\u0646\u064f"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0632\u0651\u064e\u0627\u0642\u064f", "word_translation": "(is) the All-Provider", "word_transliteration": "al-razaqu"}, {"word_arabic_uthmani": "\u0630\u064f\u0648", "word_translation": "Possessor", "word_transliteration": "dhu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0648\u0651\u064e\u0629\u0650", "word_translation": "(of) Power", "word_transliteration": "al-quwati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u062a\u0650\u064a\u0646\u064f", "word_translation": "the Strong", "word_transliteration": "al-matinu"}]}