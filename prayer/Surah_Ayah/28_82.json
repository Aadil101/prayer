{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u0628\u064e\u062d\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u062a\u064e\u0645\u064e\u0646\u0651\u064e\u0648\u06e1\u0627\u0652 \u0645\u064e\u0643\u064e\u0627\u0646\u064e\u0647\u064f\u06e5 \u0628\u0650\u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0633\u0650 \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u0648\u064e\u064a\u06e1\u0643\u064e\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u064a\u064e\u0628\u06e1\u0633\u064f\u0637\u064f \u0671\u0644\u0631\u0651\u0650\u0632\u06e1\u0642\u064e \u0644\u0650\u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0645\u0650\u0646\u06e1 \u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6 \u0648\u064e\u064a\u064e\u0642\u06e1\u062f\u0650\u0631\u064f\u06d6 \u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653 \u0623\u064e\u0646 \u0645\u0651\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u0644\u064e\u062e\u064e\u0633\u064e\u0641\u064e \u0628\u0650\u0646\u064e\u0627\u06d6 \u0648\u064e\u064a\u06e1\u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u0627 \u064a\u064f\u0641\u06e1\u0644\u0650\u062d\u064f \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u0628\u064e\u062d\u064e", "word_translation": "And began", "word_transliteration": "wa-asbaha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u062a\u064e\u0645\u064e\u0646\u0651\u064e\u0648\u06e1\u0627\u0652", "word_translation": "(had) wished", "word_transliteration": "tamannaw"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u064e\u0627\u0646\u064e\u0647\u064f\u06e5", "word_translation": "his position", "word_transliteration": "makanahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0633\u0650", "word_translation": "the day before", "word_transliteration": "bil-amsi"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "(to) say", "word_transliteration": "yaquluna"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u06e1\u0643\u064e\u0623\u064e\u0646\u0651\u064e", "word_translation": "Ah! That", "word_transliteration": "wayka-anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u064a\u064e\u0628\u06e1\u0633\u064f\u0637\u064f", "word_translation": "extends", "word_transliteration": "yabsutu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u0650\u0632\u06e1\u0642\u064e", "word_translation": "the provision", "word_transliteration": "al-riz'qa"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0646", "word_translation": "for whom", "word_transliteration": "liman"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6", "word_translation": "His slaves", "word_transliteration": "ibadihi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0642\u06e1\u062f\u0650\u0631\u064f\u06d6", "word_translation": "and restricts it", "word_transliteration": "wayaqdiru"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653", "word_translation": "If not", "word_transliteration": "lawla"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646\u0651\u064e", "word_translation": "Allah had favored", "word_transliteration": "manna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah had favored", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "[to] us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0644\u064e\u062e\u064e\u0633\u064e\u0641\u064e", "word_translation": "He would have caused it to swallow us", "word_transliteration": "lakhasafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0646\u064e\u0627\u06d6", "word_translation": "He would have caused it to swallow us", "word_transliteration": "bina"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u06e1\u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Ah! That", "word_transliteration": "wayka-annahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0641\u06e1\u0644\u0650\u062d\u064f", "word_translation": "will succeed", "word_transliteration": "yuf'lihu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "the disbelievers", "word_transliteration": "al-kafiruna"}]}