{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650\u06d7 \u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u064e \u0644\u064e\u0623\u0653\u062a\u0650\u064a\u064e\u0629\u065e\u06d6 \u0641\u064e\u0671\u0635\u06e1\u0641\u064e\u062d\u0650 \u0671\u0644\u0635\u0651\u064e\u0641\u06e1\u062d\u064e \u0671\u0644\u06e1\u062c\u064e\u0645\u0650\u064a\u0644\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627", "word_translation": "We created", "word_transliteration": "khalaqna"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "and the earth", "word_transliteration": "wal-arda"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u0653", "word_translation": "(is) between them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650\u06d7", "word_translation": "in truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u064e", "word_translation": "the Hour", "word_transliteration": "al-sa'ata"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u0653\u062a\u0650\u064a\u064e\u0629\u065e\u06d6", "word_translation": "(is) surely coming", "word_transliteration": "laatiyatun"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0635\u06e1\u0641\u064e\u062d\u0650", "word_translation": "So overlook", "word_transliteration": "fa-is'fahi"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0641\u06e1\u062d\u064e", "word_translation": "(with) forgiveness", "word_transliteration": "al-safha"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0645\u0650\u064a\u0644\u064e", "word_translation": "gracious", "word_transliteration": "al-jamila"}]}