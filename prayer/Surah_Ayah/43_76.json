{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0638\u064e\u0644\u064e\u0645\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0647\u064f\u0645\u064f \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0638\u064e\u0644\u064e\u0645\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "We wronged them", "word_transliteration": "zalamnahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646", "word_translation": "but", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they were", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "themselves", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "wrongdoers", "word_transliteration": "al-zalimina"}]}