{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0646\u064e\u062d\u06e1\u0646\u064f \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f \u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u0657\u0627 \u0648\u064e\u0623\u064e\u0648\u06e1\u0644\u064e\u0670\u062f\u0657\u0627 \u0648\u064e\u0645\u064e\u0627 \u0646\u064e\u062d\u06e1\u0646\u064f \u0628\u0650\u0645\u064f\u0639\u064e\u0630\u0651\u064e\u0628\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "And they say", "word_transliteration": "waqalu"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "We", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f", "word_translation": "(have) more", "word_transliteration": "aktharu"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u0657\u0627", "word_translation": "wealth", "word_transliteration": "amwalan"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0648\u06e1\u0644\u064e\u0670\u062f\u0657\u0627", "word_translation": "and children", "word_transliteration": "wa-awladan"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "we", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064f\u0639\u064e\u0630\u0651\u064e\u0628\u0650\u064a\u0646\u064e", "word_translation": "will be punished", "word_transliteration": "bimu'adhabina"}]}