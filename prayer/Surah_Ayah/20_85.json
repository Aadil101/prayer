{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0642\u064e\u062f\u06e1 \u0641\u064e\u062a\u064e\u0646\u0651\u064e\u0627 \u0642\u064e\u0648\u06e1\u0645\u064e\u0643\u064e \u0645\u0650\u0646\u06e2 \u0628\u064e\u0639\u06e1\u062f\u0650\u0643\u064e \u0648\u064e\u0623\u064e\u0636\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0633\u0651\u064e\u0627\u0645\u0650\u0631\u0650\u064a\u0651\u064f"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "But indeed, We", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "[verily]", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0646\u0651\u064e\u0627", "word_translation": "We (have) tried", "word_transliteration": "fatanna"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064e\u0643\u064e", "word_translation": "your people", "word_transliteration": "qawmaka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e2", "word_translation": "after you", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u0650\u0643\u064e", "word_translation": "after you", "word_transliteration": "ba'dika"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0636\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u064f", "word_translation": "and has led them astray", "word_transliteration": "wa-adallahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0627\u0645\u0650\u0631\u0650\u064a\u0651\u064f", "word_translation": "the Samiri", "word_transliteration": "al-samiriyu"}]}