{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653 \u0623\u064e\u0646\u0634\u064e\u0623\u064e \u0644\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u06e1\u0639\u064e \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064e \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0641\u06e1\u0640\u0654\u0650\u062f\u064e\u0629\u064e\u06da \u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627 \u0645\u0651\u064e\u0627 \u062a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0634\u064e\u0623\u064e", "word_translation": "produced", "word_transliteration": "ansha-a"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u064f", "word_translation": "for you", "word_transliteration": "lakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u06e1\u0639\u064e", "word_translation": "the hearing", "word_transliteration": "al-sam'a"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064e", "word_translation": "and the sight", "word_transliteration": "wal-absara"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0641\u06e1\u0640\u0654\u0650\u062f\u064e\u0629\u064e\u06da", "word_translation": "and the feeling", "word_transliteration": "wal-afidata"}, {"word_arabic_uthmani": "\u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627", "word_translation": "little", "word_transliteration": "qalilan"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "(is) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062a\u064e\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "you give thanks", "word_transliteration": "tashkuruna"}]}