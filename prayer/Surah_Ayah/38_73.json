{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0633\u064e\u062c\u064e\u062f\u064e \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f \u0643\u064f\u0644\u0651\u064f\u0647\u064f\u0645\u06e1 \u0623\u064e\u062c\u06e1\u0645\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0633\u064e\u062c\u064e\u062f\u064e", "word_translation": "So prostrated", "word_transliteration": "fasajada"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f", "word_translation": "the Angels", "word_transliteration": "al-malaikatu"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064f\u0647\u064f\u0645\u06e1", "word_translation": "all of them", "word_transliteration": "kulluhum"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0645\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "together", "word_transliteration": "ajma'una"}]}