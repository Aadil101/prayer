{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064f\u062d\u064e\u0627\u0653\u062f\u0651\u064f\u0648\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5 \u0643\u064f\u0628\u0650\u062a\u064f\u0648\u0627\u0652 \u0643\u064e\u0645\u064e\u0627 \u0643\u064f\u0628\u0650\u062a\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1\u06da \u0648\u064e\u0642\u064e\u062f\u06e1 \u0623\u064e\u0646\u0632\u064e\u0644\u06e1\u0646\u064e\u0627\u0653 \u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u06ed \u0628\u064e\u064a\u0651\u0650\u0646\u064e\u0670\u062a\u0656\u06da \u0648\u064e\u0644\u0650\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e \u0639\u064e\u0630\u064e\u0627\u0628\u065e \u0645\u0651\u064f\u0647\u0650\u064a\u0646\u065e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u064e\u0627\u0653\u062f\u0651\u064f\u0648\u0646\u064e", "word_translation": "oppose", "word_transliteration": "yuhadduna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5", "word_translation": "and His Messenger", "word_transliteration": "warasulahu"}, {"word_arabic_uthmani": "\u0643\u064f\u0628\u0650\u062a\u064f\u0648\u0627\u0652", "word_translation": "(will) be disgraced", "word_transliteration": "kubitu"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u0627", "word_translation": "as", "word_transliteration": "kama"}, {"word_arabic_uthmani": "\u0643\u064f\u0628\u0650\u062a\u064e", "word_translation": "were disgraced", "word_transliteration": "kubita"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before them", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1\u06da", "word_translation": "before them", "word_transliteration": "qablihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And certainly", "word_transliteration": "waqad"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0632\u064e\u0644\u06e1\u0646\u064e\u0627\u0653", "word_translation": "We have sent down", "word_transliteration": "anzalna"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u06ed", "word_translation": "Verses", "word_transliteration": "ayatin"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u0651\u0650\u0646\u064e\u0670\u062a\u0656\u06da", "word_translation": "clear", "word_transliteration": "bayyinatin"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "And for the disbelievers", "word_transliteration": "walil'kafirina"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u065e", "word_translation": "(is) a punishment", "word_transliteration": "adhabun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0647\u0650\u064a\u0646\u065e", "word_translation": "humiliating", "word_transliteration": "muhinun"}]}