{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u062a\u064e\u0641\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0627\u0652 \u0641\u0650\u064a\u0653 \u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06d7 \u0645\u0651\u064e\u0627 \u062e\u064e\u0644\u064e\u0642\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0648\u064e\u0623\u064e\u062c\u064e\u0644\u0656 \u0645\u0651\u064f\u0633\u064e\u0645\u0651\u0657\u0649\u06d7 \u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0628\u0650\u0644\u0650\u0642\u064e\u0627\u0653\u064a\u0655\u0650 \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1 \u0644\u064e\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Do not", "word_transliteration": "awalam"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0641\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "they ponder", "word_transliteration": "yatafakkaru"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0653", "word_translation": "within", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06d7", "word_translation": "themselves", "word_transliteration": "anfusihim"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064e", "word_translation": "Allah (has) created", "word_transliteration": "khalaqa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah (has) created", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "and the earth", "word_transliteration": "wal-arda"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u0653", "word_translation": "(is) between them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "in truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062c\u064e\u0644\u0656", "word_translation": "and (for) a term", "word_transliteration": "wa-ajalin"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0633\u064e\u0645\u0651\u0657\u0649\u06d7", "word_translation": "appointed", "word_transliteration": "musamman"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627", "word_translation": "many", "word_transliteration": "kathiran"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "the people", "word_transliteration": "al-nasi"}, {"w