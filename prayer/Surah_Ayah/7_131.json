{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0630\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u062d\u064e\u0633\u064e\u0646\u064e\u0629\u064f \u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0644\u064e\u0646\u064e\u0627 \u0647\u064e\u0670\u0630\u0650\u0647\u0650\u06e6\u06d6 \u0648\u064e\u0625\u0650\u0646 \u062a\u064f\u0635\u0650\u0628\u06e1\u0647\u064f\u0645\u06e1 \u0633\u064e\u064a\u0651\u0650\u0626\u064e\u0629\u065e \u064a\u064e\u0637\u0651\u064e\u064a\u0651\u064e\u0631\u064f\u0648\u0627\u0652 \u0628\u0650\u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0648\u064e\u0645\u064e\u0646 \u0645\u0651\u064e\u0639\u064e\u0647\u064f\u06e5\u0653\u06d7 \u0623\u064e\u0644\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0637\u064e\u0640\u0670\u0653\u0626\u0650\u0631\u064f\u0647\u064f\u0645\u06e1 \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0630\u064e\u0627", "word_translation": "But when", "word_transliteration": "fa-idha"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1\u0647\u064f\u0645\u064f", "word_translation": "came to them", "word_transliteration": "jaathumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0633\u064e\u0646\u064e\u0629\u064f", "word_translation": "the good", "word_transliteration": "al-hasanatu"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "they said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0627", "word_translation": "For us", "word_transliteration": "lana"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u0650\u0647\u0650\u06e6\u06d6", "word_translation": "(is) this", "word_transliteration": "hadhihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "And if", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u062a\u064f\u0635\u0650\u0628\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "afflicts them", "word_transliteration": "tusib'hum"}, {"word_arabic_uthmani": "\u0633\u064e\u064a\u0651\u0650\u0626\u064e\u0629\u065e", "word_translation": "bad", "word_transliteration": "sayyi-atun"}, {"word_arabic_uthmani": "\u064a\u064e\u0637\u0651\u064e\u064a\u0651\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "they ascribe evil omens", "word_transliteration": "yattayyaru"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "to Musa", "word_transliteration": "bimusa"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "and who", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0639\u064e\u0647\u064f\u06e5\u0653\u06d7", "word_translation": "(were) with him", "word_transliteration": "ma'ahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627\u0653", "word_translation": "Behold", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0637\u064e\u0640\u0670\u0653\u0626\u0650\u0631\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their evil omens", "word_transliteration": "tairuhum"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "(are) with", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e", "word_translation": "but", "word_transliteration": "walakinna"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "most of them", "word_transliteration": "aktharahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "know", "word_transliteration": "ya'lamuna"}]}