{"ayahs": {"ayah_uthmani": "\u0671\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0647\u064e\u0627 \u0628\u0650\u0633\u064e\u0644\u064e\u0670\u0645\u0656\u06d6 \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u064a\u064e\u0648\u06e1\u0645\u064f \u0671\u0644\u06e1\u062e\u064f\u0644\u064f\u0648\u062f\u0650"}, "words": [{"word_arabic_uthmani": "\u0671\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0647\u064e\u0627", "word_translation": "Enter it", "word_transliteration": "ud'khuluha"}, {"word_arabic_uthmani": "\u0628\u0650\u0633\u064e\u0644\u064e\u0670\u0645\u0656\u06d6", "word_translation": "in peace", "word_transliteration": "bisalamin"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064f", "word_translation": "(is) a Day", "word_transliteration": "yawmu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064f\u0644\u064f\u0648\u062f\u0650", "word_translation": "(of) Eternity", "word_transliteration": "al-khuludi"}]}