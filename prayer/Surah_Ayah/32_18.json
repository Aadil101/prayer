{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0641\u064e\u0645\u064e\u0646 \u0643\u064e\u0627\u0646\u064e \u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0657\u0627 \u0643\u064e\u0645\u064e\u0646 \u0643\u064e\u0627\u0646\u064e \u0641\u064e\u0627\u0633\u0650\u0642\u0657\u0627\u06da \u0644\u0651\u064e\u0627 \u064a\u064e\u0633\u06e1\u062a\u064e\u0648\u064f\u06e5\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0645\u064e\u0646", "word_translation": "Then is one who", "word_transliteration": "afaman"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0657\u0627", "word_translation": "a believer", "word_transliteration": "mu'minan"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u0646", "word_translation": "like (him) who", "word_transliteration": "kaman"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0641\u064e\u0627\u0633\u0650\u0642\u0657\u0627\u06da", "word_translation": "defiantly disobedient", "word_transliteration": "fasiqan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u0648\u064f\u06e5\u0646\u064e", "word_translation": "they are equal", "word_transliteration": "yastawuna"}]}