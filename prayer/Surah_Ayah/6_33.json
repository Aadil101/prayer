{"ayahs": {"ayah_uthmani": "\u0642\u064e\u062f\u06e1 \u0646\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u064a\u064e\u062d\u06e1\u0632\u064f\u0646\u064f\u0643\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e\u06d6 \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064f\u0643\u064e\u0630\u0651\u0650\u0628\u064f\u0648\u0646\u064e\u0643\u064e \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u064a\u064e\u062c\u06e1\u062d\u064e\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "Indeed", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u0646\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "We know", "word_transliteration": "na'lamu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "that it", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u064e\u062d\u06e1\u0632\u064f\u0646\u064f\u0643\u064e", "word_translation": "grieves you", "word_transliteration": "layahzunuka"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "what", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e\u06d6", "word_translation": "they say", "word_transliteration": "yaquluna"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "And indeed, they", "word_transliteration": "fa-innahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0643\u064e\u0630\u0651\u0650\u0628\u064f\u0648\u0646\u064e\u0643\u064e", "word_translation": "deny you", "word_transliteration": "yukadhibunaka"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e", "word_translation": "but", "word_transliteration": "walakinna"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimina"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650", "word_translation": "the Verses", "word_transliteration": "biayati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u064a\u064e\u062c\u06e1\u062d\u064e\u062f\u064f\u0648\u0646\u064e", "word_translation": "they reject", "word_transliteration": "yajhaduna"}]}