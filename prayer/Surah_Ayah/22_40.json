nslation": "And surely Allah will help", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(those) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u0635\u064f\u0631\u064f\u0647\u064f\u06e5\u0653\u06da", "word_translation": "help Him", "word_transliteration": "yansuruhu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u0648\u0650\u064a\u0651\u064c", "word_translation": "(is) surely All-Strong", "word_transliteration": "laqawiyyun"}, {"word_arabic_uthmani": "\u0639\u064e\u0632\u0650\u064a\u0632\u064c", "word_translation": "All-Mighty", "word_transliteration": "azizun"}]}