{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0623\u064e\u0642\u06e1\u0633\u064e\u0645\u06e1\u062a\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0646\u064e\u0627\u0644\u064f\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u0650\u0631\u064e\u062d\u06e1\u0645\u064e\u0629\u064d\u06da \u0671\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e \u0644\u064e\u0627 \u062e\u064e\u0648\u06e1\u0641\u064c \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u062d\u06e1\u0632\u064e\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650", "word_translation": "Are these", "word_transliteration": "ahaulai"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "the ones whom", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0623\u064e\u0642\u06e1\u0633\u064e\u0645\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "you had sworn", "word_transliteration": "aqsamtum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(that) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u064e\u0627\u0644\u064f\u0647\u064f\u0645\u064f", "word_translation": "(will) grant them", "word_transliteration": "yanaluhumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u062d\u06e1\u0645\u064e\u0629\u064d\u06da", "word_translation": "Mercy", "word_transliteration": "birahmatin"}, {"word_arabic_uthmani": "\u0671\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "Enter", "word_transliteration": "ud'khulu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e", "word_translation": "Paradise", "word_transliteration": "al-janata"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(There will be) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062e\u064e\u0648\u06e1\u0641\u064c", "word_translation": "fear", "word_transliteration": "khawfun"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "upon you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627\u0653", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u06e1\u0632\u064e\u0646\u064f\u0648\u0646\u064e", "word_translation": "will grieve", "word_transliteration": "tahzanuna"}]}