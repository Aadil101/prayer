{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u0644\u064e\u0623\u0653\u062a\u0650\u064a\u064e\u0646\u0651\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u06e2 \u0628\u064e\u064a\u06e1\u0646\u0650 \u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1 \u0648\u064e\u0645\u0650\u0646\u06e1 \u062e\u064e\u0644\u06e1\u0641\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0639\u064e\u0646\u06e1 \u0623\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0639\u064e\u0646 \u0634\u064e\u0645\u064e\u0627\u0653\u0626\u0650\u0644\u0650\u0647\u0650\u0645\u06e1\u06d6 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u062c\u0650\u062f\u064f \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1 \u0634\u064e\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u0653\u062a\u0650\u064a\u064e\u0646\u0651\u064e\u0647\u064f\u0645", "word_translation": "surely, I will come to them", "word_transliteration": "laatiyannahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e2", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u0650", "word_translation": "before", "word_transliteration": "bayni"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "them", "word_transliteration": "aydihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1", "word_translation": "and from", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0641\u0650\u0647\u0650\u0645\u06e1", "word_translation": "behind them", "word_transliteration": "khalfihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0646\u06e1", "word_translation": "and from", "word_transliteration": "wa'an"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their right", "word_transliteration": "aymanihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0646", "word_translation": "and from", "word_transliteration": "wa'an"}, {"word_arabic_uthmani": "\u0634\u064e\u0645\u064e\u0627\u0653\u0626\u0650\u0644\u0650\u0647\u0650\u0645\u06e1\u06d6", "word_translation": "their left", "word_transliteration": "shamailihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u062c\u0650\u062f\u064f", "word_translation": "You (will) find", "word_transliteration": "tajidu"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "most of them", "word_transliteration": "aktharahum"}, {"word_arabic_uthmani": "\u0634\u064e\u0670\u0643\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "grateful", "word_transliteration": "shakirina"}]}