{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u06e1 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0643\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u06e1", "word_translation": "And put your trust", "word_transliteration": "watawakkal"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "in", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "And Allah is sufficient", "word_transliteration": "wakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "And Allah is sufficient", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u0650\u064a\u0644\u0657\u0627", "word_translation": "(as) Disposer of affairs", "word_transliteration": "wakilan"}]}