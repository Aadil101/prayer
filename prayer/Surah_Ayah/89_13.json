{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0635\u064e\u0628\u0651\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0633\u064e\u0648\u06e1\u0637\u064e \u0639\u064e\u0630\u064e\u0627\u0628\u064d"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0635\u064e\u0628\u0651\u064e", "word_translation": "So poured", "word_transliteration": "fasabba"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "on them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbuka"}, {"word_arabic_uthmani": "\u0633\u064e\u0648\u06e1\u0637\u064e", "word_translation": "scourge", "word_transliteration": "sawta"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064d", "word_translation": "(of) punishment", "word_transliteration": "adhabin"}]}