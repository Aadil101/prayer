{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064f\u0624\u06e1\u0630\u064f\u0648\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5 \u0644\u064e\u0639\u064e\u0646\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0641\u0650\u064a \u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627 \u0648\u064e\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650 \u0648\u064e\u0623\u064e\u0639\u064e\u062f\u0651\u064e \u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u0657\u0627 \u0645\u0651\u064f\u0647\u0650\u064a\u0646\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0630\u064f\u0648\u0646\u064e", "word_translation": "annoy", "word_transliteration": "yu'dhuna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5", "word_translation": "and His Messenger", "word_transliteration": "warasulahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0646\u064e\u0647\u064f\u0645\u064f", "word_translation": "Allah has cursed them", "word_transliteration": "la'anahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah has cursed them", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627", "word_translation": "the world", "word_transliteration": "al-dun'ya"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650", "word_translation": "and the Hereafter", "word_transliteration": "wal-akhirati"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0639\u064e\u062f\u0651\u064e", "word_translation": "and has prepared", "word_transliteration": "wa-a'adda"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u0657\u0627", "word_translation": "a punishment", "word_transliteration": "adhaban"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0647\u0650\u064a\u0646\u0657\u0627", "word_translation": "humiliating", "word_transliteration": "muhinan"}]}