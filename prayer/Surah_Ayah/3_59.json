{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0645\u064e\u062b\u064e\u0644\u064e \u0639\u0650\u064a\u0633\u064e\u0649\u0670 \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650 \u0621\u064e\u0627\u062f\u064e\u0645\u064e\u06d6 \u062e\u064e\u0644\u064e\u0642\u064e\u0647\u064f\u06e5 \u0645\u0650\u0646 \u062a\u064f\u0631\u064e\u0627\u0628\u0656 \u062b\u064f\u0645\u0651\u064e \u0642\u064e\u0627\u0644\u064e \u0644\u064e\u0647\u064f\u06e5 \u0643\u064f\u0646 \u0641\u064e\u064a\u064e\u0643\u064f\u0648\u0646\u064f"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0645\u064e\u062b\u064e\u0644\u064e", "word_translation": "(the) likeness", "word_transliteration": "mathala"}, {"word_arabic_uthmani": "\u0639\u0650\u064a\u0633\u064e\u0649\u0670", "word_translation": "(of) Isa", "word_transliteration": "isa"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "near", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650", "word_translation": "(is) like (the) likeness", "word_transliteration": "kamathali"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062f\u064e\u0645\u064e\u06d6", "word_translation": "(of) Adam", "word_transliteration": "adama"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064e\u0647\u064f\u06e5", "word_translation": "He created him", "word_transliteration": "khalaqahu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u064e\u0627\u0628\u0656", "word_translation": "dust", "word_transliteration": "turabin"}, {"word_arabic_uthmani": "\u062b\u064f\u064