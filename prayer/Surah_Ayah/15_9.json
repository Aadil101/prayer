{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627 \u0646\u064e\u062d\u06e1\u0646\u064f \u0646\u064e\u0632\u0651\u064e\u0644\u06e1\u0646\u064e\u0627 \u0671\u0644\u0630\u0651\u0650\u0643\u06e1\u0631\u064e \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0644\u064e\u0647\u064f\u06e5 \u0644\u064e\u062d\u064e\u0670\u0641\u0650\u0638\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "We", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0646\u064e\u0632\u0651\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "have sent down", "word_transliteration": "nazzalna"}, {"word_arabic_uthmani": "\u0671\u0644\u0630\u0651\u0650\u0643\u06e1\u0631\u064e", "word_translation": "the Reminder", "word_transliteration": "al-dhik'ra"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "and indeed We", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "of it", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u062d\u064e\u0670\u0641\u0650\u0638\u064f\u0648\u0646\u064e", "word_translation": "(are) surely Guardians", "word_transliteration": "lahafizuna"}]}