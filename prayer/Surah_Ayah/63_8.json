{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u0644\u064e\u0626\u0650\u0646 \u0631\u0651\u064e\u062c\u064e\u0639\u06e1\u0646\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u06e1\u0645\u064e\u062f\u0650\u064a\u0646\u064e\u0629\u0650 \u0644\u064e\u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064e\u0646\u0651\u064e \u0671\u0644\u06e1\u0623\u064e\u0639\u064e\u0632\u0651\u064f \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u0671\u0644\u06e1\u0623\u064e\u0630\u064e\u0644\u0651\u064e\u06da \u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u0639\u0650\u0632\u0651\u064e\u0629\u064f \u0648\u064e\u0644\u0650\u0631\u064e\u0633\u064f\u0648\u0644\u0650\u0647\u0650\u06e6 \u0648\u064e\u0644\u0650\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e \u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e \u0644\u064e\u0627 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "They say", "word_transliteration": "yaquluna"}, {"word_arabic_uthmani": "\u0644\u064e\u0626\u0650\u0646", "word_translation": "If", "word_transliteration": "la-in"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u062c\u064e\u0639\u06e1\u0646\u064e\u0627\u0653", "word_translation": "we return", "word_transliteration": "raja'na"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u062f\u0650\u064a\u0646\u064e\u0629\u0650", "word_translation": "Al-Madinah", "word_transliteration": "al-madinati"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064e\u0646\u0651\u064e", "word_translation": "surely will expel", "word_transliteration": "layukh'rijanna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0639\u064e\u0632\u0651\u064f", "word_translation": "the more honorable", "word_transliteration": "al-a'azu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "from it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0630\u064e\u0644\u0651\u064e\u06da", "word_translation": "the more humble", "word_transliteration": "al-adhala"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "But for Allah", "word_transliteration": "walillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0632\u0651\u064e\u0629\u064f", "word_translation": "(is) the honor", "word_transliteration": "al-'izatu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0631\u064e\u0633\u064f\u0648\u0644\u0650\u0647\u0650\u06e6", "word_translation": "and for His Messenger", "word_transliteration": "walirasulihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "and for the believers", "word_transliteration": "walil'mu'minina"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e", "word_translation": "but", "word_transliteration": "walakinna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "the hypocrites", "word_transliteration": "al-munafiqina"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "know", "word_transliteration": "ya'lamuna"}]}