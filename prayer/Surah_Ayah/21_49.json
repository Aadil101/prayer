{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u062e\u06e1\u0634\u064e\u0648\u06e1\u0646\u064e \u0631\u064e\u0628\u0651\u064e\u0647\u064f\u0645 \u0628\u0650\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650 \u0648\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u0650 \u0645\u064f\u0634\u06e1\u0641\u0650\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "Those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0634\u064e\u0648\u06e1\u0646\u064e", "word_translation": "fear", "word_transliteration": "yakhshawna"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0647\u064f\u0645", "word_translation": "their Lord", "word_transliteration": "rabbahum"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650", "word_translation": "in the unseen", "word_transliteration": "bil-ghaybi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u0650", "word_translation": "the Hour", "word_transliteration": "al-sa'ati"}, {"word_arabic_uthmani": "\u0645\u064f\u0634\u06e1\u0641\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "(are) afraid", "word_transliteration": "mush'fiquna"}]}