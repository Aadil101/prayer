{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0646\u062a\u064e\u0638\u0650\u0631\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u0651\u064e\u0627 \u0645\u064f\u0646\u062a\u064e\u0638\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0646\u062a\u064e\u0638\u0650\u0631\u064f\u0648\u0653\u0627\u0652", "word_translation": "And wait", "word_transliteration": "wa-intaziru"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "indeed we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0645\u064f\u0646\u062a\u064e\u0638\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(are) ones who wait", "word_transliteration": "muntaziruna"}]}