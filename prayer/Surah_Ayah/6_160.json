{"ayahs": {"ayah_uthmani": "\u0645\u064e\u0646 \u062c\u064e\u0627\u0653\u0621\u064e \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0633\u064e\u0646\u064e\u0629\u0650 \u0641\u064e\u0644\u064e\u0647\u064f\u06e5 \u0639\u064e\u0634\u06e1\u0631\u064f \u0623\u064e\u0645\u06e1\u062b\u064e\u0627\u0644\u0650\u0647\u064e\u0627\u06d6 \u0648\u064e\u0645\u064e\u0646 \u062c\u064e\u0627\u0653\u0621\u064e \u0628\u0650\u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0626\u064e\u0629\u0650 \u0641\u064e\u0644\u064e\u0627 \u064a\u064f\u062c\u06e1\u0632\u064e\u0649\u0670\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u0650\u062b\u06e1\u0644\u064e\u0647\u064e\u0627 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064f\u0638\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "Whoever", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "came", "word_transliteration": "jaa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0633\u064e\u0646\u064e\u0629\u0650", "word_translation": "with a good deed", "word_transliteration": "bil-hasanati"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0647\u064f\u06e5", "word_translation": "then for him", "word_transliteration": "falahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0634\u06e1\u0631\u064f", "word_translation": "(is) ten (times)", "word_transliteration": "ashru"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u062b\u064e\u0627\u0644\u0650\u0647\u064e\u0627\u06d6", "word_translation": "the like of it", "word_transliteration": "amthaliha"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "And whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "came", "word_transliteration": "jaa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0626\u064e\u0629\u0650", "word_translation": "with an evil deed", "word_transliteration": "bil-sayi-ati"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "then not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u064a\u064f\u062c\u06e1\u0632\u064e\u0649\u0670\u0653", "word_translation": "he will be recompensed", "word_transliteration": "yuj'za"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u0650\u062b\u06e1\u0644\u064e\u0647\u064e\u0627", "word_translation": "the like of it", "word_transliteration": "mith'laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "will not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0638\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "(be) wronged", "word_transliteration": "yuz'lamuna"}]}