{"ayahs": {"ayah_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e \u064a\u064e\u062d\u06e1\u0634\u064f\u0631\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0645\u064e\u0627 \u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0641\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0621\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u0623\u064e\u0636\u06e1\u0644\u064e\u0644\u06e1\u062a\u064f\u0645\u06e1 \u0639\u0650\u0628\u064e\u0627\u062f\u0650\u064a \u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650 \u0623\u064e\u0645\u06e1 \u0647\u064f\u0645\u06e1 \u0636\u064e\u0644\u0651\u064f\u0648\u0627\u0652 \u0671\u0644\u0633\u0651\u064e\u0628\u0650\u064a\u0644\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "And (the) Day", "word_transliteration": "wayawma"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0634\u064f\u0631\u064f\u0647\u064f\u0645\u06e1", "word_translation": "He will gather them", "word_transliteration": "yahshuruhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e", "word_translation": "they worship", "word_transliteration": "ya'buduna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "besides Allah", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides Allah", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "besides Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "and He will say", "word_transliteration": "fayaqulu"}, {"word_arabic_uthmani": "\u0621\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "Did you", "word_transliteration": "a-antum"}, {"word_arabic_uthmani": "\u0623\u064e\u0636\u06e1\u0644\u064e\u0644\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "[you] mislead", "word_transliteration": "adlaltum"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u064a", "word_translation": "My slaves", "word_transliteration": "ibadi"}, {"word_arabic_uthmani": "\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650", "word_translation": "these", "word_transliteration": "haulai"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0636\u064e\u0644\u0651\u064f\u0648\u0627\u0652", "word_translation": "went astray", "word_transliteration": "dallu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0628\u0650\u064a\u0644\u064e", "word_translation": "(from) the way", "word_transliteration": "al-sabila"}]}