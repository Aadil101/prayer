{"ayahs": {"ayah_uthmani": "\u0645\u0651\u064e\u0627 \u0671\u0644\u06e1\u0645\u064e\u0633\u0650\u064a\u062d\u064f \u0671\u0628\u06e1\u0646\u064f \u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0631\u064e\u0633\u064f\u0648\u0644\u065e \u0642\u064e\u062f\u06e1 \u062e\u064e\u0644\u064e\u062a\u06e1 \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650 \u0671\u0644\u0631\u0651\u064f\u0633\u064f\u0644\u064f \u0648\u064e\u0623\u064f\u0645\u0651\u064f\u0647\u064f\u06e5 \u0635\u0650\u062f\u0651\u0650\u064a\u0642\u064e\u0629\u065e\u06d6 \u0643\u064e\u0627\u0646\u064e\u0627 \u064a\u064e\u0623\u06e1\u0643\u064f\u0644\u064e\u0627\u0646\u0650 \u0671\u0644\u0637\u0651\u064e\u0639\u064e\u0627\u0645\u064e\u06d7 \u0671\u0646\u0638\u064f\u0631\u06e1 \u0643\u064e\u064a\u06e1\u0641\u064e \u0646\u064f\u0628\u064e\u064a\u0651\u0650\u0646\u064f \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u0653\u064a\u064e\u0670\u062a\u0650 \u062b\u064f\u0645\u0651\u064e \u0671\u0646\u0638\u064f\u0631\u06e1 \u0623\u064e\u0646\u0651\u064e\u0649\u0670 \u064a\u064f\u0624\u06e1\u0641\u064e\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0633\u0650\u064a\u062d\u064f", "word_translation": "(is) the Messiah", "word_transliteration": "al-masihu"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u0646\u064f", "word_translation": "son", "word_transliteration": "ub'nu"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064e", "word_translation": "(of) Maryam", "word_transliteration": "maryama"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u065e", "word_translation": "a Messenger", "word_transliteration": "rasulun"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "certainly", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u062a\u06e1", "word_translation": "had passed", "word_transliteration": "khalat"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650", "word_translation": "before him", "word_transliteration": "qablihi"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064f\u0633\u064f\u0644\u064f", "word_translation": "the Messengers", "word_transliteration": "al-rusulu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0645\u0651\u064f\u0647\u064f\u06e5", "word_translation": "And his mother", "word_transliteration": "wa-ummuhu"}, {"word_arabic_uthmani": "\u0635\u0650\u062f\u0651\u0650\u064a\u0642\u064e\u0629\u065e\u06d6", "word_translation": "(was) truthful", "word_transliteration": "siddiqatun"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e\u0627", "word_translation": "They both used to", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u0643\u064f\u0644\u064e\u0627\u0646\u0650", "word_translation": "eat", "word_transliteration": "yakulani"}, {"word_arabic_uthmani": "\u0671\u0644\u0637\u0651\u064e\u0639\u064e\u0627\u0645\u064e\u06d7", "word_translation": "[the] food", "word_transliteration": "al-ta'ama"}, {"word_arabic_uthmani": "\u0671\u0646\u0638\u064f\u0631\u06e1", "word_translation": "See", "word_transliteration": "unzur"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "how", "word_transliteration": "kayfa"}, {"word_arabic_uthmani": "\u0646\u064f\u0628\u064e\u064a\u0651\u0650\u0646\u064f", "word_translation": "We make clear", "word_transliteration": "nubayyinu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "to them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani":