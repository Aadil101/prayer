{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0643\u064f\u0645\u06e1\u06d6 \u0644\u064e\u0627 \u064a\u064e\u0636\u064f\u0631\u0651\u064f\u0643\u064f\u0645 \u0645\u0651\u064e\u0646 \u0636\u064e\u0644\u0651\u064e \u0625\u0650\u0630\u064e\u0627 \u0671\u0647\u06e1\u062a\u064e\u062f\u064e\u064a\u06e1\u062a\u064f\u0645\u06e1\u06da \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0645\u064e\u0631\u06e1\u062c\u0650\u0639\u064f\u0643\u064f\u0645\u06e1 \u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627 \u0641\u064e\u064a\u064f\u0646\u064e\u0628\u0651\u0650\u0626\u064f\u0643\u064f\u0645 \u0628\u0650\u0645\u064e\u0627 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O you", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "Upon you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "(is to guard) yourselves", "word_transliteration": "anfusakum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0636\u064f\u0631\u0651\u064f\u0643\u064f\u0645", "word_translation": "will harm you", "word_transliteration": "yadurrukum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646", "word_translation": "(those) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0636\u064e\u0644\u0651\u064e", "word_translation": "(have gone) astray", "word_transliteration": "dalla"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u0671\u0647\u06e1\u062a\u064e\u062f\u064e\u064a\u06e1\u062a\u064f\u0645\u06e1\u06da", "word_translation": "you have been guided", "word_transliteration": "ih'tadaytum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "To", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u06e1\u062c\u0650\u0639\u064f\u0643\u064f\u0645\u06e1", "word_translation": "(is) your return", "word_transliteration": "marji'ukum"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627", "word_translation": "all", "word_transliteration": "jami'an"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064f\u0646\u064e\u0628\u0651\u0650\u0626\u064f\u0643\u064f\u0645", "word_translation": "then He will inform you", "word_transliteration": "fayunabbi-ukum"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you used to", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "do", "word_transliteration": "ta'maluna"}]}