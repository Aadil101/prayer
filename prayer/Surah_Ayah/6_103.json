{"ayahs": {"ayah_uthmani": "\u0644\u0651\u064e\u0627 \u062a\u064f\u062f\u06e1\u0631\u0650\u0643\u064f\u0647\u064f \u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064f \u0648\u064e\u0647\u064f\u0648\u064e \u064a\u064f\u062f\u06e1\u0631\u0650\u0643\u064f \u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064e\u06d6 \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u0644\u0651\u064e\u0637\u0650\u064a\u0641\u064f \u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(Can) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064f\u062f\u06e1\u0631\u0650\u0643\u064f\u0647\u064f", "word_translation": "grasp Him", "word_transliteration": "tud'rikuhu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064f", "word_translation": "the visions", "word_transliteration": "al-absaru"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "but He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u064a\u064f\u062f\u06e1\u0631\u0650\u0643\u064f", "word_translation": "(can) grasp", "word_transliteration": "yud'riku"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064e\u06d6", "word_translation": "(all) the vision", "word_transliteration": "al-absara"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He (is)", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0637\u0650\u064a\u0641\u064f", "word_translation": "the All-Subtle", "word_transliteration": "al-latifu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0628\u0650\u064a\u0631\u064f", "word_translation": "the All-Aware", "word_transliteration": "al-khabiru"}]}