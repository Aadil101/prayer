{"ayahs": {"ayah_uthmani": "\u064a\u064e\u062d\u06e1\u0644\u0650\u0641\u064f\u0648\u0646\u064e \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0644\u064e\u0643\u064f\u0645\u06e1 \u0644\u0650\u064a\u064f\u0631\u06e1\u0636\u064f\u0648\u0643\u064f\u0645\u06e1 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064f\u0647\u064f\u06e5\u0653 \u0623\u064e\u062d\u064e\u0642\u0651\u064f \u0623\u064e\u0646 \u064a\u064f\u0631\u06e1\u0636\u064f\u0648\u0647\u064f \u0625\u0650\u0646 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0644\u0650\u0641\u064f\u0648\u0646\u064e", "word_translation": "They swear", "word_transliteration": "yahlifuna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "by Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "to you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u0631\u06e1\u0636\u064f\u0648\u0643\u064f\u0645\u06e1", "word_translation": "to please you", "word_transliteration": "liyur'dukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064f\u0647\u064f\u06e5\u0653", "word_translation": "and His Messenger", "word_transliteration": "warasuluhu"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u0642\u0651\u064f", "word_translation": "(have) more right", "word_transliteration": "ahaqqu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u06e1\u0636\u064f\u0648\u0647\u064f", "word_translation": "they should please Him", "word_transliteration": "yur'duhu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they are", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "believers", "word_transliteration": "mu'minina"}]}