{"ayahs": {"ayah_uthmani": "\u0643\u064e\u062a\u064e\u0628\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0623\u064e\u063a\u06e1\u0644\u0650\u0628\u064e\u0646\u0651\u064e \u0623\u064e\u0646\u064e\u0627\u06e0 \u0648\u064e\u0631\u064f\u0633\u064f\u0644\u0650\u064a\u0653\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0642\u064e\u0648\u0650\u064a\u0651\u064c \u0639\u064e\u0632\u0650\u064a\u0632\u065e"}, "words": [{"word_arabic_uthmani": "\u0643\u064e\u062a\u064e\u0628\u064e", "word_translation": "Allah has decreed", "word_transliteration": "kataba"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah has decreed", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u064e\u063a\u06e1\u0644\u0650\u0628\u064e\u0646\u0651\u064e", "word_translation": "Surely, I will overcome", "word_transliteration": "la-aghlibanna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "I", "word_transliteration": "ana"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064f\u0633\u064f\u0644\u0650\u064a\u0653\u06da", "word_translation": "and My Messengers", "word_transliteration": "warusuli"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u0650\u064a\u0651\u064c", "word_translation": "(is) All-Strong", "word_transliteration": "qawiyyun"}, {"word_arabic_uthmani": "\u0639\u064e\u0632\u0650\u064a\u0632\u065e", "word_translation": "All-Mighty", "word_transliteration": "azizun"}]}