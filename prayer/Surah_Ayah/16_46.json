{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0648\u06e1 \u064a\u064e\u0623\u06e1\u062e\u064f\u0630\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u062a\u064e\u0642\u064e\u0644\u0651\u064f\u0628\u0650\u0647\u0650\u0645\u06e1 \u0641\u064e\u0645\u064e\u0627 \u0647\u064f\u0645 \u0628\u0650\u0645\u064f\u0639\u06e1\u062c\u0650\u0632\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "Or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062e\u064f\u0630\u064e\u0647\u064f\u0645\u06e1", "word_translation": "that He may seize them", "word_transliteration": "yakhudhahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u064e\u0644\u0651\u064f\u0628\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their going to and fro", "word_transliteration": "taqallubihim"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627", "word_translation": "then not", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u0647\u064f\u0645", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064f\u0639\u06e1\u062c\u0650\u0632\u0650\u064a\u0646\u064e", "word_translation": "will be able to escape", "word_transliteration": "bimu'jizina"}]}