{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e\u0626\u0650\u0630\u0656 \u0644\u0651\u064e\u0627 \u062a\u064e\u0646\u0641\u064e\u0639\u064f \u0671\u0644\u0634\u0651\u064e\u0641\u064e\u0670\u0639\u064e\u0629\u064f \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064e\u0646\u06e1 \u0623\u064e\u0630\u0650\u0646\u064e \u0644\u064e\u0647\u064f \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f \u0648\u064e\u0631\u064e\u0636\u0650\u064a\u064e \u0644\u064e\u0647\u064f\u06e5 \u0642\u064e\u0648\u06e1\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e\u0626\u0650\u0630\u0656", "word_translation": "(On) that Day", "word_transliteration": "yawma-idhin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0646\u0641\u064e\u0639\u064f", "word_translation": "will benefit", "word_transliteration": "tanfa'u"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u0641\u064e\u0670\u0639\u064e\u0629\u064f", "word_translation": "the intercession", "word_transliteration": "al-shafa'atu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u06e1", "word_translation": "(to) whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0623\u064e\u0630\u0650\u0646\u064e", "word_translation": "has given permission", "word_transliteration": "adhina"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f", "word_translation": "[to him]", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmanu"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0636\u0650\u064a\u064e", "word_translation": "and He has accepted", "word_transliteration": "waradiya"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0644\u0657\u0627", "word_translation": "a word", "word_transliteration": "qawlan"}]}