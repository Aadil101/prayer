{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0630\u06e1 \u062a\u064e\u0633\u06e1\u062a\u064e\u063a\u0650\u064a\u062b\u064f\u0648\u0646\u064e \u0631\u064e\u0628\u0651\u064e\u0643\u064f\u0645\u06e1 \u0641\u064e\u0671\u0633\u06e1\u062a\u064e\u062c\u064e\u0627\u0628\u064e \u0644\u064e\u0643\u064f\u0645\u06e1 \u0623\u064e\u0646\u0651\u0650\u064a \u0645\u064f\u0645\u0650\u062f\u0651\u064f\u0643\u064f\u0645 \u0628\u0650\u0623\u064e\u0644\u06e1\u0641\u0656 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u0650 \u0645\u064f\u0631\u06e1\u062f\u0650\u0641\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "When", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u062a\u064e\u0633\u06e1\u062a\u064e\u063a\u0650\u064a\u062b\u064f\u0648\u0646\u064e", "word_translation": "you were seeking help", "word_transliteration": "tastaghithuna"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(of) your Lord", "word_transliteration": "rabbakum"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0633\u06e1\u062a\u064e\u062c\u064e\u0627\u0628\u064e", "word_translation": "and He answered", "word_transliteration": "fa-is'tajaba"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "[to] you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u0650\u064a", "word_translation": "Indeed I am", "word_transliteration": "anni"}, {"word_arabic_uthmani": "\u0645\u064f\u0645\u0650\u062f\u0651\u064f\u0643\u064f\u0645", "word_translation": "going to reinforce you", "word_transliteration": "mumiddukum"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0644\u06e1\u0641\u0656", "word_translation": "with a thousand", "word_transliteration": "bi-alfin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u0650", "word_translation": "the Angels", "word_transliteration": "al-malaikati"}, {"word_arabic_uthmani": "\u0645\u064f\u0631\u06e1\u062f\u0650\u0641\u0650\u064a\u0646\u064e", "word_translation": "one after another", "word_transliteration": "mur'difina"}]}