{"ayahs": {"ayah_uthmani": "\u0631\u0651\u0650\u0632\u06e1\u0642\u0657\u0627 \u0644\u0651\u0650\u0644\u06e1\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u06d6 \u0648\u064e\u0623\u064e\u062d\u06e1\u064a\u064e\u064a\u06e1\u0646\u064e\u0627 \u0628\u0650\u0647\u0650\u06e6 \u0628\u064e\u0644\u06e1\u062f\u064e\u0629\u0657 \u0645\u0651\u064e\u064a\u06e1\u062a\u0657\u0627\u06da \u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0671\u0644\u06e1\u062e\u064f\u0631\u064f\u0648\u062c\u064f"}, "words": [{"word_arabic_uthmani": "\u0631\u0651\u0650\u0632\u06e1\u0642\u0657\u0627", "word_translation": "A provision", "word_transliteration": "riz'qan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u06e1\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u06d6", "word_translation": "for the slaves", "word_transliteration": "lil'ibadi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062d\u06e1\u064a\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "and We give life", "word_transliteration": "wa-ahyayna"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "therewith", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1\u062f\u064e\u0629\u0657", "word_translation": "(to) a land", "word_transliteration": "baldatan"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u064a\u06e1\u062a\u0657\u0627\u06da", "word_translation": "dead", "word_transliteration": "maytan"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "Thus", "word_transliteration": "kadhalika"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064f\u0631\u064f\u0648\u062c\u064f", "word_translation": "(will be) the coming forth", "word_transliteration": "al-khuruju"}]}