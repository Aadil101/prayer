{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627 \u0646\u0650\u0639\u06e1\u0645\u064e\u0629\u064f \u0631\u064e\u0628\u0651\u0650\u064a \u0644\u064e\u0643\u064f\u0646\u062a\u064f \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u062d\u06e1\u0636\u064e\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "And if not", "word_transliteration": "walawla"}, {"word_arabic_uthmani": "\u0646\u0650\u0639\u06e1\u0645\u064e\u0629\u064f", "word_translation": "(for the) Grace", "word_transliteration": "ni'matu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a", "word_translation": "(of) my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0646\u062a\u064f", "word_translation": "certainly I (would) have been", "word_transliteration": "lakuntu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "among", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u062d\u06e1\u0636\u064e\u0631\u0650\u064a\u0646\u064e", "word_translation": "those brought", "word_transliteration": "al-muh'darina"}]}