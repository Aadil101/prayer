{"ayahs": {"ayah_uthmani": "\u0621\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f\u0648\u0646\u064e\u0647\u064f\u06e5\u0653 \u0623\u064e\u0645\u06e1 \u0646\u064e\u062d\u06e1\u0646\u064f \u0671\u0644\u06e1\u062e\u064e\u0670\u0644\u0650\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0621\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "Is it you", "word_transliteration": "a-antum"}, {"word_arabic_uthmani": "\u062a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f\u0648\u0646\u064e\u0647\u064f\u06e5\u0653", "word_translation": "who create it", "word_transliteration": "takhluqunahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "(are) We", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0670\u0644\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "the Creators", "word_transliteration": "al-khaliquna"}]}