{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064f \u0645\u064e\u062f\u06e1\u064a\u064e\u0646\u064e\u06d6 \u0648\u064e\u0643\u064f\u0630\u0651\u0650\u0628\u064e \u0645\u064f\u0648\u0633\u064e\u0649\u0670\u06d6 \u0641\u064e\u0623\u064e\u0645\u06e1\u0644\u064e\u064a\u06e1\u062a\u064f \u0644\u0650\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e \u062b\u064f\u0645\u0651\u064e \u0623\u064e\u062e\u064e\u0630\u06e1\u062a\u064f\u0647\u064f\u0645\u06e1\u06d6 \u0641\u064e\u0643\u064e\u064a\u06e1\u0641\u064e \u0643\u064e\u0627\u0646\u064e \u0646\u064e\u0643\u0650\u064a\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064f", "word_translation": "And the inhabitants", "word_transliteration": "wa-ashabu"}, {"word_arabic_uthmani": "\u0645\u064e\u062f\u06e1\u064a\u064e\u0646\u064e\u06d6", "word_translation": "(of) Madyan", "word_transliteration": "madyana"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064f\u0630\u0651\u0650\u0628\u064e", "word_translation": "And Musa was denied", "word_transliteration": "wakudhiba"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u06d6", "word_translation": "And Musa was denied", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0645\u06e1\u0644\u064e\u064a\u06e1\u062a\u064f", "word_translation": "so I granted respite", "word_transliteration": "fa-amlaytu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "to the disbelievers", "word_transliteration": "lil'kafirina"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u064e\u0630\u06e1\u062a\u064f\u0647\u064f\u0645\u06e1\u06d6", "word_translation": "I seized them", "word_transliteration": "akhadhtuhum"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "and how", "word_transliteration": "fakayfa"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "was", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0646\u064e\u0643\u0650\u064a\u0631\u0650", "word_translation": "My punishment", "word_transliteration": "nakiri"}]}