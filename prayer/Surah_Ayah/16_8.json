{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u062e\u064e\u064a\u06e1\u0644\u064e \u0648\u064e\u0671\u0644\u06e1\u0628\u0650\u063a\u064e\u0627\u0644\u064e \u0648\u064e\u0671\u0644\u06e1\u062d\u064e\u0645\u0650\u064a\u0631\u064e \u0644\u0650\u062a\u064e\u0631\u06e1\u0643\u064e\u0628\u064f\u0648\u0647\u064e\u0627 \u0648\u064e\u0632\u0650\u064a\u0646\u064e\u0629\u0657\u06da \u0648\u064e\u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f \u0645\u064e\u0627 \u0644\u064e\u0627 \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u062e\u064e\u064a\u06e1\u0644\u064e", "word_translation": "And horses", "word_transliteration": "wal-khayla"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0628\u0650\u063a\u064e\u0627\u0644\u064e", "word_translation": "and mules", "word_transliteration": "wal-bighala"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u062d\u064e\u0645\u0650\u064a\u0631\u064e", "word_translation": "and donkeys", "word_transliteration": "wal-hamira"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064e\u0631\u06e1\u0643\u064e\u0628\u064f\u0648\u0647\u064e\u0627", "word_translation": "for you to ride them", "word_transliteration": "litarkabuha"}, {"word_arabic_uthmani": "\u0648\u064e\u0632\u0650\u064a\u0646\u064e\u0629\u0657\u06da", "word_translation": "and (as) adornment", "word_transliteration": "wazinatan"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f", "word_translation": "And He creates", "word_transliteration": "wayakhluqu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "you know", "word_transliteration": "ta'lamuna"}]}