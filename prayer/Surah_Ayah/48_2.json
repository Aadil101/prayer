{"ayahs": {"ayah_uthmani": "\u0644\u0651\u0650\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064e \u0644\u064e\u0643\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064e\u0627 \u062a\u064e\u0642\u064e\u062f\u0651\u064e\u0645\u064e \u0645\u0650\u0646 \u0630\u064e\u0646\u06e2\u0628\u0650\u0643\u064e \u0648\u064e\u0645\u064e\u0627 \u062a\u064e\u0623\u064e\u062e\u0651\u064e\u0631\u064e \u0648\u064e\u064a\u064f\u062a\u0650\u0645\u0651\u064e \u0646\u0650\u0639\u06e1\u0645\u064e\u062a\u064e\u0647\u064f\u06e5 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e \u0648\u064e\u064a\u064e\u0647\u06e1\u062f\u0650\u064a\u064e\u0643\u064e \u0635\u0650\u0631\u064e\u0670\u0637\u0657\u0627 \u0645\u0651\u064f\u0633\u06e1\u062a\u064e\u0642\u0650\u064a\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u0650\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064e", "word_translation": "That may forgive", "word_transliteration": "liyaghfira"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e", "word_translation": "for you", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u064e\u062f\u0651\u064e\u0645\u064e", "word_translation": "preceded", "word_transliteration": "taqaddama"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0630\u064e\u0646\u06e2\u0628\u0650\u0643\u064e", "word_translation": "your fault", "word_transliteration": "dhanbika"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u064e\u062e\u0651\u064e\u0631\u064e", "word_translation": "will follow", "word_transliteration": "ta-akhara"}, {"word_arabic_