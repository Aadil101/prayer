{"ayahs": {"ayah_uthmani": "\u0633\u064e\u0648\u064e\u0627\u0653\u0621\u065e \u0645\u0651\u0650\u0646\u0643\u064f\u0645 \u0645\u0651\u064e\u0646\u06e1 \u0623\u064e\u0633\u064e\u0631\u0651\u064e \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u064e \u0648\u064e\u0645\u064e\u0646 \u062c\u064e\u0647\u064e\u0631\u064e \u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u0645\u064e\u0646\u06e1 \u0647\u064f\u0648\u064e \u0645\u064f\u0633\u06e1\u062a\u064e\u062e\u06e1\u0641\u0650\u06ed \u0628\u0650\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650 \u0648\u064e\u0633\u064e\u0627\u0631\u0650\u0628\u064f\u06e2 \u0628\u0650\u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u0633\u064e\u0648\u064e\u0627\u0653\u0621\u065e", "word_translation": "(It is) same (to Him)", "word_transliteration": "sawaon"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u0643\u064f\u0645", "word_translation": "[of you]", "word_transliteration": "minkum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646\u06e1", "word_translation": "(one) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u064e\u0631\u0651\u064e", "word_translation": "conceals", "word_transliteration": "asarra"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u064e", "word_translation": "the speech", "word_transliteration": "al-qawla"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "or (one) who", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u062c\u064e\u0647\u064e\u0631\u064e", "word_translation": "publicizes", "word_transliteration": "jahara"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646\u06e1", "word_translation": "and (one) who", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "[he]", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0645\u064f\u0633\u06e1\u062a\u064e\u062e\u06e1\u0641\u0650\u06ed", "word_translation": "(is) hidden", "word_transliteration": "mus'takhfin"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650", "word_translation": "by night", "word_transliteration": "bi-al-layli"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064e\u0627\u0631\u0650\u0628\u064f\u06e2", "word_translation": "or goes freely", "word_transliteration": "wasaribun"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u0650", "word_translation": "by day", "word_transliteration": "bil-nahari"}]}