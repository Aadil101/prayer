{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0621\u064e\u0623\u064e\u0670\u0644\u0650\u0647\u064e\u062a\u064f\u0646\u064e\u0627 \u062e\u064e\u064a\u06e1\u0631\u064c \u0623\u064e\u0645\u06e1 \u0647\u064f\u0648\u064e\u06da \u0645\u064e\u0627 \u0636\u064e\u0631\u064e\u0628\u064f\u0648\u0647\u064f \u0644\u064e\u0643\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u062c\u064e\u062f\u064e\u0644\u064e\u06e2\u0627\u06da \u0628\u064e\u0644\u06e1 \u0647\u064f\u0645\u06e1 \u0642\u064e\u0648\u06e1\u0645\u064c \u062e\u064e\u0635\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "And they said", "word_transliteration": "waqalu"}, {"word_arabic_uthmani": "\u0621\u064e\u0623\u064e\u0670\u0644\u0650\u0647\u064e\u062a\u064f\u0646\u064e\u0627", "word_translation": "Are our gods", "word_transliteration": "aalihatuna"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u064c", "word_translation": "better", "word_transliteration": "khayrun"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e\u06da", "word_translation": "he", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0636\u064e\u0631\u064e\u0628\u064f\u0648\u0647\u064f", "word_translation": "they present it", "word_transliteration": "darabuhu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e", "word_translation": "to you", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u062c\u064e\u062f\u064e\u0644\u064e\u06e2\u0627\u06da", "word_translation": "(for) argument", "word_transliteration": "jadalan"}, {"word_arabi