{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u062a\u0650\u064a\u0653 \u0623\u064e\u062d\u06e1\u0635\u064e\u0646\u064e\u062a\u06e1 \u0641\u064e\u0631\u06e1\u062c\u064e\u0647\u064e\u0627 \u0641\u064e\u0646\u064e\u0641\u064e\u062e\u06e1\u0646\u064e\u0627 \u0641\u0650\u064a\u0647\u064e\u0627 \u0645\u0650\u0646 \u0631\u0651\u064f\u0648\u062d\u0650\u0646\u064e\u0627 \u0648\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064e\u0627 \u0648\u064e\u0671\u0628\u06e1\u0646\u064e\u0647\u064e\u0627\u0653 \u0621\u064e\u0627\u064a\u064e\u0629\u0657 \u0644\u0651\u0650\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u062a\u0650\u064a\u0653", "word_translation": "And she who", "word_transliteration": "wa-allati"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u06e1\u0635\u064e\u0646\u064e\u062a\u06e1", "word_translation": "guarded", "word_transliteration": "ahsanat"}, {"word_arabic_uthmani": "\u0641\u064e\u0631\u06e1\u062c\u064e\u0647\u064e\u0627", "word_translation": "her chastity", "word_transliteration": "farjaha"}, {"word_arabic_uthmani": "\u0641\u064e\u0646\u064e\u0641\u064e\u062e\u06e1\u0646\u064e\u0627", "word_translation": "so We breathed", "word_transliteration": "fanafakhna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "into her", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064f\u0648\u062d\u0650\u0646\u064e\u0627", "word_translation": "Our Spirit", "word_transliteration": "ruhina"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064e\u0627", "word_translation": "and We made her", "word_transliteration": "waja'alnaha"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0628\u06e1\u0646\u064e\u0647\u064e\u0627\u0653", "word_translation": "and her son", "word_transliteration": "wa-ib'naha"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0629\u0657", "word_translation": "a sign", "word_transliteration": "ayatan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "for the worlds", "word_transliteration": "lil'alamina"}]}