{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u0644\u064e\u0646\u064e\u0646\u0632\u0650\u0639\u064e\u0646\u0651\u064e \u0645\u0650\u0646 \u0643\u064f\u0644\u0651\u0650 \u0634\u0650\u064a\u0639\u064e\u0629\u064d \u0623\u064e\u064a\u0651\u064f\u0647\u064f\u0645\u06e1 \u0623\u064e\u0634\u064e\u062f\u0651\u064f \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650 \u0639\u0650\u062a\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0646\u0632\u0650\u0639\u064e\u0646\u0651\u064e", "word_translation": "surely We will drag out", "word_transliteration": "lananzi'anna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0634\u0650\u064a\u0639\u064e\u0629\u064d", "word_translation": "sect", "word_transliteration": "shi'atin"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u0651\u064f\u0647\u064f\u0645\u06e1", "word_translation": "those of them", "word_transliteration": "ayyuhum"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u064e\u062f\u0651\u064f", "word_translation": "(who were) worst", "word_transliteration": "ashaddu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "against", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmani"}, {"word_arabic_uthmani": "\u0639\u0650\u062a\u0650\u064a\u0651\u0657\u0627", "word_translation": "(in) rebellion", "word_transliteration": "itiyyan"}]}