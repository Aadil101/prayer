{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0642\u064e\u0627\u0644\u064e \u0644\u064e\u0647\u064f\u0645\u06e1 \u0631\u064e\u0633\u064f\u0648\u0644\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0646\u064e\u0627\u0642\u064e\u0629\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0633\u064f\u0642\u06e1\u064a\u064e\u0670\u0647\u064e\u0627"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "But said", "word_transliteration": "faqala"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "to them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u064f", "word_translation": "(the) Messenger", "word_transliteration": "rasulu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u0642\u064e\u0629\u064e", "word_translation": "(It is the) she-camel", "word_transliteration": "naqata"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064f\u0642\u06e1\u064a\u064e\u0670\u0647\u064e\u0627", "word_translation": "and her drink", "word_transliteration": "wasuq'yaha"}]}