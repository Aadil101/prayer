{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0645\u0651\u064e\u0646\u06e1 \u062d\u064e\u0648\u06e1\u0644\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0631\u064e\u0627\u0628\u0650 \u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u064f\u0648\u0646\u064e\u06d6 \u0648\u064e\u0645\u0650\u0646\u06e1 \u0623\u064e\u0647\u06e1\u0644\u0650 \u0671\u0644\u06e1\u0645\u064e\u062f\u0650\u064a\u0646\u064e\u0629\u0650 \u0645\u064e\u0631\u064e\u062f\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0646\u0651\u0650\u0641\u064e\u0627\u0642\u0650 \u0644\u064e\u0627 \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0647\u064f\u0645\u06e1\u06d6 \u0646\u064e\u062d\u06e1\u0646\u064f \u0646\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0647\u064f\u0645\u06e1\u06da \u0633\u064e\u0646\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0647\u064f\u0645 \u0645\u0651\u064e\u0631\u0651\u064e\u062a\u064e\u064a\u06e1\u0646\u0650 \u062b\u064f\u0645\u0651\u064e \u064a\u064f\u0631\u064e\u062f\u0651\u064f\u0648\u0646\u064e \u0625\u0650\u0644\u064e\u0649\u0670 \u0639\u064e\u0630\u064e\u0627\u0628\u064d \u0639\u064e\u0638\u0650\u064a\u0645\u0656"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0645\u0651\u064e\u0646\u06e1", "word_translation": "And among those", "word_transliteration": "wamimman"}, {"word_arabic_uthmani": "\u062d\u064e\u0648\u06e1\u0644\u064e\u0643\u064f\u0645", "word_translation": "around you", "word_transliteration": "hawlakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0631\u064e\u0627\u0628\u0650", "word_translation": "the bedouins", "word_transliteration": "al-a'rabi"}, {"word_arabic_uthmani": "\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u064f\u0648\u0646\u064e\u06d6", "word_translation": "(are) hypocrites", "word_transliteration": "munafiquna"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1", "word_translation": "and (also) from", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u0650", "word_translation": "people", "word_transliteration": "ahli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u062f\u0650\u064a\u0646\u064e\u0629\u0650", "word_translation": "(of) the Madinah", "word_transliteration": "al-madinati"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u064e\u062f\u064f\u0648\u0627\u0652", "word_translation": "They persist", "word_transliteration": "maradu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "in", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u0650\u0641\u064e\u0627\u0642\u0650", "word_translation": "the hypocrisy", "word_transliteration": "al-nifaqi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0647\u064f\u0645\u06e1\u06d6", "word_translation": "you know them", "word_transliteration": "ta'lamuhum"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "We", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0646\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0647\u064f\u0645\u06e1\u06da", "word_translation": "[We] know them", "word_transliteration": "na'lamuhum"}, {"word_arabic_uthmani": "\u0633\u064e\u0646\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f\u0647\u064f\u0645", "word_translation": "We will punish them", "word_transliteration": "sanu'adhibuhum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0631\u0651\u064e\u062a\u064e\u064a\u06e1\u0646\u0650", "word_translation": "twice", "word_transliteration": "marratayni"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u064a\u064f\u063