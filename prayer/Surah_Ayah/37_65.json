{"ayahs": {"ayah_uthmani": "\u0637\u064e\u0644\u06e1\u0639\u064f\u0647\u064e\u0627 \u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5 \u0631\u064f\u0621\u064f\u0648\u0633\u064f \u0671\u0644\u0634\u0651\u064e\u064a\u064e\u0670\u0637\u0650\u064a\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0637\u064e\u0644\u06e1\u0639\u064f\u0647\u064e\u0627", "word_translation": "Its emerging fruit", "word_transliteration": "tal'uha"}, {"word_arabic_uthmani": "\u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "(is) as if it", "word_transliteration": "ka-annahu"}, {"word_arabic_uthmani": "\u0631\u064f\u0621\u064f\u0648\u0633\u064f", "word_translation": "(was) heads", "word_transliteration": "ruusu"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u064e\u0670\u0637\u0650\u064a\u0646\u0650", "word_translation": "(of) the devils", "word_transliteration": "al-shayatini"}]}