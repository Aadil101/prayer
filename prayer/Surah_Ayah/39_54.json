{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0650\u064a\u0628\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0644\u064e\u0649\u0670 \u0631\u064e\u0628\u0651\u0650\u0643\u064f\u0645\u06e1 \u0648\u064e\u0623\u064e\u0633\u06e1\u0644\u0650\u0645\u064f\u0648\u0627\u0652 \u0644\u064e\u0647\u064f\u06e5 \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650 \u0623\u064e\u0646 \u064a\u064e\u0623\u06e1\u062a\u0650\u064a\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u064f \u062b\u064f\u0645\u0651\u064e \u0644\u064e\u0627 \u062a\u064f\u0646\u0635\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0650\u064a\u0628\u064f\u0648\u0653\u0627\u0652", "word_translation": "And turn", "word_transliteration": "wa-anibu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064f\u0645\u06e1", "word_translation": "your Lord", "word_transliteration": "rabbikum"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0633\u06e1\u0644\u0650\u0645\u064f\u0648\u0627\u0652", "word_translation": "and submit", "word_transliteration": "wa-aslimu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "to Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650", "word_translation": "before", "word_transliteration": "qabli"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "[that]", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650\u064a\u064e\u0643\u064f\u0645\u064f", "word_translation": "comes to you", "word_transliteration": "yatiyakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u064f", "wor