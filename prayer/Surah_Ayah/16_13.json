{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0630\u064e\u0631\u064e\u0623\u064e \u0644\u064e\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0645\u064f\u062e\u06e1\u062a\u064e\u0644\u0650\u0641\u064b\u0627 \u0623\u064e\u0644\u06e1\u0648\u064e\u0670\u0646\u064f\u0647\u064f\u06e5\u0653\u06da \u0625\u0650\u0646\u0651\u064e \u0641\u0650\u064a \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0644\u064e\u0623\u0653\u064a\u064e\u0629\u0657 \u0644\u0651\u0650\u0642\u064e\u0648\u06e1\u0645\u0656 \u064a\u064e\u0630\u0651\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0630\u064e\u0631\u064e\u0623\u064e", "word_translation": "He multiplied", "word_transliteration": "dhara-a"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0645\u064f\u062e\u06e1\u062a\u064e\u0644\u0650\u0641\u064b\u0627", "word_translation": "(of) varying", "word_transliteration": "mukh'talifan"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u06e1\u0648\u064e\u0670\u0646\u064f\u0647\u064f\u06e5\u0653\u06da", "word_translation": "colors", "word_transliteration": "alwanuhu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "that", "word_transliteration": "dhalika"