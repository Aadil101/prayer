{"ayahs": {"ayah_uthmani": "\u0641\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0649\u0670 \u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0648\u064e\u0642\u064e\u0627\u0644\u064e \u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0644\u064e\u0642\u064e\u062f\u06e1 \u0623\u064e\u0628\u06e1\u0644\u064e\u063a\u06e1\u062a\u064f\u0643\u064f\u0645\u06e1 \u0631\u0650\u0633\u064e\u0627\u0644\u064e\u0629\u064e \u0631\u064e\u0628\u0651\u0650\u064a \u0648\u064e\u0646\u064e\u0635\u064e\u062d\u06e1\u062a\u064f \u0644\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646 \u0644\u0651\u064e\u0627 \u062a\u064f\u062d\u0650\u0628\u0651\u064f\u0648\u0646\u064e \u0671\u0644\u0646\u0651\u064e\u0640\u0670\u0635\u0650\u062d\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0649\u0670", "word_translation": "So he turned away", "word_transliteration": "fatawalla"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "from them", "word_transliteration": "anhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "and he said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "O my people!", "word_transliteration": "yaqawmi"}, {"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "Verily", "word_transliteration": "laqad"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0644\u064e\u063a\u06e1\u062a\u064f\u0643\u064f\u0645\u06e1", "word_translation": "I have conveyed to you", "word_transliteration": "ablaghtukum"}, {"word_arabic_uthmani": "\u0631\u0650\u0633\u064e\u0627\u0644\u064e\u0629\u064e", "word_translation": "(the) Message", "word_transliteration": "risalata"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a", "word_translation": "(of) my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u0635\u064e\u062d\u06e1\u062a\u064f", "word_translation": "and [I] advised", "word_transliteration": "wanasahtu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "[to] you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646", "word_translation": "but", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064f\u062d\u0650\u0628\u0651\u064f\u0648\u0646\u064e", "word_translation": "you like", "word_transliteration": "tuhibbuna"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0640\u0670\u0635\u0650\u062d\u0650\u064a\u0646\u064e", "word_translation": "the advisers", "word_transliteration": "al-nasihina"}]}