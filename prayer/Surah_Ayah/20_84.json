{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0647\u064f\u0645\u06e1 \u0623\u064f\u0648\u0652\u0644\u064e\u0627\u0653\u0621\u0650 \u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u062b\u064e\u0631\u0650\u064a \u0648\u064e\u0639\u064e\u062c\u0650\u0644\u06e1\u062a\u064f \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e \u0631\u064e\u0628\u0651\u0650 \u0644\u0650\u062a\u064e\u0631\u06e1\u0636\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "They", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0627\u0653\u0621\u0650", "word_translation": "(are) close", "word_transliteration": "ulai"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "upon", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064e\u062b\u064e\u0631\u0650\u064a", "word_translation": "my tracks", "word_transliteration": "athari"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u062c\u0650\u0644\u06e1\u062a\u064f", "word_translation": "and I hastened", "word_transliteration": "wa'ajil'tu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e", "word_translation": "to you", "word_transliteration": "ilayka"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064e\u0631\u06e1\u0636\u064e\u0649\u0670", "word_translation": "that You be pleased", "word_transliteration": "litarda"}]}