{"ayahs": {"ayah_uthmani": "\u0643\u064e\u062f\u064e\u0623\u06e1\u0628\u0650 \u0621\u064e\u0627\u0644\u0650 \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1\u06da \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0641\u064e\u0623\u064e\u062e\u064e\u0630\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u0650\u0630\u064f\u0646\u064f\u0648\u0628\u0650\u0647\u0650\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0642\u064e\u0648\u0650\u064a\u0651\u065e \u0634\u064e\u062f\u0650\u064a\u062f\u064f \u0671\u0644\u06e1\u0639\u0650\u0642\u064e\u0627\u0628\u0650"}, "words": [{"word_arabic_uthmani": "\u0643\u064e\u062f\u064e\u0623\u06e1\u0628\u0650", "word_translation": "Like (the) way", "word_transliteration": "kadabi"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u0650", "word_translation": "(of) people", "word_transliteration": "ali"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "(of) Firaun", "word_transliteration": "fir'awna"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "(were) from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1\u06da", "word_translation": "before them", "word_transliteration": "qablihim"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "They disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650", "word_translation": "in (the) Signs", "word_transliteration": "biayati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u062e\u064e\u0630\u064e\u0647\u064f\u0645\u064f", "word_translation": "so seized them", "word_transliteration": "fa-akhadhahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0630\u064f\u0646\u064f\u0648\u0628\u0650\u0647\u0650\u0645\u06e1\u06da", "word_translation": "for their sins", "word_transliteration": "bidhunubihim"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u0650\u064a\u0651\u065e", "word_translation": "(is) All-Strong", "word_transliteration": "qawiyyun"}, {"word_arabic_uthmani": "\u0634\u064e\u062f\u0650\u064a\u062f\u064f", "word_translation": "(and) severe", "word_transliteration": "shadidu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0642\u064e\u0627\u0628\u0650", "word_translation": "(in) the penalty", "word_transliteration": "al-'iqabi"}]}