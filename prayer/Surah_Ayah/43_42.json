{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0648\u06e1 \u0646\u064f\u0631\u0650\u064a\u064e\u0646\u0651\u064e\u0643\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0648\u064e\u0639\u064e\u062f\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645 \u0645\u0651\u064f\u0642\u06e1\u062a\u064e\u062f\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "Or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0646\u064f\u0631\u0650\u064a\u064e\u0646\u0651\u064e\u0643\u064e", "word_translation": "We show you", "word_transliteration": "nuriyannaka"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "that which", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u062f\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "We have promised them", "word_transliteration": "wa'adnahum"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "then indeed We", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645", "word_translation": "over them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0642\u06e1\u062a\u064e\u062f\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "have full power", "word_transliteration": "muq'tadiruna"}]}