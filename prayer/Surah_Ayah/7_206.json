{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0639\u0650\u0646\u062f\u064e \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0644\u064e\u0627 \u064a\u064e\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e \u0639\u064e\u0646\u06e1 \u0639\u0650\u0628\u064e\u0627\u062f\u064e\u062a\u0650\u0647\u0650\u06e6 \u0648\u064e\u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f\u0648\u0646\u064e\u0647\u064f\u06e5 \u0648\u064e\u0644\u064e\u0647\u064f\u06e5 \u064a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0646\u064e\u06e4\u06e9"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "(are) near", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(do) they turn away in pride", "word_transliteration": "yastakbiruna"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1", "word_translation": "from", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u064e\u062a\u0650\u0647\u0650\u06e6", "word_translation": "His worship", "word_transliteration": "ibadatihi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f\u0648\u0646\u064e\u0647\u064f\u06e5", "word_translation": "And they glorify Him", "word_transliteration": "wayusabbihunahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u06e5", "word_translation": "and to Him", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0646\u064e\u06e4\u06e9", "word_translation": "they prostrate", "word_transliteration": "yasjuduna"}]}