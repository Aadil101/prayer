{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u064f \u062c\u064e\u0670\u0647\u0650\u062f\u0650 \u0671\u0644\u06e1\u0643\u064f\u0641\u0651\u064e\u0627\u0631\u064e \u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e \u0648\u064e\u0671\u063a\u06e1\u0644\u064f\u0638\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1\u06da \u0648\u064e\u0645\u064e\u0623\u06e1\u0648\u064e\u0649\u0670\u0647\u064f\u0645\u06e1 \u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064f\u06d6 \u0648\u064e\u0628\u0650\u0626\u06e1\u0633\u064e \u0671\u0644\u06e1\u0645\u064e\u0635\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u064f", "word_translation": "Prophet", "word_transliteration": "al-nabiyu"}, {"word_arabic_uthmani": "\u062c\u064e\u0670\u0647\u0650\u062f\u0650", "word_translation": "Strive", "word_transliteration": "jahidi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064f\u0641\u0651\u064e\u0627\u0631\u064e", "word_translation": "(against) the disbelievers", "word_transliteration": "al-kufara"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "and the hypocrites", "word_transliteration": "wal-munafiqina"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u063a\u06e1\u0644\u064f\u0638\u06e1", "word_translation": "and be stern", "word_transliteration": "wa-ugh'luz"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1\u06da", "word_translation": "with them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0623\u06e1\u0648\u064e\u0649\u0670\u0647\u064f\u0645\u06e1", "word_translation": "And their abode", "word_transliteration": "wamawahum"}, {"word_arabic_uthmani": "\u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064f\u06d6", "word_translation": "(is) Hell", "word_transliteration": "jahannamu"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u0650\u0626\u06e1\u0633\u064e", "word_translation": "and wretched is", "word_transliteration": "wabi'sa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0635\u0650\u064a\u0631\u064f", "word_translation": "the destination", "word_transliteration": "al-masiru"}]}