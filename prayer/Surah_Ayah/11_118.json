{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1 \u0634\u064e\u0627\u0653\u0621\u064e \u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0644\u064e\u062c\u064e\u0639\u064e\u0644\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064e \u0623\u064f\u0645\u0651\u064e\u0629\u0657 \u0648\u064e\u0670\u062d\u0650\u062f\u064e\u0629\u0657\u06d6 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0632\u064e\u0627\u0644\u064f\u0648\u0646\u064e \u0645\u064f\u062e\u06e1\u062a\u064e\u0644\u0650\u0641\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0634\u064e\u0627\u0653\u0621\u064e", "word_translation": "your Lord (had) willed", "word_transliteration": "shaa"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "your Lord (had) willed", "word_transliteration": "rabbuka"}, {"word_arabic_uthmani": "\u0644\u064e\u062c\u064e\u0639\u064e\u0644\u064e", "word_translation": "surely He (could) have made", "word_transliteration": "laja'ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064e", "word_translation": "the mankind", "word_transliteration": "al-nasa"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u064e\u0629\u0657", "word_translation": "one community", "word_transliteration": "ummatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0670\u062d\u0650\u062f\u064e\u0629\u0657\u06d6", "word_translation": "one community", "word_transliteration": "wahidatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "but not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0632\u064e\u0627\u0644\u064f\u0648\u0646\u064e", "word_translation": "they will cease", "word_transliteration": "yazaluna"}, {"word_arabic_uthmani": "\u0645\u064f\u062e\u06e1\u062a\u064e\u0644\u0650\u0641\u0650\u064a\u0646\u064e", "word_translation": "to differ", "word_transliteration": "mukh'talifina"}]}