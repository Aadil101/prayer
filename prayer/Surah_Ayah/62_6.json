{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0647\u064e\u0627\u062f\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646 \u0632\u064e\u0639\u064e\u0645\u06e1\u062a\u064f\u0645\u06e1 \u0623\u064e\u0646\u0651\u064e\u0643\u064f\u0645\u06e1 \u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064f \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0641\u064e\u062a\u064e\u0645\u064e\u0646\u0651\u064e\u0648\u064f\u0627\u0652 \u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u0635\u064e\u0670\u062f\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "you (who)", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0647\u064e\u0627\u062f\u064f\u0648\u0653\u0627\u0652", "word_translation": "(are) Jews", "word_transliteration": "hadu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "If", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0632\u064e\u0639\u064e\u0645\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "you claim", "word_transliteration": "za'amtum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "that you", "word_transliteration": "annakum"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064f", "word_translation": "(are) allies", "word_tran