{"ayahs": {"ayah_uthmani": "\u0645\u064e\u062b\u064e\u0644\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e \u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650 \u0671\u0644\u06e1\u0639\u064e\u0646\u0643\u064e\u0628\u064f\u0648\u062a\u0650 \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e\u062a\u06e1 \u0628\u064e\u064a\u06e1\u062a\u0657\u0627\u06d6 \u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0623\u064e\u0648\u06e1\u0647\u064e\u0646\u064e \u0671\u0644\u06e1\u0628\u064f\u064a\u064f\u0648\u062a\u0650 \u0644\u064e\u0628\u064e\u064a\u06e1\u062a\u064f \u0671\u0644\u06e1\u0639\u064e\u0646\u0643\u064e\u0628\u064f\u0648\u062a\u0650\u06da \u0644\u064e\u0648\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u064e\u062b\u064e\u0644\u064f", "word_translation": "(The) example", "word_transliteration": "mathalu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652", "word_translation": "take", "word_transliteration": "ittakhadhu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "besides", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e", "word_translation": "protectors", "word_transliteration": "awliyaa"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650", "word_translation": "(is) like", "word_transliteration": "kamathali"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0646\u0643\u064e\u0628\u064f\u0648\u062a\u0650", "word_translation": "the spider", "word_transliteration": "al-'ankabuti"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e\u062a\u06e1", "word_translation": "who takes", "word_transliteration": "ittakhadhat"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u062a\u0657\u0627\u06d6", "word_translation": "a house", "word_transliteration": "baytan"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0647\u064e\u0646\u064e", "word_translation": "the weakest", "word_transliteration": "awhana"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064f\u064a\u064f\u0648\u062a\u0650", "word_translation": "(of) houses", "word_transliteration": "al-buyuti"}, {"word_arabic_uthmani": "\u0644\u064e\u0628\u064e\u064a\u06e1\u062a\u064f", "word_translation": "(is) surely (the) house", "word_transliteration": "labaytu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0646\u0643\u064e\u0628\u064f\u0648\u062a\u0650\u06da", "word_translation": "(of) the spider", "word_transliteration": "al-'ankabuti"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1", "word_translation": "if (only)", "word_transliteration": "law"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "know", "word_transliteration": "ya'lamuna"}]}