{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652 \u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0646\u064e\u0633\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0641\u064e\u0623\u064e\u0646\u0633\u064e\u0649\u0670\u0647\u064f\u0645\u06e1 \u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0647\u064f\u0645\u06e1\u06da \u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652", "word_translation": "be", "word_transliteration": "takunu"}, {"word_arabic_uthmani": "\u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "like those who", "word_transliteration": "ka-alladhina"}, {"word_arabic_uthmani": "\u0646\u064e\u0633\u064f\u0648\u0627\u0652", "word_translation": "forgot", "word_transliteration": "nasu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0646\u0633\u064e\u0649\u0670\u0647\u064f\u0645\u06e1", "word_translation": "so He made them forget", "word_transliteration": "fa-ansahum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "themselves", "word_transliteration": "anfusahum"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "[they]", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "(are) the defiantly disobedient", "word_transliteration": "al-fasiquna"}]}