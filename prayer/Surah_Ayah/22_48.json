{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0643\u064e\u0623\u064e\u064a\u0651\u0650\u0646 \u0645\u0651\u0650\u0646 \u0642\u064e\u0631\u06e1\u064a\u064e\u0629\u064d \u0623\u064e\u0645\u06e1\u0644\u064e\u064a\u06e1\u062a\u064f \u0644\u064e\u0647\u064e\u0627 \u0648\u064e\u0647\u0650\u064a\u064e \u0638\u064e\u0627\u0644\u0650\u0645\u064e\u0629\u065e \u062b\u064f\u0645\u0651\u064e \u0623\u064e\u062e\u064e\u0630\u06e1\u062a\u064f\u0647\u064e\u0627 \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u0651\u064e \u0671\u0644\u06e1\u0645\u064e\u0635\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0623\u064e\u064a\u0651\u0650\u0646", "word_translation": "And how many", "word_transliteration": "waka-ayyin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u06e1\u064a\u064e\u0629\u064d", "word_translation": "a township", "word_transliteration": "qaryatin"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0644\u064e\u064a\u06e1\u062a\u064f", "word_translation": "I gave respite", "word_transliteration": "amlaytu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064e\u0627", "word_translation": "to it", "word_transliteration": "laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u0650\u064a\u064e", "word_translation": "while it", "word_transliteration": "wahiya"}, {"word_arabic_uthmani": "\u0638\u064e\u0627\u0644\u0650\u0645\u064e\u0629\u065e", "word_translation": "(was) doing wrong", "word_transliteration": "zalimatun"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u064e\u0630\u06e1\u062a\u064f\u0647\u064e\u0627", "word_translation": "I seized it", "word_transliteration": "akhadhtuha"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u064a\u0651\u064e", "word_translation": "and to Me", "word_transliteration": "wa-ilayya"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0635\u0650\u064a\u0631\u064f", "word_translation": "(is) the destination", "word_transliteration": "al-masiru"}]}