{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0671\u0646\u062a\u064e\u0642\u064e\u0645\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u064e\u0627 \u0644\u064e\u0628\u0650\u0625\u0650\u0645\u064e\u0627\u0645\u0656 \u0645\u0651\u064f\u0628\u0650\u064a\u0646\u0656"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0671\u0646\u062a\u064e\u0642\u064e\u0645\u06e1\u0646\u064e\u0627", "word_translation": "So We took retribution", "word_transliteration": "fa-intaqamna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "from them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u064e\u0627", "word_translation": "and indeed they both", "word_transliteration": "wa-innahuma"}, {"word_arabic_uthmani": "\u0644\u064e\u0628\u0650\u0625\u0650\u0645\u064e\u0627\u0645\u0656", "word_translation": "(were) on a highway", "word_transliteration": "labi-imamin"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0628\u0650\u064a\u0646\u0656", "word_translation": "clear", "word_transliteration": "mubinin"}]}