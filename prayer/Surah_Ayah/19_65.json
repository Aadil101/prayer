{"ayahs": {"ayah_uthmani": "\u0631\u0651\u064e\u0628\u0651\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627 \u0641\u064e\u0671\u0639\u06e1\u0628\u064f\u062f\u06e1\u0647\u064f \u0648\u064e\u0671\u0635\u06e1\u0637\u064e\u0628\u0650\u0631\u06e1 \u0644\u0650\u0639\u0650\u0628\u064e\u0670\u062f\u064e\u062a\u0650\u0647\u0650\u06e6\u06da \u0647\u064e\u0644\u06e1 \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0644\u064e\u0647\u064f\u06e5 \u0633\u064e\u0645\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u064f", "word_translation": "Lord", "word_transliteration": "rabbu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627", "word_translation": "(is) between both of them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0639\u06e1\u0628\u064f\u062f\u06e1\u0647\u064f", "word_translation": "so worship Him", "word_transliteration": "fa-u'bud'hu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0635\u06e1\u0637\u064e\u0628\u0650\u0631\u06e1", "word_translation": "and be constant", "word_transliteration": "wa-is'tabir"}, {"word_arabic_uthmani": "\u0644\u0650\u0639\u0650\u0628\u064e\u0670\u062f\u064e\u062a\u0650\u0647\u0650\u06e6\u06da", "word_translation": "in His worship", "word_transliteration": "li'ibadatihi"}, {"word_arabic_uthmani": "\u0647\u064e\u0644\u06e1", "word_translation": "Do", "word_transliteration": "hal"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "you know", "word_transliteration": "ta'lamu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "for Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0633\u064e\u0645\u0650\u064a\u0651\u0657\u0627", "word_translation": "any similarity", "word_transliteration": "samiyyan"}]}