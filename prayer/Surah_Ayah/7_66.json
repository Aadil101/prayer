{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0623\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6\u0653 \u0625\u0650\u0646\u0651\u064e\u0627 \u0644\u064e\u0646\u064e\u0631\u064e\u0649\u0670\u0643\u064e \u0641\u0650\u064a \u0633\u064e\u0641\u064e\u0627\u0647\u064e\u0629\u0656 \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0644\u064e\u0646\u064e\u0638\u064f\u0646\u0651\u064f\u0643\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0643\u064e\u0670\u0630\u0650\u0628\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0623\u064f", "word_translation": "the chiefs", "word_transliteration": "al-mala-u"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6\u0653", "word_translation": "his people", "word_transliteration": "qawmihi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0631\u064e\u0649\u0670\u0643\u064e", "word_translation": "surely see you", "word_transliteration": "lanaraka"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0633\u064e\u0641\u064