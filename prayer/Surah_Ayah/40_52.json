{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e \u0644\u064e\u0627 \u064a\u064e\u0646\u0641\u064e\u0639\u064f \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e \u0645\u064e\u0639\u06e1\u0630\u0650\u0631\u064e\u062a\u064f\u0647\u064f\u0645\u06e1\u06d6 \u0648\u064e\u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0639\u06e1\u0646\u064e\u0629\u064f \u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1 \u0633\u064f\u0648\u0653\u0621\u064f \u0671\u0644\u062f\u0651\u064e\u0627\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(The) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u0641\u064e\u0639\u064f", "word_translation": "will benefit", "word_transliteration": "yanfa'u"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimina"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u06e1\u0630\u0650\u0631\u064e\u062a\u064f\u0647\u064f\u0645\u06e1\u06d6", "word_translation": "their excuse", "word_transliteration": "ma'dhiratuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "and for them", "word_transliteration": "walahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0639\u06e1\u0646\u064e\u0629\u064f", "word_translation": "(is) the curse", "word_transliteration": "al-la'natu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and for them", "word_transliteration": "walahum"}, {"word_arabic_uthmani": "\u0633\u064f\u0648\u0653\u0621\u064f", "word_translation": "(is the) worst", "word_transliteration": "suu"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064e\u0627\u0631\