{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0627 \u0645\u0650\u0646\u0651\u064e\u0627 \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064f\u0648\u0646\u064e \u0648\u064e\u0645\u0650\u0646\u0651\u064e\u0627 \u062f\u064f\u0648\u0646\u064e \u0630\u064e\u0670\u0644\u0650\u0643\u064e\u06d6 \u0643\u064f\u0646\u0651\u064e\u0627 \u0637\u064e\u0631\u064e\u0627\u0653\u0626\u0650\u0642\u064e \u0642\u0650\u062f\u064e\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0627", "word_translation": "And that", "word_transliteration": "wa-anna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0651\u064e\u0627", "word_translation": "among us", "word_transliteration": "minna"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064f\u0648\u0646\u064e", "word_translation": "(are) the righteous", "word_transliteration": "al-salihuna"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u0651\u064e\u0627", "word_translation": "and among us", "word_transliteration": "waminna"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u064e", "word_translation": "(are) other than", "word_transliteration": "duna"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e\u06d6", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "We", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0637\u064e\u0631\u064e\u0627\u0653\u0626\u0650\u0642\u064e", "word_translation": "(are on) ways", "word_transliteration": "taraiqa"}, {"word_arabic_uthmani": "\u0642\u0650\u062f\u064e\u062f\u0657\u0627", "word_translation": "different", "word_transliteration": "qidadan"}]}