{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u064e\u0634\u064e\u0631\u065e \u0645\u0651\u0650\u062b\u06e1\u0644\u064f\u0646\u064e\u0627 \u0648\u064e\u0625\u0650\u0646 \u0646\u0651\u064e\u0638\u064f\u0646\u0651\u064f\u0643\u064e \u0644\u064e\u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0643\u064e\u0670\u0630\u0650\u0628\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "you", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "(are) except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u064e\u0634\u064e\u0631\u065e", "word_translation": "a man", "word_transliteration": "basharun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u062b\u06e1\u0644\u064f\u0646\u064e\u0627", "word_translation": "like us", "word_transliteration": "mith'luna"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "and indeed", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0638\u064f\u0646\u0651\u064f\u0643\u064e", "word_translation": "we think you", "word_transliteration": "nazunnuka"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u0650\u0646\u064e", "word_translation": "surely (are) of", "word_transliteration": "lamina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0630\u0650\u0628\u0650\u064a\u0646\u064e", "word_translation": "the liars", "word_transliteration": "al-kadhibina"}]}