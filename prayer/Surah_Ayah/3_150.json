{"ayahs": {"ayah_uthmani": "\u0628\u064e\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064e\u0648\u06e1\u0644\u064e\u0649\u0670\u0643\u064f\u0645\u06e1\u06d6 \u0648\u064e\u0647\u064f\u0648\u064e \u062e\u064e\u064a\u06e1\u0631\u064f \u0671\u0644\u0646\u0651\u064e\u0640\u0670\u0635\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0628\u064e\u0644\u0650", "word_translation": "Nay", "word_transliteration": "bali"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0648\u06e1\u0644\u064e\u0649\u0670\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "(is) your Protector", "word_transliteration": "mawlakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u064f", "word_translation": "(is the) best", "word_transliteration": "khayru"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0640\u0670\u0635\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(of) the Helpers", "word_transliteration": "al-nasirina"}]}