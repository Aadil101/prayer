{"ayahs": {"ayah_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e \u0646\u064f\u0633\u064e\u064a\u0651\u0650\u0631\u064f \u0671\u0644\u06e1\u062c\u0650\u0628\u064e\u0627\u0644\u064e \u0648\u064e\u062a\u064e\u0631\u064e\u0649 \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0628\u064e\u0627\u0631\u0650\u0632\u064e\u0629\u0657 \u0648\u064e\u062d\u064e\u0634\u064e\u0631\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0641\u064e\u0644\u064e\u0645\u06e1 \u0646\u064f\u063a\u064e\u0627\u062f\u0650\u0631\u06e1 \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0623\u064e\u062d\u064e\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "And the Day", "word_transliteration": "wayawma"}, {"word_arabic_uthmani": "\u0646\u064f\u0633\u064e\u064a\u0651\u0650\u0631\u064f", "word_translation": "We will cause (to) move", "word_transliteration": "nusayyiru"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u0650\u0628\u064e\u0627\u0644\u064e", "word_translation": "the mountains", "word_transliteration": "al-jibala"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0631\u064e\u0649", "word_translation": "and you will see", "word_transliteration": "watara"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "the earth", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0628\u064e\u0627\u0631\u0650\u0632\u064e\u0629\u0657", "word_translation": "(as) a leveled plain", "word_transliteration": "barizatan"}, {"word_arabic_uthmani": "\u0648\u064e\u062d\u064e\u0634\u064e\u0631\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "and We will gather them", "word_transliteration": "wahasharnahum"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u06e1", "word_translation": "and not", "word_transliteration": "falam"}, {"word_arabic_uthmani": "\u0646\u064f\u063a\u064e\u0627\u062f\u0650\u0631\u06e1", "word_translation": "We will leave behind", "word_transliteration": "nughadir"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "from them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u0657\u0627", "word_translation": "anyone", "word_transliteration": "ahadan"}]}