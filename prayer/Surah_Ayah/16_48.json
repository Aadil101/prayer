{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652 \u0625\u0650\u0644\u064e\u0649\u0670 \u0645\u064e\u0627 \u062e\u064e\u0644\u064e\u0642\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u0650\u0646 \u0634\u064e\u064a\u06e1\u0621\u0656 \u064a\u064e\u062a\u064e\u0641\u064e\u064a\u0651\u064e\u0624\u064f\u0627\u0652 \u0638\u0650\u0644\u064e\u0670\u0644\u064f\u0647\u064f\u06e5 \u0639\u064e\u0646\u0650 \u0671\u0644\u06e1\u064a\u064e\u0645\u0650\u064a\u0646\u0650 \u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0645\u064e\u0627\u0653\u0626\u0650\u0644\u0650 \u0633\u064f\u062c\u0651\u064e\u062f\u0657\u0627 \u0644\u0651\u0650\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0647\u064f\u0645\u06e1 \u062f\u064e\u0670\u062e\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Have not", "word_transliteration": "awalam"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652", "word_translation": "they seen", "word_transliteration": "yaraw"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "[towards]", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064e", "word_translation": "Allah has created", "word_transliteration": "khalaqa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah has created", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "a thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0641\u064e\u064a\u0651\u064e\u0624\u064f\u0627\u0652", "word_translation": "Incline", "word_transliteration": "yatafayya-u"}, {"word_arabic_uthmani": "\u0638\u0650\u0644\u064e\u0670\u0644\u064f\u0647\u064f\u06e5", "word_translation": "their shadows", "word_transliteration": "zilaluhu"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0650", "word_translation": "to", "word_transliteration": "ani"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u064a\u064e\u0645\u0650\u064a\u0646\u0650", "word_translation": "the right", "word_transliteration": "al-yamini"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0645\u064e\u0627\u0653\u0626\u0650\u0644\u0650", "word_translation": "and to the left", "word_transliteration": "wal-shamaili"}, {"word_arabic_uthmani": "\u0633\u064f\u062c\u0651\u064e\u062f\u0657\u0627", "word_translation": "prostrating", "word_transliteration": "sujjadan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "to Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u062f\u064e\u0670\u062e\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(are) humble", "word_transliteration": "dakhiruna"}]}