{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e \u064a\u064f\u0643\u06e1\u0634\u064e\u0641\u064f \u0639\u064e\u0646 \u0633\u064e\u0627\u0642\u0656 \u0648\u064e\u064a\u064f\u062f\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0633\u0651\u064f\u062c\u064f\u0648\u062f\u0650 \u0641\u064e\u0644\u064e\u0627 \u064a\u064e\u0633\u06e1\u062a\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(The) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u064a\u064f\u0643\u06e1\u0634\u064e\u0641\u064f", "word_translation": "will be uncovered", "word_transliteration": "yuk'shafu"}, {"word_arabic_uthmani": "\u0639\u064e\u0646", "word_translation": "from", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0633\u064e\u0627\u0642\u0656", "word_translation": "the shin", "word_transliteration": "saqin"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u062f\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "and they will be called", "word_transliteration": "wayud'awna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064f\u062c\u064f\u0648\u062f\u0650", "word_translation": "prostrate", "word_transliteration": "al-sujudi"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "but not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0646\u064e", "word_translation": "they will be able", "word_transliteration": "yastati'una"}]}