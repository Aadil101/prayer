{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e \u064a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0646\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u064e\u062c\u06e1\u062f\u064e\u0627\u062b\u0650 \u0633\u0650\u0631\u064e\u0627\u0639\u0657\u0627 \u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0625\u0650\u0644\u064e\u0649\u0670 \u0646\u064f\u0635\u064f\u0628\u0656 \u064a\u064f\u0648\u0641\u0650\u0636\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(The) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0646\u064e", "word_translation": "they will come out", "word_transliteration": "yakhrujuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u062c\u06e1\u062f\u064e\u0627\u062b\u0650", "word_translation": "the graves", "word_transliteration": "al-ajdathi"}, {"word_arabic_uthmani": "\u0633\u0650\u0631\u064e\u0627\u0639\u0657\u0627", "word_translation": "rapidly", "word_transliteration": "sira'an"}, {"word_arabic_uthmani": "\u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "as if they (were)", "word_transliteration": "ka-annahum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0646\u064f\u0635\u064f\u0628\u0656", "word_translation": "a goal", "word_transliteration": "nusubin"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0641\u0650\u0636\u064f\u0648\u0646\u064e", "word_translation": "hastening", "word_transliteration": "yufiduna"}]}