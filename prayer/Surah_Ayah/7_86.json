{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0642\u06e1\u0639\u064f\u062f\u064f\u0648\u0627\u0652 \u0628\u0650\u0643\u064f\u0644\u0651\u0650 \u0635\u0650\u0631\u064e\u0670\u0637\u0656 \u062a\u064f\u0648\u0639\u0650\u062f\u064f\u0648\u0646\u064e \u0648\u064e\u062a\u064e\u0635\u064f\u062f\u0651\u064f\u0648\u0646\u064e \u0639\u064e\u0646 \u0633\u064e\u0628\u0650\u064a\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0645\u064e\u0646\u06e1 \u0621\u064e\u0627\u0645\u064e\u0646\u064e \u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u062a\u064e\u0628\u06e1\u063a\u064f\u0648\u0646\u064e\u0647\u064e\u0627 \u0639\u0650\u0648\u064e\u062c\u0657\u0627\u06da \u0648\u064e\u0671\u0630\u06e1\u0643\u064f\u0631\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0630\u06e1 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627 \u0641\u064e\u0643\u064e\u062b\u0651\u064e\u0631\u064e\u0643\u064f\u0645\u06e1\u06d6 \u0648\u064e\u0671\u0646\u0638\u064f\u0631\u064f\u0648\u0627\u0652 \u0643\u064e\u064a\u06e1\u0641\u064e \u0643\u064e\u0627\u0646\u064e \u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f \u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u06e1\u0639\u064f\u062f\u064f\u0648\u0627\u0652", "word_translation": "sit", "word_transliteration": "taq'udu"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064f\u0644\u0651\u0650", "word_translation": "on every", "word_transliteration": "bikulli"}, {"word_arabic_uthmani": "\u0635\u0650\u0631\u064e\u0670\u0637\u0656", "word_translation": "path", "word_transliteration": "siratin"}, {"word_arabic_uthmani": "\u062a\u064f\u0648\u0639\u0650\u062f\u064f\u0648\u0646\u064e", "word_translation": "threatening", "word_transliteration": "tu'iduna"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0635\u064f\u062f\u0651\u064f\u0648\u0646\u064e", "word_translation": "and hindering", "word_transliteration": "watasudduna"}, {"word_arabic_uthmani": "\u0639\u064e\u0646", "word_translation": "from", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u0650", "word_translation": "(the) way", "word_transliteration": "sabili"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u06e1", "word_translation": "(those) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064e", "word_translation": "believe", "word_transliteration": "amana"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "in Him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0628\u06e1\u063a\u064f\u0648\u0646\u064e\u0647\u064e\u0627", "word_translation": "and seeking (to make) it", "word_transliteration": "watabghunaha"}, {"word_arabic_uthmani": "\u0639\u0650\u0648\u064e\u062c\u0657\u0627\u06da", "word_translation": "crooked", "word_transliteration": "iwajan"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0630\u06e1\u0643\u064f\u0631\u064f\u0648\u0653\u0627\u0652", "word_translation": "And remember", "word_transliteration": "wa-udh'kuru"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "when", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you were", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627", "word_translation": "few", "word_transliteration": "qalilan"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u062b\u0651\u064e\u0631\u064e\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "and He increased you", "word_transliteration": "fa