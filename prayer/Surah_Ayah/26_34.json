{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0644\u0650\u0644\u06e1\u0645\u064e\u0644\u064e\u0625\u0650 \u062d\u064e\u0648\u06e1\u0644\u064e\u0647\u064f\u06e5\u0653 \u0625\u0650\u0646\u0651\u064e \u0647\u064e\u0670\u0630\u064e\u0627 \u0644\u064e\u0633\u064e\u0670\u062d\u0650\u0631\u064c \u0639\u064e\u0644\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0645\u064e\u0644\u064e\u0625\u0650", "word_translation": "to the chiefs", "word_transliteration": "lil'mala-i"}, {"word_arabic_uthmani": "\u062d\u064e\u0648\u06e1\u0644\u064e\u0647\u064f\u06e5\u0653", "word_translation": "around him", "word_transliteration": "hawlahu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "this", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0644\u064e\u0633\u064e\u0670\u062d\u0650\u0631\u064c", "word_translation": "(is) surely a magician", "word_transliteration": "lasahirun"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u065e", "word_translation": "learned", "word_transliteration": "alimun"}]}