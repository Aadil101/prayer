{"ayahs": {"ayah_uthmani": "\u0644\u0651\u064e\u0648\u06e1\u0644\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064f\u0648 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0628\u0650\u0623\u064e\u0631\u06e1\u0628\u064e\u0639\u064e\u0629\u0650 \u0634\u064f\u0647\u064e\u062f\u064e\u0627\u0653\u0621\u064e\u06da \u0641\u064e\u0625\u0650\u0630\u06e1 \u0644\u064e\u0645\u06e1 \u064a\u064e\u0623\u06e1\u062a\u064f\u0648\u0627\u0652 \u0628\u0650\u0671\u0644\u0634\u0651\u064f\u0647\u064e\u062f\u064e\u0627\u0653\u0621\u0650 \u0641\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0643\u064e\u0670\u0630\u0650\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "Why (did) not", "word_transliteration": "lawla"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064f\u0648", "word_translation": "they bring", "word_transliteration": "jau"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "for it", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0631\u06e1\u0628\u064e\u0639\u064e\u0629\u0650", "word_translation": "four", "word_transliteration": "bi-arba'ati"}, {"word_arabic_uthmani": "\u0634\u064f\u0647\u064e\u062f\u064e\u0627\u0653\u0621\u064e\u06da", "word_translation": "witnesses", "word_transliteration": "shuhadaa"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0630\u06e1", "word_translation": "Then when", "word_transliteration": "fa-idh"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u064f\u0648\u0627\u0652", "word_translation": "they brought", "word_transliteration": "yatu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0634\u0651\u064f\u0647\u064e\u062f\u064e\u0627\u0653\u0621\u0650", "word_translation": "the witnesses", "word_transliteration": "bil-shuhadai"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "then those", "word_transliteration": "fa-ulaika"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "near Allah", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "near Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "they", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0630\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "(are) the liars", "word_transliteration": "al-kadhibuna"}]}