{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0634\u064e\u0647\u0650\u064a\u062f\u064e\u06e2\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0646\u064e\u0627 \u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1 \u0625\u0650\u0646 \u0643\u064f\u0646\u0651\u064e\u0627 \u0639\u064e\u0646\u06e1 \u0639\u0650\u0628\u064e\u0627\u062f\u064e\u062a\u0650\u0643\u064f\u0645\u06e1 \u0644\u064e\u063a\u064e\u0670\u0641\u0650\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "So sufficient", "word_transliteration": "fakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(is) Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0634\u064e\u0647\u0650\u064a\u062f\u064e\u06e2\u0627", "word_translation": "(as) a witness", "word_transliteration": "shahidan"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0646\u064e\u0627", "word_translation": "between us", "word_transliteration": "baynana"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1", "word_translation": "and between you", "word_transliteration": "wabaynakum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "that", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "we were", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1", "word_translation": "of", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u064e\u062a\u0650\u0643\u064f\u0645\u06e1", "word_translation": "your worship", "word_transliteration": "ibadatikum"}, {"word_arabic_uthmani": "\u0644\u064e\u063a\u064e\u0670\u0641\u0650\u0644\u0650\u064a\u0646\u064e", "word_translation": "certainly unaware", "word_transliteration": "laghafilina"}]}