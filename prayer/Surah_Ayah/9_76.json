{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6 \u0628\u064e\u062e\u0650\u0644\u064f\u0648\u0627\u0652 \u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0648\u0627\u0652 \u0648\u0651\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0639\u06e1\u0631\u0650\u0636\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653", "word_translation": "But when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645", "word_translation": "He gave them", "word_transliteration": "atahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6", "word_translation": "His Bounty", "word_transliteration": "fadlihi"}, {"word_arabic_uthmani": "\u0628\u064e\u062e\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "they became stingy", "word_transliteration": "bakhilu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0648\u0627\u0652", "word_translation": "and turned away", "word_transliteration": "watawallaw"}, {"word_arabic_uthmani": "\u0648\u0651\u064e\u0647\u064f\u0645", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0639\u06e1\u0631\u0650\u0636\u064f\u0648\u0646\u064e", "word_translation": "(were) averse", "word_transliteration": "mu'riduna"}]}