{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u064a\u064e\u0670\u0648\u064e\u064a\u06e1\u0644\u064e\u0646\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e\u0627 \u0643\u064f\u0646\u0651\u064e\u0627 \u0638\u064e\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0648\u064e\u064a\u06e1\u0644\u064e\u0646\u064e\u0627\u0653", "word_translation": "O woe to us!", "word_transliteration": "yawaylana"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed [we]", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "we were", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0638\u064e\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "wrongdoers", "word_transliteration": "zalimina"}]}