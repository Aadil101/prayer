{"ayahs": {"ayah_uthmani": "\u06de\u0644\u064e\u064a\u06e1\u0633\u064f\u0648\u0627\u0652 \u0633\u064e\u0648\u064e\u0627\u0653\u0621\u0657\u06d7 \u0645\u0651\u0650\u0646\u06e1 \u0623\u064e\u0647\u06e1\u0644\u0650 \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u0623\u064f\u0645\u0651\u064e\u0629\u065e \u0642\u064e\u0627\u0653\u0626\u0650\u0645\u064e\u0629\u065e \u064a\u064e\u062a\u06e1\u0644\u064f\u0648\u0646\u064e \u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0621\u064e\u0627\u0646\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650 \u0648\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0644\u064e\u064a\u06e1\u0633\u064f\u0648\u0627\u0652", "word_translation": "They are not", "word_transliteration": "laysu"}, {"word_arabic_uthmani": "\u0633\u064e\u0648\u064e\u0627\u0653\u0621\u0657\u06d7", "word_translation": "(the) same", "word_transliteration": "sawaan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "among", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u0650", "word_translation": "(the) People", "word_transliteration": "ahli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "(of) the Book", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u064e\u0629\u065e", "word_translation": "(is) a community", "word_transliteration": "ummatun"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0653\u0626\u0650\u0645\u064e\u0629\u065e", "word_translation": "standing", "word_transliteration": "qaimatun"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u06e1\u0644\u064f\u0648\u0646\u064e", "word_translation": "(and) reciting", "word_transliteration": "yatluna"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650", "word_translation": "(the) Verses", "word_transliteration": "ayati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0646\u064e\u0627\u0653\u0621\u064e", "word_translation": "(in the) hours", "word_transliteration": "anaa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650", "word_translation": "(of) the night", "word_transliteration": "al-layli"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0646\u064e", "word_translation": "prostrate", "word_transliteration": "yasjuduna"}]}