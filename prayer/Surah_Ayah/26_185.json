{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0633\u064e\u062d\u0651\u064e\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "you", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "(are) of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0633\u064e\u062d\u0651\u064e\u0631\u0650\u064a\u0646\u064e", "word_translation": "those bewitched", "word_transliteration": "al-musaharina"}]}