{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0639\u0650\u0638\u064f\u0643\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u0646 \u062a\u064e\u0639\u064f\u0648\u062f\u064f\u0648\u0627\u0652 \u0644\u0650\u0645\u0650\u062b\u06e1\u0644\u0650\u0647\u0650\u06e6\u0653 \u0623\u064e\u0628\u064e\u062f\u064b\u0627 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645 \u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0639\u0650\u0638\u064f\u0643\u064f\u0645\u064f", "word_translation": "Allah warns you", "word_transliteration": "ya'izukumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah warns you", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u064f\u0648\u062f\u064f\u0648\u0627\u0652", "word_translation": "you return", "word_transliteration": "ta'udu"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u0650\u062b\u06e1\u0644\u0650\u0647\u0650\u06e6\u0653", "word_translation": "(to the) like of it", "word_transliteration": "limith'lihi"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u064e\u062f\u064b\u0627", "word_translation": "ever", "word_transliteration": "abadan"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645", "word_translation": "you are", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "believers", "word_transliteration": "mu'minina"}]}