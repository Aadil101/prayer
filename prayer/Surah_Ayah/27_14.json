{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062c\u064e\u062d\u064e\u062f\u064f\u0648\u0627\u0652 \u0628\u0650\u0647\u064e\u0627 \u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u064a\u06e1\u0642\u064e\u0646\u064e\u062a\u06e1\u0647\u064e\u0627\u0653 \u0623\u064e\u0646\u0641\u064f\u0633\u064f\u0647\u064f\u0645\u06e1 \u0638\u064f\u0644\u06e1\u0645\u0657\u0627 \u0648\u064e\u0639\u064f\u0644\u064f\u0648\u0651\u0657\u0627\u06da \u0641\u064e\u0671\u0646\u0638\u064f\u0631\u06e1 \u0643\u064e\u064a\u06e1\u0641\u064e \u0643\u064e\u0627\u0646\u064e \u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f \u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u062d\u064e\u062f\u064f\u0648\u0627\u0652", "word_translation": "And they rejected", "word_transliteration": "wajahadu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u064e\u0627", "word_translation": "them", "word_transliteration": "biha"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u064a\u06e1\u0642\u064e\u0646\u064e\u062a\u06e1\u0647\u064e\u0627\u0653", "word_translation": "though were convinced with them (signs)", "word_transliteration": "wa-is'tayqanatha"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u064f\u0647\u064f\u0645\u06e1", "word_translation": "themselves", "word_transliteration": "anfusuhum"}, {"word_arabic_uthmani": "\u0638\u064f\u0644\u06e1\u0645\u0657\u0627", "word_translation": "(out of) injustice", "word_transliteration": "zul'man"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064f\u0644\u064f\u0648\u0651\u0657\u0627\u06da", "word_translation": "and haughtiness", "word_transliteration": "wa'uluwwan"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0646\u0638\u064f\u0631\u06e1", "word_translation": "So see", "word_transliteration": "fa-unzur"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "how", "word_transliteration": "kayfa"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "was", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f", "word_translation": "(the) end", "word_transliteration": "aqibatu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e", "word_translation": "(of) the corrupters", "word_transliteration": "al-muf'sidina"}]}