{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0646\u06e1 \u0623\u064e\u0638\u06e1\u0644\u064e\u0645\u064f \u0645\u0650\u0645\u0651\u064e\u0646\u0650 \u0671\u0641\u06e1\u062a\u064e\u0631\u064e\u0649\u0670 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0643\u064e\u0630\u0650\u0628\u064b\u0627 \u0623\u064e\u0648\u06e1 \u0643\u064e\u0630\u0651\u064e\u0628\u064e \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0647\u0650\u06e6\u0653\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u0627 \u064a\u064f\u0641\u06e1\u0644\u0650\u062d\u064f \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646\u06e1", "word_translation": "And who", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u0623\u064e\u0638\u06e1\u0644\u064e\u0645\u064f", "word_translation": "(is) more unjust", "word_transliteration": "azlamu"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0646\u0650", "word_translation": "than (he) who", "word_transliteration": "mimmani"}, {"word_arabic_uthmani": "\u0671\u0641\u06e1\u062a\u064e\u0631\u064e\u0649\u0670", "word_translation": "invents", "word_transliteration": "if'tara"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "against", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0650\u0628\u064b\u0627", "word_translation": "a lie", "word_transliteration": "kadhiban"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064e", "word_translation": "rejects", "word_transliteration": "kadhaba"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u