{"ayahs": {"ayah_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e \u0646\u064e\u062d\u06e1\u0634\u064f\u0631\u064f \u0645\u0650\u0646 \u0643\u064f\u0644\u0651\u0650 \u0623\u064f\u0645\u0651\u064e\u0629\u0656 \u0641\u064e\u0648\u06e1\u062c\u0657\u0627 \u0645\u0651\u0650\u0645\u0651\u064e\u0646 \u064a\u064f\u0643\u064e\u0630\u0651\u0650\u0628\u064f \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627 \u0641\u064e\u0647\u064f\u0645\u06e1 \u064a\u064f\u0648\u0632\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "And (the) Day", "word_transliteration": "wayawma"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0634\u064f\u0631\u064f", "word_translation": "We will gather", "word_transliteration": "nahshuru"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u064e\u0629\u0656", "word_translation": "nation", "word_transliteration": "ummatin"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u06e1\u062c\u0657\u0627", "word_translation": "a troop", "word_transliteration": "fawjan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0646", "word_translation": "of (those) who", "word_transliteration": "mimman"}, {"word_arabic_uthmani": "\u064a\u064f\u0643\u064e\u0630\u0651\u0650\u0628\u064f", "word_translation": "deny", "word_transliteration": "yukadhibu"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627", "word_translation": "Our Signs", "word_transliteration": "biayatina"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "fahum"}, {"word_arabic_uthmani": "\u064a\u064f\u06