{"ayahs": {"ayah_uthmani": "\u0645\u0651\u064e\u0646 \u064a\u064f\u0637\u0650\u0639\u0650 \u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064e \u0641\u064e\u0642\u064e\u062f\u06e1 \u0623\u064e\u0637\u064e\u0627\u0639\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e\u06d6 \u0648\u064e\u0645\u064e\u0646 \u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0649\u0670 \u0641\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u062d\u064e\u0641\u0650\u064a\u0638\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0645\u0651\u064e\u0646", "word_translation": "(He) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064f\u0637\u0650\u0639\u0650", "word_translation": "obeys", "word_transliteration": "yuti'i"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064e", "word_translation": "the Messenger", "word_transliteration": "al-rasula"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u06e1", "word_translation": "then surely", "word_transliteration": "faqad"}, {"word_arabic_uthmani": "\u0623\u064e\u0637\u064e\u0627\u0639\u064e", "word_translation": "he obeyed", "word_transliteration": "ata'a"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e\u06d6", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "and whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0649\u0670", "word_translation": "turns away", "word_transliteration": "tawalla"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627\u0653", "word_translation": "then not", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e", "word_translation": "We (have) sent you", "word_transliteration": "arsalnaka"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "over them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u062d\u064e\u0641\u0650\u064a\u0638\u0657\u0627", "word_translation": "(as) a guardian", "word_transliteration": "hafizan"}]}