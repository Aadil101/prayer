{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u0657 \u0644\u0651\u0650\u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652 \u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u0650\u0632\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652", "word_translation": "And they have taken", "word_transliteration": "wa-ittakhadhu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "besides Allah", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides Allah", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "besides Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u0657", "word_translation": "gods", "word_transliteration": "alihatan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652", "word_translation": "that they may be", "word_transliteration": "liyakunu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0639\u0650\u0632\u0651\u0657\u0627", "word_translation": "an honor", "word_transliteration": "izzan"}]}