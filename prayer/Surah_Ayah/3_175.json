{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f \u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064f \u064a\u064f\u062e\u064e\u0648\u0651\u0650\u0641\u064f \u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u06e5 \u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0647\u064f\u0645\u06e1 \u0648\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0646\u0650 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645 \u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "(It is) only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f", "word_translation": "that", "word_transliteration": "dhalikumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064f", "word_translation": "the Shaitaan", "word_transliteration": "al-shaytanu"}, {"word_arabic_uthmani": "\u064a\u064f\u062e\u064e\u0648\u0651\u0650\u0641\u064f", "word_translation": "frightens (you)", "word_transliteration": "yukhawwifu"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u06e5", "word_translation": "(of) his allies", "word_transliteration": "awliyaahu"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "So (do) not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u062a\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0647\u064f\u0645\u06e1", "word_translation": "fear them", "word_transliteration": "takhafuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0646\u0650", "word_translation": "but fear Me", "word_transliteration": "wakhafuni"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645", "word_translation": "you are", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "believers", "word_transliteration": "mu'minina"}]}