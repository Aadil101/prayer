ion": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u0650\u0628\u0651\u064f", "word_translation": "loves", "word_transliteration": "yuhibbu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u062d\u06e1\u0633\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "the good-doers", "word_transliteration": "al-muh'sinina"}]}