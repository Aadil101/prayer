{"ayahs": {"ayah_uthmani": "\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650 \u0642\u064e\u0648\u06e1\u0645\u064f\u0646\u064e\u0627 \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6\u0653 \u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u0657\u06d6 \u0644\u0651\u064e\u0648\u06e1\u0644\u064e\u0627 \u064a\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645 \u0628\u0650\u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u0650\u06ed \u0628\u064e\u064a\u0651\u0650\u0646\u0656\u06d6 \u0641\u064e\u0645\u064e\u0646\u06e1 \u0623\u064e\u0638\u06e1\u0644\u064e\u0645\u064f \u0645\u0650\u0645\u0651\u064e\u0646\u0650 \u0671\u0641\u06e1\u062a\u064e\u0631\u064e\u0649\u0670 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0643\u064e\u0630\u0650\u0628\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650", "word_translation": "These", "word_transliteration": "haulai"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064f\u0646\u064e\u0627", "word_translation": "our people", "word_transliteration": "qawmuna"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652", "word_translation": "have taken", "word_transliteration": "ittakhadhu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "besides Him", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6\u0653", "word_translation": "besides Him", "word_transliteration": "dunihi"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u0657\u06d6", "word_translation": "gods", "word_transliteration": "alihatan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "Why not", "word_transliteration": "lawla"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u064e", "word_translation": "they come", "word_transliteration": "yatuna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645", "word_translation": "to them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0628\u0650\u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u0650\u06ed", "word_translation": "with an authority", "word_transliteration": "bisul'tanin"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u0651\u0650\u0646\u0656\u06d6", "word_translation": "clear", "word_transliteration": "bayyinin"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0646\u06e1", "word_translation": "And who", "word_transliteration": "faman"}, {"word_arabic_uthmani": "\u0623\u064e\u0638\u06e1\u0644\u064e\u0645\u064f", "word_translation": "(is) more wrong", "word_transliteration": "azlamu"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0646\u0650", "word_translation": "than (one) who", "word_transliteration": "mimmani"}, {"word_arabic_uthmani": "\u0671\u0641\u06e1\u062a\u064e\u0631\u064e\u0649\u0670", "word_translation": "invents", "word_transliteration": "if'tara"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "against", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0650\u0628\u0657\u0627", "word_translation": "a lie", "word_transliteration": "kadhiban"}]}