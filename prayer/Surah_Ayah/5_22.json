{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u064a\u064e\u0670\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653 \u0625\u0650\u0646\u0651\u064e \u0641\u0650\u064a\u0647\u064e\u0627 \u0642\u064e\u0648\u06e1\u0645\u0657\u0627 \u062c\u064e\u0628\u0651\u064e\u0627\u0631\u0650\u064a\u0646\u064e \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0644\u064e\u0646 \u0646\u0651\u064e\u062f\u06e1\u062e\u064f\u0644\u064e\u0647\u064e\u0627 \u062d\u064e\u062a\u0651\u064e\u0649\u0670 \u064a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u0641\u064e\u0625\u0650\u0646 \u064a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u062f\u064e\u0670\u062e\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653", "word_translation": "O Musa!", "word_transliteration": "yamusa"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "in it", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0657\u0627", "word_translation": "(are) people", "word_transliteration": "qawman"}, {"word_arabic_uthmani": "\u062c\u064e\u0628\u0651\u064e\u0627\u0631\u0650\u064a\u0646\u064e", "word_translation": "(of) tyrannical strength", "word_transliteration": "jabbarina"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "and indeed we", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0644\u064e\u0646", "word_translation": "never", "word_transliteration": "lan"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u062f\u06e1\u062e\u064f\u0644\u064e\u0647\u064e\u0627", "word_translation": "will enter it", "word_transliteration": "nadkhulaha"}, {"word_arabic_uthmani": "\u062d\u064e\u062a\u0651\u064e\u0649\u0670", "word_translation": "until", "word_transliteration": "hatta"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0627\u0652", "word_translation": "they leave", "word_transliteration": "yakhruju"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "from it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646", "word_translation": "and if", "word_transliteration": "fa-in"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0627\u0652", "word_translation": "they leave", "word_transliteration": "yakhruju"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "[from] it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "then certainly we (will)", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u062f\u064e\u0670\u062e\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "enter (it)", "word_transliteration": "dakhiluna"}]}