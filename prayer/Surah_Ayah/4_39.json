{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0630\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0644\u064e\u0648\u06e1 \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u0650 \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u0650 \u0648\u064e\u0623\u064e\u0646\u0641\u064e\u0642\u064f\u0648\u0627\u0652 \u0645\u0650\u0645\u0651\u064e\u0627 \u0631\u064e\u0632\u064e\u0642\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06da \u0648\u064e\u0643\u064e\u0627\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u0650\u0647\u0650\u0645\u06e1 \u0639\u064e\u0644\u0650\u064a\u0645\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0630\u064e\u0627", "word_translation": "And what", "word_transliteration": "wamadha"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "(is) against them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1", "word_translation": "if", "word_transliteration": "law"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "they believed", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "in Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u0650", "word_translation": "and the Day", "word_transliteration": "wal-yawmi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u0650", "word_translation": "the Last", "word_transliteration": "al-akhiri"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0641\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "and spent", "word_transli