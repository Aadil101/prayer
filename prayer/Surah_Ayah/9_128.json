{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0642\u064e\u062f\u06e1 \u062c\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1 \u0631\u064e\u0633\u064f\u0648\u0644\u065e \u0645\u0651\u0650\u0646\u06e1 \u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0643\u064f\u0645\u06e1 \u0639\u064e\u0632\u0650\u064a\u0632\u064c \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0645\u064e\u0627 \u0639\u064e\u0646\u0650\u062a\u0651\u064f\u0645\u06e1 \u062d\u064e\u0631\u0650\u064a\u0635\u064c \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645 \u0628\u0650\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e \u0631\u064e\u0621\u064f\u0648\u0641\u065e \u0631\u0651\u064e\u062d\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "Certainly", "word_transliteration": "laqad"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(has) come to you", "word_transliteration": "jaakum"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u065e", "word_translation": "a Messenger", "word_transliteration": "rasulun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0643\u064f\u0645\u06e1", "word_translation": "yourselves", "word_transliteration": "anfusikum"}, {"word_arabic_uthmani": "\u0639\u064e\u0632\u0650\u064a\u0632\u064c", "word_translation": "Grievous", "word_transliteration": "azizun"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "to him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "(is) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0650\u062a\u0651\u064f\u0645\u06e1", "word_translation": "you suffer", "word_transliteration": "anittum"}, {"word_arabic_uthmani": "\u062d\u064e\u0631\u0650\u064a\u0635\u064c", "word_translation": "(he is) concerned", "word_transliteration": "harisun"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645", "word_translation": "over you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "to the believers", "word_transliteration": "bil-mu'minina"}, {"word_arabic_uthmani": "\u0631\u064e\u0621\u064f\u0648\u0641\u065e", "word_translation": "(he is) kind", "word_transliteration": "raufun"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u065e", "word_translation": "(and) merciful", "word_transliteration": "rahimun"}]}