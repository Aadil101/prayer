{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a \u0648\u064e\u062c\u064e\u062f\u062a\u0651\u064f \u0671\u0645\u06e1\u0631\u064e\u0623\u064e\u0629\u0657 \u062a\u064e\u0645\u06e1\u0644\u0650\u0643\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0623\u064f\u0648\u062a\u0650\u064a\u064e\u062a\u06e1 \u0645\u0650\u0646 \u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0648\u064e\u0644\u064e\u0647\u064e\u0627 \u0639\u064e\u0631\u06e1\u0634\u064c \u0639\u064e\u0638\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u062f\u062a\u0651\u064f", "word_translation": "found", "word_transliteration": "wajadttu"}, {"word_arabic_uthmani": "\u0671\u0645\u06e1\u0631\u064e\u0623\u064e\u0629\u0657", "word_translation": "a woman", "word_transliteration": "im'ra-atan"}, {"word_arabic_uthmani": "\u062a\u064e\u0645\u06e1\u0644\u0650\u0643\u064f\u0647\u064f\u0645\u06e1", "word_translation": "ruling them", "word_transliteration": "tamlikuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0648\u062a\u0650\u064a\u064e\u062a\u06e1", "word_translation": "and she has been given", "word_transliteration": "wautiyat"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064e\u0627", "word_translation": "and for her", "word_transliteration": "walaha"}, {"word_arabic_uthmani": "\u0639\u064e\u0631\u06e1\u0634\u064c", "word_translation": "(is) a throne", "word_transliteration": "arshun"}, {"word_arabic_uthmani": "\u0639\u064e\u0638\u0650\u064a\u0645\u06