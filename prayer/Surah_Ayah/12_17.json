{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0628\u064e\u0627\u0646\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e\u0627 \u0630\u064e\u0647\u064e\u0628\u06e1\u0646\u064e\u0627 \u0646\u064e\u0633\u06e1\u062a\u064e\u0628\u0650\u0642\u064f \u0648\u064e\u062a\u064e\u0631\u064e\u0643\u06e1\u0646\u064e\u0627 \u064a\u064f\u0648\u0633\u064f\u0641\u064e \u0639\u0650\u0646\u062f\u064e \u0645\u064e\u062a\u064e\u0670\u0639\u0650\u0646\u064e\u0627 \u0641\u064e\u0623\u064e\u0643\u064e\u0644\u064e\u0647\u064f \u0671\u0644\u0630\u0651\u0650\u0626\u06e1\u0628\u064f\u06d6 \u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064e \u0628\u0650\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0656 \u0644\u0651\u064e\u0646\u064e\u0627 \u0648\u064e\u0644\u064e\u0648\u06e1 \u0643\u064f\u0646\u0651\u064e\u0627 \u0635\u064e\u0670\u062f\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0628\u064e\u0627\u0646\u064e\u0627\u0653", "word_translation": "O our father!", "word_transliteration": "yaabana"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0630\u064e\u0647\u064e\u0628\u06e1\u0646\u064e\u0627", "word_translation": "[we] went", "word_transliteration": "dhahabna"}, {"word_arabic_uthmani": "\u0646\u064e\u0633\u06e1\u062a\u064e\u0628\u0650\u0642\u064f", "word_translation": "racing each other", "word_transliteration": "nastabiqu"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0631\u064e\u0643\u06e1\u0646\u064e\u0627", "word_translation": "and we left", "word_transliteration": "watarakna"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0633\u064f\u0641\u064e", "word_translation": "Yusuf", "word_transliteration": "yusufa"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "with", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0645\u064e\u062a\u064e\u0670\u0639\u0650\u0646\u064e\u0627", "word_translation": "our possessions", "word_transliteration": "mata'ina"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0643\u064e\u0644\u064e\u0647\u064f", "word_translation": "and ate him", "word_transliteration": "fa-akalahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0630\u0651\u0650\u0626\u06e1\u0628\u064f\u06d6", "word_translation": "the wolf", "word_transliteration": "al-dhi'bu"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "But not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "you", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0656", "word_translation": "(will) believe", "word_transliteration": "bimu'minin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0646\u064e\u0627", "word_translation": "us", "word_transliteration": "lana"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "even if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "we are", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0635\u064e\u0670\u062f\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "truthful", "word_transliteration": "sadiqina"}]}