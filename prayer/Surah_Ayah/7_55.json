{"ayahs": {"ayah_uthmani": "\u0671\u062f\u06e1\u0639\u064f\u0648\u0627\u0652 \u0631\u064e\u0628\u0651\u064e\u0643\u064f\u0645\u06e1 \u062a\u064e\u0636\u064e\u0631\u0651\u064f\u0639\u0657\u0627 \u0648\u064e\u062e\u064f\u0641\u06e1\u064a\u064e\u0629\u064b\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u0627 \u064a\u064f\u062d\u0650\u0628\u0651\u064f \u0671\u0644\u06e1\u0645\u064f\u0639\u06e1\u062a\u064e\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u062f\u06e1\u0639\u064f\u0648\u0627\u0652", "word_translation": "Call upon", "word_transliteration": "id'u"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your Lord", "word_transliteration": "rabbakum"}, {"word_arabic_uthmani": "\u062a\u064e\u0636\u064e\u0631\u0651\u064f\u0639\u0657\u0627", "word_translation": "humbly", "word_transliteration": "tadarru'an"}, {"word_arabic_uthmani": "\u0648\u064e\u062e\u064f\u0641\u06e1\u064a\u064e\u0629\u064b\u06da", "word_translation": "and privately", "word_transliteration": "wakhuf'yatan"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u0650\u0628\u0651\u064f", "word_translation": "love", "word_transliteration": "yuhibbu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0639\u06e1\u062a\u064e\u062f\u0650\u064a\u0646\u064e", "word_translation": "the transgressors", "word_transliteration": "al-mu'tadina"}]}