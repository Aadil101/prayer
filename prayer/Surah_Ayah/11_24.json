{"ayahs": {"ayah_uthmani": "\u06de\u0645\u064e\u062b\u064e\u0644\u064f \u0671\u0644\u06e1\u0641\u064e\u0631\u0650\u064a\u0642\u064e\u064a\u06e1\u0646\u0650 \u0643\u064e\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0645\u064e\u0649\u0670 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0635\u064e\u0645\u0651\u0650 \u0648\u064e\u0671\u0644\u06e1\u0628\u064e\u0635\u0650\u064a\u0631\u0650 \u0648\u064e\u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u0650\u06da \u0647\u064e\u0644\u06e1 \u064a\u064e\u0633\u06e1\u062a\u064e\u0648\u0650\u064a\u064e\u0627\u0646\u0650 \u0645\u064e\u062b\u064e\u0644\u064b\u0627\u06da \u0623\u064e\u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0645\u064e\u062b\u064e\u0644\u064f", "word_translation": "(The) example", "word_transliteration": "mathalu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0631\u0650\u064a\u0642\u064e\u064a\u06e1\u0646\u0650", "word_translation": "(of) the two parties", "word_transliteration": "al-fariqayni"}, {"word_arabic_uthmani": "\u0643\u064e\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0645\u064e\u0649\u0670", "word_translation": "(is) like the blind", "word_transliteration": "kal-a'ma"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0635\u064e\u0645\u0651\u0650", "word_translation": "and the deaf", "word_transliteration": "wal-asami"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0628\u064e\u0635\u0650\u064a\u0631\u0650", "word_translation": "and the seer", "word_transliteration": "wal-basiri"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u0650\u06da", "word_translation": "and the hearer", "word_transliteration": "wal-sami'i"}, {"word_arabic_uthmani": "\u0647\u064e\u0644\u06e1", "word_translation": "Are", "word_transliteration": "hal"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u0648\u0650\u064a\u064e\u0627\u0646\u0650", "word_translation": "they equal", "word_transliteration": "yastawiyani"}, {"word_arabic_uthmani": "\u0645\u064e\u062b\u064e\u0644\u064b\u0627\u06da", "word_translation": "(in) comparison", "word_transliteration": "mathalan"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0627", "word_translation": "Then will not", "word_transliteration": "afala"}, {"word_arabic_uthmani": "\u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "you take heed", "word_transliteration": "tadhakkaruna"}]}