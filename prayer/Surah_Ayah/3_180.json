{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u0628\u06e1\u062e\u064e\u0644\u064f\u0648\u0646\u064e \u0628\u0650\u0645\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u0650\u0646 \u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6 \u0647\u064f\u0648\u064e \u062e\u064e\u064a\u06e1\u0631\u0657\u0627 \u0644\u0651\u064e\u0647\u064f\u0645\u06d6 \u0628\u064e\u0644\u06e1 \u0647\u064f\u0648\u064e \u0634\u064e\u0631\u0651\u065e \u0644\u0651\u064e\u0647\u064f\u0645\u06e1\u06d6 \u0633\u064e\u064a\u064f\u0637\u064e\u0648\u0651\u064e\u0642\u064f\u0648\u0646\u064e \u0645\u064e\u0627 \u0628\u064e\u062e\u0650\u0644\u064f\u0648\u0627\u0652 \u0628\u0650\u0647\u0650\u06e6 \u064a\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650\u06d7 \u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0645\u0650\u064a\u0631\u064e\u0670\u062b\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u0650\u0645\u064e\u0627 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e \u062e\u064e\u0628\u0650\u064a\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (let) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e", "word_translation": "think", "word_transliteration": "yahsabanna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u0628\u06e1\u062e\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "withhold", "word_transliteration": "yabkhaluna"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627\u0653", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0647\u064f\u0645\u064f", "word_translation": "(has) given them", "word_transliteration": "atahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u0650\u0647\u0650\u06e6", "word_translation": "His Bounty", "word_transliteration": "fadlihi"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "(that) it", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u0657\u0627", "word_translation": "(is) good", "word_transliteration": "khayran"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u0645\u06d6", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "it", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0634\u064e\u0631\u0651\u065e", "word_translation": "(is) bad", "word_transliteration": "sharrun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u0645\u06e1\u06d6", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0633\u064e\u064a\u064f\u0637\u064e\u0648\u0651\u064e\u0642\u064f\u0648\u0646\u064e", "word_translation": "Their necks will be encircled", "word_transliteration": "sayutawwaquna"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "(with) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0628\u064e\u062e\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "they withheld", "word_transliteration": "bakhilu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "[with it]", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(on the) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650\u06d7", "word_translation": "(of) [the] Resurrection", "word_transliteration": "al-qiyamati"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "And for Allah", "word_transliteration": "walillahi"}, {"word_arabic_uthmani": "\u0645\u0650\u064a\u0631\u064e\u0670\u062b\u064f", "word_translation": "(is the) heritage", "word_transliteration": "mirathu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d7", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "with what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "you do", "word_transliteration": "ta'maluna"}, {"word_arabic_uthmani": "\u062e\u064e\u0628\u0650\u064a\u0631\u065e", "word_translation": "(is) All-Aware", "word_transliteration": "khabirun"}]}