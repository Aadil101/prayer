{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064f\u0644\u0650 \u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0644\u064e\u0645\u06e1 \u064a\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u06e1 \u0648\u064e\u0644\u064e\u062f\u0657\u0627 \u0648\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0643\u064f\u0646 \u0644\u0651\u064e\u0647\u064f\u06e5 \u0634\u064e\u0631\u0650\u064a\u0643\u065e \u0641\u0650\u064a \u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u0650 \u0648\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0643\u064f\u0646 \u0644\u0651\u064e\u0647\u064f\u06e5 \u0648\u064e\u0644\u0650\u064a\u0651\u065e \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0630\u0651\u064f\u0644\u0651\u0650\u06d6 \u0648\u064e\u0643\u064e\u0628\u0651\u0650\u0631\u06e1\u0647\u064f \u062a\u064e\u0643\u06e1\u0628\u0650\u064a\u0631\u064e\u06e2\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064f\u0644\u0650", "word_translation": "And say", "word_transliteration": "waquli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0645\u06e1\u062f\u064f", "word_translation": "All Praise", "word_transliteration": "al-hamdu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "(is) for Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "has not taken", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u06e1", "word_translation": "has not taken", "word_transliteration": "yattakhidh"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u062f\u0657\u0627", "word_translation": "a son", "word_transliteration": "waladan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "and not", "word_transliteration": "walam"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0646", "word_translation": "is", "word_transliteration": "yakun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u06e5", "word_translation": "for Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0634\u064e\u0631\u0650\u064a\u0643\u065e", "word_translation": "a partner", "word_transliteration": "sharikun"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u0650", "word_translation": "the dominion", "word_transliteration": "al-mul'ki"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "and not", "word_transliteration": "walam"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0646", "word_translation": "is", "word_transliteration": "yakun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u06e5", "word_translation": "for Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u0651\u065e", "word_translation": "any protector", "word_transliteration": "waliyyun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "out of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0630\u0651\u064f\u0644\u0651\u0650\u06d6", "word_translation": "weakness", "word_transliteration": "al-dhuli"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0628\u0651\u0650\u0631\u06e1\u0647\u064f", "word_translation": "And magnify Him", "word_transliteration": "wakabbir'hu"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u06e1\u0628\u0650\u064a\u0631\u064e\u06e2\u0627", "word_translation": "(with all) magnificence", "word_transliteration": "takbiran"}]}