{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653 \u0623\u064f\u0646\u0632\u0650\u0644\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0621\u064e\u0627\u064a\u064e\u0670\u062a\u065e \u0645\u0651\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650\u0647\u0650\u06e6\u06da \u0642\u064f\u0644\u06e1 \u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0671\u0644\u06e1\u0623\u0653\u064a\u064e\u0670\u062a\u064f \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u064e\u0627\u06e0 \u0646\u064e\u0630\u0650\u064a\u0631\u065e \u0645\u0651\u064f\u0628\u0650\u064a\u0646\u064c"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "And they say", "word_transliteration": "waqalu"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1\u0644\u064e\u0627\u0653", "word_translation": "Why not", "word_transliteration": "lawla"}, {"word_arabic_uthmani": "\u0623\u064f\u0646\u0632\u0650\u0644\u064e", "word_translation": "are sent down", "word_transliteration": "unzila"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "to him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0670\u062a\u065e", "word_translation": "(the) Signs", "word_transliteration": "ayatun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650\u0647\u0650\u06e6\u06da", "word_translation": "his Lord", "word_transliteration": "rabbihi"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u064a\u064e\u0670\u062a\u064f", "word_translation": "the Signs", "word_transliteration": "al-ayatu"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "(are) with", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653", "word_translation": "and only", "word_transliteration": "wa-innama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "I (am)", "word_transliteration": "ana"}, {"word_arabic_uthmani": "\u0646\u064e\u0630\u0650\u064a\u0631\u065e", "word_translation": "a warner", "word_transliteration": "nadhirun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0628\u0650\u064a\u0646\u064c", "word_translation": "clear", "word_transliteration": "mubinun"}]}