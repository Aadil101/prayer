{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1 \u0646\u064e\u0627\u062f\u064e\u0649\u0670 \u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653 \u0623\u064e\u0646\u0650 \u0671\u0626\u06e1\u062a\u0650 \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1", "word_translation": "And when", "word_transliteration": "wa-idh"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u062f\u064e\u0649\u0670", "word_translation": "your Lord called", "word_transliteration": "nada"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "your Lord called", "word_transliteration": "rabbuka"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0650", "word_translation": "[that]", "word_transliteration": "ani"}, {"word_arabic_uthmani": "\u0671\u0626\u06e1\u062a\u0650", "word_translation": "Go", "word_transliteration": "i'ti"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(to) the people", "word_transliteration": "al-qawma"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "(who are) wrongdoers", "word_transliteration": "al-zalimina"}]}