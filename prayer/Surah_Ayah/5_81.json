{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u0650 \u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064f\u0646\u0632\u0650\u0644\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u0645\u064e\u0627 \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0647\u064f\u0645\u06e1 \u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e \u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627 \u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they had", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "believed", "word_transliteration": "yu'minuna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "in Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u0650", "word_translation": "and the Prophet", "word_transliteration": "wal-nabiyi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064f\u0646\u0632\u0650\u0644\u064e", "word_translation": "has been revealed", "word_transliteration": "unzila"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "to him", "word_transliteration": "ilayhi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0647\u064f\u0645\u06e1", "word_translation": "they (would have) taken them", "word_transliteration": "ittakhadhuhum"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e", "word_translation": "(as) allies", "word_transliteration": "awliyaa"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e", "word_translation": "[and] but", "word_transliteration": "walakinna"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627", "word_translation": "many", "word_transliteration": "kathiran"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "of them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "(are) defiantly disobedient", "word_transliteration": "fasiquna"}]}