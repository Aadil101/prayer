{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627 \u0641\u0650\u064a \u0642\u064e\u0631\u06e1\u064a\u064e\u0629\u0656 \u0645\u0651\u0650\u0646 \u0646\u0651\u064e\u0628\u0650\u064a\u0651\u064d \u0625\u0650\u0644\u0651\u064e\u0627\u0653 \u0623\u064e\u062e\u064e\u0630\u06e1\u0646\u064e\u0627\u0653 \u0623\u064e\u0647\u06e1\u0644\u064e\u0647\u064e\u0627 \u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u0623\u06e1\u0633\u064e\u0627\u0653\u0621\u0650 \u0648\u064e\u0671\u0644\u0636\u0651\u064e\u0631\u0651\u064e\u0627\u0653\u0621\u0650 \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0636\u0651\u064e\u0631\u0651\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We sent", "word_transliteration": "arsalna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u06e1\u064a\u064e\u0629\u0656", "word_translation": "a city", "word_transliteration": "qaryatin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "[of]", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u064d", "word_translation": "any Prophet", "word_transliteration": "nabiyyin"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627\u0653", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u064e\u0630\u06e1\u0646\u064e\u0627\u0653", "word_translation": "We seized", "word_transliteration": "akhadhna"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064e\u0647\u064e\u0627", "word_translation": "its people", "word_transliteration": "ahlaha"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u0623\u06e1\u0633\u064e\u0627\u0653\u0621\u0650", "word_translation": "with adversity", "word_transliteration": "bil-basai"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0636\u0651\u064e\u0631\u0651\u064e\u0627\u0653\u0621\u0650", "word_translation": "and hardship", "word_transliteration": "wal-darai"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so that they may", "word_transliteration": "la'allahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0636\u0651\u064e\u0631\u0651\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "(become) humble", "word_transliteration": "yaddarra'una"}]}