ndeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimina"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064c", "word_translation": "(is) a punishment", "word_transliteration": "adhabun"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0650\u064a\u0645\u065e", "word_translation": "painful", "word_transliteration": "alimun"}]}