{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u064e\u0646\u064e\u0627\u0653 \u0623\u064e\u062c\u0650\u064a\u0628\u064f\u0648\u0627\u0652 \u062f\u064e\u0627\u0639\u0650\u064a\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0621\u064e\u0627\u0645\u0650\u0646\u064f\u0648\u0627\u0652 \u0628\u0650\u0647\u0650\u06e6 \u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1 \u0644\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646 \u0630\u064f\u0646\u064f\u0648\u0628\u0650\u0643\u064f\u0645\u06e1 \u0648\u064e\u064a\u064f\u062c\u0650\u0631\u06e1\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064d \u0623\u064e\u0644\u0650\u064a\u0645\u0656"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u064e\u0646\u064e\u0627\u0653", "word_translation": "O our people!", "word_transliteration": "yaqawmana"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u0650\u064a\u0628\u064f\u0648\u0627\u0652", "word_translation": "Respond", "word_transliteration": "ajibu"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0639\u0650\u064a\u064e", "word_translation": "(to the) caller", "word_transliteration": "da'iya"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0621\u064e\u0627\u0645\u0650\u0646\u064f\u0648\u0627\u0652", "word_translation": "and believe", "word_transliteration": "waaminu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "in him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1", "word_translation": "He will forgive", "word_transliteration": "yaghfir"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0630\u064f\u0646\u064f\u0648\u0628\u0650\u0643\u064f\u0645\u06e1", "word_translation": "your sins", "word_transliteration": "dhunubikum"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u062c\u0650\u0631\u06e1\u0643\u064f\u0645", "word_translation": "and will protect you", "word_transliteration": "wayujir'kum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064d", "word_translation": "a punishment", "word_transliteration": "adhabin"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0650\u064a\u0645\u0656", "word_translation": "painful", "word_transliteration": "alimin"}]}