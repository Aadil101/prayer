{"ayahs": {"ayah_uthmani": "\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e \u0644\u0650\u0644\u06e1\u0645\u064f\u0634\u06e1\u0631\u0650\u0643\u0650\u064a\u0646\u064e \u0623\u064e\u0646 \u064a\u064e\u0639\u06e1\u0645\u064f\u0631\u064f\u0648\u0627\u0652 \u0645\u064e\u0633\u064e\u0670\u062c\u0650\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0634\u064e\u0670\u0647\u0650\u062f\u0650\u064a\u0646\u064e \u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645 \u0628\u0650\u0671\u0644\u06e1\u0643\u064f\u0641\u06e1\u0631\u0650\u06da \u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u062d\u064e\u0628\u0650\u0637\u064e\u062a\u06e1 \u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0641\u0650\u064a \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650 \u0647\u064f\u0645\u06e1 \u062e\u064e\u0670\u0644\u0650\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "(It) is not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "(It) is not", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0645\u064f\u0634\u06e1\u0631\u0650\u0643\u0650\u064a\u0646\u064e", "word_translation": "for the polytheists", "word_transliteration": "lil'mush'rikina"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064f\u0631\u064f\u0648\u0627\u0652", "word_translation": "they maintain", "word_transliteration": "ya'muru"}, {"word_arabic_uthmani": "\u0645\u064e\u0633\u064e\u0670\u062c\u0650\u062f\u064e", "word_translation": "(the) masajid of Allah", "word_transliteration": "masajida"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(the) masajid of Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0634\u064e\u0670\u0647\u0650\u062f\u0650\u064a\u0646\u064e", "word_translation": "(while) witnessing", "word_transliteration": "shahidina"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "against", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645", "word_translation": "themselves", "word_transliteration": "anfusihim"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0643\u064f\u0641\u06e1\u0631\u0650\u06da", "word_translation": "[with] disbelief", "word_transliteration": "bil-kuf'ri"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "(For) those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u062d\u064e\u0628\u0650\u0637\u064e\u062a\u06e1", "word_translation": "worthless", "word_transliteration": "habitat"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064f\u0647\u064f\u0645\u06e1", "word_translation": "(are) their deeds", "word_transliteration": "a'maluhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u0650\u064a", "word_translation": "and in", "word_transliteration": "wafi"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650", "word_translation": "the Fire", "word_transliteration": "al-nari"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u062e\u064e\u0670\u0644\u0650\u062f\u064f\u0648\u0646\u064e", "word_translation": "(will) abide forever", "word_transliteration": "khaliduna"}]}