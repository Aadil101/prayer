{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u06e5 \u0645\u064e\u0646 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6 \u0643\u064f\u0644\u0651\u065e \u0644\u0651\u064e\u0647\u064f\u06e5 \u0642\u064e\u0670\u0646\u0650\u062a\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u06e5", "word_translation": "And to Him (belongs)", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whoever", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u065e", "word_translation": "All", "word_transliteration": "kullun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u06e5", "word_translation": "to Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0642\u064e\u0670\u0646\u0650\u062a\u064f\u0648\u0646\u064e", "word_translation": "(are) obedient", "word_transliteration": "qanituna"}]}