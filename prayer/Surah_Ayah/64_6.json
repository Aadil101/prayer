{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5 \u0643\u064e\u0627\u0646\u064e\u062a \u062a\u0651\u064e\u0623\u06e1\u062a\u0650\u064a\u0647\u0650\u0645\u06e1 \u0631\u064f\u0633\u064f\u0644\u064f\u0647\u064f\u0645 \u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u064a\u0651\u0650\u0646\u064e\u0670\u062a\u0650 \u0641\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0628\u064e\u0634\u064e\u0631\u065e \u064a\u064e\u0647\u06e1\u062f\u064f\u0648\u0646\u064e\u0646\u064e\u0627 \u0641\u064e\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0648\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0648\u0627\u0652\u06d6 \u0648\u0651\u064e\u0671\u0633\u06e1\u062a\u064e\u063a\u06e1\u0646\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u063a\u064e\u0646\u0650\u064a\u0651\u064c \u062d\u064e\u0645\u0650\u064a\u062f\u065e"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "(is) because", "word_transliteration": "bi-annahu"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e\u062a", "word_translation": "had", "word_transliteration": "kanat"}, {"word_arabic_uthmani": "\u062a\u0651\u064e\u0623\u06e1\u062a\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "come to them", "word_transliteration": "tatihim"}, {"word_arabic_uthmani": "\u0631\u064f\u0633\u064f\u0644\u064f\u0647\u064f\u0645", "word_translation": "their Messengers", "word_transliteration": "rusuluhum"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u064a\u0651\u0650\u0646\u064e\u0670\u062a\u0650", "word_translation": "with clear proofs", "word_transliteration": "bil-bayinati"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "but they said", "word_transliteration": "faqalu"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u064e\u0634\u064e\u0631\u065e", "word_translation": "Shall human beings", "word_transliteration": "abasharun"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062f\u064f\u0648\u0646\u064e\u0646\u064e\u0627", "word_translation": "guide us", "word_transliteration": "yahdunana"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "So they disbelieved", "word_transliteration": "fakafaru"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0648\u0627\u0652\u06d6", "word_translation": "and turned away", "word_transliteration": "watawallaw"}, {"word_arabic_uthmani": "\u0648\u0651\u064e\u0671\u0633\u06e1\u062a\u064e\u063a\u06e1\u0646\u064e\u0649", "word_translation": "And can do without them", "word_transliteration": "wa-is'taghna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06da", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u063a\u064e\u0646\u0650\u064a\u0651\u064c", "word_translation": "(is) Self-sufficient", "word_transliteration": "ghaniyyun"}, {"word_arabic_uthmani": "\u062d\u064e\u0645\u0650\u064a\u062f\u065e", "word_translation": "Praiseworthy", "word_transliteration": "hamidun"}]}