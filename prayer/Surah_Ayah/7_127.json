{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0623\u064f \u0645\u0650\u0646 \u0642\u064e\u0648\u06e1\u0645\u0650 \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0623\u064e\u062a\u064e\u0630\u064e\u0631\u064f \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064f\u06e5 \u0644\u0650\u064a\u064f\u0641\u06e1\u0633\u0650\u062f\u064f\u0648\u0627\u0652 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u064a\u064e\u0630\u064e\u0631\u064e\u0643\u064e \u0648\u064e\u0621\u064e\u0627\u0644\u0650\u0647\u064e\u062a\u064e\u0643\u064e\u06da \u0642\u064e\u0627\u0644\u064e \u0633\u064e\u0646\u064f\u0642\u064e\u062a\u0651\u0650\u0644\u064f \u0623\u064e\u0628\u06e1\u0646\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0646\u064e\u0633\u06e1\u062a\u064e\u062d\u06e1\u064a\u0650\u06e6 \u0646\u0650\u0633\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0641\u064e\u0648\u06e1\u0642\u064e\u0647\u064f\u0645\u06e1 \u0642\u064e\u0670\u0647\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "And said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0623\u064f", "word_translation": "the chiefs", "word_transliteration": "al-mala-u"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "(the) people", "word_transliteration": "qawmi"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "(of) Firaun", "word_transliteration": "fir'awna"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064e\u0630\u064e\u0631\u064f", "word_translation": "Will you leave", "word_transliteration": "atadharu"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064f\u06e5", "word_translation": "and his people", "word_transliteration": "waqawmahu"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u0641\u06e1\u0633\u0650\u062f\u064f\u0648\u0627\u0652", "word_translation": "so that they cause corruption", "word_transliteration": "liyuf'sidu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0630\u064e\u0631\u064e\u0643\u064e", "word_translation": "and forsake you", "word_transliteration": "wayadharaka"}, {"word_arabic_uthmani": "\u0648\u064e\u0621\u064e\u0627\u0644\u0650\u0647\u064e\u062a\u064e\u0643\u064e\u06da", "word_translation": "and your gods", "word_transliteration": "waalihataka"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0633\u064e\u0646\u064f\u0642\u064e\u062a\u0651\u0650\u0644\u064f", "word_translation": "We will kill", "word_transliteration": "sanuqattilu"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0646\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their sons", "word_transliteration": "abnaahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u0633\u06e1\u062a\u064e\u062d\u06e1\u064a\u0650\u06e6", "word_translation": "and we will let live", "word_transliteration": "wanastahyi"}, {"word_arabic_uthmani": "\u0646\u0650\u0633\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their women", "word_transliteration": "nisaahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "and indeed we", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u06e1\u0642\u064e\u0647\u064f\u0645\u06e1", "word_translation": "over them", "word_transliteration": "fawqahum"}, {"word_arabic_uthmani": "\u0642\u064e\u0670\u0647\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(are) subjugators", "word_transliteration": "qahiruna"}]}