{"ayahs": {"ayah_uthmani": "\u0633\u064e\u0627\u0653\u0621\u064e \u0645\u064e\u062b\u064e\u0644\u064b\u0627 \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652 \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627 \u0648\u064e\u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064e\u0638\u06e1\u0644\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0633\u064e\u0627\u0653\u0621\u064e", "word_translation": "Evil", "word_transliteration": "saa"}, {"word_arabic_uthmani": "\u0645\u064e\u062b\u064e\u0644\u064b\u0627", "word_translation": "(as) an example", "word_transliteration": "mathalan"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064f", "word_translation": "(are) the people", "word_transliteration": "al-qawmu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "denied", "word_transliteration": "kadhabu"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627", "word_translation": "Our Signs", "word_transliteration": "biayatina"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and themselves", "word_transliteration": "wa-anfusahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they used to", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064e\u0638\u06e1\u0644\u0650\u0645\u064f\u0648\u0646\u064e", "word_translation": "wrong", "word_transliteration": "yazlimuna"}]}