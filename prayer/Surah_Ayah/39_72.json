{"ayahs": {"ayah_uthmani": "\u0642\u0650\u064a\u0644\u064e \u0671\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0628\u06e1\u0648\u064e\u0670\u0628\u064e \u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e \u062e\u064e\u0670\u0644\u0650\u062f\u0650\u064a\u0646\u064e \u0641\u0650\u064a\u0647\u064e\u0627\u06d6 \u0641\u064e\u0628\u0650\u0626\u06e1\u0633\u064e \u0645\u064e\u062b\u06e1\u0648\u064e\u0649 \u0671\u0644\u06e1\u0645\u064f\u062a\u064e\u0643\u064e\u0628\u0651\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u0650\u064a\u0644\u064e", "word_translation": "It will be said", "word_transliteration": "qila"}, {"word_arabic_uthmani": "\u0671\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "Enter", "word_transliteration": "ud'khulu"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0648\u064e\u0670\u0628\u064e", "word_translation": "(the) gates", "word_transliteration": "abwaba"}, {"word_arabic_uthmani": "\u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e", "word_translation": "(of) Hell", "word_transliteration": "jahannama"}, {"word_arabic_uthmani": "\u062e\u064e\u0670\u0644\u0650\u062f\u0650\u064a\u0646\u064e", "word_translation": "(to) abide eternally", "word_transliteration": "khalidina"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627\u06d6", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0641\u064e\u0628\u0650\u0626\u06e1\u0633\u064e", "word_translation": "and wretched is", "word_transliteration": "fabi'sa"}, {"word_arabic_uthmani": "\u0645\u064e\u062b\u06e1\u0648\u064e\u0649", "word_translation": "(the) abode", "word_transliteration": "mathwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u062a\u064e\u0643\u064e\u0628\u0651\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(of) the arrogant", "word_transliteration": "al-mutakabirina"}]}