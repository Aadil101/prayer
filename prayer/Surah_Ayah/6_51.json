{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0630\u0650\u0631\u06e1 \u0628\u0650\u0647\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0646\u064e \u0623\u064e\u0646 \u064a\u064f\u062d\u06e1\u0634\u064e\u0631\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0644\u064e\u0649\u0670 \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1 \u0644\u064e\u064a\u06e1\u0633\u064e \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6 \u0648\u064e\u0644\u0650\u064a\u0651\u065e \u0648\u064e\u0644\u064e\u0627 \u0634\u064e\u0641\u0650\u064a\u0639\u065e \u0644\u0651\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u062a\u0651\u064e\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0630\u0650\u0631\u06e1", "word_translation": "And warn", "word_transliteration": "wa-andhir"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650", "word_translation": "with it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0646\u064e", "word_translation": "fear", "word_transliteration": "yakhafuna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u06e1\u0634\u064e\u0631\u064f\u0648\u0653\u0627\u0652", "word_translation": "they will be gathered", "word_transliteration": "yuh'sharu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their Lord", "word_transliteration": "rabbihim"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "not", "word_transliteration": "laysa"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6", "word_translation": "other than Him", "word_transliteration": "dunihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u0651\u065e", "word_translation": "any protector", "word_transliteration": "waliyyun"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0634\u064e\u0641\u0650\u064a\u0639\u065e", "word_translation": "any intercessor", "word_transliteration": "shafi'un"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so that they may", "word_transliteration": "la'allahum"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u0651\u064e\u0642\u064f\u0648\u0646\u064e", "word_translation": "(become) righteous", "word_transliteration": "yattaquna"}]}