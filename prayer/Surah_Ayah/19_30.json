{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0625\u0650\u0646\u0651\u0650\u064a \u0639\u064e\u0628\u06e1\u062f\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0646\u0650\u064a\u064e \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e \u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e\u0646\u0650\u064a \u0646\u064e\u0628\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I am", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0639\u064e\u0628\u06e1\u062f\u064f", "word_translation": "a slave", "word_transliteration": "abdu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0646\u0650\u064a\u064e", "word_translation": "He gave me", "word_transliteration": "ataniya"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e", "word_translation": "the Scripture", "word_transliteration": "al-kitaba"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e\u0646\u0650\u064a", "word_translation": "and made me", "word_transliteration": "waja'alani"}, {"word_arabic_uthmani": "\u0646\u064e\u0628\u0650\u064a\u0651\u0657\u0627", "word_translation": "a Prophet", "word_transliteration": "nabiyyan"}]}