{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0643\u064f\u0644\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650 \u0645\u064e\u0627 \u0631\u064e\u0632\u064e\u0642\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1 \u0648\u064e\u0671\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0627\u0652 \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u0625\u0650\u064a\u0651\u064e\u0627\u0647\u064f \u062a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O you", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe[d]", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "Eat", "word_transliteration": "kulu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650", "word_translation": "(the) good", "word_transliteration": "tayyibati"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0631\u064e\u0632\u064e\u0642\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1", "word_translation": "We have provided you", "word_transliteration": "razaqnakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0634\u06e1\u0643\u064f\u0631\u064f\u0648\u0627\u0652", "word_translation": "and be grateful", "word_transliteration": "wa-ush'kuru"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "to Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0625\u0650\u064a\u0651\u064e\u0627\u0647\u064f", "word_translation": "alone", "word_transliteration": "iyyahu"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e", "word_translation": "worship Him", "word_transliteration": "ta'buduna"}]}