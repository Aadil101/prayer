{"ayahs": {"ayah_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1 \u0644\u064e\u0647\u064f\u0645\u06e1 \u0623\u064e\u0648\u06e1 \u0644\u064e\u0627 \u062a\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1 \u0644\u064e\u0647\u064f\u0645\u06e1 \u0625\u0650\u0646 \u062a\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1 \u0644\u064e\u0647\u064f\u0645\u06e1 \u0633\u064e\u0628\u06e1\u0639\u0650\u064a\u0646\u064e \u0645\u064e\u0631\u0651\u064e\u0629\u0657 \u0641\u064e\u0644\u064e\u0646 \u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0647\u064f\u0645\u06e1\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u0650\u0647\u0650\u06e6\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0627 \u064a\u064e\u0647\u06e1\u062f\u0650\u064a \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1", "word_translation": "Ask forgiveness", "word_transliteration": "is'taghfir"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1", "word_translation": "ask forgiveness", "word_transliteration": "tastaghfir"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "If", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u062a\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1", "word_translation": "you ask forgiveness", "word_transliteration": "tastaghfir"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u06e1\u0639\u0650\u064a\u0646\u064e", "word_translation": "seventy", "word_transliteration": "sab'ina"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u0651\u064e\u0629\u0657", "word_translation": "times", "word_transliteration": "marratan"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0646", "word_translation": "never", "word_transliteration": "falan"}, {"word_arabic_uthmani": "\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064e", "word_translation": "will Allah forgive", "word_transliteration": "yaghfira"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "will Allah forgive", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "[for] them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "(is) because they", "word_transliteration": "bi-annahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "in Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u