{"ayahs": {"ayah_uthmani": "\u0647\u064f\u0648\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u06e1\u062e\u064e\u0670\u0644\u0650\u0642\u064f \u0671\u0644\u06e1\u0628\u064e\u0627\u0631\u0650\u0626\u064f \u0671\u0644\u06e1\u0645\u064f\u0635\u064e\u0648\u0651\u0650\u0631\u064f\u06d6 \u0644\u064e\u0647\u064f \u0671\u0644\u06e1\u0623\u064e\u0633\u06e1\u0645\u064e\u0627\u0653\u0621\u064f \u0671\u0644\u06e1\u062d\u064f\u0633\u06e1\u0646\u064e\u0649\u0670\u06da \u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f \u0644\u064e\u0647\u064f\u06e5 \u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6 \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0670\u0644\u0650\u0642\u064f", "word_translation": "the Creator", "word_transliteration": "al-khaliqu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0627\u0631\u0650\u0626\u064f", "word_translation": "the Inventor", "word_transliteration": "al-bari-u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0635\u064e\u0648\u0651\u0650\u0631\u064f\u06d6", "word_translation": "the Fashioner", "word_transliteration": "al-musawiru"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f", "word_translation": "For Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0633\u06e1\u0645\u064e\u0627\u0653\u0621\u064f", "word_translation": "(are) the names", "word_transliteration": "al-asmau"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064f\u0633\u06e1\u0646\u064e\u0649\u0670\u06da", "word_translation": "the beautiful", "word_transliteration": "al-hus'na"}, {"word_arabic_uthmani": "\u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f", "word_translation": "Glorifies", "word_transliteration": "yusabbihu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f", "word_translation": "(is) the All-Mighty", "word_transliteration": "al-'azizu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f", "word_translation": "the All-Wise", "word_transliteration": "al-hakimu"}]}