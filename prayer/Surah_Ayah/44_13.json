{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0649\u0670 \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0630\u0651\u0650\u0643\u06e1\u0631\u064e\u0649\u0670 \u0648\u064e\u0642\u064e\u062f\u06e1 \u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1 \u0631\u064e\u0633\u064f\u0648\u0644\u065e \u0645\u0651\u064f\u0628\u0650\u064a\u0646\u065e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0649\u0670", "word_translation": "How can", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "(there be) for them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0630\u0651\u0650\u0643\u06e1\u0631\u064e\u0649\u0670", "word_translation": "the reminder", "word_transliteration": "al-dhik'ra"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1", "word_translation": "when verily", "word_transliteration": "waqad"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1", "word_translation": "had come to them", "word_transliteration": "jaahum"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u065e", "word_translation": "a Messenger", "word_transliteration": "rasulun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0628\u0650\u064a\u0646\u065e", "word_translation": "clear", "word_transliteration": "mubinun"}]}