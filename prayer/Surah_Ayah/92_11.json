{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u064a\u064f\u063a\u06e1\u0646\u0650\u064a \u0639\u064e\u0646\u06e1\u0647\u064f \u0645\u064e\u0627\u0644\u064f\u0647\u064f\u06e5\u0653 \u0625\u0650\u0630\u064e\u0627 \u062a\u064e\u0631\u064e\u062f\u0651\u064e\u0649\u0670\u0653"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u064a\u064f\u063a\u06e1\u0646\u0650\u064a", "word_translation": "will avail", "word_transliteration": "yugh'ni"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f", "word_translation": "him", "word_transliteration": "anhu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0644\u064f\u0647\u064f\u06e5\u0653", "word_translation": "his wealth", "word_transliteration": "maluhu"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e\u062f\u0651\u064e\u0649\u0670\u0653", "word_translation": "he falls", "word_transliteration": "taradda"}]}