{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1 \u0623\u064e\u0646\u062c\u064e\u064a\u06e1\u0646\u064e\u0670\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u06e1 \u0621\u064e\u0627\u0644\u0650 \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u064a\u064e\u0633\u064f\u0648\u0645\u064f\u0648\u0646\u064e\u0643\u064f\u0645\u06e1 \u0633\u064f\u0648\u0653\u0621\u064e \u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u0650 \u064a\u064f\u0642\u064e\u062a\u0651\u0650\u0644\u064f\u0648\u0646\u064e \u0623\u064e\u0628\u06e1\u0646\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u062d\u06e1\u064a\u064f\u0648\u0646\u064e \u0646\u0650\u0633\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1\u06da \u0648\u064e\u0641\u0650\u064a \u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645 \u0628\u064e\u0644\u064e\u0627\u0653\u0621\u065e \u0645\u0651\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064f\u0645\u06e1 \u0639\u064e\u0638\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1", "word_translation": "And when", "word_transliteration": "wa-idh"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062c\u064e\u064a\u06e1\u0646\u064e\u0670\u0643\u064f\u0645", "word_translation": "We saved you", "word_transliteration": "anjaynakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u0650", "word_translation": "(the) people", "word_transliteration": "ali"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "(of) Firaun", "word_transliteration": "fir'awna"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u064f\u0648\u0645\u064f\u0648\u0646\u064e\u0643\u064f\u0645\u06e1", "word_translation": "who were afflicting you", "word_transliteration": "yasumunakum"}, {"word_arabic_uthmani": "\u0633\u064f\u0648\u0653\u0621\u064e", "word_translation": "(with) worst", "word_transliteration": "sua"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u0650", "word_translation": "(of) torment", "word_transliteration": "al-'adhabi"}, {"word_arabic_uthmani": "\u064a\u064f\u0642\u064e\u062a\u0651\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "they were killing", "word_transliteration": "yuqattiluna"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0646\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your sons", "word_transliteration": "abnaakum"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u062d\u06e1\u064a\u064f\u0648\u0646\u064e", "word_translation": "and letting live", "word_transliteration": "wayastahyuna"}, {"word_arabic_uthmani": "\u0646\u0650\u0633\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1\u06da", "word_translation": "your women", "word_transliteration": "nisaakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u0650\u064a", "word_translation": "And in", "word_transliteration": "wafi"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645", "word_translation": "that", "word_transliteration": "dhalikum"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u064e\u0627\u0653\u0621\u065e", "word_translation": "(was) a trial", "word_transliteration": "balaon"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064f\u0645\u06e1", "word_translation": "your Lord", "word_transliteration": "rabbikum"}, {"word_arabic_uthmani": "\u0639\u064e\u0638\u0650\u064a\u0645\u065e", "word_translation": "great", "word_transliteration": "azimun"}]}