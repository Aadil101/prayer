{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0646\u064e\u062a\u06e1\u0644\u064f\u0648\u0647\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u0653\u064a\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u0630\u0651\u0650\u0643\u06e1\u0631\u0650 \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0646\u064e\u062a\u06e1\u0644\u064f\u0648\u0647\u064f", "word_translation": "(is what) We recite [it]", "word_transliteration": "natluhu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e", "word_translation": "to you", "word_transliteration": "alayka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u064a\u064e\u0670\u062a\u0650", "word_translation": "the Verses", "word_transliteration": "al-ayati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0630\u0651\u0650\u0643\u06e1\u0631\u0650", "word_translation": "and the Reminder", "word_transliteration": "wal-dhik'ri"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u0650", "word_translation": "[the] Wise", "word_transliteration": "al-hakimi"}]}