{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0671\u0644\u06e1\u0645\u064e\u0644\u0650\u0643\u0650 \u0671\u0644\u06e1\u0642\u064f\u062f\u0651\u064f\u0648\u0633\u0650 \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u0650 \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f", "word_translation": "Glorifies", "word_transliteration": "yusabbihu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u0650\u0643\u0650", "word_translation": "the Sovereign", "word_transliteration": "al-maliki"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u062f\u0651\u064f\u0648\u0633\u0650", "word_translation": "the Holy", "word_transliteration": "al-qudusi"}, {"