{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064e\u0646\u0635\u064f\u0648\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Indeed they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "surely they", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0646\u0635\u064f\u0648\u0631\u064f\u0648\u0646\u064e", "word_translation": "(would be) the victorious", "word_transliteration": "al-mansuruna"}]}