{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652 \u0623\u064e\u0646\u0651\u064e\u0627 \u0646\u064e\u0623\u06e1\u062a\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0646\u064e\u0646\u0642\u064f\u0635\u064f\u0647\u064e\u0627 \u0645\u0650\u0646\u06e1 \u0623\u064e\u0637\u06e1\u0631\u064e\u0627\u0641\u0650\u0647\u064e\u0627\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064e\u062d\u06e1\u0643\u064f\u0645\u064f \u0644\u064e\u0627 \u0645\u064f\u0639\u064e\u0642\u0651\u0650\u0628\u064e \u0644\u0650\u062d\u064f\u0643\u06e1\u0645\u0650\u0647\u0650\u06e6\u06da \u0648\u064e\u0647\u064f\u0648\u064e \u0633\u064e\u0631\u0650\u064a\u0639\u064f \u0671\u0644\u06e1\u062d\u0650\u0633\u064e\u0627\u0628\u0650"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Did not", "word_transliteration": "awalam"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652", "word_translation": "they see", "word_transliteration": "yaraw"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0627", "word_translation": "that We", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u0646\u064e\u0623\u06e1\u062a\u0650\u064a", "word_translation": "come", "word_transliteration": "nati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "(to) the land", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0646\u064e\u0646\u0642\u064f\u0635\u064f\u0647\u064e\u0627", "word_translation": "reducing it", "word_transliteration": "nanqusuha"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0637\u06e1\u0631\u064e\u0627\u0641\u0650\u0647\u064e\u0627\u06da", "word_translation": "its borders", "word_transliteration": "atrafiha"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0643\u064f\u0645\u064f", "word_translation": "judges", "word_transliteration": "yahkumu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(there is) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0645\u064f\u0639\u064e\u0642\u0651\u0650\u0628\u064e", "word_translation": "adjuster", "word_transliteration": "mu'aqqiba"}, {"word_arabic_uthmani": "\u0644\u0650\u062d\u064f\u0643\u06e1\u0645\u0650\u0647\u0650\u06e6\u06da", "word_translation": "(of) His Judgment", "word_transliteration": "lihuk'mihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0633\u064e\u0631\u0650\u064a\u0639\u064f", "word_translation": "(is) Swift", "word_transliteration": "sari'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u0650\u0633\u064e\u0627\u0628\u0650", "word_translation": "(in) the reckoning", "word_transliteration": "al-hisabi"}]}