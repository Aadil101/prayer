{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062a\u0650\u0644\u06e1\u0643\u064e \u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u062b\u064e\u0670\u0644\u064f \u0646\u064e\u0636\u06e1\u0631\u0650\u0628\u064f\u0647\u064e\u0627 \u0644\u0650\u0644\u0646\u0651\u064e\u0627\u0633\u0650\u06d6 \u0648\u064e\u0645\u064e\u0627 \u064a\u064e\u0639\u06e1\u0642\u0650\u0644\u064f\u0647\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062a\u0650\u0644\u06e1\u0643\u064e", "word_translation": "And these", "word_transliteration": "watil'ka"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u062b\u064e\u0670\u0644\u064f", "word_translation": "examples", "word_transliteration": "al-amthalu"}, {"word_arabic_uthmani": "\u0646\u064e\u0636\u06e1\u0631\u0650\u0628\u064f\u0647\u064e\u0627", "word_translation": "We set forth", "word_transliteration": "nadribuha"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0646\u0651\u064e\u0627\u0633\u0650\u06d6", "word_translation": "to mankind", "word_transliteration": "lilnnasi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "but not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0642\u0650\u0644\u064f\u0647\u064e\u0627\u0653", "word_translation": "will understand them", "word_transliteration": "ya'qiluha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e", "word_translation": "those of knowledge", "word_transliteration": "al-'alimuna"}]}