{"ayahs": {"ayah_uthmani": "\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e \u0644\u0650\u0646\u064e\u0628\u0650\u064a\u0651\u064d \u0623\u064e\u0646 \u064a\u064e\u0643\u064f\u0648\u0646\u064e \u0644\u064e\u0647\u064f\u06e5\u0653 \u0623\u064e\u0633\u06e1\u0631\u064e\u0649\u0670 \u062d\u064e\u062a\u0651\u064e\u0649\u0670 \u064a\u064f\u062b\u06e1\u062e\u0650\u0646\u064e \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da \u062a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e \u0639\u064e\u0631\u064e\u0636\u064e \u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064f\u0631\u0650\u064a\u062f\u064f \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u064e\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0632\u0650\u064a\u0632\u064c \u062d\u064e\u0643\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0644\u0650\u0646\u064e\u0628\u0650\u064a\u0651\u064d", "word_translation": "for a Prophet", "word_transliteration": "linabiyyin"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0648\u0646\u064e", "word_translation": "(there) should be", "word_transliteration": "yakuna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5\u0653", "word_translation": "for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u0631\u064e\u0649\u0670", "word_translation": "prisoners of war", "word_transliteration": "asra"}, {"word_arabic_uthmani": "\u062d\u064e\u062a\u0651\u064e\u0649\u0670", "word_translation": "until", "word_transliteration": "hatta"}, {"word_arabic_uthmani": "\u064a\u064f\u062b\u06e1\u062e\u0650\u0646\u064e", "word_translation": "he has battled strenuously", "word_transliteration": "yuth'khina"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da", "word_translation": "the land", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e", "word_translation": "You desire", "word_transliteration": "turiduna"}, {"word_arabic_uthmani": "\u0639\u064e\u0631\u064e\u0636\u064e", "word_translation": "(the) commodities", "word_transliteration": "arada"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627", "word_translation": "(of) the world", "word_transliteration": "al-dun'ya"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "but Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f", "word_translation": "desires", "word_transliteration": "yuridu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u064e\u06d7", "word_translation": "(for you) the Hereafter", "word_transliteration": "al-akhirata"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0632\u0650\u064a\u0632\u064c", "word_translation": "(is) All-Mighty", "word_transliteration": "azizun"}, {"word_arabic_uthmani": "\u062d\u064e\u0643\u0650\u064a\u0645\u065e", "word_translation": "All-Wise", "word_transliteration": "hakimun"}]}