{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062a\u0650\u0644\u06e1\u0643\u064e \u0639\u064e\u0627\u062f\u065e\u06d6 \u062c\u064e\u062d\u064e\u062f\u064f\u0648\u0627\u0652 \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650 \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0639\u064e\u0635\u064e\u0648\u06e1\u0627\u0652 \u0631\u064f\u0633\u064f\u0644\u064e\u0647\u064f\u06e5 \u0648\u064e\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0645\u06e1\u0631\u064e \u0643\u064f\u0644\u0651\u0650 \u062c\u064e\u0628\u0651\u064e\u0627\u0631\u064d \u0639\u064e\u0646\u0650\u064a\u062f\u0656"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062a\u0650\u0644\u06e1\u0643\u064e", "word_translation": "And this", "word_transliteration": "watil'ka"}, {"word_arabic_uthmani": "\u0639\u064e\u0627\u062f\u065e\u06d6", "word_translation": "(was) Aad", "word_transliteration": "adun"}, {"word_arabic_uthmani": "\u062c\u064e\u062d\u064e\u062f\u064f\u0648\u0627\u0652", "word_translation": "they rejected", "word_transliteration": "jahadu"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650", "word_translation": "(the) Signs", "word_transliteration": "biayati"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1", "word_translation": "(of) their Lord", "word_transliteration": "rabbihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0635\u064e\u0648\u06e1\u0627\u0652", "word_translation": "and disobeyed", "word_transliteration": "wa'asaw"}, {"word_arabic_uthmani": "\u0631\u064f\u0633\u064f\u0644\u064e\u0647\u064f\u06e5", "word_translation": "His Messengers", "word_transliteration": "rusulahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064f\u0648\u0653\u0627\u0652", "word_translation": "and followed", "word_transliteration": "wa-ittaba'u"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0631\u064e", "word_translation": "(the) command", "word_transliteration": "amra"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "(of) every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u062c\u064e\u0628\u0651\u064e\u0627\u0631\u064d", "word_translation": "tyrant", "word_transliteration": "jabbarin"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0650\u064a\u062f\u0656", "word_translation": "obstinate", "word_transliteration": "anidin"}]}