{"ayahs": {"ayah_uthmani": "\u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6\u06d6 \u0641\u064e\u0643\u0650\u064a\u062f\u064f\u0648\u0646\u0650\u064a \u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627 \u062b\u064f\u0645\u0651\u064e \u0644\u064e\u0627 \u062a\u064f\u0646\u0638\u0650\u0631\u064f\u0648\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "Other than Him", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6\u06d6", "word_translation": "Other than Him", "word_transliteration": "dunihi"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u0650\u064a\u062f\u064f\u0648\u0646\u0650\u064a", "word_translation": "So plot against me", "word_transliteration": "fakiduni"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0639\u0657\u0627", "word_translation": "all together", "word_transliteration": "jami'an"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064f\u0646\u0638\u0650\u0631\u064f\u0648\u0646\u0650", "word_translation": "give me respite", "word_transliteration": "tunziruni"}]}