{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u0671\u0633\u06e1\u062a\u064e\u0648\u064e\u0649\u0670\u0653 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650 \u0648\u064e\u0647\u0650\u064a\u064e \u062f\u064f\u062e\u064e\u0627\u0646\u065e \u0641\u064e\u0642\u064e\u0627\u0644\u064e \u0644\u064e\u0647\u064e\u0627 \u0648\u064e\u0644\u0650\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0671\u0626\u06e1\u062a\u0650\u064a\u064e\u0627 \u0637\u064e\u0648\u06e1\u0639\u064b\u0627 \u0623\u064e\u0648\u06e1 \u0643\u064e\u0631\u06e1\u0647\u0657\u0627 \u0642\u064e\u0627\u0644\u064e\u062a\u064e\u0627\u0653 \u0623\u064e\u062a\u064e\u064a\u06e1\u0646\u064e\u0627 \u0637\u064e\u0627\u0653\u0626\u0650\u0639\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0648\u064e\u0649\u0670\u0653", "word_translation": "He directed (Himself)", "word_transliteration": "is'tawa"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "towards", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650", "word_translation": "the heaven", "word_transliteration": "al-samai"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u0650\u064a\u064e", "word_translation": "while it (was)", "word_transliteration": "wahiya"}, {"word_arabic_uthmani": "\u062f\u064f\u062e\u064e\u0627\u0646\u065e", "word_translation": "smoke", "word_transliteration": "dukhanun"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "and He said", "word_transliteration": "faqala"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064e\u0627", "word_translation": "to it", "word_transliteration": "laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and to the earth", "word_transliteration": "walil'ardi"}, {"word_arabic_uthmani": "\u0671\u0626\u06e1\u062a\u0650\u064a\u064e\u0627", "word_translation": "Come both of you", "word_transliteration": "i'tiya"}, {"word_arabic_uthmani": "\u0637\u064e\u0648\u06e1\u0639\u064b\u0627", "word_translation": "willingly", "word_transliteration": "taw'an"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0643\u064e\u0631\u06e1\u0647\u0657\u0627", "word_translation": "unwillingly", "word_transliteration": "karhan"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e\u062a\u064e\u0627\u0653", "word_translation": "They both said", "word_transliteration": "qalata"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "We come", "word_transliteration": "atayna"}, {"word_arabic_uthmani": "\u0637\u064e\u0627\u0653\u0626\u0650\u0639\u0650\u064a\u0646\u064e", "word_translation": "willingly", "word_transliteration": "tai'ina"}]}