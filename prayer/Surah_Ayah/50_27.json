{"ayahs": {"ayah_uthmani": "\u06de\u0642\u064e\u0627\u0644\u064e \u0642\u064e\u0631\u0650\u064a\u0646\u064f\u0647\u064f\u06e5 \u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627 \u0645\u064e\u0627\u0653 \u0623\u064e\u0637\u06e1\u063a\u064e\u064a\u06e1\u062a\u064f\u0647\u064f\u06e5 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646 \u0643\u064e\u0627\u0646\u064e \u0641\u0650\u064a \u0636\u064e\u0644\u064e\u0670\u0644\u0650\u06ed \u0628\u064e\u0639\u0650\u064a\u062f\u0656"}, "words": [{"word_arabic_uthmani": "\u06de\u0642\u064e\u0627\u0644\u064e", "word_translation": "Will say", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u0650\u064a\u0646\u064f\u0647\u064f\u06e5", "word_translation": "his companion", "word_transliteration": "qarinuhu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627", "word_translation": "Our Lord", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064e\u0637\u06e1\u063a\u064e\u064a\u06e1\u062a\u064f\u0647\u064f\u06e5", "word_translation": "I made him transgress", "word_transliteration": "atghaytuhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646", "word_translation": "but", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "he was", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0636\u064e\u0644\u064e\u0670\u0644\u0650\u06ed", "word_translation": "error", "word_transliteration": "dalalin"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u0650\u064a\u062f\u0656", "word_translation": "far", "word_transliteration": "ba'idin"}]}