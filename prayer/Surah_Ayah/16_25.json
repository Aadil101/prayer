{"ayahs": {"ayah_uthmani": "\u0644\u0650\u064a\u064e\u062d\u06e1\u0645\u0650\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0648\u06e1\u0632\u064e\u0627\u0631\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0627\u0645\u0650\u0644\u064e\u0629\u0657 \u064a\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650 \u0648\u064e\u0645\u0650\u0646\u06e1 \u0623\u064e\u0648\u06e1\u0632\u064e\u0627\u0631\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064f\u0636\u0650\u0644\u0651\u064f\u0648\u0646\u064e\u0647\u064f\u0645 \u0628\u0650\u063a\u064e\u064a\u06e1\u0631\u0650 \u0639\u0650\u0644\u06e1\u0645\u064d\u06d7 \u0623\u064e\u0644\u064e\u0627 \u0633\u064e\u0627\u0653\u0621\u064e \u0645\u064e\u0627 \u064a\u064e\u0632\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u0650\u064a\u064e\u062d\u06e1\u0645\u0650\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "That they may bear", "word_transliteration": "liyahmilu"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0632\u064e\u0627\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their own burdens", "word_transliteration": "awzarahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0645\u0650\u0644\u064e\u0629\u0657", "word_translation": "(in) full", "word_transliteration": "kamilatan"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "on (the) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650", "word_translation": "(of) the Resurrection", "word_transliteration": "al-qiyamati"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1", "word_translation": "and of", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0632\u064e\u0627\u0631\u0650", "word_translation": "the burdens", "word_transliteration": "awzari"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those whom", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064f\u0636\u0650\u0644\u0651\u064f\u0648\u0646\u064e\u0647\u064f\u0645", "word_translation": "they misled [them]", "word_transliteration": "yudillunahum"}, {"word_arabic_uthmani": "\u0628\u0650\u063a\u064e\u064a\u06e1\u0631\u0650", "word_translation": "without", "word_transliteration": "bighayri"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u064d\u06d7", "word_translation": "knowledge", "word_transliteration": "il'min"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627", "word_translation": "Unquestionably", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0633\u064e\u0627\u0653\u0621\u064e", "word_translation": "evil", "word_transliteration": "saa"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "(is) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0632\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "they will bear", "word_transliteration": "yaziruna"}]}