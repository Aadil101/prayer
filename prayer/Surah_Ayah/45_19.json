{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0646 \u064a\u064f\u063a\u06e1\u0646\u064f\u0648\u0627\u0652 \u0639\u064e\u0646\u0643\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627\u06da \u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e \u0628\u064e\u0639\u06e1\u0636\u064f\u0647\u064f\u0645\u06e1 \u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064f \u0628\u064e\u0639\u06e1\u0636\u0656\u06d6 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0648\u064e\u0644\u0650\u064a\u0651\u064f \u0671\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Indeed they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0646", "word_translation": "never", "word_transliteration": "lan"}, {"word_arabic_uthmani": "\u064a\u064f\u063a\u06e1\u0646\u064f\u0648\u0627\u0652", "word_translation": "will avail", "word_transliteration": "yugh'nu"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0643\u064e", "word_translation": "you", "word_transliteration": "anka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "against", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627\u06da", "word_translation": "(in) anything", "word_transliteration": "shayan"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimina"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064f\u0647\u064f\u0645\u06e1", "word_translation": "some of them", "word_transliteration": "ba'duhum"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064f", "word_translation": "(are) allies", "word_transliteration": "awliyau"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u0656\u06d6", "word_translation": "(of) others", "word_transliteration": "ba'din"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "and Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u0651\u064f", "word_translation": "(is the) Protector", "word_transliteration": "waliyyu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e", "word_translation": "(of) the righteous", "word_transliteration": "al-mutaqina"}]}