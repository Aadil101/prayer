{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0643\u064f\u0648\u0646\u064e\u0646\u0651\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652 \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0641\u064e\u062a\u064e\u0643\u064f\u0648\u0646\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u062e\u064e\u0670\u0633\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u064f\u0648\u0646\u064e\u0646\u0651\u064e", "word_translation": "be", "word_transliteration": "takunanna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "deny", "word_transliteration": "kadhabu"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650", "word_translation": "(the) Signs of Allah", "word_transliteration": "biayati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(the) Signs of Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0643\u064f\u0648\u0646\u064e", "word_translation": "then you will be", "word_transliteration": "fatakuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "among", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0670\u0633\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the losers", "word_transliteration": "al-khasirina"}]}