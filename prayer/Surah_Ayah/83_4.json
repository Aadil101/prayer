{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0627 \u064a\u064e\u0638\u064f\u0646\u0651\u064f \u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645 \u0645\u0651\u064e\u0628\u06e1\u0639\u064f\u0648\u062b\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627", "word_translation": "Do not", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u064a\u064e\u0638\u064f\u0646\u0651\u064f", "word_translation": "think", "word_transliteration": "yazunnu"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645", "word_translation": "that they", "word_transliteration": "annahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0628\u06e1\u0639\u064f\u0648\u062b\u064f\u0648\u0646\u064e", "word_translation": "(will be) resurrected", "word_transliteration": "mab'uthuna"}]}