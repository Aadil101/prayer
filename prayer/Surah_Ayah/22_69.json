{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064e\u062d\u06e1\u0643\u064f\u0645\u064f \u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1 \u064a\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650 \u0641\u0650\u064a\u0645\u064e\u0627 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u0650 \u062a\u064e\u062e\u06e1\u062a\u064e\u0644\u0650\u0641\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0643\u064f\u0645\u064f", "word_translation": "will judge", "word_transliteration": "yahkumu"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1", "word_translation": "between you", "word_transliteration": "baynakum"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(on the) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650", "word_translation": "(of) the Resurrection", "word_transliteration": "al-qiyamati"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0645\u064e\u0627", "word_translation": "concerning what", "word_transliteration": "fima"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you used (to)", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650", "word_translation": "in it", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u062a\u064e\u062e\u06e1\u062a\u064e\u0644\u0650\u0641\u064f\u0648\u0646\u064e", "word_translation": "differ", "word_transliteration": "takhtalifuna"}]}