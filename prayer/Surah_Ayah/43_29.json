{"ayahs": {"ayah_uthmani": "\u0628\u064e\u0644\u06e1 \u0645\u064e\u062a\u0651\u064e\u0639\u06e1\u062a\u064f \u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650 \u0648\u064e\u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1 \u062d\u064e\u062a\u0651\u064e\u0649\u0670 \u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u065e \u0645\u0651\u064f\u0628\u0650\u064a\u0646\u065e"}, "words": [{"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0645\u064e\u062a\u0651\u064e\u0639\u06e1\u062a\u064f", "word_translation": "I gave enjoyment", "word_transliteration": "matta'tu"}, {"word_arabic_uthmani": "\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650", "word_translation": "(to) these", "word_transliteration": "haulai"}, {"word_arabic_uthmani": "\u0648\u064e\u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and their forefathers", "word_transliteration": "waabaahum"}, {"word_arabic_uthmani": "\u062d\u064e\u062a\u0651\u064e\u0649\u0670", "word_translation": "until", "word_transliteration": "hatta"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u064f", "word_translation": "came to them", "word_transliteration": "jaahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f", "word_translation": "the truth", "word_transliteration": "al-haqu"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u065e", "word_translation": "and a Messenger", "word_transliteration": "warasulun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0628\u0650\u064a\u0646\u065e", "word_translation": "clear", "word_transliteration": "mubinun"}]}