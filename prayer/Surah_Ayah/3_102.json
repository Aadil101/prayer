{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u062d\u064e\u0642\u0651\u064e \u062a\u064f\u0642\u064e\u0627\u062a\u0650\u0647\u0650\u06e6 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0645\u064f\u0648\u062a\u064f\u0646\u0651\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645 \u0645\u0651\u064f\u0633\u06e1\u0644\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O you", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe[d]!", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "Fear", "word_transliteration": "ittaqu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u062d\u064e\u0642\u0651\u064e", "word_translation": "(as is His) right", "word_transliteration": "haqqa"}, {"word_arabic_uthmani": "\u062a\u064f\u0642\u064e\u0627\u062a\u0650\u0647\u0650\u06e6", "word_translation": "(that) He (should) be feared", "word_transliteration": "tuqatihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0645\u064f\u0648\u062a\u064f\u0646\u0651\u064e", "word_translation": "die", "word_transliteration": "tamutunna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645", "word_translation": "[while you]", "word_transliteration": "wa-antum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0633\u06e1\u0644\u0650\u0645\u064f\u0648\u0646\u064e", "word_translation": "(as) Muslims", "word_transliteration": "mus'limuna"}]}