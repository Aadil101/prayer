{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0646 \u062a\u064e\u0646\u0641\u064e\u0639\u064e\u0643\u064f\u0645\u06e1 \u0623\u064e\u0631\u06e1\u062d\u064e\u0627\u0645\u064f\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627\u0653 \u0623\u064e\u0648\u06e1\u0644\u064e\u0670\u062f\u064f\u0643\u064f\u0645\u06e1\u06da \u064a\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650 \u064a\u064e\u0641\u06e1\u0635\u0650\u0644\u064f \u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u0650\u0645\u064e\u0627 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e \u0628\u064e\u0635\u0650\u064a\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0646", "word_translation": "Never", "word_transliteration": "lan"}, {"word_arabic_uthmani": "\u062a\u064e\u0646\u0641\u064e\u0639\u064e\u0643\u064f\u0645\u06e1", "word_translation": "will benefit you", "word_transliteration": "tanfa'akum"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u062d\u064e\u0627\u0645\u064f\u0643\u064f\u0645\u06e1", "word_translation": "your relatives", "word_transliteration": "arhamukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627\u0653", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u064e\u0670\u062f\u064f\u0643\u064f\u0645\u06e1\u06da", "word_translation": "your children", "word_transliteration": "awladukum"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(on the) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u0650\u064a\u064e\u0670\u0645\u064e\u0629\u0650", "word_translation": "(of) the Resurrection", "word_transliteration": "al-qiyamati"}, {"word_arabic_uthmani": "\u064a\u064e\u0641\u06e1\u0635\u0650\u0644\u064f", "word_translation": "He will judge", "word_transliteration": "yafsilu"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064f\u0645\u06e1\u06da", "word_translation": "between you", "word_transliteration": "baynakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "you do", "word_transliteration": "ta'maluna"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u0650\u064a\u0631\u065e", "word_translation": "(is) All-Seer", "word_transliteration": "basirun"}]}