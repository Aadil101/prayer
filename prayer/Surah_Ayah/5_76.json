{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u0623\u064e\u062a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0645\u064e\u0627 \u0644\u064e\u0627 \u064a\u064e\u0645\u06e1\u0644\u0650\u0643\u064f \u0644\u064e\u0643\u064f\u0645\u06e1 \u0636\u064e\u0631\u0651\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u0646\u064e\u0641\u06e1\u0639\u0657\u0627\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0647\u064f\u0648\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e", "word_translation": "Do you worship", "word_transliteration": "ata'buduna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u06e1\u0644\u0650\u0643\u064f", "word_translation": "has power", "word_transliteration": "yamliku"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "to (cause) you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0636\u064e\u0631\u0651\u0657\u0627", "word_translation": "any harm", "word_transliteration": "darran"}, {"word_arabic_uthmani": "\u0648\u064e\u06