{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u06e6 \u0643\u0650\u062a\u064e\u0670\u0628\u064f \u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653 \u0625\u0650\u0645\u064e\u0627\u0645\u0657\u0627 \u0648\u064e\u0631\u064e\u062d\u06e1\u0645\u064e\u0629\u0657\u06da \u0648\u064e\u0647\u064e\u0670\u0630\u064e\u0627 \u0643\u0650\u062a\u064e\u0670\u0628\u065e \u0645\u0651\u064f\u0635\u064e\u062f\u0651\u0650\u0642\u065e \u0644\u0651\u0650\u0633\u064e\u0627\u0646\u064b\u0627 \u0639\u064e\u0631\u064e\u0628\u0650\u064a\u0651\u0657\u0627 \u0644\u0651\u0650\u064a\u064f\u0646\u0630\u0650\u0631\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0638\u064e\u0644\u064e\u0645\u064f\u0648\u0627\u0652 \u0648\u064e\u0628\u064f\u0634\u06e1\u0631\u064e\u0649\u0670 \u0644\u0650\u0644\u06e1\u0645\u064f\u062d\u06e1\u0633\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646", "word_translation": "And before it", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u06e6", "word_translation": "And before it", "word_transliteration": "qablihi"}, {"word_arabic_uthmani": "\u0643\u0650\u062a\u064e\u0670\u0628\u064f", "word_translation": "(was the) Scripture", "word_transliteration": "kitabu"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653", "word_translation": "(of) Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0625\u0650\u0645\u064e\u0627\u0645\u0657\u0627", "word_translation": "(as) a guide", "word_transliteration": "imaman"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u062d\u06e1\u0645\u064e\u0629\u0657\u06da", "word_translation": "and a mercy", "word_transliteration": "warahmatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "And this", "word_transliteration": "wahadha"}, {"word_arabic_uthmani": "\u0643\u0650\u062a\u064e\u0670\u0628\u065e", "word_translation": "(is) a Book", "word_transliteration": "kitabun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0635\u064e\u062f\u0651\u0650\u0642\u065e", "word_translation": "confirming", "word_transliteration": "musaddiqun"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0633\u064e\u0627\u0646\u064b\u0627", "word_translation": "(in) language", "word_transliteration": "lisanan"}, {"word_arabic_uthmani": "\u0639\u064e\u0631\u064e\u0628\u0650\u064a\u0651\u0657\u0627", "word_translation": "Arabic", "word_transliteration": "arabiyyan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u064a\u064f\u0646\u0630\u0650\u0631\u064e", "word_translation": "to warn", "word_transliteration": "liyundhira"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0638\u064e\u0644\u064e\u0645\u064f\u0648\u0627\u0652", "word_translation": "do wrong", "word_transliteration": "zalamu"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064f\u0634\u06e1\u0631\u064e\u0649\u0670", "word_translation": "and (as) a glad tidings", "word_transliteration": "wabush'ra"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0645\u064f\u062d\u06e1\u0633\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "for the good-doers", "word_transliteration": "lil'muh'sinina"}]}