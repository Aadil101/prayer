{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da \u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0643\u0650\u064a\u0644\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "And for Allah", "word_transliteration": "walillahi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "(is) whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "And is sufficient", "word_transliteration": "wakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u0650\u064a\u0644\u064b\u0627", "word_translation": "(as) a Disposer of affairs", "word_transliteration": "wakilan"}]}