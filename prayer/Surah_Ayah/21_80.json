{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0639\u064e\u0644\u0651\u064e\u0645\u06e1\u0646\u064e\u0670\u0647\u064f \u0635\u064e\u0646\u06e1\u0639\u064e\u0629\u064e \u0644\u064e\u0628\u064f\u0648\u0633\u0656 \u0644\u0651\u064e\u0643\u064f\u0645\u06e1 \u0644\u0650\u062a\u064f\u062d\u06e1\u0635\u0650\u0646\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u06e2 \u0628\u064e\u0623\u06e1\u0633\u0650\u0643\u064f\u0645\u06e1\u06d6 \u0641\u064e\u0647\u064e\u0644\u06e1 \u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u0634\u064e\u0670\u0643\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0644\u0651\u064e\u0645\u06e1\u0646\u064e\u0670\u0647\u064f", "word_translation": "And We taught him", "word_transliteration": "wa'allamnahu"}, {"word_arabic_uthmani": "\u0635\u064e\u0646\u06e1\u0639\u064e\u0629\u064e", "word_translation": "(the) making", "word_transliteration": "san'ata"}, {"word_arabic_uthmani": "\u0644\u064e\u0628\u064f\u0648\u0633\u0656", "word_translation": "(of) coats of armor", "word_transliteration": "labusin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064f\u062d\u06e1\u0635\u0650\u0646\u064e\u0643\u064f\u0645", "word_translation": "to protect you", "word_transliteration": "lituh'sinakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e2", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u0623\u06e1\u0633\u0650\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "your battle", "word_transliteration": "basikum"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064e\u0644\u06e1", "word_translation": "Then will", "word_transliteration": "fahal"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u0634\u064e\u0670\u0643\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(be) grateful", "word_transliteration": "shakiruna"}]}