ration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d7", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062f\u0650\u064a", "word_translation": "guide", "word_transliteration": "yahdi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064e", "word_translation": "the people", "word_transliteration": "al-qawma"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimina"}]}