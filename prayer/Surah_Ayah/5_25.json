{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0631\u064e\u0628\u0651\u0650 \u0625\u0650\u0646\u0651\u0650\u064a \u0644\u064e\u0627\u0653 \u0623\u064e\u0645\u06e1\u0644\u0650\u0643\u064f \u0625\u0650\u0644\u0651\u064e\u0627 \u0646\u064e\u0641\u06e1\u0633\u0650\u064a \u0648\u064e\u0623\u064e\u062e\u0650\u064a\u06d6 \u0641\u064e\u0671\u0641\u06e1\u0631\u064f\u0642\u06e1 \u0628\u064e\u064a\u06e1\u0646\u064e\u0646\u064e\u0627 \u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u0650 \u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "O my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed, I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0644\u064e\u0627\u0653", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0644\u0650\u0643\u064f", "word_translation": "(have) power", "word_transliteration": "amliku"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0633\u0650\u064a", "word_translation": "(over) myself", "word_transliteration": "nafsi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062e\u0650\u064a\u06d6", "word_translation": "and my brother", "word_transliteration": "wa-akhi"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0641\u06e1\u0631\u064f\u0642\u06e1", "word_translation": "so (make a) separation", "word_transliteration": "fa-uf'ruq"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0646\u064e\u0627", "word_translation": "between us", "word_transliteration": "baynana"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e", "word_translation": "and between", "word_transliteration": "wabayna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "the people", "word_transliteration": "al-qawmi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "(the) defiantly disobedient", "word_transliteration": "al-fasiqina"}]}