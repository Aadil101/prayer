{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653 \u0623\u064e\u0631\u0650\u0646\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u064e\u064a\u06e1\u0646\u0650 \u0623\u064e\u0636\u064e\u0644\u0651\u064e\u0627\u0646\u064e\u0627 \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u0650 \u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u0650 \u0646\u064e\u062c\u06e1\u0639\u064e\u0644\u06e1\u0647\u064f\u0645\u064e\u0627 \u062a\u064e\u062d\u06e1\u062a\u064e \u0623\u064e\u0642\u06e1\u062f\u064e\u0627\u0645\u0650\u0646\u064e\u0627 \u0644\u0650\u064a\u064e\u0643\u064f\u0648\u0646\u064e\u0627 \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u064e\u0633\u06e1\u0641\u064e\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "And (will) say", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653", "word_translation": "Our Lord!", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u0650\u0646\u064e\u0627", "word_translation": "Show us", "word_transliteration": "arina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u064e\u064a\u06e1\u0646\u0650", "word_translation": "those who", "word_transliteration": "alladhayni"}, {"word_arabic_uthmani": "\u0623\u064e\u0636\u064e\u0644\u0651\u064e\u0627\u0646\u064e\u0627", "word_translation": "misled us", "word_transliteration": "adallana"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u0650", "word_translation": "the jinn", "word_transliteration": "al-jini"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u0650", "word_translation": "and the men", "word_transliteration": "wal-insi"}, {"word_arabic_uthmani": "\u0646\u064e\u062c\u06e1\u0639\u064e\u0644\u06e1\u0647\u064f\u0645\u064e\u0627", "word_translation": "(so) we may put them", "word_transliteration": "naj'alhuma"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u06e1\u062a\u064e", "word_translation": "under", "word_transliteration": "tahta"}, {"word_arabic_uthmani": "\u0623\u064e\u0642\u06e1\u062f\u064e\u0627\u0645\u0650\u0646\u064e\u0627", "word_translation": "our feet", "word_transliteration": "aqdamina"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064e\u0643\u064f\u0648\u0646\u064e\u0627", "word_translation": "that they be", "word_transliteration": "liyakuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0633\u06e1\u0641\u064e\u0644\u0650\u064a\u0646\u064e", "word_translation": "the lowest", "word_transliteration": "al-asfalina"}]}