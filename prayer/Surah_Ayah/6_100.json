{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064f\u0648\u0627\u0652 \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u064e \u0648\u064e\u062e\u064e\u0644\u064e\u0642\u064e\u0647\u064f\u0645\u06e1\u06d6 \u0648\u064e\u062e\u064e\u0631\u064e\u0642\u064f\u0648\u0627\u0652 \u0644\u064e\u0647\u064f\u06e5 \u0628\u064e\u0646\u0650\u064a\u0646\u064e \u0648\u064e\u0628\u064e\u0646\u064e\u0670\u062a\u0650\u06ed \u0628\u0650\u063a\u064e\u064a\u06e1\u0631\u0650 \u0639\u0650\u0644\u06e1\u0645\u0656\u06da \u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0647\u064f\u06e5 \u0648\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649\u0670 \u0639\u064e\u0645\u0651\u064e\u0627 \u064a\u064e\u0635\u0650\u0641\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "And they make", "word_transliteration": "waja'alu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "with Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0621\u064e", "word_translation": "partners", "word_transliteration": "shurakaa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u064e", "word_translation": "jinn", "word_transliteration": "al-jina"}, {"word_arabic_uthmani": "\u0648\u064e\u062e\u064e\u0644\u064e\u0642\u064e\u0647\u064f\u0645\u06e1\u06d6", "word_translation": "though He has created them", "word_transliteration": "wakhalaqahum"}, {"word_arabic_uthmani": "\u0648\u064e\u062e\u064e\u0631\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "and they falsely attribute", "word_transliteration": "wakharaqu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "to Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0628\u064e\u0646\u0650\u064a\u0646\u064e", "word_translation": "sons", "word_transliteration": "banina"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u0646\u064e\u0670\u062a\u0650\u06ed", "word_translation": "and daughters", "word_transliteration": "wabanatin"}, {"word_arabic_uthmani": "\u0628\u0650\u063a\u064e\u064a\u06e1\u0631\u0650", "word_translation": "without", "word_transliteration": "bighayri"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u0656\u06da", "word_translation": "knowledge", "word_transliteration": "il'min"}, {"word_arabic_uthmani": "\u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0647\u064f\u06e5", "word_translation": "Glorified is He", "word_transliteration": "sub'hanahu"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649\u0670", "word_translation": "and Exalted", "word_transliteration": "wata'ala"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "above what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u064a\u064e\u0635\u0650\u0641\u064f\u0648\u0646\u064e", "word_translation": "they attribute", "word_transliteration": "yasifuna"}]}