{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653 \u0623\u064e\u062a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u0644\u0650\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0644\u064e\u0645\u0651\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1\u06d6 \u0623\u064e\u0633\u0650\u062d\u06e1\u0631\u064c \u0647\u064e\u0670\u0630\u064e\u0627 \u0648\u064e\u0644\u064e\u0627 \u064a\u064f\u0641\u06e1\u0644\u0650\u062d\u064f \u0671\u0644\u0633\u0651\u064e\u0640\u0670\u062d\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Musa said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653", "word_translation": "Musa said", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "Do you say", "word_transliteration": "ataquluna"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "about the truth", "word_transliteration": "lil'haqqi"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "when", "word_transliteration": "lamma"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "it has come to you", "word_transliteration": "jaakum"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u0650\u062d\u06e1\u0631\u064c", "word_translation": "Is this magic", "word_transliteration": "asih'run"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "Is this magic", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "But (will) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064f\u0641\u06e1\u0644\u0650\u062d\u064f", "word_translation": "succeed", "word_transliteration": "yuf'lihu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0640\u0670\u062d\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "the magicians", "word_transliteration": "al-sahiruna"}]}