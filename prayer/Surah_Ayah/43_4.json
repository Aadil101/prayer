{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0641\u0650\u064a\u0653 \u0623\u064f\u0645\u0651\u0650 \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u0644\u064e\u062f\u064e\u064a\u06e1\u0646\u064e\u0627 \u0644\u064e\u0639\u064e\u0644\u0650\u064a\u0651\u064c \u062d\u064e\u0643\u0650\u064a\u0645\u064c"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "And indeed it", "word_transliteration": "wa-innahu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0653", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u0650", "word_translation": "(the) Mother", "word_transliteration": "ummi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "(of) the Book", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u0644\u064e\u062f\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "with Us", "word_transliteration": "ladayna"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0650\u064a\u0651\u064c", "word_translation": "surely exalted", "word_transliteration": "la'aliyyun"}, {"word_arabic_uthmani": "\u062d\u064e\u0643\u0650\u064a\u0645\u064c", "word_translation": "full of wisdom", "word_transliteration": "hakimun"}]}