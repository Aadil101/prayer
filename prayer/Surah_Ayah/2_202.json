{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0644\u064e\u0647\u064f\u0645\u06e1 \u0646\u064e\u0635\u0650\u064a\u0628\u065e \u0645\u0651\u0650\u0645\u0651\u064e\u0627 \u0643\u064e\u0633\u064e\u0628\u064f\u0648\u0627\u0652\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0633\u064e\u0631\u0650\u064a\u0639\u064f \u0671\u0644\u06e1\u062d\u0650\u0633\u064e\u0627\u0628\u0650"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u0650\u064a\u0628\u065e", "word_translation": "(is) a share", "word_transliteration": "nasibun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0643\u064e\u0633\u064e\u0628\u064f\u0648\u0627\u0652\u06da", "word_translation": "they earned", "word_transliteration": "kasabu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "and Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0633\u064e\u0631\u0650\u064a\u0639\u064f", "word_translation": "(is) swift", "word_transliteration": "sari'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u0650\u0633\u064e\u0627\u0628\u0650", "word_translation": "(in taking) account", "word_transliteration": "al-hisabi"}]}