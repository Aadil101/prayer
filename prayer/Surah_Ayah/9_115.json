{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u0650\u064a\u064f\u0636\u0650\u0644\u0651\u064e \u0642\u064e\u0648\u06e1\u0645\u064e\u06e2\u0627 \u0628\u064e\u0639\u06e1\u062f\u064e \u0625\u0650\u0630\u06e1 \u0647\u064e\u062f\u064e\u0649\u0670\u0647\u064f\u0645\u06e1 \u062d\u064e\u062a\u0651\u064e\u0649\u0670 \u064a\u064f\u0628\u064e\u064a\u0651\u0650\u0646\u064e \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u064e\u0627 \u064a\u064e\u062a\u0651\u064e\u0642\u064f\u0648\u0646\u064e\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0628\u0650\u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u064d \u0639\u064e\u0644\u0650\u064a\u0645\u064c"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(for) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u0636\u0650\u0644\u0651\u064e", "word_translation": "that He lets go astray", "word_transliteration": "liyudilla"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064e\u06e2\u0627", "word_translation": "a people", "word_transliteration": "qawman"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u064e", "word_translation": "after", "word_transliteration": "ba'da"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "[when]", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0647\u064e\u062f\u064e\u0649\u0670\u0647\u064f\u0645\u06e1", "word_translation": "He has guided them", "word_transliteration": "hadahum"}, {"word_arabic_uthmani": "\u062d\u064e\u062a\u0651\u064e\u0649\u0670", "word_translation": "until", "word_transliteration": "hatta"}, {"word_arabic_uthmani": "\u064a\u064f\u0628\u064e\u064a\u0651\u0650\u0646\u064e", "word_translation": "He makes clear", "word_transliteration": "yubayyina"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "to them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u0651\u064e\u0642\u064f\u0648\u0646\u064e\u06da", "word_translation": "they should fear", "word_transliteration": "yattaquna"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064f\u0644\u0651\u0650", "word_translation": "(of) every", "word_transliteration": "bikulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u064d", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u064c", "word_translation": "(is) All-Knower", "word_transliteration": "alimun"}]}