{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0627 \u0645\u064f\u0648\u0633\u064e\u0649 \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e \u0648\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627 \u0645\u064e\u0639\u064e\u0647\u064f\u06e5\u0653 \u0623\u064e\u062e\u064e\u0627\u0647\u064f \u0647\u064e\u0670\u0631\u064f\u0648\u0646\u064e \u0648\u064e\u0632\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And verily", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "We gave", "word_transliteration": "atayna"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e", "word_translation": "the Scripture", "word_transliteration": "al-kitaba"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "and We appointed", "word_transliteration": "waja'alna"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e\u0647\u064f\u06e5\u0653", "word_translation": "with him", "word_transliteration": "ma'ahu"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u064e\u0627\u0647\u064f", "word_translation": "his brother", "word_transliteration": "akhahu"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0631\u064f\u0648\u0646\u064e", "word_translation": "Harun", "word_transliteration": "haruna"}, {"word_arabic_uthmani": "\u0648\u064e\u0632\u0650\u064a\u0631\u0657\u0627", "word_translation": "(as) an assistant", "word_transliteration": "waziran"}]}