{"ayahs": {"ayah_uthmani": "\u06de\u0623\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0623\u06e1\u0646\u0650 \u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0646 \u062a\u064e\u062e\u06e1\u0634\u064e\u0639\u064e \u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645\u06e1 \u0644\u0650\u0630\u0650\u0643\u06e1\u0631\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0645\u064e\u0627 \u0646\u064e\u0632\u064e\u0644\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652 \u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0623\u064f\u0648\u062a\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u064f \u0641\u064e\u0637\u064e\u0627\u0644\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0645\u064e\u062f\u064f \u0641\u064e\u0642\u064e\u0633\u064e\u062a\u06e1 \u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645\u06e1\u06d6 \u0648\u064e\u0643\u064e\u062b\u0650\u064a\u0631\u065e \u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Has not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u0646\u0650", "word_translation": "come (the) time", "word_transliteration": "yani"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "for those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0653\u0627\u0652", "word_translation": "believed", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u062e\u06e1\u0634\u064e\u0639\u064e", "word_translation": "become humble", "word_transliteration": "takhsha'a"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their hearts", "word_transliteration": "qulubuhum"}, {"word_arabic_uthmani": "\u0644\u0650\u0630\u0650\u0643\u06e1\u0631\u0650", "word_translation": "at (the) remembrance", "word_transliteration": "lidhik'ri"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0646\u064e\u0632\u064e\u0644\u064e", "word_translation": "has come down", "word_transliteration": "nazala"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "the truth", "word_transliteration": "al-haqi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652", "word_translation": "they become", "word_transliteration": "yakunu"}, {"word_arabic_uthmani": "\u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "like those who", "word_transliteration": "ka-alladhina"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u062a\u064f\u0648\u0627\u0652", "word_translation": "were given", "word_transliteration": "utu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e", "word_translation": "the Book", "word_transliteration": "al-kitaba"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f", "word_translation": "before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u0641\u064e\u0637\u064e\u0627\u0644\u064e", "word_translation": "(and) was prolonged", "word_transliteration": "fatala"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f", "word_translation": "for them", "word_transliteration": "alayhimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0645\u064e\u062f\u064f", "word_translation": "the term", "word_transliteration": "al-amadu"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u0633\u064e\u062a\u06e1", "word_translation": "so hardened", "word_transliteration": "faqasat"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645\u06e1\u06d6", "word_translation": "their hearts", "word_transliteration": "qulubuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u062b\u0650\u064a\u0631\u065e", "word_translation": "and many", "word_transliteration": "wakathirun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "of them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "(are) defiantly disobedient", "word_transliteration": "fasiquna"}]}