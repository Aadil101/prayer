{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652 \u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u062e\u064e\u0631\u064e\u062c\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u062f\u0650\u064a\u064e\u0670\u0631\u0650\u0647\u0650\u0645 \u0628\u064e\u0637\u064e\u0631\u0657\u0627 \u0648\u064e\u0631\u0650\u0626\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0648\u064e\u064a\u064e\u0635\u064f\u062f\u0651\u064f\u0648\u0646\u064e \u0639\u064e\u0646 \u0633\u064e\u0628\u0650\u064a\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u0650\u0645\u064e\u0627 \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e \u0645\u064f\u062d\u0650\u064a\u0637\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652", "word_translation": "be", "word_transliteration": "takunu"}, {"word_arabic_uthmani": "\u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "like those who", "word_transliteration": "ka-alladhina"}, {"word_arabic_uthmani": "\u062e\u064e\u0631\u064e\u062c\u064f\u0648\u0627\u0652", "word_translation": "came forth", "word_transliteration": "kharaju"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u0650\u064a\u064e\u0670\u0631\u0650\u0647\u0650\u0645", "word_translation": "their homes", "word_transliteration": "diyarihim"}, {"word_arabic_uthmani": "\u0628\u064e\u0637\u064e\u0631\u0657\u0627", "word_translation": "boastfully", "word_transliteration": "bataran"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u0650\u0626\u064e\u0627\u0653\u0621\u064e", "word_translation": "and showing off", "word_transliteration": "wariaa"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "(to) the people", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0635\u064f\u062f\u0651\u064f\u0648\u0646\u064e", "word_translation": "and hinder (them)", "word_transliteration": "wayasudduna"}, {"word_arabic_uthmani": "\u0639\u064e\u0646", "word_translation": "from", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u0650", "word_translation": "(the) way", "word_transliteration": "sabili"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "they do", "word_transliteration": "ya'maluna"}, {"word_arabic_uthmani": "\u0645\u064f\u062d\u0650\u064a\u0637\u065e", "word_translation": "(is) All-Encompassing", "word_transliteration": "muhitun"}]}