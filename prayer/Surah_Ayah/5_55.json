{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0648\u064e\u0644\u0650\u064a\u0651\u064f\u0643\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064f\u0647\u064f\u06e5 \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064f\u0642\u0650\u064a\u0645\u064f\u0648\u0646\u064e \u0671\u0644\u0635\u0651\u064e\u0644\u064e\u0648\u0670\u0629\u064e \u0648\u064e\u064a\u064f\u0624\u06e1\u062a\u064f\u0648\u0646\u064e \u0671\u0644\u0632\u0651\u064e\u0643\u064e\u0648\u0670\u0629\u064e \u0648\u064e\u0647\u064f\u0645\u06e1 \u0631\u064e\u0670\u0643\u0650\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u0651\u064f\u0643\u064f\u0645\u064f", "word_translation": "your ally", "word_transliteration": "waliyyukumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064f\u0647\u064f\u06e5", "word_translation": "and His Messenger", "word_transliteration": "warasuluhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064f\u0642\u0650\u064a\u0645\u064f\u0648\u0646\u064e", "word_translation": "establish", "word_transliteration": "yuqimuna"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0644\u064e\u0648\u0670\u0629\u064e", "word_translation": "the prayer", "word_transliteration": "al-salata"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0624\u06e1\u062a\u064f\u0648\u0646\u064e", "word_translation": "and give", "word_transliteration": "wayu'tuna"}, {"word_arabic_uthmani": "\u0671\u0644\u0632\u0651\u064e\u0643\u064e\u0648\u0670\u0629\u064e", "word_translation": "zakah", "word_transliteration": "al-zakata"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0631\u064e\u0670\u0643\u0650\u0639\u064f\u0648\u0646\u064e", "word_translation": "(are) those who bow down", "word_transliteration": "raki'una"}]}