{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1 \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u0634\u064e\u0627\u0639\u0650\u0631\u065e \u0646\u0651\u064e\u062a\u064e\u0631\u064e\u0628\u0651\u064e\u0635\u064f \u0628\u0650\u0647\u0650\u06e6 \u0631\u064e\u064a\u06e1\u0628\u064e \u0671\u0644\u06e1\u0645\u064e\u0646\u064f\u0648\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "(do) they say", "word_transliteration": "yaquluna"}, {"word_arabic_uthmani": "\u0634\u064e\u0627\u0639\u0650\u0631\u065e", "word_translation": "A poet", "word_transliteration": "sha'irun"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u062a\u064e\u0631\u064e\u0628\u0651\u064e\u0635\u064f", "word_translation": "we wait", "word_transliteration": "natarabbasu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "for him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0631\u064e\u064a\u06e1\u0628\u064e", "word_translation": "a misfortune of time", "word_transliteration": "rayba"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0646\u064f\u0648\u0646\u0650", "word_translation": "a misfortune of time", "word_transliteration": "al-manuni"}]}