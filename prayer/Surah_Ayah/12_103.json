{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0648\u064e\u0644\u064e\u0648\u06e1 \u062d\u064e\u0631\u064e\u0635\u06e1\u062a\u064e \u0628\u0650\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f", "word_translation": "most", "word_transliteration": "aktharu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "(of) the mankind", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "even though", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u062d\u064e\u0631\u064e\u0635\u06e1\u062a\u064e", "word_translation": "you desire", "word_transliteration": "harasta"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "(will be) believers", "word_transliteration": "bimu'minina"}]}