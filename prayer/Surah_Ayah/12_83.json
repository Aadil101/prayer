{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0628\u064e\u0644\u06e1 \u0633\u064e\u0648\u0651\u064e\u0644\u064e\u062a\u06e1 \u0644\u064e\u0643\u064f\u0645\u06e1 \u0623\u064e\u0646\u0641\u064f\u0633\u064f\u0643\u064f\u0645\u06e1 \u0623\u064e\u0645\u06e1\u0631\u0657\u0627\u06d6 \u0641\u064e\u0635\u064e\u0628\u06e1\u0631\u065e \u062c\u064e\u0645\u0650\u064a\u0644\u064c\u06d6 \u0639\u064e\u0633\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u0646 \u064a\u064e\u0623\u06e1\u062a\u0650\u064a\u064e\u0646\u0650\u064a \u0628\u0650\u0647\u0650\u0645\u06e1 \u062c\u064e\u0645\u0650\u064a\u0639\u064b\u0627\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0633\u064e\u0648\u0651\u064e\u0644\u064e\u062a\u06e1", "word_translation": "have enticed", "word_transliteration": "sawwalat"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u064f\u0643\u064f\u0645\u06e1", "word_translation": "your souls", "word_transliteration": "anfusukum"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0631\u0657\u0627\u06d6", "word_translation": "something", "word_transliteration": "amran"}, {"word_arabic_uthmani": "\u0641\u064e\u0635\u064e\u0628\u06e1\u0631\u065e", "word_translation": "so patience", "word_transliteration": "fasabrun"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0644\u064c\u06d6", "word_translation": "(is) beautiful", "word_transliteration": "jamilun"}, {"word_arabic_uthmani": "\u0639\u064e\u0633\u064e\u0649", "word_translation": "Perhaps", "word_transliteration": "asa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "will bring them to me", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650\u064a\u064e\u0646\u0650\u064a", "word_translation": "will bring them to me", "word_transliteration": "yatiyani"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u0645\u06e1", "word_translation": "will bring them to me", "word_transliteration": "bihim"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0639\u064b\u0627\u06da", "word_translation": "all", "word_transliteration": "jami'an"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed, He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "(is) the All-Knower", "word_transliteration": "al-'alimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f", "word_translation": "All-Wise", "word_transliteration": "al-hakimu"}]}