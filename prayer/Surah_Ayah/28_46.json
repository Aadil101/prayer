{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0643\u064f\u0646\u062a\u064e \u0628\u0650\u062c\u064e\u0627\u0646\u0650\u0628\u0650 \u0671\u0644\u0637\u0651\u064f\u0648\u0631\u0650 \u0625\u0650\u0630\u06e1 \u0646\u064e\u0627\u062f\u064e\u064a\u06e1\u0646\u064e\u0627 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646 \u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0629\u0657 \u0645\u0651\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064e \u0644\u0650\u062a\u064f\u0646\u0630\u0650\u0631\u064e \u0642\u064e\u0648\u06e1\u0645\u0657\u0627 \u0645\u0651\u064e\u0627\u0653 \u0623\u064e\u062a\u064e\u0649\u0670\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u0646\u0651\u064e\u0630\u0650\u064a\u0631\u0656 \u0645\u0651\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0643\u064e \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064e", "word_translation": "you were", "word_transliteration": "kunta"}, {"word_arabic_uthmani": "\u0628\u0650\u062c\u064e\u0627\u0646\u0650\u0628\u0650", "word_translation": "at (the) side", "word_transliteration": "bijanibi"}, {"word_arabic_uthmani": "\u0671\u0644\u0637\u0651\u064f\u0648\u0631\u0650", "word_translation": "(of) the Tur", "word_transliteration": "al-turi"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "when", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u062f\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "We called", "word_transliteration": "nadayna"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646", "word_translation": "But", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0629\u0657", "word_translation": "(as) a mercy", "word_transliteration": "rahmatan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064f\u0646\u0630\u0650\u0631\u064e", "word_translation": "so that you warn", "word_transliteration": "litundhira"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0657\u0627", "word_translation": "a people", "word_transliteration": "qawman"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627\u0653", "word_translation": "not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064e\u0649\u0670\u0647\u064f\u0645", "word_translation": "(had) come to them", "word_transliteration": "atahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0630\u0650\u064a\u0631\u0656", "word_translation": "warner", "word_transliteration": "nadhirin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "before you", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0643\u064e", "word_translation": "before you", "word_transliteration": "qablika"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so that they may", "word_transliteration": "la'allahum"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "remember", "word_transliteration": "yatadhakkaruna"}]}