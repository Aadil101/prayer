{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0631\u064e\u0628\u0651\u064e\u0643\u064e \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u062e\u064e\u0644\u0651\u064e\u0640\u0670\u0642\u064f \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbaka"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0644\u0651\u064e\u0640\u0670\u0642\u064f", "word_translation": "(is) the Creator", "word_transliteration": "al-khalaqu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "the All-Knower", "word_transliteration": "al-'alimu"}]}