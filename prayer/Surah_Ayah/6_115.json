{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062a\u064e\u0645\u0651\u064e\u062a\u06e1 \u0643\u064e\u0644\u0650\u0645\u064e\u062a\u064f \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0635\u0650\u062f\u06e1\u0642\u0657\u0627 \u0648\u064e\u0639\u064e\u062f\u06e1\u0644\u0657\u0627\u06da \u0644\u0651\u064e\u0627 \u0645\u064f\u0628\u064e\u062f\u0651\u0650\u0644\u064e \u0644\u0650\u0643\u064e\u0644\u0650\u0645\u064e\u0670\u062a\u0650\u0647\u0650\u06e6\u06da \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0645\u0651\u064e\u062a\u06e1", "word_translation": "And (has been) fulfilled", "word_transliteration": "watammat"}, {"word_arabic_uthmani": "\u0643\u064e\u0644\u0650\u0645\u064e\u062a\u064f", "word_translation": "(the) word", "word_transliteration": "kalimatu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "(of) your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0635\u0650\u062f\u06e1\u0642\u0657\u0627", "word_translation": "(in) truth", "word_transliteration": "sid'qan"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u062f\u06e1\u0644\u0657\u0627\u06da", "word_translation": "and justice", "word_transliteration": "wa'adlan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "No", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0645\u064f\u0628\u064e\u062f\u0651\u0650\u0644\u064e", "word_translation": "one can change", "word_transliteration": "mubaddila"}, {"word_arabic_uthmani": "\u0644\u0650\u0643\u064e\u0644\u0650\u0645\u064e\u0670\u062a\u0650\u0647\u0650\u06e6\u06da", "word_translation": "His words", "word_transliteration": "likalimatihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f", "word_translation": "(is) the All-Hearer", "word_transliteration": "al-sami'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "the All-Knower", "word_transliteration": "al-'alimu"}]}