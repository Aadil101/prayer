{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u064e\u0627\u06e0 \u0631\u064e\u0633\u064f\u0648\u0644\u064f \u0631\u064e\u0628\u0651\u0650\u0643\u0650 \u0644\u0650\u0623\u064e\u0647\u064e\u0628\u064e \u0644\u064e\u0643\u0650 \u063a\u064f\u0644\u064e\u0670\u0645\u0657\u0627 \u0632\u064e\u0643\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "I am", "word_transliteration": "ana"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u064f", "word_translation": "a Messenger", "word_transliteration": "rasulu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u0650", "word_translation": "(from) your Lord", "word_transliteration": "rabbiki"}, {"word_arabic_uthmani": "\u0644\u0650\u0623\u064e\u0647\u064e\u0628\u064e", "word_translation": "that I (may) bestow", "word_transliteration": "li-ahaba"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u0650", "word_translation": "on you", "word_transliteration": "laki"}, {"word_arabic_uthmani": "\u063a\u064f\u0644\u064e\u0670\u0645\u0657\u0627", "word_translation": "a son", "word_transliteration": "ghulaman"}, {"word_arabic_uthmani": "\u0632\u064e\u0643\u0650\u064a\u0651\u0657\u0627", "word_translation": "pure", "word_transliteration": "zakiyyan"}]}