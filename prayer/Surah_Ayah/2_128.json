{"ayahs": {"ayah_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627 \u0648\u064e\u0671\u062c\u06e1\u0639\u064e\u0644\u06e1\u0646\u064e\u0627 \u0645\u064f\u0633\u06e1\u0644\u0650\u0645\u064e\u064a\u06e1\u0646\u0650 \u0644\u064e\u0643\u064e \u0648\u064e\u0645\u0650\u0646 \u0630\u064f\u0631\u0651\u0650\u064a\u0651\u064e\u062a\u0650\u0646\u064e\u0627\u0653 \u0623\u064f\u0645\u0651\u064e\u0629\u0657 \u0645\u0651\u064f\u0633\u06e1\u0644\u0650\u0645\u064e\u0629\u0657 \u0644\u0651\u064e\u0643\u064e \u0648\u064e\u0623\u064e\u0631\u0650\u0646\u064e\u0627 \u0645\u064e\u0646\u064e\u0627\u0633\u0650\u0643\u064e\u0646\u064e\u0627 \u0648\u064e\u062a\u064f\u0628\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627\u0653\u06d6 \u0625\u0650\u0646\u0651\u064e\u0643\u064e \u0623\u064e\u0646\u062a\u064e \u0671\u0644\u062a\u0651\u064e\u0648\u0651\u064e\u0627\u0628\u064f \u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627", "word_translation": "Our Lord!", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u062c\u06e1\u0639\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "[and] Make us", "word_transliteration": "wa-ij'alna"}, {"word_arabic_uthmani": "\u0645\u064f\u0633\u06e1\u0644\u0650\u0645\u064e\u064a\u06e1\u0646\u0650", "word_translation": "both submissive", "word_transliteration": "mus'limayni"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e", "word_translation": "to You", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646", "word_translation": "And from", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u0630\u064f\u0631\u0651\u0650\u064a\u0651\u064e\u062a\u0650\u0646\u064e\u0627\u0653", "word_translation": "our offspring", "word_transliteration": "dhurriyyatina"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u064e\u0629\u0657", "word_translation": "a community", "word_transliteration": "ummatan"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0633\u06e1\u0644\u0650\u0645\u064e\u0629\u0657", "word_translation": "submissive", "word_transliteration": "mus'limatan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0643\u064e", "word_translation": "to You", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0631\u0650\u0646\u064e\u0627", "word_translation": "And show us", "word_transliteration": "wa-arina"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u064e\u0627\u0633\u0650\u0643\u064e\u0646\u064e\u0627", "word_translation": "our ways of worship", "word_transliteration": "manasikana"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064f\u0628\u06e1", "word_translation": "and turn", "word_transliteration": "watub"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627\u0653\u06d6", "word_translation": "to us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "Indeed You!", "word_transliteration": "innaka"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "[You] (are)", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0671\u0644\u062a\u0651\u064e\u0648\u0651\u064e\u0627\u0628\u064f", "word_translation": "the Oft-returning", "word_transliteration": "al-tawabu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f", "word_translation": "the Most Merciful", "word_transliteration": "al-rahimu"}]}