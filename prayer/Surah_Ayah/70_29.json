{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0647\u064f\u0645\u06e1 \u0644\u0650\u0641\u064f\u0631\u064f\u0648\u062c\u0650\u0647\u0650\u0645\u06e1 \u062d\u064e\u0670\u0641\u0650\u0638\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "And those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "[they]", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0644\u0650\u0641\u064f\u0631\u064f\u0648\u062c\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their modesty", "word_transliteration": "lifurujihim"}, {"word_arabic_uthmani": "\u062d\u064e\u0670\u0641\u0650\u0638\u064f\u0648\u0646\u064e", "word_translation": "(are) guardians", "word_transliteration": "hafizuna"}]}