{"ayahs": {"ayah_uthmani": "\u06de\u0643\u064e\u0630\u0651\u064e\u0628\u064e\u062a\u06e1 \u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1 \u0642\u064e\u0648\u06e1\u0645\u064f \u0646\u064f\u0648\u062d\u0656 \u0641\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652 \u0639\u064e\u0628\u06e1\u062f\u064e\u0646\u064e\u0627 \u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0645\u064e\u062c\u06e1\u0646\u064f\u0648\u0646\u065e \u0648\u064e\u0671\u0632\u06e1\u062f\u064f\u062c\u0650\u0631\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0643\u064e\u0630\u0651\u064e\u0628\u064e\u062a\u06e1", "word_translation": "Denied", "word_transliteration": "kadhabat"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "before them", "word_transliteration": "qablahum"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064f", "word_translation": "(the) people", "word_transliteration": "qawmu"}, {"word_arabic_uthmani": "\u0646\u064f\u0648\u062d\u0656", "word_translation": "(of) Nuh", "word_transliteration": "nuhin"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "and they denied", "word_transliteration": "fakadhabu"}, {"word_arabic_uthmani": "\u0639\u064e\u0628\u06e1\u062f\u064e\u0646\u064e\u0627", "word_translation": "Our slave", "word_transliteration": "abdana"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "and said", "word_transliteration": "waqalu"}, {"word_arabic_uthmani": "\u0645\u064e\u062c\u06e1\u0646\u064f\u0648\u0646\u065e", "word_translation": "A madman", "word_transliteration": "majnunun"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0632\u06e1\u062f\u064f\u062c\u0650\u0631\u064e", "word_translation": "and he was repelled", "word_transliteration": "wa-uz'dujira"}]}