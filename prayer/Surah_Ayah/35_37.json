{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0635\u06e1\u0637\u064e\u0631\u0650\u062e\u064f\u0648\u0646\u064e \u0641\u0650\u064a\u0647\u064e\u0627 \u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653 \u0623\u064e\u062e\u06e1\u0631\u0650\u062c\u06e1\u0646\u064e\u0627 \u0646\u064e\u0639\u06e1\u0645\u064e\u0644\u06e1 \u0635\u064e\u0670\u0644\u0650\u062d\u064b\u0627 \u063a\u064e\u064a\u06e1\u0631\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0643\u064f\u0646\u0651\u064e\u0627 \u0646\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u06da \u0623\u064e\u0648\u064e\u0644\u064e\u0645\u06e1 \u0646\u064f\u0639\u064e\u0645\u0651\u0650\u0631\u06e1\u0643\u064f\u0645 \u0645\u0651\u064e\u0627 \u064a\u064e\u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f \u0641\u0650\u064a\u0647\u0650 \u0645\u064e\u0646 \u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064e \u0648\u064e\u062c\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u0646\u0651\u064e\u0630\u0650\u064a\u0631\u064f\u06d6 \u0641\u064e\u0630\u064f\u0648\u0642\u064f\u0648\u0627\u0652 \u0641\u064e\u0645\u064e\u0627 \u0644\u0650\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0646\u0651\u064e\u0635\u0650\u064a\u0631\u064d"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "And they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0635\u06e1\u0637\u064e\u0631\u0650\u062e\u064f\u0648\u0646\u064e", "word_translation": "will cry", "word_transliteration": "yastarikhuna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653", "word_translation": "Our Lord!", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u06e1\u0631\u0650\u062c\u06e1\u0646\u064e\u0627", "word_translation": "Bring us out", "word_transliteration": "akhrij'na"}, {"word_arabic_uthmani": "\u0646\u064e\u0639\u06e1\u0645\u064e\u0644\u06e1", "word_translation": "we will do", "word_transliteration": "na'mal"}, {"word_arabic_uthmani": "\u0635\u064e\u0670\u0644\u0650\u062d\u064b\u0627", "word_translation": "righteous (deeds)", "word_transliteration": "salihan"}, {"word_arabic_uthmani": "\u063a\u064e\u064a\u06e1\u0631\u064e", "word_translation": "other than", "word_transliteration": "ghayra"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(that) which", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "we used", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0646\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u06da", "word_translation": "(to) do", "word_transliteration": "na'malu"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Did not", "word_transliteration": "awalam"}, {"word_arabic_uthmani": "\u0646\u064f\u0639\u064e\u0645\u0651\u0650\u0631\u06e1\u0643\u064f\u0645", "word_translation": "We give you life long enough", "word_transliteration": "nu'ammir'kum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "that", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f", "word_translation": "(would) receive admonition", "word_transliteration": "yatadhakkaru"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650", "word_translation": "therein", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whoever", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064e", "word_translation": "receives admonition", "word_transliteration": "tadhakkara"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u064f", "word_translation": "And came to you", "word_transliteration": "wajaakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0630\u0650\u064a\u0631\u064f\u06d6", "word_translation": "the warner", "word_transliteration": "al-nadhiru"}, {"word_arabic_uthmani": "\u0641\u064e\u0630\u064f\u0648\u0642\u064f\u0648\u0627\u0652", "word_translation": "So taste", "word_transliteration": "fadhuqu"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627", "word_translation": "then not", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "(is) for the wrongdoers", "word_transliteration": "lilzzalimina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0635\u0650\u064a\u0631\u064d", "word_translation": "helper", "word_transliteration": "nasirin"}]}