{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0670\u0628\u064e\u0646\u0650\u064a\u0653 \u0625\u0650\u0633\u06e1\u0631\u064e\u0640\u0670\u0653\u0621\u0650\u064a\u0644\u064e \u0642\u064e\u062f\u06e1 \u0623\u064e\u0646\u062c\u064e\u064a\u06e1\u0646\u064e\u0670\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u06e1 \u0639\u064e\u062f\u064f\u0648\u0651\u0650\u0643\u064f\u0645\u06e1 \u0648\u064e\u0648\u064e\u0670\u0639\u064e\u062f\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1 \u062c\u064e\u0627\u0646\u0650\u0628\u064e \u0671\u0644\u0637\u0651\u064f\u0648\u0631\u0650 \u0671\u0644\u06e1\u0623\u064e\u064a\u06e1\u0645\u064e\u0646\u064e \u0648\u064e\u0646\u064e\u0632\u0651\u064e\u0644\u06e1\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064e\u0646\u0651\u064e \u0648\u064e\u0671\u0644\u0633\u0651\u064e\u0644\u06e1\u0648\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0670\u0628\u064e\u0646\u0650\u064a\u0653", "word_translation": "O Children of Israel", "word_transliteration": "yabani"}, {"word_arabic_uthmani": "\u0625\u0650\u0633\u06e1\u0631\u064e\u0640\u0670\u0653\u0621\u0650\u064a\u0644\u064e", "word_translation": "O Children of Israel", "word_transliteration": "is'raila"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "Verily", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062c\u064e\u064a\u06e1\u0646\u064e\u0670\u0643\u064f\u0645", "word_translation": "We delivered you", "word_transliteration": "anjaynakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064f\u0648\u0651\u0650\u0643\u064f\u0645\u06e1", "word_translation": "your enemy", "word_transliteration": "aduwwikum"}, {"word_arabic_uthmani": "\u0648\u064e\u0648\u064e\u0670\u0639\u064e\u062f\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1", "word_translation": "and We made a covenant with you", "word_transliteration": "wawa'adnakum"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0646\u0650\u0628\u064e", "word_translation": "on (the) side", "word_transliteration": "janiba"}, {"word_arabic_uthmani": "\u0671\u0644\u0637\u0651\u064f\u0648\u0631\u0650", "word_translation": "(of) the Mount", "word_transliteration": "al-turi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u064a\u06e1\u0645\u064e\u0646\u064e", "word_translation": "the right", "word_transliteration": "al-aymana"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u0632\u0651\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "and We sent down", "word_transliteration": "wanazzalna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u064f", "word_translation": "to you", "word_transliteration": "alaykumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0646\u0651\u064e", "word_translation": "the Manna", "word_transliteration": "al-mana"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0633\u0651\u064e\u0644\u06e1\u0648\u064e\u0649\u0670", "word_translation": "and the quails", "word_transliteration": "wal-salwa"}]}