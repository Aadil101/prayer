{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0647\u064f\u062f\u0657\u0649 \u0645\u0651\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1\u06d6 \u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0644\u0650\u062d\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "(are) on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0647\u064f\u062f\u0657\u0649", "word_translation": "Guidance", "word_transliteration": "hudan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1\u06d6", "word_translation": "their Lord", "word_transliteration": "rabbihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "and those", "word_transliteration": "wa-ulaika"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "they", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0644\u0650\u062d\u064f\u0648\u0646\u064e", "word_translation": "(are) the successful ones", "word_transliteration": "al-muf'lihuna"}]}