{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645 \u0645\u0651\u064e\u0646 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f \u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645 \u0645\u0651\u064e\u0646 \u0644\u0651\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f \u0628\u0650\u0647\u0650\u06e6\u06da \u0648\u064e\u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0628\u0650\u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645", "word_translation": "And of them", "word_transliteration": "wamin'hum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646", "word_translation": "(is one) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f", "word_translation": "believes", "word_transliteration": "yu'minu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "in it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645", "word_translation": "and of them", "word_transliteration": "wamin'hum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646", "word_translation": "(is one) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f", "word_translation": "believe", "word_transliteration": "yu'minu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6\u06da", "word_translation": "in it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "And your Lord", "word_transliteration": "warabbuka"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\