{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627 \u0628\u064e\u0644\u064e\u0648\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0643\u064e\u0645\u064e\u0627 \u0628\u064e\u0644\u064e\u0648\u06e1\u0646\u064e\u0627\u0653 \u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064e \u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u0650 \u0625\u0650\u0630\u06e1 \u0623\u064e\u0642\u06e1\u0633\u064e\u0645\u064f\u0648\u0627\u0652 \u0644\u064e\u064a\u064e\u0635\u06e1\u0631\u0650\u0645\u064f\u0646\u0651\u064e\u0647\u064e\u0627 \u0645\u064f\u0635\u06e1\u0628\u0650\u062d\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u064e\u0648\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "have tried them", "word_transliteration": "balawnahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u0627", "word_translation": "as", "word_transliteration": "kama"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u064e\u0648\u06e1\u0646\u064e\u0627\u0653", "word_translation": "We tried", "word_transliteration": "balawna"}, {"word_arabic_uthmani": "\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064e", "word_translation": "(the) companions", "word_transliteration": "ashaba"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u0650", "word_translation": "(of) the garden", "word_transliteration": "al-janati"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "when", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0623\u064e\u0642\u06e1\u0633\u064e\u0645\u064f\u0648\u0627\u0652", "word_translation": "they swore", "word_transliteration": "aqsamu"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u064e\u0635\u06e1\u0631\u0650\u0645\u064f\u0646\u0651\u064e\u0647\u064e\u0627", "word_translation": "to pluck its fruit", "word_transliteration": "layasrimunnaha"}, {"word_arabic_uthmani": "\u0645\u064f\u0635\u06e1\u0628\u0650\u062d\u0650\u064a\u0646\u064e", "word_translation": "(in the) morning", "word_transliteration": "mus'bihina"}]}