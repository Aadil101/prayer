{"ayahs": {"ayah_uthmani": "\u0644\u0651\u0650\u0644\u0651\u064e\u0647\u0650 \u0645\u064f\u0644\u06e1\u0643\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da \u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f \u0645\u064e\u0627 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da \u064a\u064e\u0647\u064e\u0628\u064f \u0644\u0650\u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0625\u0650\u0646\u064e\u0670\u062b\u0657\u0627 \u0648\u064e\u064a\u064e\u0647\u064e\u0628\u064f \u0644\u0650\u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0671\u0644\u0630\u0651\u064f\u0643\u064f\u0648\u0631\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "To Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "(belongs the) dominion", "word_transliteration": "mul'ku"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f", "word_translation": "He creates", "word_transliteration": "yakhluqu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u064e\u0628\u064f", "word_translation": "He grants", "word_transliteration": "yahabu"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0646", "word_translation": "to whom", "word_transliteration": "liman"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u064e\u0670\u062b\u0657\u0627", "word_translation": "females", "word_transliteration": "inathan"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0647\u064e\u0628\u064f", "word_translation": "and He grants", "word_transliteration": "wayahabu"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0646", "word_translation": "to whom", "word_transliteration": "liman"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0671\u0644\u0630\u0651\u064f\u0643\u064f\u0648\u0631\u064e", "word_translation": "[the] males", "word_transliteration": "al-dhukura"}]}