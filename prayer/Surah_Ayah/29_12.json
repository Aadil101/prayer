{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0671\u062a\u0651\u064e\u0628\u0650\u0639\u064f\u0648\u0627\u0652 \u0633\u064e\u0628\u0650\u064a\u0644\u064e\u0646\u064e\u0627 \u0648\u064e\u0644\u06e1\u0646\u064e\u062d\u06e1\u0645\u0650\u0644\u06e1 \u062e\u064e\u0637\u064e\u0670\u064a\u064e\u0670\u0643\u064f\u0645\u06e1 \u0648\u064e\u0645\u064e\u0627 \u0647\u064f\u0645 \u0628\u0650\u062d\u064e\u0670\u0645\u0650\u0644\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u06e1 \u062e\u064e\u0637\u064e\u0670\u064a\u064e\u0670\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u0634\u064e\u064a\u06e1\u0621\u064d\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0643\u064e\u0670\u0630\u0650\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "And said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "to those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u0628\u0650\u0639\u064f\u0648\u0627\u0652", "word_translation": "Follow", "word_transliteration": "ittabi'u"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u064e\u0646\u064e\u0627", "word_translation": "our way", "word_transliteration": "sabilana"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u06e1\u0646\u064e\u062d\u06e1\u0645\u0650\u0644\u06e1", "word_translation": "and we will carry", "word_transliteration": "walnahmil"}, {"word_arabic_uthmani": "\u062e\u064e\u0637\u064e\u0670\u064a\u064e\u0670\u0643\u064f\u0645\u06e1", "word_translation": "your sins", "word_transliteration": "khatayakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "But not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0647\u064f\u0645", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0628\u0650\u062d\u064e\u0670\u0645\u0650\u0644\u0650\u064a\u0646\u064e", "word_translation": "(are) going to carry", "word_transliteration": "bihamilina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062e\u064e\u0637\u064e\u0670\u064a\u064e\u0670\u0647\u064f\u0645", "word_translation": "their sins", "word_transliteration": "khatayahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u064d\u06d6", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Indeed they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e\u0670\u0630\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "(are) surely liars", "word_transliteration": "lakadhibuna"}]}