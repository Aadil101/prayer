{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u062e\u064e\u0644\u064e\u0642\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0648\u064e\u0623\u064e\u0646\u0632\u064e\u0644\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650 \u0645\u064e\u0627\u0653\u0621\u0657 \u0641\u064e\u0623\u064e\u062e\u06e1\u0631\u064e\u062c\u064e \u0628\u0650\u0647\u0650\u06e6 \u0645\u0650\u0646\u064e \u0671\u0644\u062b\u0651\u064e\u0645\u064e\u0631\u064e\u0670\u062a\u0650 \u0631\u0650\u0632\u06e1\u0642\u0657\u0627 \u0644\u0651\u064e\u0643\u064f\u0645\u06e1\u06d6 \u0648\u064e\u0633\u064e\u062e\u0651\u064e\u0631\u064e \u0644\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u064e \u0644\u0650\u062a\u064e\u062c\u06e1\u0631\u0650\u064a\u064e \u0641\u0650\u064a \u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u0650 \u0628\u0650\u0623\u064e\u0645\u06e1\u0631\u0650\u0647\u0650\u06e6\u06d6 \u0648\u064e\u0633\u064e\u062e\u0651\u064e\u0631\u064e \u0644\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0646\u06e1\u0647\u064e\u0670\u0631\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064e", "word_translation": "created", "word_transliteration": "khalaqa"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "and the earth", "word_transliteration": "wal-arda"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0632\u064e\u0644\u064e", "word_translation": "and sent down", "word_transliteration": "wa-anzala"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650", "word_translation": "the sky", "word_transliteration": "al-samai"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653\u0621\u0657", "word_translation": "water", "word_transliteration": "maan"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u062e\u06e1\u0631\u064e\u062c\u064e", "word_translation": "then brought forth", "word_transliteration": "fa-akhraja"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "from it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u062b\u0651\u064e\u0645\u064e\u0631\u064e\u0670\u062a\u0650", "word_translation": "the fruits", "word_transliteration": "al-thamarati"}, {"word_arabic_uthmani": "\u0631\u0650\u0632\u06e1\u0642\u0657\u0627", "word_translation": "(as) a provision", "word_transliteration": "riz'qan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064e\u062e\u0651\u064e\u0631\u064e", "word_translation": "and subjected", "word_transliteration": "wasakhara"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u064f", "word_translation": "for you", "word_transliteration": "lakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u064e", "word_translation": "the ships", "word_transliteration": "al-ful'ka"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064e\u062c\u06e1\u0631\u0650\u064a\u