{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0625\u0650\u0644\u0651\u064e\u0627 \u0648\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f", "word_translation": "believe", "word_transliteration": "yu'minu"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645", "word_translation": "most of them", "word_transliteration": "aktharuhum"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "in Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "associate partners with Him", "word_transliteration": "mush'rikuna"}]}