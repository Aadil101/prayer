{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0671\u0646\u062a\u064e\u0642\u064e\u0645\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1\u06d6 \u0641\u064e\u0671\u0646\u0638\u064f\u0631\u06e1 \u0643\u064e\u064a\u06e1\u0641\u064e \u0643\u064e\u0627\u0646\u064e \u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f \u0671\u0644\u06e1\u0645\u064f\u0643\u064e\u0630\u0651\u0650\u0628\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0671\u0646\u062a\u064e\u0642\u064e\u0645\u06e1\u0646\u064e\u0627", "word_translation": "So We took retribution", "word_transliteration": "fa-intaqamna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1\u06d6", "word_translation": "from them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0646\u0638\u064f\u0631\u06e1", "word_translation": "Then see", "word_transliteration": "fa-unzur"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "how", "word_transliteration": "kayfa"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "was", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f", "word_translation": "(the) end", "word_transliteration": "aqibatu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0643\u064e\u0630\u0651\u0650\u0628\u0650\u064a\u0646\u064e", "word_translation": "(of) the deniers", "word_transliteration": "al-mukadhibina"}]}