{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u064e\u0627 \u062c\u064e\u0645\u064e\u0627\u0644\u064c \u062d\u0650\u064a\u0646\u064e \u062a\u064f\u0631\u0650\u064a\u062d\u064f\u0648\u0646\u064e \u0648\u064e\u062d\u0650\u064a\u0646\u064e \u062a\u064e\u0633\u06e1\u0631\u064e\u062d\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "And for you", "word_transliteration": "walakum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "in them", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u064e\u0627\u0644\u064c", "word_translation": "(is) beauty", "word_transliteration": "jamalun"}, {"word_arabic_uthmani": "\u062d\u0650\u064a\u0646\u064e", "word_translation": "when", "word_transliteration": "hina"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u0650\u064a\u062d\u064f\u0648\u0646\u064e", "word_translation": "you bring them in", "word_transliteration": "turihuna"}, {"word_arabic_uthmani": "\u0648\u064e\u062d\u0650\u064a\u0646\u064e", "word_translation": "and when", "word_transliteration": "wahina"}, {"word_arabic_uthmani": "\u062a\u064e\u0633\u06e1\u0631\u064e\u062d\u064f\u0648\u0646\u064e", "word_translation": "you take them out", "word_transliteration": "tasrahuna"}]}