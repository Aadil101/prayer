{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0671\u0637\u0651\u064e\u064a\u0651\u064e\u0631\u06e1\u0646\u064e\u0627 \u0628\u0650\u0643\u064e \u0648\u064e\u0628\u0650\u0645\u064e\u0646 \u0645\u0651\u064e\u0639\u064e\u0643\u064e\u06da \u0642\u064e\u0627\u0644\u064e \u0637\u064e\u0640\u0670\u0653\u0626\u0650\u0631\u064f\u0643\u064f\u0645\u06e1 \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6 \u0628\u064e\u0644\u06e1 \u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u0642\u064e\u0648\u06e1\u0645\u065e \u062a\u064f\u0641\u06e1\u062a\u064e\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0671\u0637\u0651\u064e\u064a\u0651\u064e\u0631\u06e1\u0646\u064e\u0627", "word_translation": "We consider you a bad omen", "word_transliteration": "ittayyarna"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064e", "word_translation": "We consider you a bad omen", "word_transliteration": "bika"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u0650\u0645\u064e\u0646", "word_translation": "and those", "word_transliteration": "wabiman"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0639\u064e\u0643\u064e\u06da", "word_translation": "with you", "word_transliteration": "ma'aka"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0637\u064e\u0640\u0670\u0653\u0626\u0650\u0631\u064f\u0643\u064f\u0645\u06e1", "word_translation": "Your bad omen", "word_transliteration": "tairukum"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "(is) with", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u065e", "word_translation": "(are) a people", "word_transliteration": "qawmun"}, {"word_arabic_uthmani": "\u062a\u064f\u0641\u06e1\u062a\u064e\u0646\u064f\u0648\u0646\u064e", "word_translation": "being tested", "word_transliteration": "tuf'tanuna"}]}