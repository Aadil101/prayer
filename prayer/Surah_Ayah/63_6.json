{"ayahs": {"ayah_uthmani": "\u0633\u064e\u0648\u064e\u0627\u0653\u0621\u064c \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0623\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u064e\u0631\u06e1\u062a\u064e \u0644\u064e\u0647\u064f\u0645\u06e1 \u0623\u064e\u0645\u06e1 \u0644\u064e\u0645\u06e1 \u062a\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1 \u0644\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0646 \u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0647\u064f\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0627 \u064a\u064e\u0647\u06e1\u062f\u0650\u064a \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0633\u064e\u0648\u064e\u0627\u0653\u0621\u064c", "word_translation": "(It) is same", "word_transliteration": "sawaon"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "for them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u064e\u0631\u06e1\u062a\u064e", "word_translation": "whether you ask forgiveness", "word_transliteration": "astaghfarta"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "(do) not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u062a\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u06e1", "word_translation": "ask forgiveness", "word_transliteration": "tastaghfir"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_tr