{"ayahs": {"ayah_uthmani": "\u06de\u0642\u064e\u0627\u0644\u064e \u0623\u064e\u0644\u064e\u0645\u06e1 \u0623\u064e\u0642\u064f\u0644 \u0644\u0651\u064e\u0643\u064e \u0625\u0650\u0646\u0651\u064e\u0643\u064e \u0644\u064e\u0646 \u062a\u064e\u0633\u06e1\u062a\u064e\u0637\u0650\u064a\u0639\u064e \u0645\u064e\u0639\u0650\u064a\u064e \u0635\u064e\u0628\u06e1\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u06de\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Did not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u0623\u064e\u0642\u064f\u0644", "word_translation": "I say", "word_transliteration": "aqul"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0643\u064e", "word_translation": "to you", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "that you", "word_transliteration": "innaka"}, {"word_arabic_uthmani": "\u0644\u064e\u0646", "word_translation": "never", "word_transliteration": "lan"}, {"word_arabic_uthmani": "\u062a\u064e\u0633\u06e1\u062a\u064e\u0637\u0650\u064a\u0639\u064e", "word_translation": "will be able", "word_transliteration": "tastati'a"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u0650\u064a\u064e", "word_translation": "with me", "word_transliteration": "ma'iya"}, {"word_arabic_uthmani": "\u0635\u064e\u0628\u06e1\u0631\u0657\u0627", "word_translation": "(to have) patience", "word_transliteration": "sabran"}]}