{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0648\u064e\u0631\u0650\u062b\u064e \u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064f \u062f\u064e\u0627\u0648\u064f\u06e5\u062f\u064e\u06d6 \u0648\u064e\u0642\u064e\u0627\u0644\u064e \u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f \u0639\u064f\u0644\u0651\u0650\u0645\u06e1\u0646\u064e\u0627 \u0645\u064e\u0646\u0637\u0650\u0642\u064e \u0671\u0644\u0637\u0651\u064e\u064a\u06e1\u0631\u0650 \u0648\u064e\u0623\u064f\u0648\u062a\u0650\u064a\u0646\u064e\u0627 \u0645\u0650\u0646 \u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u064d\u06d6 \u0625\u0650\u0646\u0651\u064e \u0647\u064e\u0670\u0630\u064e\u0627 \u0644\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0641\u064e\u0636\u06e1\u0644\u064f \u0671\u0644\u06e1\u0645\u064f\u0628\u0650\u064a\u0646\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0648\u064e\u0631\u0650\u062b\u064e", "word_translation": "And inherited", "word_transliteration": "wawaritha"}, {"word_arabic_uthmani": "\u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064f", "word_translation": "Sulaiman", "word_transliteration": "sulaymanu"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0648\u064f\u06e5\u062f\u064e\u06d6", "word_translation": "Dawood", "word_transliteration": "dawuda"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "And he said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f", "word_translation": "people!", "word_transliteration": "al-nasu"}, {"word_arabic_uthmani": "\u0639\u064f\u0644\u0651\u0650\u0645\u06e1\u0646\u064e\u0627", "word_translation": "We have been taught", "word_transliteration": "ullim'na"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u0637\u0650\u0642\u064e", "word_translation": "(the) language", "word_transliteration": "mantiqa"}, {"word_arabic_uthmani": "\u0671\u0644\u0637\u0651\u064e\u064a\u06e1\u0631\u0650", "word_translation": "(of) the birds", "word_transliteration": "al-tayri"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0648\u062a\u0650\u064a\u0646\u064e\u0627", "word_translation": "and we have been given", "word_transliteration": "wautina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u064d\u06d6", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "this", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0648\u064e", "word_translation": "surely it", "word_transliteration": "lahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0636\u06e1\u0644\u064f", "word_translation": "(is) the favor", "word_transliteration": "al-fadlu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0628\u0650\u064a\u0646\u064f", "word_translation": "evident", "word_transliteration": "al-mubinu"}]}