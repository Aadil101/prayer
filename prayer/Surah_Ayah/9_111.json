eration": "bi'ahdihi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "than", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0633\u06e1\u062a\u064e\u0628\u06e1\u0634\u0650\u0631\u064f\u0648\u0627\u0652", "word_translation": "So rejoice", "word_transliteration": "fa-is'tabshiru"}, {"word_arabic_uthmani": "\u0628\u0650\u0628\u064e\u064a\u06e1\u0639\u0650\u0643\u064f\u0645\u064f", "word_translation": "in your transaction", "word_transliteration": "bibay'ikumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "which", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0628\u064e\u0627\u064a\u064e\u0639\u06e1\u062a\u064f\u0645", "word_translation": "you have contracted", "word_transliteration": "baya'tum"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6\u06da", "word_translation": "[with it]", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "And that", "word_transliteration": "wadhalika"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "it", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0648\u06e1\u0632\u064f", "word_translation": "(is) the success", "word_transliteration": "al-fawzu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u064f", "word_translation": "the great", "word_transliteration": "al-'azimu"}]}