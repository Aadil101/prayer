{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u062c\u0650\u0626\u06e1\u062a\u064e\u0646\u064e\u0627 \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0623\u064e\u0645\u06e1 \u0623\u064e\u0646\u062a\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0640\u0670\u0639\u0650\u0628\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u0650\u0626\u06e1\u062a\u064e\u0646\u064e\u0627", "word_translation": "Have you come to us", "word_transliteration": "aji'tana"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "with the truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "you", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "(are) of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0640\u0670\u0639\u0650\u0628\u0650\u064a\u0646\u064e", "word_translation": "those who play", "word_transliteration": "al-la'ibina"}]}