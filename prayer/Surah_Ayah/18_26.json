{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0628\u0650\u0645\u064e\u0627 \u0644\u064e\u0628\u0650\u062b\u064f\u0648\u0627\u0652\u06d6 \u0644\u064e\u0647\u064f\u06e5 \u063a\u064e\u064a\u06e1\u0628\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6 \u0623\u064e\u0628\u06e1\u0635\u0650\u0631\u06e1 \u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u0623\u064e\u0633\u06e1\u0645\u0650\u0639\u06e1\u06da \u0645\u064e\u0627 \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6 \u0645\u0650\u0646 \u0648\u064e\u0644\u0650\u064a\u0651\u0656 \u0648\u064e\u0644\u064e\u0627 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f \u0641\u0650\u064a \u062d\u064f\u0643\u06e1\u0645\u0650\u0647\u0650\u06e6\u0653 \u0623\u064e\u062d\u064e\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u0650", "word_translation": "Say", "word_transliteration": "quli"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "knows best", "word_transliteration": "a'lamu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "about what (period)", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0644\u064e\u0628\u0650\u062b\u064f\u0648\u0627\u0652\u06d6", "word_translation": "they remained", "word_transliteration": "labithu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "For Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u063a\u064e\u064a\u06e1\u0628\u064f", "word_translation": "(is the) unseen", "word_transliteration": "ghaybu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0635\u0650\u0631\u06e1", "word_translation": "How clearly He sees", "word_transliteration": "absir"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "[of it]", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0633\u06e1\u0645\u0650\u0639\u06e1\u06da", "word_translation": "And how clearly He hears", "word_transliteration": "wa-asmi'"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "besides Him", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6", "word_translation": "besides Him", "word_transliteration": "dunihi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u0651\u0656", "word_translation": "protector", "word_transliteration": "waliyyin"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f", "word_translation": "He shares", "word_transliteration": "yush'riku"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "[in]", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u062d\u064f\u0643\u06e1\u0645\u0650\u0647\u0650\u06e6\u0653", "word_translation": "His Commands", "word_transliteration": "huk'mihi"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u0657\u0627", "word_translation": "(with) anyone", "word_transliteration": "ahadan"}]}