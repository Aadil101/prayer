{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0646 \u0644\u0651\u064e\u0627 \u062a\u064e\u0639\u06e1\u0644\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6 \u0625\u0650\u0646\u0651\u0650\u064a\u0653 \u0621\u064e\u0627\u062a\u0650\u064a\u0643\u064f\u0645 \u0628\u0650\u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u0656 \u0645\u0651\u064f\u0628\u0650\u064a\u0646\u0656"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646", "word_translation": "And that", "word_transliteration": "wa-an"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064f\u0648\u0627\u0652", "word_translation": "exalt yourselves", "word_transliteration": "ta'lu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "against", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u0650\u064a\u0643\u064f\u0645", "word_translation": "[I] have come to you", "word_transliteration": "atikum"}, {"word_arabic_uthmani": "\u0628\u0650\u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u0656", "word_translation": "with an authority", "word_transliteration": "bisul'tanin"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0628\u0650\u064a\u0646\u0656", "word_translation": "clear", "word_transliteration": "mubinin"}]}