{"ayahs": {"ayah_uthmani": "\u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1 \u0642\u064e\u0631\u0650\u064a\u0628\u0657\u0627\u06d6 \u0630\u064e\u0627\u0642\u064f\u0648\u0627\u0652 \u0648\u064e\u0628\u064e\u0627\u0644\u064e \u0623\u064e\u0645\u06e1\u0631\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064c \u0623\u064e\u0644\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650", "word_translation": "Like (the) example", "word_transliteration": "kamathali"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1", "word_translation": "before them", "word_transliteration": "qablihim"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u0650\u064a\u0628\u0657\u0627\u06d6", "word_translation": "shortly", "word_transliteration": "qariban"}, {"word_arabic_uthmani": "\u0630\u064e\u0627\u0642\u064f\u0648\u0627\u0652", "word_translation": "they tasted", "word_transliteration": "dhaqu"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u0627\u0644\u064e", "word_translation": "(the) evil result", "word_transliteration": "wabala"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0631\u0650\u0647\u0650\u0645\u06e1", "word_translation": "(of) their affair", "word_transliteration": "amrihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and for them", "word_transliteration": "walahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064c", "word_translation": "(is) a punishment", "word_transliteration": "adhabun"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0650\u064a\u0645\u065e", "word_translation": "painful", "word_transliteration": "alimun"}]}