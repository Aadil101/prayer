{"ayahs": {"ayah_uthmani": "\u06de\u0642\u064e\u0670\u0644\u064e \u0623\u064e\u0648\u064e\u0644\u064e\u0648\u06e1 \u062c\u0650\u0626\u06e1\u062a\u064f\u0643\u064f\u0645 \u0628\u0650\u0623\u064e\u0647\u06e1\u062f\u064e\u0649\u0670 \u0645\u0650\u0645\u0651\u064e\u0627 \u0648\u064e\u062c\u064e\u062f\u062a\u0651\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1\u06d6 \u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u0651\u064e\u0627 \u0628\u0650\u0645\u064e\u0627\u0653 \u0623\u064f\u0631\u06e1\u0633\u0650\u0644\u06e1\u062a\u064f\u0645 \u0628\u0650\u0647\u0650\u06e6 \u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0642\u064e\u0670\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "Even if", "word_transliteration": "awalaw"}, {"word_arabic_uthmani": "\u062c\u0650\u0626\u06e1\u062a\u064f\u0643\u064f\u0645", "word_translation": "I brought you", "word_transliteration": "ji'tukum"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0647\u06e1\u062f\u064e\u0649\u0670", "word_translation": "better guidance", "word_transliteration": "bi-ahda"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0627", "word_translation": "than what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u062f\u062a\u0651\u064f\u0645\u06e1", "word_translation": "you found", "word_transliteration": "wajadttum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "on it", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "your forefathers", "word_transliteration": "abaakum"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627\u0653", "word_translation": "with what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0623\u064f\u0631\u06e1\u0633\u0650\u0644\u06e1\u062a\u064f\u0645", "word_translation": "you are sent", "word_transliteration": "ur'sil'tum"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with [it]", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(are) disbelievers", "word_transliteration": "kafiruna"}]}