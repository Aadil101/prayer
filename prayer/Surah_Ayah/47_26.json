{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0631\u0650\u0647\u064f\u0648\u0627\u0652 \u0645\u064e\u0627 \u0646\u064e\u0632\u0651\u064e\u0644\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0633\u064e\u0646\u064f\u0637\u0650\u064a\u0639\u064f\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a \u0628\u064e\u0639\u06e1\u0636\u0650 \u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0631\u0650\u06d6 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0625\u0650\u0633\u06e1\u0631\u064e\u0627\u0631\u064e\u0647\u064f\u0645\u06e1"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "(is) because they", "word_transliteration": "bi-annahum"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "[they] said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "to those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0631\u0650\u0647\u064f\u0648\u0627\u0652", "word_translation": "hate", "word_transliteration": "karihu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0646\u064e\u0632\u0651\u064e\u0644\u064e", "word_translation": "Allah has revealed", "word_transliteration": "nazzala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah has revealed", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0633\u064e\u0646\u064f\u0637\u0650\u064a\u0639\u064f\u0643\u064f\u0645\u06e1", "word_translation": "We will obey you", "word_transliteration": "sanuti'ukum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u0650", "word_translation": "part", "word_transliteration": "ba'di"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0631\u0650\u06d6", "word_translation": "(of) the matter", "word_transliteration": "al-amri"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "But Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "knows", "word_transliteration": "ya'lamu"}, {"word_arabic_uthmani": "\u0625\u0650\u0633\u06e1\u0631\u064e\u0627\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their secrets", "word_transliteration": "is'rarahum"}]}