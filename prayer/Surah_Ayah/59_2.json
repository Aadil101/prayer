ion": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u062a\u064e\u0633\u0650\u0628\u064f\u0648\u0627\u0652\u06d6", "word_translation": "they expected", "word_transliteration": "yahtasibu"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0630\u064e\u0641\u064e", "word_translation": "and He cast", "word_transliteration": "waqadhafa"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "into", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0647\u0650\u0645\u064f", "word_translation": "their hearts", "word_transliteration": "qulubihimu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064f\u0639\u06e1\u0628\u064e\u06da", "word_translation": "[the] terror", "word_transliteration": "al-ru'ba"}, {"word_arabic_uthmani": "\u064a\u064f\u062e\u06e1\u0631\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "they destroyed", "word_transliteration": "yukh'ribuna"}, {"word_arabic_uthmani": "\u0628\u064f\u064a\u064f\u0648\u062a\u064e\u0647\u064f\u0645", "word_translation": "their houses", "word_transliteration": "buyutahum"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "with their hands", "word_transliteration": "bi-aydihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u064a\u06e1\u062f\u0650\u064a", "word_translation": "and the hands", "word_transliteration": "wa-aydi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "(of) the believers", "word_transliteration": "al-mu'minina"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0639\u06e1\u062a\u064e\u0628\u0650\u0631\u064f\u0648\u0627\u0652", "word_translation": "So take a lesson", "word_transliteration": "fa-i'tabiru"}, {"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064f\u0648\u0652\u0644\u0650\u064a", "word_translation": "O those endowed", "word_transliteration": "yauli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u0650", "word_translation": "(with) insight", "word_transliteration": "al-absari"}]}