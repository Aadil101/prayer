{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u064a\u064e\u0635\u064f\u062f\u0651\u064f\u0648\u0646\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0646\u0650 \u0671\u0644\u0633\u0651\u064e\u0628\u0650\u064a\u0644\u0650 \u0648\u064e\u064a\u064e\u062d\u06e1\u0633\u064e\u0628\u064f\u0648\u0646\u064e \u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0647\u06e1\u062a\u064e\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "And indeed they", "word_transliteration": "wa-innahum"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u064e\u0635\u064f\u062f\u0651\u064f\u0648\u0646\u064e\u0647\u064f\u0645\u06e1", "word_translation": "surely turn them away", "word_transliteration": "layasuddunahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0650", "word_translation": "from", "word_transliteration": "ani"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0628\u0650\u064a\u0644\u0650", "word_translation": "the Path", "word_transliteration": "al-sabili"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062d\u06e1\u0633\u064e\u0628\u064f\u0648\u0646\u064e", "word_translation": "and they think", "word_transliteration": "wayahsabuna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645", "word_translation": "that they", "word_transliteration": "annahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0647\u06e1\u062a\u064e\u062f\u064f\u0648\u0646\u064e", "word_translation": "(are) guided", "word_transliteration": "muh'taduna"}]}