{"ayahs": {"ayah_uthmani": "\u0644\u064e\u062e\u064e\u0644\u06e1\u0642\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0623\u064e\u0643\u06e1\u0628\u064e\u0631\u064f \u0645\u0650\u0646\u06e1 \u062e\u064e\u0644\u06e1\u0642\u0650 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0644\u064e\u0627 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u062e\u064e\u0644\u06e1\u0642\u064f", "word_translation": "Surely (the) creation", "word_transliteration": "lakhalqu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u0628\u064e\u0631\u064f", "word_translation": "(is) greater", "word_transliteration": "akbaru"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "than", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0642\u0650", "word_translation": "(the) creation", "word_transliteration": "khalqi"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "(of) the mankind", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e", "word_translation": "but", "word_transliteration": "walakinna"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e", "word_translation": "most", "word_transliteration": "akthara"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "(of) the people", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "know", "word_transliteration": "ya'lamuna"}]}