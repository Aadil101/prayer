{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0644\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0644\u064e\u0627 \u064a\u064e\u0647\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064c \u0623\u064e\u0644\u0650\u064a\u0645\u064c"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "believe", "word_transliteration": "yu'minuna"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650", "word_translation": "in the Verses", "word_transliteration": "biayati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u064f", "word_translation": "Allah will guide them", "word_transliteration": "yahdihimu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah will guide them", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and for them", "word_transliteration": "walahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064c", "word_translation": "(is) a punishment", "word_transliteration": "adhabun"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0650\u064a\u0645\u064c", "word_translation": "painful", "word_transliteration": "alimun"}]}