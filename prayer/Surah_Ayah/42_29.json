{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1 \u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0647\u0650\u06e6 \u062e\u064e\u0644\u06e1\u0642\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u062b\u0651\u064e \u0641\u0650\u064a\u0647\u0650\u0645\u064e\u0627 \u0645\u0650\u0646 \u062f\u064e\u0627\u0653\u0628\u0651\u064e\u0629\u0656\u06da \u0648\u064e\u0647\u064f\u0648\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u062c\u064e\u0645\u06e1\u0639\u0650\u0647\u0650\u0645\u06e1 \u0625\u0650\u0630\u064e\u0627 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0642\u064e\u062f\u0650\u064a\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1", "word_translation": "And among", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0647\u0650\u06e6", "word_translation": "His Signs", "word_transliteration": "ayatihi"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0642\u064f", "word_translation": "(is the) creation", "word_transliteration": "khalqu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u062b\u0651\u064e", "word_translation": "He has dispersed", "word_transliteration": "batha"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650\u0645\u064e\u0627", "word_translation": "in both of them", "word_transliteration": "fihima"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0653\u0628\u0651\u064e\u0629\u0656\u06da", "word_translation": "(the) creatures", "word_transliteration": "dabbatin"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "(is) over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u06e1\u0639\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their gathering", "word_transliteration": "jam'ihim"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0650\u064a\u0631\u065e", "word_translation": "All-Powerful", "word_transliteration": "qadirun"}]}