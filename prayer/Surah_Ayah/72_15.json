{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627 \u0671\u0644\u06e1\u0642\u064e\u0670\u0633\u0650\u0637\u064f\u0648\u0646\u064e \u0641\u064e\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0644\u0650\u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e \u062d\u064e\u0637\u064e\u0628\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627", "word_translation": "And as for", "word_transliteration": "wa-amma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0670\u0633\u0650\u0637\u064f\u0648\u0646\u064e", "word_translation": "the unjust", "word_transliteration": "al-qasituna"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they will be", "word_transliteration": "fakanu"}, {"word_arabic_uthmani": "\u0644\u0650\u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e", "word_translation": "for Hell", "word_transliteration": "lijahannama"}, {"word_arabic_uthmani": "\u062d\u064e\u0637\u064e\u0628\u0657\u0627", "word_translation": "firewood", "word_transliteration": "hataban"}]}