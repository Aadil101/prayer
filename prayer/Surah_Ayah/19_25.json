{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0647\u064f\u0632\u0651\u0650\u064a\u0653 \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u0650 \u0628\u0650\u062c\u0650\u0630\u06e1\u0639\u0650 \u0671\u0644\u0646\u0651\u064e\u062e\u06e1\u0644\u064e\u0629\u0650 \u062a\u064f\u0633\u064e\u0670\u0642\u0650\u0637\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u0650 \u0631\u064f\u0637\u064e\u0628\u0657\u0627 \u062c\u064e\u0646\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0632\u0651\u0650\u064a\u0653", "word_translation": "And shake", "word_transliteration": "wahuzzi"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u0650", "word_translation": "towards you", "word_transliteration": "ilayki"}, {"word_arabic_uthmani": "\u0628\u0650\u062c\u0650\u0630\u06e1\u0639\u0650", "word_translation": "(the) trunk", "word_transliteration": "bijidh'i"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u062e\u06e1\u0644\u064e\u0629\u0650", "word_translation": "(of) the date-palm", "word_transliteration": "al-nakhlati"}, {"word_arabic_uthmani": "\u062a\u064f\u0633\u064e\u0670\u0642\u0650\u0637\u06e1", "word_translation": "it will drop", "word_transliteration": "tusaqit"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u0650", "word_translation": "upon you", "word_transliteration": "alayki"}, {"word_arabic_uthmani": "\u0631\u064f\u0637\u064e\u0628\u0657\u0627", "word_translation": "fresh dates", "word_transliteration": "rutaban"}, {"word_arabic_uthmani": "\u062c\u064e\u0646\u0650\u064a\u0651\u0657\u0627", "word_translation": "ripe", "word_transliteration": "janiyyan"}]}