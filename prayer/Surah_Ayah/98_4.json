{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u062a\u064e\u0641\u064e\u0631\u0651\u064e\u0642\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0623\u064f\u0648\u062a\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u0650\u0646\u06e2 \u0628\u064e\u0639\u06e1\u062f\u0650 \u0645\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0628\u064e\u064a\u0651\u0650\u0646\u064e\u0629\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u064e\u0631\u0651\u064e\u0642\u064e", "word_translation": "became divided", "word_transliteration": "tafarraqa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u062a\u064f\u0648\u0627\u0652", "word_translation": "were given", "word_transliteration": "utu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u064e", "word_translation": "the Book", "word_transliteration": "al-kitaba"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "until", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e2", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u0650", "word_translation": "after", "word_transliteration": "ba'di"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1\u0647\u064f\u0645\u064f", "word_translation": "came (to) them", "word_transliteration": "jaathumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u064a\u065