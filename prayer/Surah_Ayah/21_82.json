{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e \u0671\u0644\u0634\u0651\u064e\u064a\u064e\u0670\u0637\u0650\u064a\u0646\u0650 \u0645\u064e\u0646 \u064a\u064e\u063a\u064f\u0648\u0635\u064f\u0648\u0646\u064e \u0644\u064e\u0647\u064f\u06e5 \u0648\u064e\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e \u0639\u064e\u0645\u064e\u0644\u0657\u0627 \u062f\u064f\u0648\u0646\u064e \u0630\u064e\u0670\u0644\u0650\u0643\u064e\u06d6 \u0648\u064e\u0643\u064f\u0646\u0651\u064e\u0627 \u0644\u064e\u0647\u064f\u0645\u06e1 \u062d\u064e\u0670\u0641\u0650\u0638\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e", "word_translation": "And of", "word_transliteration": "wamina"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u064e\u0670\u0637\u0650\u064a\u0646\u0650", "word_translation": "the devils", "word_transliteration": "al-shayatini"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(were some) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u063a\u064f\u0648\u0635\u064f\u0648\u0646\u064e", "word_translation": "would dive", "word_transliteration": "yaghusuna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "and would do", "word_transliteration": "waya'maluna"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u064e\u0644\u0657\u0627", "word_translation": "work", "word_transliteration": "amalan"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u064e", "word_translation": "other than", "word_transliteration": "duna"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e\u06d6", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation"