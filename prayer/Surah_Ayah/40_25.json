{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645 \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0645\u0650\u0646\u06e1 \u0639\u0650\u0646\u062f\u0650\u0646\u064e\u0627 \u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0671\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0628\u06e1\u0646\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0645\u064e\u0639\u064e\u0647\u064f\u06e5 \u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u062d\u06e1\u064a\u064f\u0648\u0627\u0652 \u0646\u0650\u0633\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1\u06da \u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u064a\u06e1\u062f\u064f \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0641\u0650\u064a \u0636\u064e\u0644\u064e\u0670\u0644\u0656"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "Then when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645", "word_translation": "he brought to them", "word_transliteration": "jaahum"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "the truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u0650\u0646\u064e\u0627", "word_translation": "Us", "word_transliteration": "indina"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "they said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0671\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "Kill", "word_transliteration": "uq'tulu"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0646\u064e\u0627\u0653\u0621\u064e", "word_translation": "(the) sons", "word_transliteration": "abnaa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e\u0647\u064f\u06e5", "word_translation": "with him", "word_transliteration": "ma'ahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u062d\u06e1\u064a\u064f\u0648\u0627\u0652", "word_translation": "and let live", "word_transliteration": "wa-is'tahyu"}, {"word_arabic_uthmani": "\u0646\u0650\u0633\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "their women", "word_transliteration": "nisaahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u062f\u064f", "word_translation": "(is the) plot", "word_transliteration": "kaydu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(of) the disbelievers", "word_transliteration": "al-kafirina"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0636\u064e\u0644\u064e\u0670\u0644\u0656", "word_translation": "error", "word_transliteration": "dalalin"}]}