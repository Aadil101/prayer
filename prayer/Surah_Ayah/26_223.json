{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0644\u06e1\u0642\u064f\u0648\u0646\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u06e1\u0639\u064e \u0648\u064e\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645\u06e1 \u0643\u064e\u0670\u0630\u0650\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0644\u06e1\u0642\u064f\u0648\u0646\u064e", "word_translation": "They pass on", "word_transliteration": "yul'quna"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u06e1\u0639\u064e", "word_translation": "(what is) heard", "word_transliteration": "al-sam'a"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645\u06e1", "word_translation": "and most of them", "word_transliteration": "wa-aktharuhum"}, {"word_arabic_uthmani": "\u0643\u064e\u0670\u0630\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "(are) liars", "word_transliteration": "kadhibuna"}]}