{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0641\u064e\u0645\u064e\u0646 \u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f \u0643\u064e\u0645\u064e\u0646 \u0644\u0651\u064e\u0627 \u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f\u06da \u0623\u064e\u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0645\u064e\u0646", "word_translation": "Then is He Who", "word_transliteration": "afaman"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f", "word_translation": "creates", "word_transliteration": "yakhluqu"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u0646", "word_translation": "like one who", "word_transliteration": "kaman"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064f\u06da", "word_translation": "create", "word_transliteration": "yakhluqu"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0627", "word_translation": "Then will you not", "word_transliteration": "afala"}, {"word_arabic_uthmani": "\u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "remember", "word_transliteration": "tadhakkaruna"}]}