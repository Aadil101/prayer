{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0639\u064e\u0635\u064e\u0649\u0670 \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064f \u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064e \u0641\u064e\u0623\u064e\u062e\u064e\u0630\u06e1\u0646\u064e\u0670\u0647\u064f \u0623\u064e\u062e\u06e1\u0630\u0657\u0627 \u0648\u064e\u0628\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0639\u064e\u0635\u064e\u0649\u0670", "word_translation": "But disobeyed", "word_transliteration": "fa'asa"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064f", "word_translation": "Firaun", "word_transliteration": "fir'awnu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064e", "word_translation": "the Messenger", "word_transliteration": "al-rasula"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u062e\u064e\u0630\u06e1\u0646\u064e\u0670\u0647\u064f", "word_translation": "so We seized him", "word_transliteration": "fa-akhadhnahu"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u06e1\u0630\u0657\u0627", "word_translation": "(with) a seizure", "word_transliteration": "akhdhan"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u0650\u064a\u0644\u0657\u0627", "word_translation": "ruinous", "word_transliteration": "wabilan"}]}