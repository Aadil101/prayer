{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0644\u064e\u0627 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e \u0644\u064e\u0648\u06e1\u0644\u064e\u0627 \u064a\u064f\u0643\u064e\u0644\u0651\u0650\u0645\u064f\u0646\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u0648\u06e1 \u062a\u064e\u0623\u06e1\u062a\u0650\u064a\u0646\u064e\u0627\u0653 \u0621\u064e\u0627\u064a\u064e\u0629\u065e\u06d7 \u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0642\u064e\u0627\u0644\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645 \u0645\u0651\u0650\u062b\u06e1\u0644\u064e \u0642\u064e\u0648\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1\u06d8 \u062a\u064e\u0634\u064e\u0670\u0628\u064e\u0647\u064e\u062a\u06e1 \u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645\u06e1\u06d7 \u0642\u064e\u062f\u06e1 \u0628\u064e\u064a\u0651\u064e\u0646\u0651\u064e\u0627 \u0671\u0644\u06e1\u0623\u0653\u064a\u064e\u0670\u062a\u0650 \u0644\u0650\u0642\u064e\u0648\u06e1\u0645\u0656 \u064a\u064f\u0648\u0642\u0650\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "And said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "know", "word_transliteration": "ya'lamuna"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "Why not", "word_transliteration": "lawla"}, {"word_arabic_uthmani": "\u064a\u064f\u0643\u064e\u0644\u0651\u0650\u0645\u064f\u0646\u064e\u0627", "word_translation": "speaks to us", "word_transliteration": "yukallimuna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u062a\u0650\u064a\u0646\u064e\u0627\u0653", "word_translation": "comes to us", "word_transliteration": "tatina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0629\u065e\u06d7", "word_translation": "a sign", "word_transliteration": "ayatun"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "Like that", "word_transliteration": "kadhalika"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645", "word_translation": "before them", "word_transliteration": "qablihim"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u062b\u06e1\u0644\u064e", "word_translation": "similar", "word_transliteration": "mith'la"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1\u06d8", "word_translation": "their saying", "word_transliteration": "qawlihim"}, {"word_arabic_uthmani": "\u062a\u064e\u0634\u064e\u0670\u0628\u064e\u0647\u064e\u062a\u06e1", "word_translation": "Became alike", "word_transliteration": "tashabahat"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645\u06e1\u06d7", "word_translation": "their hearts", "word_transliteration": "qulubuhum"}, {"word_arabi