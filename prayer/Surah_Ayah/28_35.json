{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0633\u064e\u0646\u064e\u0634\u064f\u062f\u0651\u064f \u0639\u064e\u0636\u064f\u062f\u064e\u0643\u064e \u0628\u0650\u0623\u064e\u062e\u0650\u064a\u0643\u064e \u0648\u064e\u0646\u064e\u062c\u06e1\u0639\u064e\u0644\u064f \u0644\u064e\u0643\u064f\u0645\u064e\u0627 \u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u0657\u0627 \u0641\u064e\u0644\u064e\u0627 \u064a\u064e\u0635\u0650\u0644\u064f\u0648\u0646\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u064e\u0627 \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627\u0653\u06da \u0623\u064e\u0646\u062a\u064f\u0645\u064e\u0627 \u0648\u064e\u0645\u064e\u0646\u0650 \u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e\u0643\u064f\u0645\u064e\u0627 \u0671\u0644\u06e1\u063a\u064e\u0670\u0644\u0650\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0633\u064e\u0646\u064e\u0634\u064f\u062f\u0651\u064f", "word_translation": "We will strengthen", "word_transliteration": "sanashuddu"}, {"word_arabic_uthmani": "\u0639\u064e\u0636\u064f\u062f\u064e\u0643\u064e", "word_translation": "your arm", "word_transliteration": "adudaka"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u062e\u0650\u064a\u0643\u064e", "word_translation": "through your brother", "word_transliteration": "bi-akhika"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u062c\u06e1\u0639\u064e\u0644\u064f", "word_translation": "and We will make", "word_transliteration": "wanaj'alu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u064e\u0627", "word_translation": "for both of you", "word_transliteration": "lakuma"}, {"word_arabic_uthmani": "\u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u0657\u0627", "word_translation": "an authority", "word_transliteration": "sul'tanan"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "so not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u064a\u064e\u0635\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "they will reach", "word_transliteration": "yasiluna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u064e\u0627", "word_translation": "to both of you", "word_transliteration": "ilaykuma"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627\u0653\u06da", "word_translation": "Through Our Signs", "word_transliteration": "biayatina"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645\u064e\u0627", "word_translation": "you two", "word_transliteration": "antuma"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646\u0650", "word_translation": "and (those) who", "word_transliteration": "wamani"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e\u0643\u064f\u0645\u064e\u0627", "word_translation": "follow you", "word_transliteration": "ittaba'akuma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u0670\u0644\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "(will) be the dominant", "word_transliteration": "al-ghalibuna"}]}