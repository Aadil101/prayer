{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0643\u064f \u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u0631\u06e1\u062c\u064e\u0627\u0653\u0626\u0650\u0647\u064e\u0627\u06da \u0648\u064e\u064a\u064e\u062d\u06e1\u0645\u0650\u0644\u064f \u0639\u064e\u0631\u06e1\u0634\u064e \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0641\u064e\u0648\u06e1\u0642\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0648\u06e1\u0645\u064e\u0626\u0650\u0630\u0656 \u062b\u064e\u0645\u064e\u0670\u0646\u0650\u064a\u064e\u0629\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0643\u064f", "word_translation": "And the Angels", "word_transliteration": "wal-malaku"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "(will be) on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u062c\u064e\u0627\u0653\u0626\u0650\u0647\u064e\u0627\u06da", "word_translation": "its edges", "word_transliteration": "arjaiha"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062d\u06e1\u0645\u0650\u0644\u064f", "word_translation": "and will bear", "word_transliteration": "wayahmilu"}, {"word_arabic_uthmani": "\u0639\u064e\u0631\u06e1\u0634\u064e", "word_translation": "(the) Throne", "word_transliteration": "arsha"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "(of) your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u06e1\u0642\u064e\u0647\u064f\u0645\u06e1", "word_translation": "above them", "word_transliteration": "fawqahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e\u0626\u0650\u0630\u0656", "word_translation": "that Day", "word_transliteration": "yawma-idhin"}, {"word_arabic_uthmani": "\u062b\u064e\u0645\u064e\u0670\u0646\u0650\u064a\u064e\u0629\u065e", "word_translation": "eight", "word_transliteration": "thamaniyatun"}]}