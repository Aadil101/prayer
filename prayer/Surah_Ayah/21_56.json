{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0628\u064e\u0644 \u0631\u0651\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1 \u0631\u064e\u0628\u0651\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0641\u064e\u0637\u064e\u0631\u064e\u0647\u064f\u0646\u0651\u064e \u0648\u064e\u0623\u064e\u0646\u064e\u0627\u06e0 \u0639\u064e\u0644\u064e\u0649\u0670 \u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0634\u0651\u064e\u0640\u0670\u0647\u0650\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0628\u064e\u0644", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1", "word_translation": "your Lord", "word_transliteration": "rabbukum"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f", "word_translation": "(is the) Lord", "word_transliteration": "rabbu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0641\u064e\u0637\u064e\u0631\u064e\u0647\u064f\u0646\u0651\u064e", "word_translation": "created them", "word_transliteration": "fatarahunna"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "and I am", "word_transliteration": "wa-ana"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645", "word_translation": "that", "word_transliteration": "dhalikum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u0640\u0670\u0647\u0650\u062f\u0650\u064a\u0646\u064e", "word_translation": "the witnesses", "word_transliteration": "al-shahidina"}]}