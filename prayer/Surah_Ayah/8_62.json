{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646 \u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0646 \u064a\u064e\u062e\u06e1\u062f\u064e\u0639\u064f\u0648\u0643\u064e \u0641\u064e\u0625\u0650\u0646\u0651\u064e \u062d\u064e\u0633\u06e1\u0628\u064e\u0643\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06da \u0647\u064f\u0648\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653 \u0623\u064e\u064a\u0651\u064e\u062f\u064e\u0643\u064e \u0628\u0650\u0646\u064e\u0635\u06e1\u0631\u0650\u0647\u0650\u06e6 \u0648\u064e\u0628\u0650\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "But if", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0653\u0627\u0652", "word_translation": "they intend", "word_transliteration": "yuridu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "to", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u062f\u064e\u0639\u064f\u0648\u0643\u064e", "word_translation": "deceive you", "word_transliteration": "yakhda'uka"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "then indeed", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u062d\u064e\u0633\u06e1\u0628\u064e\u0643\u064e", "word_translation": "is sufficient for you", "word_transliteration": "hasbaka"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06da", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u0651\u064e\u062f\u064e\u0643\u064e", "word_translation": "supported you", "word_transliteration": "ayyadaka"}, {"word_arabic_uthmani": "\u0628\u0650\u0646\u064e\u0635\u06e1\u0631\u0650\u0647\u0650\u06e6", "word_translation": "with His help", "word_transliteration": "binasrihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u0650\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "and with the believers", "word_transliteration": "wabil-mu'minina"}]}