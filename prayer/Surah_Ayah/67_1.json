{"ayahs": {"ayah_uthmani": "\u062a\u064e\u0628\u064e\u0670\u0631\u064e\u0643\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0628\u0650\u064a\u064e\u062f\u0650\u0647\u0650 \u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u064f \u0648\u064e\u0647\u064f\u0648\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0642\u064e\u062f\u0650\u064a\u0631\u064c"}, "words": [{"word_arabic_uthmani": "\u062a\u064e\u0628\u064e\u0670\u0631\u064e\u0643\u064e", "word_translation": "Blessed is", "word_transliteration": "tabaraka"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "He", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0628\u0650\u064a\u064e\u062f\u0650\u0647\u0650", "word_translation": "in Whose Hand", "word_transliteration": "biyadihi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "(is) the Dominion", "word_transliteration": "al-mul'ku"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "(is) over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0650\u064a\u0631\u064c", "word_translation": "All-Powerful", "word_transliteration": "qadirun"}]}