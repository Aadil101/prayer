{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u0650 \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6\u0653 \u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e\u06d6 \u0641\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0648\u064e\u0644\u0650\u064a\u0651\u064f \u0648\u064e\u0647\u064f\u0648\u064e \u064a\u064f\u062d\u06e1\u064a\u0650 \u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e\u0649\u0670 \u0648\u064e\u0647\u064f\u0648\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0642\u064e\u062f\u0650\u064a\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u0650", "word_translation": "Or", "word_transliteration": "ami"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652", "word_translation": "have they taken", "word_transliteration": "ittakhadhu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "besides Him", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6\u0653", "word_translation": "besides Him", "word_transliteration": "dunihi"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e\u06d6", "word_translation": "protectors", "word_transliteration": "awliyaa"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "But Allah", "word_transliteration": "fal-lahu"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0648\u064e\u0644\u0650\u064a\u0651\u064f", "word_translation": "(is) the Protector", "word_transliteration": "al-waliyu"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He",