{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1 \u062a\u064e\u0631\u064e \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0623\u064f\u0648\u062a\u064f\u0648\u0627\u0652 \u0646\u064e\u0635\u0650\u064a\u0628\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u0628\u0650\u0671\u0644\u06e1\u062c\u0650\u0628\u06e1\u062a\u0650 \u0648\u064e\u0671\u0644\u0637\u0651\u064e\u0640\u0670\u063a\u064f\u0648\u062a\u0650 \u0648\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650 \u0623\u064e\u0647\u06e1\u062f\u064e\u0649\u0670 \u0645\u0650\u0646\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0633\u064e\u0628\u0650\u064a\u0644\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Do not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e", "word_translation": "you see", "word_transliteration": "tara"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "[towards]", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u062a\u064f\u0648\u0627\u0652", "word_translation": "were given", "word_transliteration": "utu"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u0650\u064a\u0628\u0657\u0627", "word_translation": "a portion", "word_transliteration": "nasiban"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "the Book", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "They believe", "word_transliteration": "yu'minuna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062c\u0650\u0628\u06e1\u062a\u0650", "word_translation": "in the superstition", "word_transliteration": "bil-jib'ti"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0637\u0651\u064e\u0640\u0670\u063a\u064f\u0648\u062a\u0650", "word_translation": "and the false deities", "word_transliteration": "wal-taghuti"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "and they say", "word_transliteration": "wayaquluna"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "for those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieve[d]", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0647\u064e\u0640\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650", "word_translation": "These", "word_transliteration": "haulai"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u062f\u064e\u0649\u0670", "word_translation": "(are) better guided", "word_transliteration": "ahda"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "than", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe[d]", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u064b\u0627", "word_translation": "(as to the) way", "word_transliteration": "sabilan"}]}