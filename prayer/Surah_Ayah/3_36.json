{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u0648\u064e\u0636\u064e\u0639\u064e\u062a\u06e1\u0647\u064e\u0627 \u0642\u064e\u0627\u0644\u064e\u062a\u06e1 \u0631\u064e\u0628\u0651\u0650 \u0625\u0650\u0646\u0651\u0650\u064a \u0648\u064e\u0636\u064e\u0639\u06e1\u062a\u064f\u0647\u064e\u0627\u0653 \u0623\u064f\u0646\u062b\u064e\u0649\u0670 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0628\u0650\u0645\u064e\u0627 \u0648\u064e\u0636\u064e\u0639\u064e\u062a\u06e1 \u0648\u064e\u0644\u064e\u064a\u06e1\u0633\u064e \u0671\u0644\u0630\u0651\u064e\u0643\u064e\u0631\u064f \u0643\u064e\u0671\u0644\u06e1\u0623\u064f\u0646\u062b\u064e\u0649\u0670\u06d6 \u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a \u0633\u064e\u0645\u0651\u064e\u064a\u06e1\u062a\u064f\u0647\u064e\u0627 \u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064e \u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a\u0653 \u0623\u064f\u0639\u0650\u064a\u0630\u064f\u0647\u064e\u0627 \u0628\u0650\u0643\u064e \u0648\u064e\u0630\u064f\u0631\u0651\u0650\u064a\u0651\u064e\u062a\u064e\u0647\u064e\u0627 \u0645\u0650\u0646\u064e \u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u0650 \u0671\u0644\u0631\u0651\u064e\u062c\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "Then when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u0648\u064e\u0636\u064e\u0639\u064e\u062a\u06e1\u0647\u064e\u0627", "word_translation": "she delivered her", "word_transliteration": "wada'atha"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e\u062a\u06e1", "word_translation": "she said", "word_transliteration": "qalat"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "My Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0648\u064e\u0636\u064e\u0639\u06e1\u062a\u064f\u0647\u064e\u0627\u0653", "word_translation": "[I] (have) delivered [her]", "word_transliteration": "wada'tuha"}, {"word_arabic_uthmani": "\u0623\u064f\u0646\u062b\u064e\u0649\u0670", "word_translation": "a female", "word_transliteration": "untha"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "knows better", "word_transliteration": "a'lamu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "[of] what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0648\u064e\u0636\u064e\u0639\u064e\u062a\u06e1", "word_translation": "she delivered", "word_transliteration": "wada'at"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "and is not", "word_transliteration": "walaysa"}, {"word_arabic_uthmani": "\u0671\u0644\u0630\u0651\u064e\u0643\u064e\u0631\u064f", "word_translation": "the male", "word_transliteration": "al-dhakaru"}, {"word_arabic_uthmani": "\u0643\u064e\u0671\u0644\u06e1\u0623\u064f\u0646\u062b\u064e\u0649\u0670\u06d6", "word_translation": "like the female", "word_transliteration": "kal-untha"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "And that I", "word_transliteration": "wa-inni"}, {"word_arabic_uthmani": "\u0633\u064e\u0645\u0651\u064e\u064a\u06e1\u062a\u064f\u0647\u064e\u0627", "word_translation": "[I] (have) named her", "word_transliteration": "sammaytuha"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064e", "word_translation": "Maryam", "word_transliteration": "maryama"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "and that I", "word_transliteration": "wa-inni"}, {"word_arabic_uthmani": "\u0623\u064f\u0639\u0650\u064a\u0630\u064f\u0647\u064e\u0627", "word_translation": "[I] seek refuge for her", "word_transliteration": "u'idhuha"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064e", "word_translation": "in You", "word_transliteration": "bika"}, {"word_arabic_uthmani": "\u0648\u064e\u0630\u064f\u0631\u0651\u0650\u064a\u0651\u064e\u062a\u064e\u0647\u064e\u0627", "word_translation": "and her offspring", "word_transliteration": "wadhurriyyataha"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u0650", "word_translation": "the Shaitaan", "word_transliteration": "al-shaytani"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062c\u0650\u064a\u0645\u0650", "word_translation": "the rejected", "word_transliteration": "al-rajimi"}]}