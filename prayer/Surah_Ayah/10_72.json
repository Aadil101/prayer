{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0646 \u062a\u064e\u0648\u064e\u0644\u0651\u064e\u064a\u06e1\u062a\u064f\u0645\u06e1 \u0641\u064e\u0645\u064e\u0627 \u0633\u064e\u0623\u064e\u0644\u06e1\u062a\u064f\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u06e1 \u0623\u064e\u062c\u06e1\u0631\u064d\u06d6 \u0625\u0650\u0646\u06e1 \u0623\u064e\u062c\u06e1\u0631\u0650\u064a\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6 \u0648\u064e\u0623\u064f\u0645\u0650\u0631\u06e1\u062a\u064f \u0623\u064e\u0646\u06e1 \u0623\u064e\u0643\u064f\u0648\u0646\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0633\u06e1\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646", "word_translation": "But if", "word_transliteration": "fa-in"}, {"word_arabic_uthmani": "\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u064a\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "you turn away", "word_transliteration": "tawallaytum"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627", "word_translation": "then not", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u0633\u064e\u0623\u064e\u0644\u06e1\u062a\u064f\u0643\u064f\u0645", "word_translation": "I have asked you", "word_transliteration": "sa-altukum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0631\u064d\u06d6", "word_translation": "reward", "word_transliteration": "ajrin"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u06e1", "word_translation": "Not", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0631\u0650\u064a\u064e", "word_translation": "(is) my reward", "word_transliteration": "ajriya"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0645\u0650\u0631\u06e1\u062a\u064f", "word_translation": "and I have been commanded", "word_transliteration": "wa-umir'tu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u064f\u0648\u0646\u064e", "word_translation": "I be", "word_transliteration": "akuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0633\u06e1\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the Muslims", "word_transliteration": "al-mus'limina"}]}