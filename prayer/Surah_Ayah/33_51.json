_transliteration": "fi"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0643\u064f\u0645\u06e1\u06da", "word_translation": "your hearts", "word_transliteration": "qulubikum"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "And Allah is", "word_transliteration": "wakana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah is", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u064b\u0627", "word_translation": "All-Knower", "word_transliteration": "aliman"}, {"word_arabic_uthmani": "\u062d\u064e\u0644\u0650\u064a\u0645\u0657\u0627", "word_translation": "Most Forbearing", "word_transliteration": "haliman"}]}