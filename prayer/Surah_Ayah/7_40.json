{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652 \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627 \u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652 \u0639\u064e\u0646\u06e1\u0647\u064e\u0627 \u0644\u064e\u0627 \u062a\u064f\u0641\u064e\u062a\u0651\u064e\u062d\u064f \u0644\u064e\u0647\u064f\u0645\u06e1 \u0623\u064e\u0628\u06e1\u0648\u064e\u0670\u0628\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0646\u064e \u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e \u062d\u064e\u062a\u0651\u064e\u0649\u0670 \u064a\u064e\u0644\u0650\u062c\u064e \u0671\u0644\u06e1\u062c\u064e\u0645\u064e\u0644\u064f \u0641\u0650\u064a \u0633\u064e\u0645\u0651\u0650 \u0671\u0644\u06e1\u062e\u0650\u064a\u064e\u0627\u0637\u0650\u06da \u0648\u064e\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0646\u064e\u062c\u06e1\u0632\u0650\u064a \u0671\u0644\u06e1\u0645\u064f\u062c\u06e1\u0631\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "denied", "word_transliteration": "kadhabu"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627", "word_translation": "Our Verses", "word_transliteration": "biayatina"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "and (were) arrogant", "word_transliteration": "wa-is'takbaru"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064e\u0627", "word_translation": "towards them", "word_transliteration": "anha"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(will) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064f\u0641\u064e\u062a\u0651\u064e\u062d\u064f", "word_translation": "be opened", "word_transliteration": "tufattahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0648\u064e\u0670\u0628\u064f", "word_translation": "(the) doors", "word_transliteration": "abwabu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650", "word_translation": "(of) the heaven", "word_transliteration": "al-samai"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0646\u064e", "word_translation": "they will enter", "word_transliteration": "yadkhuluna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e", "word_translation": "Paradise", "word_transliteration": "al-janata"}, {"word_arabic_uthmani": "\u062d\u064e\u062a\u0651\u064e\u0649\u0670", "word_translation": "until", "word_transliteration": "hatta"}, {"word_arabic_uthmani": "\u064a\u064e\u0644\u0650\u062c\u064e", "word_translation": "passes", "word_transliteration": "yalija"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0645\u064e\u0644\u064f", "word_translation": "the camel", "word_transliteration": "al-jamalu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "through", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0633\u064e\u0645\u0651\u0650", "word_translation": "(the) eye", "word_