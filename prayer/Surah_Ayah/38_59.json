{"ayahs": {"ayah_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627 \u0641\u064e\u0648\u06e1\u062c\u065e \u0645\u0651\u064f\u0642\u06e1\u062a\u064e\u062d\u0650\u0645\u065e \u0645\u0651\u064e\u0639\u064e\u0643\u064f\u0645\u06e1 \u0644\u064e\u0627 \u0645\u064e\u0631\u06e1\u062d\u064e\u0628\u064e\u06e2\u0627 \u0628\u0650\u0647\u0650\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0635\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "This", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u06e1\u062c\u065e", "word_translation": "(is) a company", "word_transliteration": "fawjun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0642\u06e1\u062a\u064e\u062d\u0650\u0645\u065e", "word_translation": "bursting in", "word_transliteration": "muq'tahimun"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0639\u064e\u0643\u064f\u0645\u06e1", "word_translation": "with you", "word_transliteration": "ma'akum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "No", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u06e1\u062d\u064e\u0628\u064e\u06e2\u0627", "word_translation": "welcome", "word_transliteration": "marhaban"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u0645\u06e1\u06da", "word_translation": "for them", "word_transliteration": "bihim"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Indeed they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0635\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "(will) burn", "word_transliteration": "salu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650", "word_translation": "(in) the Fire", "word_transliteration": "al-nari"}]}