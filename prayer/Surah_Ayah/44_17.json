{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0641\u064e\u062a\u064e\u0646\u0651\u064e\u0627 \u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1 \u0642\u064e\u0648\u06e1\u0645\u064e \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0648\u064e\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1 \u0631\u064e\u0633\u064f\u0648\u0644\u065e \u0643\u064e\u0631\u0650\u064a\u0645\u064c"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And certainly", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0646\u0651\u064e\u0627", "word_translation": "We tried", "word_transliteration": "fatanna"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "before them", "word_transliteration": "qablahum"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(the) people", "word_transliteration": "qawma"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "(of) Firaun", "word_transliteration": "fir'awna"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and came to them", "word_transliteration": "wajaahum"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u065e", "word_translation": "a Messenger", "word_transliteration": "rasulun"}, {"word_arabic_uthmani": "\u0643\u064e\u0631\u0650\u064a\u0645\u064c", "word_translation": "noble", "word_transliteration": "karimun"}]}