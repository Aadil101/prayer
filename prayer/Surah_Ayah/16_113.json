{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1 \u0631\u064e\u0633\u064f\u0648\u0644\u065e \u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0641\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0647\u064f \u0641\u064e\u0623\u064e\u062e\u064e\u0630\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u064f \u0648\u064e\u0647\u064f\u0645\u06e1 \u0638\u064e\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And certainly", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u06e1", "word_translation": "came to them", "word_transliteration": "jaahum"}, {"word_arabic_uthmani": "\u0631\u064e\u0633\u064f\u0648\u0644\u065e", "word_translation": "a Messenger", "word_transliteration": "rasulun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "from among them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0647\u064f", "word_translation": "but they denied him", "word_transliteration": "fakadhabuhu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u062e\u064e\u0630\u064e\u0647\u064f\u0645\u064f", "word_translation": "so seized them", "word_transliteration": "fa-akhadhahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u064f", "word_translation": "the punishment", "word_transliteration": "al-'adhabu"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0638\u064e\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e", "word_translation": "(were) wrongdoers", "word_transliteration": "zalimuna"}]}