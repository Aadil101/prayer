{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0627 \u064a\u064e\u0630\u064f\u0648\u0642\u064f\u0648\u0646\u064e \u0641\u0650\u064a\u0647\u064e\u0627 \u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e\u0629\u064e \u0671\u0644\u06e1\u0623\u064f\u0648\u0644\u064e\u0649\u0670\u06d6 \u0648\u064e\u0648\u064e\u0642\u064e\u0649\u0670\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064e \u0671\u0644\u06e1\u062c\u064e\u062d\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0630\u064f\u0648\u0642\u064f\u0648\u0646\u064e", "word_translation": "they will taste", "word_transliteration": "yadhuquna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e", "word_translation": "the death", "word_transliteration": "al-mawta"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e\u0629\u064e", "word_translation": "the death", "word_transliteration": "al-mawtata"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064f\u0648\u0644\u064e\u0649\u0670\u06d6", "word_translation": "the first", "word_transliteration": "al-ula"}, {"word_arabic_uthmani": "\u0648\u064e\u0648\u064e\u0642\u064e\u0649\u0670\u0647\u064f\u0645\u06e1", "word_translation": "And He will protect them", "word_transliteration": "wawaqahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064e", "word_translation": "(from the) punishment", "word_transliteration": "adhaba"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u062d\u0650\u064a\u0645\u0650", "word_translation": "(of) the Hellfire", "word_transliteration": "al-jahimi"}]}