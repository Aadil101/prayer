{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1 \u0642\u064f\u0644\u06e1\u062a\u064f\u0645\u06e1 \u064a\u064e\u0670\u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0644\u064e\u0646 \u0646\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u064e \u0644\u064e\u0643\u064e \u062d\u064e\u062a\u0651\u064e\u0649\u0670 \u0646\u064e\u0631\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u062c\u064e\u0647\u06e1\u0631\u064e\u0629\u0657 \u0641\u064e\u0623\u064e\u062e\u064e\u0630\u064e\u062a\u06e1\u0643\u064f\u0645\u064f \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0639\u0650\u0642\u064e\u0629\u064f \u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u0646\u0638\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1", "word_translation": "And when", "word_transliteration": "wa-idh"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "you said", "word_transliteration": "qul'tum"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "O Musa", "word_transliteration": "yamusa"}, {"word_arabic_uthmani": "\u0644\u064e\u0646", "word_translation": "Never", "word_transliteration": "lan"}, {"word_arabic_uthmani": "\u0646\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u064e", "word_translation": "(will) we believe", "word_transliteration": "nu'mina"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e", "word_translation": "in you", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u062d\u064e\u062a\u0651\u064e\u0649\u0670", "word_translation": "until", "word_transliteration": "hatta"}, {"word_arabic_uthmani": "\u0646\u064e\u0631\u064e\u0649", "word_translation": "we see", "word_transliteration": "nara"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u062c\u064e\u0647\u06e1\u0631\u064e\u0629\u0657", "word_translation": "manifestly", "word_transliteration": "jahratan"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u062e\u064e\u0630\u064e\u062a\u06e1\u0643\u064f\u0645\u064f", "word_translation": "So seized you", "word_transliteration": "fa-akhadhatkumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0639\u0650\u0642\u064e\u0629\u064f", "word_translation": "the thunderbolt", "word_transliteration": "al-sa'iqatu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "while you", "word_transliteration": "wa-antum"}, {"word_arabic_uthmani": "\u062a\u064e\u0646\u0638\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "(were) looking", "word_transliteration": "tanzuruna"}]}