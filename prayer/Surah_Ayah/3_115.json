{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u06e1 \u062e\u064e\u064a\u06e1\u0631\u0656 \u0641\u064e\u0644\u064e\u0646 \u064a\u064f\u0643\u06e1\u0641\u064e\u0631\u064f\u0648\u0647\u064f\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u0650\u064a\u0645\u064f\u06e2 \u0628\u0650\u0671\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "they do", "word_transliteration": "yaf 'alu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u0656", "word_translation": "a good", "word_transliteration": "khayrin"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0646", "word_translation": "then never", "word_transliteration": "falan"}, {"word_arabic_uthmani": "\u064a\u064f\u0643\u06e1\u0641\u064e\u0631\u064f\u0648\u0647\u064f\u06d7", "word_translation": "will they be denied it", "word_transliteration": "yuk'faruhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u064f\u06e2", "word_translation": "(is) All-Knowing", "word_transliteration": "alimun"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e", "word_translation": "of the God-fearing", "word_transliteration": "bil-mutaqina"}]}