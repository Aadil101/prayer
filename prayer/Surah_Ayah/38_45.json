{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0630\u06e1\u0643\u064f\u0631\u06e1 \u0639\u0650\u0628\u064e\u0670\u062f\u064e\u0646\u064e\u0627\u0653 \u0625\u0650\u0628\u06e1\u0631\u064e\u0670\u0647\u0650\u064a\u0645\u064e \u0648\u064e\u0625\u0650\u0633\u06e1\u062d\u064e\u0670\u0642\u064e \u0648\u064e\u064a\u064e\u0639\u06e1\u0642\u064f\u0648\u0628\u064e \u0623\u064f\u0648\u0652\u0644\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u064a\u06e1\u062f\u0650\u064a \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0630\u06e1\u0643\u064f\u0631\u06e1", "word_translation": "And remember", "word_transliteration": "wa-udh'kur"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0670\u062f\u064e\u0646\u064e\u0627\u0653", "word_translation": "Our slaves", "word_transliteration": "ibadana"}, {"word_arabic_uthmani": "\u0625\u0650\u0628\u06e1\u0631\u064e\u0670\u0647\u0650\u064a\u0645\u064e", "word_translation": "Ibrahim", "word_transliteration": "ib'rahima"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0633\u06e1\u062d\u064e\u0670\u0642\u064e", "word_translation": "and Ishaq", "word_transliteration": "wa-is'haqa"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0639\u06e1\u0642\u064f\u0648\u0628\u064e", "word_translation": "and Ayyub,", "word_transliteration": "waya'quba"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u0650\u064a", "word_translation": "possessors", "word_transliteration": "uli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u064a\u06e1\u062f\u0650\u064a", "word_translation": "(of) strength", "word_transliteration": "al-aydi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u0650", "word_translation": "and vision", "word_transliteration": "wal-absari"}]}