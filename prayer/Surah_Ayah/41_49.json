{"ayahs": {"ayah_uthmani": "\u0644\u0651\u064e\u0627 \u064a\u064e\u0633\u06e1\u0640\u0654\u064e\u0645\u064f \u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e\u0670\u0646\u064f \u0645\u0650\u0646 \u062f\u064f\u0639\u064e\u0627\u0653\u0621\u0650 \u0671\u0644\u06e1\u062e\u064e\u064a\u06e1\u0631\u0650 \u0648\u064e\u0625\u0650\u0646 \u0645\u0651\u064e\u0633\u0651\u064e\u0647\u064f \u0671\u0644\u0634\u0651\u064e\u0631\u0651\u064f \u0641\u064e\u064a\u064e\u0640\u0654\u064f\u0648\u0633\u065e \u0642\u064e\u0646\u064f\u0648\u0637\u065e"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(Does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u0640\u0654\u064e\u0645\u064f", "word_translation": "get tired", "word_transliteration": "yasamu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e\u0670\u0646\u064f", "word_translation": "man", "word_transliteration": "al-insanu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0639\u064e\u0627\u0653\u0621\u0650", "word_translation": "praying", "word_transliteration": "du'ai"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u064a\u06e1\u0631\u0650", "word_translation": "(for) the good", "word_transliteration": "al-khayri"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "but if", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0633\u0651\u064e\u0647\u064f", "word_translation": "touches him", "word_transliteration": "massahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u0631\u0651\u064f", "word_translation": "the evil", "word_transliteration": "al-sharu"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064e\u0640\u0654\u064f\u0648\u0633\u065e", "word_translation": "then he gives up hope", "word_transliteration": "fayausun"}, {"word_arabic_uthmani": "\u0642\u064e\u0646\u064f\u0648\u0637\u065e", "word_translation": "(and) despairs", "word_transliteration": "qanutun"}]}