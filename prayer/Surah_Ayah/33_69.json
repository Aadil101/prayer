{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0644\u064e\u0627 \u062a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652 \u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0630\u064e\u0648\u06e1\u0627\u0652 \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0641\u064e\u0628\u064e\u0631\u0651\u064e\u0623\u064e\u0647\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u0650\u0645\u0651\u064e\u0627 \u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652\u06da \u0648\u064e\u0643\u064e\u0627\u0646\u064e \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u062c\u0650\u064a\u0647\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O you who believe", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "O you who believe", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "O you who believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652", "word_translation": "be", "word_transliteration": "takunu"}, {"word_arabic_uthmani": "\u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "like those who", "word_transliteration": "ka-alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0630\u064e\u0648\u06e1\u0627\u0652", "word_translation": "annoyed", "word_transliteration": "adhaw"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0641\u064e\u0628\u064e\u0631\u0651\u064e\u0623\u064e\u0647\u064f", "word_translation": "then Allah cleared him", "word_transliteration": "fabarra-ahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "then Allah cleared him", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652\u06da", "word_translation": "they said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "And he was", "word_transliteration": "wakana"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "near", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u0650\u064a\u0647\u0657\u0627", "word_translation": "honorable", "word_transliteration": "wajihan"}]}