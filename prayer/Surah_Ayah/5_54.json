 {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u064f", "word_translation": "(is the) Grace", "word_transliteration": "fadlu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u062a\u0650\u064a\u0647\u0650", "word_translation": "He grants", "word_transliteration": "yu'tihi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0670\u0633\u0650\u0639\u064c", "word_translation": "(is) All-Encompassing", "word_transliteration": "wasi'un"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u064c", "word_translation": "All-Knowing", "word_transliteration": "alimun"}]}