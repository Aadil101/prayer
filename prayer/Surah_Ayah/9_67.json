{"ayahs": {"ayah_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u064f\u0648\u0646\u064e \u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u064e\u0670\u062a\u064f \u0628\u064e\u0639\u06e1\u0636\u064f\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u06e2 \u0628\u064e\u0639\u06e1\u0636\u0656\u06da \u064a\u064e\u0623\u06e1\u0645\u064f\u0631\u064f\u0648\u0646\u064e \u0628\u0650\u0671\u0644\u06e1\u0645\u064f\u0646\u0643\u064e\u0631\u0650 \u0648\u064e\u064a\u064e\u0646\u06e1\u0647\u064e\u0648\u06e1\u0646\u064e \u0639\u064e\u0646\u0650 \u0671\u0644\u06e1\u0645\u064e\u0639\u06e1\u0631\u064f\u0648\u0641\u0650 \u0648\u064e\u064a\u064e\u0642\u06e1\u0628\u0650\u0636\u064f\u0648\u0646\u064e \u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u064e\u0647\u064f\u0645\u06e1\u06da \u0646\u064e\u0633\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0641\u064e\u0646\u064e\u0633\u0650\u064a\u064e\u0647\u064f\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "The hypocrite men", "word_transliteration": "al-munafiquna"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u064e\u0670\u062a\u064f", "word_translation": "and the hypocrite women", "word_transliteration": "wal-munafiqatu"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064f\u0647\u064f\u0645", "word_translation": "some of them", "word_transliteration": "ba'duhum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e2", "word_translation": "(are) of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u0656\u06da", "word_translation": "others", "word_transliteration": "ba'din"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u0645\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "They enjoin", "word_transliteration": "yamuruna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0645\u064f\u0646\u0643\u064e\u0631\u0650", "word_translation": "the wrong", "word_transliteration": "bil-munkari"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0646\u06e1\u0647\u064e\u0648\u06e1\u0646\u064e", "word_translation": "and forbid", "word_transliteration": "wayanhawna"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0650", "word_translation": "what", "word_transliteration": "ani"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0639\u06e1\u0631\u064f\u0648\u0641\u0650", "word_translation": "(is) the right", "word_transliteration": "al-ma'rufi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0642\u06e1\u0628\u0650\u0636\u064f\u0648\u0646\u064e", "word_translation": "and they close", "word_transliteration": "wayaqbiduna"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "their hands", "word_transliteration": "aydiyahum"}, {"word_arabic_uthmani": "\u0646\u064e\u0633\u064f\u0648\u0627\u0652", "word_translation": "They forget", "word_transliteration": "nasu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0641\u064e\u0646\u064e\u0633\u0650\u064a\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "so He has forgotten them", "word_transliteration": "fanasiyahum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "the hypocrites", "word_transliteration": "al-munafiqina"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "they (are)", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "the defiantly disobedient", "word_transliteration": "al-fasiquna"}]}