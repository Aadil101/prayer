{"ayahs": {"ayah_uthmani": "\u0645\u0651\u064e\u062b\u064e\u0644\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1\u06d6 \u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064f\u0647\u064f\u0645\u06e1 \u0643\u064e\u0631\u064e\u0645\u064e\u0627\u062f\u064d \u0671\u0634\u06e1\u062a\u064e\u062f\u0651\u064e\u062a\u06e1 \u0628\u0650\u0647\u0650 \u0671\u0644\u0631\u0651\u0650\u064a\u062d\u064f \u0641\u0650\u064a \u064a\u064e\u0648\u06e1\u0645\u064d \u0639\u064e\u0627\u0635\u0650\u0641\u0656\u06d6 \u0644\u0651\u064e\u0627 \u064a\u064e\u0642\u06e1\u062f\u0650\u0631\u064f\u0648\u0646\u064e \u0645\u0650\u0645\u0651\u064e\u0627 \u0643\u064e\u0633\u064e\u0628\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u0649\u0670 \u0634\u064e\u064a\u06e1\u0621\u0656\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0647\u064f\u0648\u064e \u0671\u0644\u0636\u0651\u064e\u0644\u064e\u0670\u0644\u064f \u0671\u0644\u06e1\u0628\u064e\u0639\u0650\u064a\u062f\u064f"}, "words": [{"word_arabic_uthmani": "\u0645\u0651\u064e\u062b\u064e\u0644\u064f", "word_translation": "(The) example", "word_transliteration": "mathalu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1\u06d6", "word_translation": "in their Lord", "word_transliteration": "birabbihim"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their deeds", "word_transliteration": "a'maluhum"}, {"word_arabic_uthmani": "\u0643\u064e\u0631\u064e\u0645\u064e\u0627\u062f\u064d", "word_translation": "(are) like ashes", "word_transliteration": "karamadin"}, {"word_arabic_uthmani": "\u0671\u0634\u06e1\u062a\u064e\u062f\u0651\u064e\u062a\u06e1", "word_translation": "blows furiously", "word_transliteration": "ish'taddat"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650", "word_translation": "on it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u0650\u064a\u062d\u064f", "word_translation": "the wind", "word_transliteration": "al-rihu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064d", "word_translation": "a day", "word_transliteration": "yawmin"}, {"word_arabic_uthmani": "\u0639\u064e\u0627\u0635\u0650\u0641\u0656\u06d6", "word_translation": "stormy", "word_transliteration": "asifin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "No", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u06e1\u062f\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "control (they have)", "word_transliteration": "yaqdiruna"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0643\u064e\u0633\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "they have earned", "word_transliteration": "kasabu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656\u06da", "word_translation": "anything", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "[it]", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0636\u0651\u064e\u0644\u064