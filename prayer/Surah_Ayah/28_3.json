{"ayahs": {"ayah_uthmani": "\u0646\u064e\u062a\u06e1\u0644\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e \u0645\u0650\u0646 \u0646\u0651\u064e\u0628\u064e\u0625\u0650 \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0648\u064e\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0644\u0650\u0642\u064e\u0648\u06e1\u0645\u0656 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0646\u064e\u062a\u06e1\u0644\u064f\u0648\u0627\u0652", "word_translation": "We recite", "word_transliteration": "natlu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e", "word_translation": "to you", "word_transliteration": "alayka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0628\u064e\u0625\u0650", "word_translation": "(the) news", "word_transliteration": "naba-i"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "(of) Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "and Firaun", "word_transliteration": "wafir'awna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "in truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u0644\u0650\u0642\u064e\u0648\u06e1\u0645\u0656", "word_translation": "for a people", "word_transliteration": "liqawmin"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "who believe", "word_transliteration": "yu'minuna"}]}