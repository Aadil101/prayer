{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0627\u0653 \u0623\u064e\u0634\u06e1\u064a\u064e\u0627\u0639\u064e\u0643\u064f\u0645\u06e1 \u0641\u064e\u0647\u064e\u0644\u06e1 \u0645\u0650\u0646 \u0645\u0651\u064f\u062f\u0651\u064e\u0643\u0650\u0631\u0656"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And certainly", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0627\u0653", "word_translation": "We destroyed", "word_transliteration": "ahlakna"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u06e1\u064a\u064e\u0627\u0639\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your kinds", "word_transliteration": "ashya'akum"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064e\u0644\u06e1", "word_translation": "so is (there)", "word_transliteration": "fahal"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u062f\u0651\u064e\u0643\u0650\u0631\u0656", "word_translation": "who will receive admonition", "word_transliteration": "muddakirin"}]}