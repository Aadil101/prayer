{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1 \u062a\u064e\u0631\u064e \u0643\u064e\u064a\u06e1\u0641\u064e \u0641\u064e\u0639\u064e\u0644\u064e \u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0628\u0650\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u0650 \u0671\u0644\u06e1\u0641\u0650\u064a\u0644\u0650"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Have not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e", "word_translation": "you seen", "word_transliteration": "tara"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "how", "word_transliteration": "kayfa"}, {"word_arabic_uthmani": "\u0641\u064e\u0639\u064e\u0644\u064e", "word_translation": "dealt", "word_transliteration": "fa'ala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbuka"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u0650", "word_translation": "with (the) Companions", "word_transliteration": "bi-ashabi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u0650\u064a\u0644\u0650", "word_translation": "(of the) Elephant", "word_transliteration": "al-fili"}]}