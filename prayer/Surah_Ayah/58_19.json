{"ayahs": {"ayah_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u062d\u06e1\u0648\u064e\u0630\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f \u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064f \u0641\u064e\u0623\u064e\u0646\u0633\u064e\u0649\u0670\u0647\u064f\u0645\u06e1 \u0630\u0650\u0643\u06e1\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u062d\u0650\u0632\u06e1\u0628\u064f \u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u0650\u06da \u0623\u064e\u0644\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e \u062d\u0650\u0632\u06e1\u0628\u064e \u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u0650 \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u062e\u064e\u0670\u0633\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u062d\u06e1\u0648\u064e\u0630\u064e", "word_translation": "Has overcome", "word_transliteration": "is'tahwadha"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f", "word_translation": "them", "word_transliteration": "alayhimu"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064f", "word_translation": "the Shaitaan", "word_transliteration": "al-shaytanu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0646\u0633\u064e\u0649\u0670\u0647\u064f\u0645\u06e1", "word_translation": "so he made them forget", "word_transliteration": "fa-ansahum"}, {"word_arabic_uthmani": "\u0630\u0650\u0643\u06e1\u0631\u064e", "word_translation": "(the) remembrance", "word_transliteration": "dhik'ra"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u062d\u0650\u0632\u06e1\u0628\u064f", "word_translation": "(are the) party", "word_transliteration": "hiz'bu"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u0650\u06da", "word_translation": "(of) the Shaitaan", "word_transliteration": "al-shaytani"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627\u0653", "word_translation": "No doubt!", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u062d\u0650\u0632\u06e1\u0628\u064e", "word_translation": "(the) party", "word_transliteration": "hiz'ba"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u0650", "word_translation": "(of) the Shaitaan", "word_transliteration": "al-shaytani"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "they", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0670\u0633\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(will be) the losers", "word_transliteration": "al-khasiruna"}]}