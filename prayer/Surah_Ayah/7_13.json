{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0641\u064e\u0671\u0647\u06e1\u0628\u0650\u0637\u06e1 \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u0641\u064e\u0645\u064e\u0627 \u064a\u064e\u0643\u064f\u0648\u0646\u064f \u0644\u064e\u0643\u064e \u0623\u064e\u0646 \u062a\u064e\u062a\u064e\u0643\u064e\u0628\u0651\u064e\u0631\u064e \u0641\u0650\u064a\u0647\u064e\u0627 \u0641\u064e\u0671\u062e\u06e1\u0631\u064f\u062c\u06e1 \u0625\u0650\u0646\u0651\u064e\u0643\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u063a\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "(Allah) said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0647\u06e1\u0628\u0650\u0637\u06e1", "word_translation": "Then go down", "word_transliteration": "fah'bit"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "from it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627", "word_translation": "for not", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0648\u0646\u064f", "word_translation": "it is", "word_transliteration": "yakunu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e", "word_translation": "for you", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u062a\u064e\u0643\u064e\u0628\u0651\u064e\u0631\u064e", "word_translation": "you be arrogant", "word_transliteration": "tatakabbara"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "in it", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u062e\u06e1\u0631\u064f\u062c\u06e1", "word_translation": "So get out", "word_transliteration": "fa-ukh'ruj"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "indeed you", "word_transliteration": "innaka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "(are) of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u063a\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the disgraced ones", "word_transliteration": "al-saghirina"}]}