{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0643\u064e \u0628\u064e\u0670\u062e\u0650\u0639\u065e \u0646\u0651\u064e\u0641\u06e1\u0633\u064e\u0643\u064e \u0623\u064e\u0644\u0651\u064e\u0627 \u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652 \u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0643\u064e", "word_translation": "Perhaps you", "word_transliteration": "la'allaka"}, {"word_arabic_uthmani": "\u0628\u064e\u0670\u062e\u0650\u0639\u065e", "word_translation": "(would) kill", "word_transliteration": "bakhi'un"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0641\u06e1\u0633\u064e\u0643\u064e", "word_translation": "yourself", "word_transliteration": "nafsaka"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0651\u064e\u0627", "word_translation": "that not", "word_transliteration": "alla"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652", "word_translation": "they become", "word_transliteration": "yakunu"}, {"word_arabic_uthmani": "\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "believers", "word_transliteration": "mu'minina"}]}