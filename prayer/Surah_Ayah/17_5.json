{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0630\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e \u0648\u064e\u0639\u06e1\u062f\u064f \u0623\u064f\u0648\u0644\u064e\u0649\u0670\u0647\u064f\u0645\u064e\u0627 \u0628\u064e\u0639\u064e\u062b\u06e1\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u0639\u0650\u0628\u064e\u0627\u062f\u0657\u0627 \u0644\u0651\u064e\u0646\u064e\u0627\u0653 \u0623\u064f\u0648\u0652\u0644\u0650\u064a \u0628\u064e\u0623\u06e1\u0633\u0656 \u0634\u064e\u062f\u0650\u064a\u062f\u0656 \u0641\u064e\u062c\u064e\u0627\u0633\u064f\u0648\u0627\u0652 \u062e\u0650\u0644\u064e\u0670\u0644\u064e \u0671\u0644\u062f\u0651\u0650\u064a\u064e\u0627\u0631\u0650\u06da \u0648\u064e\u0643\u064e\u0627\u0646\u064e \u0648\u064e\u0639\u06e1\u062f\u0657\u0627 \u0645\u0651\u064e\u0641\u06e1\u0639\u064f\u0648\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0630\u064e\u0627", "word_translation": "So when", "word_transliteration": "fa-idha"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "came", "word_transliteration": "jaa"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u06e1\u062f\u064f", "word_translation": "(the) promise", "word_transliteration": "wa'du"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0644\u064e\u0649\u0670\u0647\u064f\u0645\u064e\u0627", "word_translation": "(for) the first of the two", "word_transliteration": "ulahuma"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u064e\u062b\u06e1\u0646\u064e\u0627", "word_translation": "We raised", "word_transliteration": "ba'athna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "against you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0657\u0627", "word_translation": "servants", "word_transliteration": "ibadan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0646\u064e\u0627\u0653", "word_translation": "of Ours", "word_transliteration": "lana"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u0650\u064a", "word_translation": "those of great military might", "word_transliteration": "uli"}, {"word_arabic_uthmani": "\u0628\u064e\u0623\u06e1\u0633\u0656", "word_translation": "those of great military might", "word_transliteration": "basin"}, {"word_arabic_uthmani": "\u0634\u064e\u062f\u0650\u064a\u062f\u0656", "word_translation": "those of great military might", "word_transliteration": "shadidin"}, {"word_arabic_uthmani": "\u0641\u064e\u062c\u064e\u0627\u0633\u064f\u0648\u0627\u0652", "word_translation": "and they entered", "word_transliteration": "fajasu"}, {"word_arabic_uthmani": "\u062e\u0650\u0644\u064e\u0670\u0644\u064e", "word_translation": "the inner most part", "word_transliteration": "khilala"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u0650\u064a\u064e\u0627\u0631\u0650\u06da", "word_translation": "(of) the homes", "word_transliteration": "al-diyari"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "and (it) was", "word_transliteration": "wakana"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u06e1\u062f\u0657\u0627", "word_translation": "a promise", "word_transliteration": "wa'dan"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0641\u06e1\u0639\u064f\u0648\u0644\u0657\u0627", "word_translation": "fulfilled", "word_transliteration": "maf'ulan"}]}