{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0631\u064e\u0628\u0651\u0650 \u0625\u0650\u0646\u0651\u0650\u064a \u0648\u064e\u0647\u064e\u0646\u064e \u0671\u0644\u06e1\u0639\u064e\u0638\u06e1\u0645\u064f \u0645\u0650\u0646\u0651\u0650\u064a \u0648\u064e\u0671\u0634\u06e1\u062a\u064e\u0639\u064e\u0644\u064e \u0671\u0644\u0631\u0651\u064e\u0623\u06e1\u0633\u064f \u0634\u064e\u064a\u06e1\u0628\u0657\u0627 \u0648\u064e\u0644\u064e\u0645\u06e1 \u0623\u064e\u0643\u064f\u0646\u06e2 \u0628\u0650\u062f\u064f\u0639\u064e\u0627\u0653\u0626\u0650\u0643\u064e \u0631\u064e\u0628\u0651\u0650 \u0634\u064e\u0642\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "My Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed, [I]", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064e\u0646\u064e", "word_translation": "(have) weakened", "word_transliteration": "wahana"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0638\u06e1\u0645\u064f", "word_translation": "my bones", "word_transliteration": "al-'azmu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0651\u0650\u064a", "word_translation": "my bones", "word_transliteration": "minni"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0634\u06e1\u062a\u064e\u0639\u064e\u0644\u064e", "word_translation": "and flared", "word_transliteration": "wa-ish'ta'ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0623\u06e1\u0633\u064f", "word_translation": "(my) head", "word_transliteration": "al-rasu"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0628\u0657\u0627", "word_translation": "(with) white", "word_transliteration": "shayban"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "and not", "word_transliteration": "walam"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u064f\u0646\u06e2", "word_translation": "I have been", "word_transliteration": "akun"}, {"word_arabic_uthmani": "\u0628\u0650\u062f\u064f\u0639\u064e\u0627\u0653\u0626\u0650\u0643\u064e", "word_translation": "in (my) supplication (to) You", "word_transliteration": "bidu'aika"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0634\u064e\u0642\u0650\u064a\u0651\u0657\u0627", "word_translation": "unblessed", "word_transliteration": "shaqiyyan"}]}