{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u062a\u064e\u0646\u0642\u0650\u0645\u064f \u0645\u0650\u0646\u0651\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627\u0653 \u0623\u064e\u0646\u06e1 \u0621\u064e\u0627\u0645\u064e\u0646\u0651\u064e\u0627 \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650 \u0631\u064e\u0628\u0651\u0650\u0646\u064e\u0627 \u0644\u064e\u0645\u0651\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1\u0646\u064e\u0627\u06da \u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653 \u0623\u064e\u0641\u06e1\u0631\u0650\u063a\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u0635\u064e\u0628\u06e1\u0631\u0657\u0627 \u0648\u064e\u062a\u064e\u0648\u064e\u0641\u0651\u064e\u0646\u064e\u0627 \u0645\u064f\u0633\u06e1\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062a\u064e\u0646\u0642\u0650\u0645\u064f", "word_translation": "you take revenge", "word_transliteration": "tanqimu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0651\u064e\u0627\u0653", "word_translation": "from us", "word_transliteration": "minna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627\u0653", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u0651\u064e\u0627", "word_translation": "we believed", "word_transliteration": "amanna"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650", "word_translation": "in (the) Signs", "word_transliteration": "biayati"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0646\u064e\u0627", "word_translation": "(of) our Lord", "word_transliteration": "rabbina"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "when", "word_transliteration": "lamma"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1\u0646\u064e\u0627\u06da", "word_translation": "they came to us", "word_transliteration": "jaatna"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653", "word_translation": "Our Lord!", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u06e1\u0631\u0650\u063a\u06e1", "word_translation": "Pour", "word_transliteration": "afrigh"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "upon us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0635\u064e\u0628\u06e1\u0631\u0657\u0627", "word_translation": "patience", "word_transliteration": "sabran"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0648\u064e\u0641\u0651\u064e\u0646\u064e\u0627", "word_translation": "and cause us to die", "word_transliteration": "watawaffana"}, {"word_arabic_uthmani": "\u0645\u064f\u0633\u06e1\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "(as) Muslims", "word_transliteration": "mus'limina"}]}