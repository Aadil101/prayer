{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u0651\u064e\u0643\u064f\u0645\u06e1 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u064e\u0646\u064e\u0627 \u0639\u064e\u0646\u0650 \u0671\u0644\u06e1\u064a\u064e\u0645\u0650\u064a\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They will say", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "Indeed you", "word_transliteration": "innakum"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "[you] used (to)", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u064e\u0646\u064e\u0627", "word_translation": "come (to) us", "word_transliteration": "tatunana"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0650", "word_translation": "from", "word_transliteration": "ani"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u064a\u064e\u0645\u0650\u064a\u0646\u0650", "word_translation": "the right", "word_transliteration": "al-yamini"}]}