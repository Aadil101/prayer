{"ayahs": {"ayah_uthmani": "\u0643\u064f\u0644\u064f\u0648\u0627\u0652 \u0648\u064e\u062a\u064e\u0645\u064e\u062a\u0651\u064e\u0639\u064f\u0648\u0627\u0652 \u0642\u064e\u0644\u0650\u064a\u0644\u064b\u0627 \u0625\u0650\u0646\u0651\u064e\u0643\u064f\u0645 \u0645\u0651\u064f\u062c\u06e1\u0631\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0643\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "Eat", "word_transliteration": "kulu"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0645\u064e\u062a\u0651\u064e\u0639\u064f\u0648\u0627\u0652", "word_translation": "and enjoy yourselves", "word_transliteration": "watamatta'u"}, {"word_arabic_uthmani": "\u0642\u064e\u0644\u0650\u064a\u0644\u064b\u0627", "word_translation": "a little;", "word_transliteration": "qalilan"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064f\u0645", "word_translation": "indeed you", "word_transliteration": "innakum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u062c\u06e1\u0631\u0650\u0645\u064f\u0648\u0646\u064e", "word_translation": "(are) criminals", "word_transliteration": "muj'rimuna"}]}