{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0639\u064e\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u0643\u064f\u0645\u06e1 \u0648\u064e\u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627\u0652 \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064e\u0670\u062a\u0650 \u0644\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u062e\u06e1\u0644\u0650\u0641\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0643\u064e\u0645\u064e\u0627 \u0671\u0633\u06e1\u062a\u064e\u062e\u06e1\u0644\u064e\u0641\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0644\u064e\u064a\u064f\u0645\u064e\u0643\u0651\u0650\u0646\u064e\u0646\u0651\u064e \u0644\u064e\u0647\u064f\u0645\u06e1 \u062f\u0650\u064a\u0646\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0671\u0631\u06e1\u062a\u064e\u0636\u064e\u0649\u0670 \u0644\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u064a\u064f\u0628\u064e\u062f\u0651\u0650\u0644\u064e\u0646\u0651\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u06e2 \u0628\u064e\u0639\u06e1\u062f\u0650 \u062e\u064e\u0648\u06e1\u0641\u0650\u0647\u0650\u0645\u06e1 \u0623\u064e\u0645\u06e1\u0646\u0657\u0627\u06da \u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e\u0646\u0650\u064a \u0644\u064e\u0627 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e \u0628\u0650\u064a \u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627\u06da \u0648\u064e\u0645\u064e\u0646 \u0643\u064e\u0641\u064e\u0631\u064e \u0628\u064e\u0639\u06e1\u062f\u064e \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0641\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u062f\u064e", "word_translation": "Allah (has) promised", "word_transliteration": "wa'ada"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah (has) promised", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0643\u064f\u0645\u06e1", "word_translation": "among you", "word_transliteration": "minkum"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "and do", "word_transliteration": "wa'amilu"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064e\u0670\u062a\u0650", "word_translation": "righteous deeds", "word_transliteration": "al-salihati"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u062e\u06e1\u0644\u0650\u0641\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "surely He will grant them succession", "word_transliteration": "layastakhlifannahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u0627", "word_translation": "as", "word_transliteration": "kama"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u062e\u06e1\u0644\u064e\u0641\u064e", "word_translation": "He gave succession", "word_transliteration": "is'takhlafa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "to those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "(were) before them", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1", "word_translation": "(were) before them", "word_transliteration": "qablihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u064a\u064f\u0645\u064e\u0643\u0651\u0650\u0646\u064e\u0646\u0651\u064e", "word_translation": "and that He will surely establish", "word_transliteration": "walayumakkinanna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u062f\u0650\u064a\u0646\u064e\u0647\u064f\u0645\u064f", "word_translation": "their religion", "word_transliteration": "dinahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "which", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0671\u0631\u06e1\u062a\u064e\u0636\u064e\u0649\u0670", "word_translation": "He has approved", "word_transliteration": "ir'tada"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u064a\u064f\u0628\u064e\u062f\u0651\u0650\u0644\u064e\u0646\u0651\u064e\u0647\u064f\u0645", "word_translation": "and surely He will change for them", "word_transliteration": "walayubaddilannahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e2", "word_translation": "after", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u0650", "word_translation": "after", "word_transliteration": "ba'di"}, {"word_arabic_uthmani": "\u062e\u064e\u0648\u06e1\u0641\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their fear", "word_transliteration": "khawfihim"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0646\u0657\u0627\u06da", "word_translation": "security", "word_transliteration": "amnan"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e\u0646\u0650\u064a", "word_translation": "(for) they worship Me", "word_transliteration": "ya'budunani"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "they associate", "word_transliteration": "yush'rikuna"}, {"word_arabic_uthmani": "\u0628\u0650\u064a", "word_translation": "with Me", "word_transliteration": "bi"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627\u06da", "word_translation": "anything", "word_transliteration": "shayan"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "But whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064e", "word_translation": "disbelieved", "word_transliteration": "kafara"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u064e", "word_translation": "after", "word_transliteration": "ba'da"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "then those", "word_transliteration": "fa-ulaika"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "[they]", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "(are) the defiantly disobedient", "word_transliteration": "al-fasiquna"}]}