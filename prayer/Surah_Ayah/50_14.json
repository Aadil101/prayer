{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064f \u0671\u0644\u06e1\u0623\u064e\u064a\u06e1\u0643\u064e\u0629\u0650 \u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064f \u062a\u064f\u0628\u0651\u064e\u0639\u0656\u06da \u0643\u064f\u0644\u0651\u065e \u0643\u064e\u0630\u0651\u064e\u0628\u064e \u0671\u0644\u0631\u0651\u064f\u0633\u064f\u0644\u064e \u0641\u064e\u062d\u064e\u0642\u0651\u064e \u0648\u064e\u0639\u0650\u064a\u062f\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064f", "word_translation": "And (the) companions", "word_transliteration": "wa-ashabu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u064a\u06e1\u0643\u064e\u0629\u0650", "word_translation": "(of) the wood", "word_transliteration": "al-aykati"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064f", "word_translation": "and (the) people", "word_transliteration": "waqawmu"}, {"word_arabic_uthmani": "\u062a\u064f\u0628\u0651\u064e\u0639\u0656\u06da", "word_translation": "(of) Tubba", "word_transliteration": "tubba'in"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u065e", "word_translation": "All", "word_transliteration": "kullun"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064e", "word_translation": "denied", "word_transliteration": "kadhaba"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064f\u0633\u064f\u0644\u064e", "word_translation": "the Messengers", "word_transliteration": "al-rusula"}, {"word_arabic_uthmani": "\u0641\u064e\u062d\u064e\u0642\u0651\u064e", "word_translation": "so was fulfilled", "word_transliteration": "fahaqqa"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u0650\u064a\u062f\u0650", "word_translation": "My Threat", "word_transliteration": "wa'idi"}]}