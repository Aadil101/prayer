{"ayahs": {"ayah_uthmani": "\u0645\u064e\u062b\u064e\u0644\u064f\u0647\u064f\u0645\u06e1 \u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0671\u0633\u06e1\u062a\u064e\u0648\u06e1\u0642\u064e\u062f\u064e \u0646\u064e\u0627\u0631\u0657\u0627 \u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653 \u0623\u064e\u0636\u064e\u0627\u0653\u0621\u064e\u062a\u06e1 \u0645\u064e\u0627 \u062d\u064e\u0648\u06e1\u0644\u064e\u0647\u064f\u06e5 \u0630\u064e\u0647\u064e\u0628\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u0650\u0646\u064f\u0648\u0631\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u062a\u064e\u0631\u064e\u0643\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0638\u064f\u0644\u064f\u0645\u064e\u0670\u062a\u0656 \u0644\u0651\u064e\u0627 \u064a\u064f\u0628\u06e1\u0635\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u064e\u062b\u064e\u0644\u064f\u0647\u064f\u0645\u06e1", "word_translation": "Their example", "word_transliteration": "mathaluhum"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u064e\u062b\u064e\u0644\u0650", "word_translation": "(is) like (the) example", "word_transliteration": "kamathali"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(of) the one who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0648\u06e1\u0642\u064e\u062f\u064e", "word_translation": "kindled", "word_transliteration": "is'tawqada"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u0631\u0657\u0627", "word_translation": "a fire", "word_transliteration": "naran"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653", "word_translation": "then, when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u0623\u064e\u0636\u064e\u0627\u0653\u0621\u064e\u062a\u06e1", "word_translation": "it lighted", "word_transliteration": "adaat"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062d\u064e\u0648\u06e1\u0644\u064e\u0647\u064f\u06e5", "word_translation": "his surroundings", "word_transliteration": "hawlahu"}, {"word_arabic_uthmani": "\u0630\u064e\u0647\u064e\u0628\u064e", "word_translation": "took away", "word_transliteration": "dhahaba"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0646\u064f\u0648\u0631\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their light", "word_transliteration": "binurihim"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0631\u064e\u0643\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and left them", "word_transliteration": "watarakahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0638\u064f\u0644\u064f\u0645\u064e\u0670\u062a\u0656", "word_translation": "darkness[es]", "word_transliteration": "zulumatin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(so) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0628\u06e1\u0635\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(do) they see", "word_transliteration": "yub'siruna"}]}