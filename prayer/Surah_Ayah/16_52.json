{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u06e5 \u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0644\u064e\u0647\u064f \u0671\u0644\u062f\u0651\u0650\u064a\u0646\u064f \u0648\u064e\u0627\u0635\u0650\u0628\u064b\u0627\u06da \u0623\u064e\u0641\u064e\u063a\u064e\u064a\u06e1\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u062a\u064e\u062a\u0651\u064e\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u06e5", "word_translation": "And to Him (belongs)", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f", "word_translation": "and to Him", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u0650\u064a\u0646\u064f", "word_translation": "(is due) the worship", "word_transliteration": "al-dinu"}, {"word_arabic_uthmani": "\u0648\u064e\u0627\u0635\u0650\u0628\u064b\u0627\u06da", "word_translation": "constantly", "word_transliteration": "wasiban"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u063a\u064e\u064a\u06e1\u0631\u064e", "word_translation": "Then is it other (than)", "word_transliteration": "afaghayra"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u062a\u064e\u062a\u0651\u064e\u0642\u064f\u0648\u0646\u064e", "word_translation": "you fear", "word_transliteration": "tattaquna"}]}