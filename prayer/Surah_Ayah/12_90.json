{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0621\u0650\u0646\u0651\u064e\u0643\u064e \u0644\u064e\u0623\u064e\u0646\u062a\u064e \u064a\u064f\u0648\u0633\u064f\u0641\u064f\u06d6 \u0642\u064e\u0627\u0644\u064e \u0623\u064e\u0646\u064e\u0627\u06e0 \u064a\u064f\u0648\u0633\u064f\u0641\u064f \u0648\u064e\u0647\u064e\u0670\u0630\u064e\u0627\u0653 \u0623\u064e\u062e\u0650\u064a\u06d6 \u0642\u064e\u062f\u06e1 \u0645\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627\u0653\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0645\u064e\u0646 \u064a\u064e\u062a\u0651\u064e\u0642\u0650 \u0648\u064e\u064a\u064e\u0635\u06e1\u0628\u0650\u0631\u06e1 \u0641\u064e\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0627 \u064a\u064f\u0636\u0650\u064a\u0639\u064f \u0623\u064e\u062c\u06e1\u0631\u064e \u0671\u0644\u06e1\u0645\u064f\u062d\u06e1\u0633\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0623\u064e\u0621\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "Are you indeed", "word_transliteration": "a-innaka"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u064e\u0646\u062a\u064e", "word_translation": "[surely you]", "word_transliteration": "la-anta"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0633\u064f\u0641\u064f\u06d6", "word_translation": "Yusuf", "word_transliteration": "yusufu"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "I am", "word_transliteration": "ana"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0633\u064f\u0641\u064f", "word_translation": "Yusuf", "word_transliteration": "yusufu"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064e\u0670\u0630\u064e\u0627\u0653", "word_translation": "and this", "word_transliteration": "wahadha"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u0650\u064a\u06d6", "word_translation": "(is) my brother", "word_transliteration": "akhi"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "Indeed", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u0651\u064e", "word_translation": "Allah has been gracious", "word_transliteration": "manna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah has been gracious", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627\u0653\u06d6", "word_translation": "to us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed, he", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u0651\u064e\u0642\u0650", "word_translation": "fears Allah", "word_transliteration": "yattaqi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0635\u06e1\u0628\u0650\u0631\u06e1", "word_translation": "and (is) patient", "word_transliteration": "wayasbir"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "then indeed", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0636\u0650\u064a\u0639\u064f", "word_translation": "let go waste", "word_transliteration": "yudi'u"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0631\u064e", "word_translation": "(the) reward", "word_transliteration": "ajra"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u062d\u06e1\u0633\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "(of) the good-doers", "word_transliteration": "al-muh'sinina"}]}