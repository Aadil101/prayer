{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627 \u0628\u0650\u0646\u0650\u0639\u06e1\u0645\u064e\u0629\u0650 \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0641\u064e\u062d\u064e\u062f\u0651\u0650\u062b\u06e1"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627", "word_translation": "But as for", "word_transliteration": "wa-amma"}, {"word_arabic_uthmani": "\u0628\u0650\u0646\u0650\u0639\u06e1\u0645\u064e\u0629\u0650", "word_translation": "(the) Favor", "word_transliteration": "bini'mati"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "(of) your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0641\u064e\u062d\u064e\u062f\u0651\u0650\u062b\u06e1", "word_translation": "narrate", "word_transliteration": "fahaddith"}]}