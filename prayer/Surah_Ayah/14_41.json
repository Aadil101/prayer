{"ayahs": {"ayah_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627 \u0671\u063a\u06e1\u0641\u0650\u0631\u06e1 \u0644\u0650\u064a \u0648\u064e\u0644\u0650\u0648\u064e\u0670\u0644\u0650\u062f\u064e\u064a\u0651\u064e \u0648\u064e\u0644\u0650\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e \u064a\u064e\u0648\u06e1\u0645\u064e \u064a\u064e\u0642\u064f\u0648\u0645\u064f \u0671\u0644\u06e1\u062d\u0650\u0633\u064e\u0627\u0628\u064f"}, "words": [{"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627", "word_translation": "Our Lord!", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0671\u063a\u06e1\u0641\u0650\u0631\u06e1", "word_translation": "Forgive", "word_transliteration": "igh'fir"}, {"word_arabic_uthmani": "\u0644\u0650\u064a", "word_translation": "me", "word_transliteration": "li"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0648\u064e\u0670\u0644\u0650\u062f\u064e\u064a\u0651\u064e", "word_translation": "and my parents", "word_transliteration": "waliwalidayya"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "and the believers", "word_transliteration": "walil'mu'minina"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(on) the Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0645\u064f", "word_translation": "will (be) established", "word_transliteration": "yaqumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u0650\u0633\u064e\u0627\u0628\u064f", "word_translation": "the account", "word_transliteration": "al-hisabu"}]}