{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0642\u064e\u062f\u06e1 \u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0643\u064f\u0645 \u0628\u0650\u0645\u064e\u0627 \u062a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u0641\u064e\u0645\u064e\u0627 \u062a\u064e\u0633\u06e1\u062a\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0646\u064e \u0635\u064e\u0631\u06e1\u0641\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u0646\u064e\u0635\u06e1\u0631\u0657\u0627\u06da \u0648\u064e\u0645\u064e\u0646 \u064a\u064e\u0638\u06e1\u0644\u0650\u0645 \u0645\u0651\u0650\u0646\u0643\u064f\u0645\u06e1 \u0646\u064f\u0630\u0650\u0642\u06e1\u0647\u064f \u0639\u064e\u0630\u064e\u0627\u0628\u0657\u0627 \u0643\u064e\u0628\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u06e1", "word_translation": "So verily", "word_transliteration": "faqad"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0643\u064f\u0645", "word_translation": "they deny you", "word_transliteration": "kadhabukum"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "in what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "you say", "word_transliteration": "taquluna"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627", "word_translation": "so not", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u062a\u064e\u0633\u06e1\u062a\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0646\u064e", "word_translation": "you are able", "word_transliteration": "tastati'una"}, {"word_arabic_uthmani": "\u0635\u064e\u0631\u06e1\u0641\u0657\u0627", "word_translation": "(to) avert", "word_transliteration": "sarfan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u06e1\u0631\u0657\u0627\u06da", "word_translation": "(to) hel