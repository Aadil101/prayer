{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650 \u0641\u064e\u0671\u0633\u06e1\u062c\u064f\u062f\u06e1 \u0644\u064e\u0647\u064f\u06e5 \u0648\u064e\u0633\u064e\u0628\u0651\u0650\u062d\u06e1\u0647\u064f \u0644\u064e\u064a\u06e1\u0644\u0657\u0627 \u0637\u064e\u0648\u0650\u064a\u0644\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e", "word_translation": "And of", "word_transliteration": "wamina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u0650", "word_translation": "the night", "word_transliteration": "al-layli"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0633\u06e1\u062c\u064f\u062f\u06e1", "word_translation": "prostrate", "word_transliteration": "fa-us'jud"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "to Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064e\u0628\u0651\u0650\u062d\u06e1\u0647\u064f", "word_translation": "and glorify Him", "word_transliteration": "wasabbih'hu"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0644\u0657\u0627", "word_translation": "a night", "word_transliteration": "laylan"}, {"word_arabic_uthmani": "\u0637\u064e\u0648\u0650\u064a\u0644\u064b\u0627", "word_translation": "long", "word_transliteration": "tawilan"}]}