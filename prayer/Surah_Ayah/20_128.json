{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0647\u06e1\u062f\u0650 \u0644\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0645\u06e1 \u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0627 \u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0642\u064f\u0631\u064f\u0648\u0646\u0650 \u064a\u064e\u0645\u06e1\u0634\u064f\u0648\u0646\u064e \u0641\u0650\u064a \u0645\u064e\u0633\u064e\u0670\u0643\u0650\u0646\u0650\u0647\u0650\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e \u0641\u0650\u064a \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0644\u064e\u0623\u0653\u064a\u064e\u0670\u062a\u0656 \u0644\u0651\u0650\u0623\u064f\u0648\u0652\u0644\u0650\u064a \u0671\u0644\u0646\u0651\u064f\u0647\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Then has not", "word_transliteration": "afalam"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062f\u0650", "word_translation": "it guided", "word_transliteration": "yahdi"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "[for] them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u06e1", "word_translation": "how many", "word_transliteration": "kam"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0627", "word_translation": "We (have) destroyed", "word_transliteration": "ahlakna"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645", "word_translation": "before them", "word_transliteration": "qablahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0631\u064f\u0648\u0646\u0650", "word_translation": "the generations", "word_transliteration": "al-quruni"}, {"word_arabic_uthmani": "\u064a\u064e\u