{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u0631\u064f\u062f\u0651\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0645\u064e\u0648\u06e1\u0644\u064e\u0649\u0670\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650\u06da \u0623\u064e\u0644\u064e\u0627 \u0644\u064e\u0647\u064f \u0671\u0644\u06e1\u062d\u064f\u0643\u06e1\u0645\u064f \u0648\u064e\u0647\u064f\u0648\u064e \u0623\u064e\u0633\u06e1\u0631\u064e\u0639\u064f \u0671\u0644\u06e1\u062d\u064e\u0670\u0633\u0650\u0628\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0631\u064f\u062f\u0651\u064f\u0648\u0653\u0627\u0652", "word_translation": "they are returned", "word_transliteration": "ruddu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0645\u064e\u0648\u06e1\u0644\u064e\u0649\u0670\u0647\u064f\u0645\u064f", "word_translation": "their Protector", "word_transliteration": "mawlahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650\u06da", "word_translation": "[the] True", "word_transliteration": "al-haqi"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627", "word_translation": "Unquestionably", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f", "word_translation": "for Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064f\u0643\u06e1\u0645\u064f", "word_translation": "(is) the judgment", "word_transliteration": "al-huk'mu"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"w