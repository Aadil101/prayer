{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u0628\u064e\u0631\u064e\u0632\u064f\u0648\u0627\u0652 \u0644\u0650\u062c\u064e\u0627\u0644\u064f\u0648\u062a\u064e \u0648\u064e\u062c\u064f\u0646\u064f\u0648\u062f\u0650\u0647\u0650\u06e6 \u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653 \u0623\u064e\u0641\u06e1\u0631\u0650\u063a\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u0635\u064e\u0628\u06e1\u0631\u0657\u0627 \u0648\u064e\u062b\u064e\u0628\u0651\u0650\u062a\u06e1 \u0623\u064e\u0642\u06e1\u062f\u064e\u0627\u0645\u064e\u0646\u064e\u0627 \u0648\u064e\u0671\u0646\u0635\u064f\u0631\u06e1\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u0650 \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "And when", "word_transliteration": "walamma"}, {"word_arabic_uthmani": "\u0628\u064e\u0631\u064e\u0632\u064f\u0648\u0627\u0652", "word_translation": "they went forth", "word_transliteration": "barazu"}, {"word_arabic_uthmani": "\u0644\u0650\u062c\u064e\u0627\u0644\u064f\u0648\u062a\u064e", "word_translation": "to (face) Jalut", "word_transliteration": "lijaluta"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064f\u0646\u064f\u0648\u062f\u0650\u0647\u0650\u06e6", "word_translation": "and his troops", "word_transliteration": "wajunudihi"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "they said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653", "word_translation": "Our Lord", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u06e1\u0631\u0650\u063a\u06e1", "word_translation": "Pour", "word_transliteration": "afrigh"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "on us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0635\u064e\u0628\u06e1\u0631\u0657\u0627", "word_translation": "patience", "word_transliteration": "sabran"}, {"word_arabic_uthmani": "\u0648\u064e\u062b\u064e\u0628\u0651\u0650\u062a\u06e1", "word_translation": "and make firm", "word_transliteration": "wathabbit"}, {"word_arabic_uthmani": "\u0623\u064e\u0642\u06e1\u062f\u064e\u0627\u0645\u064e\u0646\u064e\u0627", "word_translation": "our feet", "word_transliteration": "aqdamana"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0646\u0635\u064f\u0631\u06e1\u0646\u064e\u0627", "word_translation": "and help us", "word_transliteration": "wa-unsur'na"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "against", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "the people", "word_transliteration": "al-qawmi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(who are) disbelieving", "word_transliteration": "al-kafirina"}]}