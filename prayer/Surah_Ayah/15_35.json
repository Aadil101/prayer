{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e \u0671\u0644\u0644\u0651\u064e\u0639\u06e1\u0646\u064e\u0629\u064e \u0625\u0650\u0644\u064e\u0649\u0670 \u064a\u064e\u0648\u06e1\u0645\u0650 \u0671\u0644\u062f\u0651\u0650\u064a\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e", "word_translation": "upon you", "word_transliteration": "alayka"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0639\u06e1\u0646\u064e\u0629\u064e", "word_translation": "(will be) the curse", "word_transliteration": "al-la'nata"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "till", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u0650", "word_translation": "(the) Day", "word_transliteration": "yawmi"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u0650\u064a\u0646\u0650", "word_translation": "(of) [the] Judgment", "word_transliteration": "al-dini"}]}