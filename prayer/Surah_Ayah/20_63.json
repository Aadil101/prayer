{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u06e1 \u0647\u064e\u0670\u0630\u064e\u0670\u0646\u0650 \u0644\u064e\u0633\u064e\u0670\u062d\u0650\u0631\u064e\u0670\u0646\u0650 \u064a\u064f\u0631\u0650\u064a\u062f\u064e\u0627\u0646\u0650 \u0623\u064e\u0646 \u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064e\u0627\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u06e1 \u0623\u064e\u0631\u06e1\u0636\u0650\u0643\u064f\u0645 \u0628\u0650\u0633\u0650\u062d\u06e1\u0631\u0650\u0647\u0650\u0645\u064e\u0627 \u0648\u064e\u064a\u064e\u0630\u06e1\u0647\u064e\u0628\u064e\u0627 \u0628\u0650\u0637\u064e\u0631\u0650\u064a\u0642\u064e\u062a\u0650\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064f\u062b\u06e1\u0644\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u06e1", "word_translation": "Indeed", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0670\u0646\u0650", "word_translation": "these two", "word_transliteration": "hadhani"}, {"word_arabic_uthmani": "\u0644\u064e\u0633\u064e\u0670\u062d\u0650\u0631\u064e\u0670\u0646\u0650", "word_translation": "[two] magicians", "word_transliteration": "lasahirani"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064e\u0627\u0646\u0650", "word_translation": "they intend", "word_transliteration": "yuridani"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064e\u0627\u0643\u064f\u0645", "word_translation": "they drive you out", "word_transliteration": "yukh'rijakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0636\u0650\u0643\u064f\u0645", "word_translation": "your land", "word_transliteration": "ardikum"}, {"word_arabic_uthmani": "\u0628\u0650\u0633\u0650\u062d\u06e1\u0631\u0650\u0647\u0650\u0645\u064e\u0627", "word_translation": "with their magic", "word_transliteration": "bisih'rihima"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0630\u06e1\u0647\u064e\u0628\u064e\u0627", "word_translation": "and do away", "word_transliteration": "wayadhhaba"}, {"word_arabic_uthmani": "\u0628\u0650\u0637\u064e\u0631\u0650\u064a\u0642\u064e\u062a\u0650\u0643\u064f\u0645\u064f", "word_translation": "with your way", "word_transliteration": "bitariqatikumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u062b\u06e1\u0644\u064e\u0649\u0670", "word_translation": "the exemplary", "word_transliteration": "al-muth'la"}]}