{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0621\u064e\u0627\u064a\u064e\u0629\u065e \u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u0623\u064e\u0646\u0651\u064e\u0627 \u062d\u064e\u0645\u064e\u0644\u06e1\u0646\u064e\u0627 \u0630\u064f\u0631\u0651\u0650\u064a\u0651\u064e\u062a\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u0650 \u0671\u0644\u06e1\u0645\u064e\u0634\u06e1\u062d\u064f\u0648\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0621\u064e\u0627\u064a\u064e\u0629\u065e", "word_translation": "And a Sign", "word_transliteration": "waayatun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0627", "word_translation": "(is) that", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u062d\u064e\u0645\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We carried", "word_transliteration": "hamalna"}, {"word_arabic_uthmani": "\u0630\u064f\u0631\u0651\u0650\u064a\u0651\u064e\u062a\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their offspring", "word_transliteration": "dhurriyyatahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u0650", "word_translation": "the ship", "word_transliteration": "al-ful'ki"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0634\u06e1\u062d\u064f\u0648\u0646\u0650", "word_translation": "laden", "word_transliteration": "al-mashhuni"}]}