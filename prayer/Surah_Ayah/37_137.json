{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0643\u064f\u0645\u06e1  \u0644\u064e\u062a\u064e\u0645\u064f\u0631\u0651\u064f\u0648\u0646\u064e  \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645  \u0645\u0651\u064f\u0635\u06e1\u0628\u0650\u062d\u0650\u064a\u0646\u064e "}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0643\u064f\u0645\u06e1 ", "word_translation": "And indeed you", "word_transliteration": "wa-innakum"}, {"word_arabic_uthmani": "\u0644\u064e\u062a\u064e\u0645\u064f\u0631\u0651\u064f\u0648\u0646\u064e ", "word_translation": "surely pass", "word_transliteration": "latamurruna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645 ", "word_translation": "by them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0635\u06e1\u0628\u0650\u062d\u0650\u064a\u0646\u064e ", "word_translation": "(in the) morning", "word_transliteration": "mus'bihina"}]}