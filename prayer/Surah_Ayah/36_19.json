{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0637\u064e\u0640\u0670\u0653\u0626\u0650\u0631\u064f\u0643\u064f\u0645 \u0645\u0651\u064e\u0639\u064e\u0643\u064f\u0645\u06e1 \u0623\u064e\u0626\u0650\u0646 \u0630\u064f\u0643\u0651\u0650\u0631\u06e1\u062a\u064f\u0645\u06da \u0628\u064e\u0644\u06e1 \u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u0642\u064e\u0648\u06e1\u0645\u065e \u0645\u0651\u064f\u0633\u06e1\u0631\u0650\u0641\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0637\u064e\u0640\u0670\u0653\u0626\u0650\u0631\u064f\u0643\u064f\u0645", "word_translation": "Your evil omen", "word_transliteration": "tairukum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0639\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(be) with you!", "word_transliteration": "ma'akum"}, {"word_arabic_uthmani": "\u0623\u064e\u0626\u0650\u0646", "word_translation": "Is it because", "word_transliteration": "a-in"}, {"word_arabic_uthmani": "\u0630\u064f\u0643\u0651\u0650\u0631\u06e1\u062a\u064f\u0645\u06da", "word_translation": "you are admonished", "word_transliteration": "dhukkir'tum"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u065e", "word_translation": "(are) a people", "word_transliteration": "qawmun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0633\u06e1\u0631\u0650\u0641\u064f\u0648\u0646\u064e", "word_translation": "transgressing", "word_transliteration": "mus'rifuna"}]}