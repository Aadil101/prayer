{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1 \u0642\u0650\u064a\u0644\u064e \u0623\u064e\u0647\u064e\u0670\u0643\u064e\u0630\u064e\u0627 \u0639\u064e\u0631\u06e1\u0634\u064f\u0643\u0650\u06d6 \u0642\u064e\u0627\u0644\u064e\u062a\u06e1 \u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5 \u0647\u064f\u0648\u064e\u06da \u0648\u064e\u0623\u064f\u0648\u062a\u0650\u064a\u0646\u064e\u0627 \u0671\u0644\u06e1\u0639\u0650\u0644\u06e1\u0645\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u064e\u0627 \u0648\u064e\u0643\u064f\u0646\u0651\u064e\u0627 \u0645\u064f\u0633\u06e1\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "So when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1", "word_translation": "she came", "word_transliteration": "jaat"}, {"word_arabic_uthmani": "\u0642\u0650\u064a\u0644\u064e", "word_translation": "it was said", "word_transliteration": "qila"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u064e\u0670\u0643\u064e\u0630\u064e\u0627", "word_translation": "Is like this", "word_transliteration": "ahakadha"}, {"word_arabic_uthmani": "\u0639\u064e\u0631\u06e1\u0634\u064f\u0643\u0650\u06d6", "word_translation": "your throne", "word_transliteration": "arshuki"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e\u062a\u06e1", "word_translation": "She said", "word_transliteration": "qalat"}, {"word_arabic_uthmani": "\u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "It is like", "word_transliteration": "ka-annahu"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e\u06da", "word_translation": "it", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0648\u062a\u0650\u064a\u0646\u064e\u0627", "word_translation": "And we were given", "word_transliteration": "wautina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0644\u06e1\u0645\u064e", "word_translation": "the knowledge", "word_transliteration": "al-'il'ma"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before her", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u064e\u0627", "word_translation": "before her", "word_transliteration": "qabliha"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "and we have been", "word_transliteration": "wakunna"}, {"word_arabic_uthmani": "\u0645\u064f\u0633\u06e1\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "Muslims", "word_transliteration": "mus'limina"}]}