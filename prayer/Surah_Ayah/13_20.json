{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064f\u0648\u0641\u064f\u0648\u0646\u064e \u0628\u0650\u0639\u064e\u0647\u06e1\u062f\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0646\u0642\u064f\u0636\u064f\u0648\u0646\u064e \u0671\u0644\u06e1\u0645\u0650\u064a\u062b\u064e\u0670\u0642\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "Those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0641\u064f\u0648\u0646\u064e", "word_translation": "fulfill", "word_transliteration": "yufuna"}, {"word_arabic_uthmani": "\u0628\u0650\u0639\u064e\u0647\u06e1\u062f\u0650", "word_translation": "the covenant", "word_transliteration": "bi'ahdi"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u0642\u064f\u0636\u064f\u0648\u0646\u064e", "word_translation": "they break", "word_transliteration": "yanquduna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u0650\u064a\u062b\u064e\u0670\u0642\u064e", "word_translation": "the contract", "word_transliteration": "al-mithaqa"}]}