{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0643\u064e\u0623\u064e\u064a\u0651\u0650\u0646 \u0645\u0651\u0650\u0646 \u062f\u064e\u0627\u0653\u0628\u0651\u064e\u0629\u0656 \u0644\u0651\u064e\u0627 \u062a\u064e\u062d\u06e1\u0645\u0650\u0644\u064f \u0631\u0650\u0632\u06e1\u0642\u064e\u0647\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064e\u0631\u06e1\u0632\u064f\u0642\u064f\u0647\u064e\u0627 \u0648\u064e\u0625\u0650\u064a\u0651\u064e\u0627\u0643\u064f\u0645\u06e1\u06da \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0623\u064e\u064a\u0651\u0650\u0646", "word_translation": "And how many", "word_transliteration": "waka-ayyin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0653\u0628\u0651\u064e\u0629\u0656", "word_translation": "a creature", "word_transliteration": "dabbatin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u06e1\u0645\u0650\u0644\u064f", "word_translation": "carry", "word_transliteration": "tahmilu"}, {"word_arabic_uthmani": "\u0631\u0650\u0632\u06e1\u0642\u064e\u0647\u064e\u0627", "word_translation": "its provision", "word_transliteration": "riz'qaha"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u0632\u064f\u0642\u064f\u0647\u064e\u0627", "word_translation": "provides (for) it", "word_transliteration": "yarzuquha"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u064a\u0651\u064e\u0627\u0643\u064f\u0645\u06e1\u06da", "word_translation": "and (for) you", "word_transliteration": "wa-iyyakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f", "word_translation": "(is) the All-Hearer", "word_transliteration": "al-sami'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "the All-Knower", "word_transliteration": "al-'alimu"}]}