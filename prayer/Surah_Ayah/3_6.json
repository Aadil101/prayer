{"ayahs": {"ayah_uthmani": "\u0647\u064f\u0648\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u064a\u064f\u0635\u064e\u0648\u0651\u0650\u0631\u064f\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u062d\u064e\u0627\u0645\u0650 \u0643\u064e\u064a\u06e1\u0641\u064e \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da \u0644\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0670\u0647\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u064a\u064f\u0635\u064e\u0648\u0651\u0650\u0631\u064f\u0643\u064f\u0645\u06e1", "word_translation": "shapes you", "word_transliteration": "yusawwirukum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u062d\u064e\u0627\u0645\u0650", "word_translation": "the wombs", "word_transliteration": "al-arhami"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "how(ever)", "word_transliteration": "kayfa"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0644\u064e\u0627\u0653", "word_translation": "(There is) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064e", "word_translation": "god", "word_transliteration": "ilaha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word