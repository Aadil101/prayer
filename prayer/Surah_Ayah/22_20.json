{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0635\u06e1\u0647\u064e\u0631\u064f \u0628\u0650\u0647\u0650\u06e6 \u0645\u064e\u0627 \u0641\u0650\u064a \u0628\u064f\u0637\u064f\u0648\u0646\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0671\u0644\u06e1\u062c\u064f\u0644\u064f\u0648\u062f\u064f"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0635\u06e1\u0647\u064e\u0631\u064f", "word_translation": "Will be melted", "word_transliteration": "yus'haru"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0628\u064f\u0637\u064f\u0648\u0646\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their bellies", "word_transliteration": "butunihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u062c\u064f\u0644\u064f\u0648\u062f\u064f", "word_translation": "and the skins", "word_transliteration": "wal-juludu"}]}