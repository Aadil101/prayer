{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627 \u0671\u0644\u0646\u0651\u064f\u0637\u06e1\u0641\u064e\u0629\u064e \u0639\u064e\u0644\u064e\u0642\u064e\u0629\u0657 \u0641\u064e\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627 \u0671\u0644\u06e1\u0639\u064e\u0644\u064e\u0642\u064e\u0629\u064e \u0645\u064f\u0636\u06e1\u063a\u064e\u0629\u0657 \u0641\u064e\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627 \u0671\u0644\u06e1\u0645\u064f\u0636\u06e1\u063a\u064e\u0629\u064e \u0639\u0650\u0638\u064e\u0670\u0645\u0657\u0627 \u0641\u064e\u0643\u064e\u0633\u064e\u0648\u06e1\u0646\u064e\u0627 \u0671\u0644\u06e1\u0639\u0650\u0638\u064e\u0670\u0645\u064e \u0644\u064e\u062d\u06e1\u0645\u0657\u0627 \u062b\u064f\u0645\u0651\u064e \u0623\u064e\u0646\u0634\u064e\u0623\u06e1\u0646\u064e\u0670\u0647\u064f \u062e\u064e\u0644\u06e1\u0642\u064b\u0627 \u0621\u064e\u0627\u062e\u064e\u0631\u064e\u06da \u0641\u064e\u062a\u064e\u0628\u064e\u0627\u0631\u064e\u0643\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064f \u0671\u0644\u06e1\u062e\u064e\u0670\u0644\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627", "word_translation": "We created", "word_transliteration": "khalaqna"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064f\u0637\u06e1\u0641\u064e\u0629\u064e", "word_translation": "the semen-drop", "word_transliteration": "al-nut'fata"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0642\u064e\u0629\u0657", "word_translation": "(into) a clinging substance", "word_transliteration": "alaqatan"}, {"word_arabic_uthmani": "\u0641\u064e\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627", "word_translation": "then We created", "word_transliteration": "fakhalaqna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u064e\u0642\u064e\u0629\u064e", "word_translation": "the clinging substance", "word_transliteration": "al-'alaqata"}, {"word_arabic_uthmani": "\u0645\u064f\u0636\u06e1\u063a\u064e\u0629\u0657", "word_translation": "(into) an embryonic lump", "word_transliteration": "mud'ghatan"}, {"word_arabic_uthmani": "\u0641\u064e\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627", "word_translation": "then We created", "word_transliteration": "fakhalaqna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0636\u06e1\u063a\u064e\u0629\u064e", "word_translation": "the embryonic lump", "word_transliteration": "al-mud'ghata"}, {"word_arabic_uthmani": "\u0639\u0650\u0638\u064e\u0670\u0645\u0657\u0627", "word_translation": "(into) bones", "word_transliteration": "izaman"}, {"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u0633\u064e\u0648\u06e1\u0646\u064e\u0627", "word_translation": "then We clothed", "word_transliteration": "fakasawna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0638\u064e\u0670\u0645\u064e", "word_translation": "the bones", "word_transliteration": "al-'izama"}, {"word_arabic_uthmani": "\u0644\u064e\u062d\u06e1\u0645\u0657\u0627", "word_translation": "(with) flesh", "word_transliteration": "lahman"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0634\u064e\u0623\u06e1\u0646\u064e\u0670\u0647\u064f", "word_translation": "We produce it", "word_transliteration": "anshanahu"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0642\u064b\u0627", "word_translation": "(as) a creation", "word_transliteration": "khalqan"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062e\u064e\u0631\u064e\u06da", "word_translation": "another", "word_transliteration": "akhara"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0628\u064e\u0627\u0631\u064e\u0643\u064e", "word_translation": "So blessed is", "word_transliteration": "fatabaraka"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u06e1\u0633\u064e\u0646\u064f", "word_translation": "(the) Best", "word_transliteration": "ahsanu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0670\u0644\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "(of) the Creators", "word_transliteration": "al-khaliqina"}]}