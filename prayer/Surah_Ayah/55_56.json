{"ayahs": {"ayah_uthmani": "\u0641\u0650\u064a\u0647\u0650\u0646\u0651\u064e \u0642\u064e\u0670\u0635\u0650\u0631\u064e\u0670\u062a\u064f \u0671\u0644\u0637\u0651\u064e\u0631\u06e1\u0641\u0650 \u0644\u064e\u0645\u06e1 \u064a\u064e\u0637\u06e1\u0645\u0650\u062b\u06e1\u0647\u064f\u0646\u0651\u064e \u0625\u0650\u0646\u0633\u065e \u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u062c\u064e\u0627\u0653\u0646\u0651\u065e"}, "words": [{"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650\u0646\u0651\u064e", "word_translation": "In them", "word_transliteration": "fihinna"}, {"word_arabic_uthmani": "\u0642\u064e\u0670\u0635\u0650\u0631\u064e\u0670\u062a\u064f", "word_translation": "(will be) companions of modest gaze", "word_transliteration": "qasiratu"}, {"word_arabic_uthmani": "\u0671\u0644\u0637\u0651\u064e\u0631\u06e1\u0641\u0650", "word_translation": "(will be) companions of modest gaze", "word_transliteration": "al-tarfi"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u0637\u06e1\u0645\u0650\u062b\u06e1\u0647\u064f\u0646\u0651\u064e", "word_translation": "has touched them", "word_transliteration": "yatmith'hunna"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0633\u065e", "word_translation": "any man", "word_transliteration": "insun"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "before them", "word_transliteration": "qablahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0646\u0651\u065e", "word_translation": "any jinn", "word_transliteration": "jannun"}]}