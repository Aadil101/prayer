{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0630\u06e1 \u0642\u064e\u0627\u0644\u064e\u062a\u0650 \u0671\u0645\u06e1\u0631\u064e\u0623\u064e\u062a\u064f \u0639\u0650\u0645\u06e1\u0631\u064e\u0670\u0646\u064e \u0631\u064e\u0628\u0651\u0650 \u0625\u0650\u0646\u0651\u0650\u064a \u0646\u064e\u0630\u064e\u0631\u06e1\u062a\u064f \u0644\u064e\u0643\u064e \u0645\u064e\u0627 \u0641\u0650\u064a \u0628\u064e\u0637\u06e1\u0646\u0650\u064a \u0645\u064f\u062d\u064e\u0631\u0651\u064e\u0631\u0657\u0627 \u0641\u064e\u062a\u064e\u0642\u064e\u0628\u0651\u064e\u0644\u06e1 \u0645\u0650\u0646\u0651\u0650\u064a\u0653\u06d6 \u0625\u0650\u0646\u0651\u064e\u0643\u064e \u0623\u064e\u0646\u062a\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "When", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e\u062a\u0650", "word_translation": "[she] said", "word_transliteration": "qalati"}, {"word_arabic_uthmani": "\u0671\u0645\u06e1\u0631\u064e\u0623\u064e\u062a\u064f", "word_translation": "(the) wife", "word_transliteration": "im'ra-atu"}, {"word_arabic_uthmani": "\u0639\u0650\u0645\u06e1\u0631\u064e\u0670\u0646\u064e", "word_translation": "(of) Imran", "word_transliteration": "im'rana"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "My Lord!", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0646\u064e\u0630\u064e\u0631\u06e1\u062a\u064f", "word_translation": "[I] vowed", "word_transliteration": "nadhartu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e", "word_translation": "to You", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0628\u064e\u0637\u06e1\u0646\u0650\u064a", "word_translation": "my womb", "word_transliteration": "batni"}, {"word_arabic_uthmani": "\u0645\u064f\u062d\u064e\u0631\u0651\u064e\u0631\u0657\u0627", "word_translation": "dedicated", "word_transliteration": "muharraran"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0642\u064e\u0628\u0651\u064e\u0644\u06e1", "word_translation": "so accept", "word_transliteration": "fataqabbal"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0651\u0650\u064a\u0653\u06d6", "word_translation": "from me", "word_transliteration": "minni"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "Indeed You", "word_transliteration": "innaka"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "You", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u0650\u064a\u0639\u064f", "word_translation": "(are) the All-Hearing", "word_transliteration": "al-sami'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "the All-Knowing", "word_transliteration": "al-'alimu"}]}