{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645 \u0645\u0651\u064f\u0624\u06e1\u0635\u064e\u062f\u064e\u0629\u065e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064e\u0627", "word_translation": "Indeed it", "word_transliteration": "innaha"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645", "word_translation": "(will be) upon them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0624\u06e1\u0635\u064e\u062f\u064e\u0629\u065e", "word_translation": "closed over", "word_transliteration": "mu'sadatun"}]}