{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064e \u0671\u0628\u06e1\u0646\u064e\u062a\u064e \u0639\u0650\u0645\u06e1\u0631\u064e\u0670\u0646\u064e \u0671\u0644\u0651\u064e\u062a\u0650\u064a\u0653 \u0623\u064e\u062d\u06e1\u0635\u064e\u0646\u064e\u062a\u06e1 \u0641\u064e\u0631\u06e1\u062c\u064e\u0647\u064e\u0627 \u0641\u064e\u0646\u064e\u0641\u064e\u062e\u06e1\u0646\u064e\u0627 \u0641\u0650\u064a\u0647\u0650 \u0645\u0650\u0646 \u0631\u0651\u064f\u0648\u062d\u0650\u0646\u064e\u0627 \u0648\u064e\u0635\u064e\u062f\u0651\u064e\u0642\u064e\u062a\u06e1 \u0628\u0650\u0643\u064e\u0644\u0650\u0645\u064e\u0670\u062a\u0650 \u0631\u064e\u0628\u0651\u0650\u0647\u064e\u0627 \u0648\u064e\u0643\u064f\u062a\u064f\u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u0643\u064e\u0627\u0646\u064e\u062a\u06e1 \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0642\u064e\u0670\u0646\u0650\u062a\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064e", "word_translation": "And Maryam", "word_transliteration": "wamaryama"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u0646\u064e\u062a\u064e", "word_translation": "(the) daughter", "word_transliteration": "ib'nata"}, {"word_arabic_uthmani": "\u0639\u0650\u0645\u06e1\u0631\u064e\u0670\u0646\u064e", "word_translation": "(of) Imran", "word_transliteration": "im'rana"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u062a\u0650\u064a\u0653", "word_translation": "who", "word_transliteration": "allati"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u06e1\u0635\u064e\u0646\u064e\u062a\u06e1", "word_translation": "guarded", "word_transliteration": "ahsanat"}, {"word_arabic_uthmani": "\u0641\u064e\u0631\u06e1\u062c\u064e\u0647\u064e\u0627", "word_translation": "her chastity", "word_transliteration": "farjaha"}, {"word_arabic_uthmani": "\u0641\u064e\u0646\u064e\u0641\u064e\u062e\u06e1\u0646\u064e\u0627", "word_translation": "so We breathed", "word_transliteration": "fanafakhna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650", "word_translation": "into it", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064f\u0648\u062d\u0650\u0646\u064e\u0627", "word_translation": "Our Spirit", "word_transliteration": "ruhina"}, {"word_arabic_uthmani": "\u0648\u064e\u0635\u064e\u062f\u0651\u064e\u0642\u064e\u062a\u06e1", "word_translation": "And she believed", "word_transliteration": "wasaddaqat"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064e\u0644\u0650\u0645\u064e\u0670\u062a\u0650", "word_translation": "(in the) Words", "word_transliteration": "bikalimati"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u064e\u0627", "word_translation": "(of) her Lord", "word_transliteration": "rabbiha"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064f\u062a\u064f\u0628\u0650\u0647\u0650\u06e6", "word_translation": "and His Books", "word_transliteration": "wakutubihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064e\u062a\u06e1", "word_translation": "and she was", "word_transliteration": "wakanat"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0670\u0646\u0650\u062a\u0650\u064a\u0646\u064e", "word_translation": "the devoutly obedient", "word_transliteration": "al-qanitina"}]}