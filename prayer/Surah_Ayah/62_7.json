{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u062a\u064e\u0645\u064e\u0646\u0651\u064e\u0648\u06e1\u0646\u064e\u0647\u064f\u06e5\u0653 \u0623\u064e\u0628\u064e\u062f\u064e\u06e2\u0627 \u0628\u0650\u0645\u064e\u0627 \u0642\u064e\u062f\u0651\u064e\u0645\u064e\u062a\u06e1 \u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u0650\u064a\u0645\u064f\u06e2 \u0628\u0650\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "But not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0645\u064e\u0646\u0651\u064e\u0648\u06e1\u0646\u064e\u0647\u064f\u06e5\u0653", "word_translation": "they will wish for it", "word_transliteration": "yatamannawnahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u064e\u062f\u064e\u06e2\u0627", "word_translation": "ever", "word_transliteration": "abadan"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "for what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0651\u064e\u0645\u064e\u062a\u06e1", "word_translation": "have sent forth", "word_transliteration": "qaddamat"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1\u06da", "word_translation": "their hands", "word_transliteration": "aydihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u064f\u06e2", "word_translation": "(is) All-Knowing", "word_transliteration": "alimun"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "of the wrongdoers", "word_transliteration": "bil-zalimina"}]}