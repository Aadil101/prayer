{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0631\u064f\u0633\u064f\u0644\u0657\u0627 \u0642\u064e\u062f\u06e1 \u0642\u064e\u0635\u064e\u0635\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u064f \u0648\u064e\u0631\u064f\u0633\u064f\u0644\u0657\u0627 \u0644\u0651\u064e\u0645\u06e1 \u0646\u064e\u0642\u06e1\u0635\u064f\u0635\u06e1\u0647\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e\u06da \u0648\u064e\u0643\u064e\u0644\u0651\u064e\u0645\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u062a\u064e\u0643\u06e1\u0644\u0650\u064a\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0631\u064f\u0633\u064f\u0644\u0657\u0627", "word_translation": "And Messengers", "word_transliteration": "warusulan"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "surely", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u0642\u064e\u0635\u064e\u0635\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "We (have) mentioned them", "word_transliteration": "qasasnahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e", "word_translation": "to you", "word_transliteration": "alayka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f", "word_translation": "before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064f\u0633\u064f\u0644\u0657\u0627", "word_translation": "and Messengers", "word_transliteration": "warusulan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u0646\u064e\u0642\u06e1\u0635\u064f\u0635\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "We (have) mentioned them", "word_transliteration": "naqsus'hum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e\u06da", "word_translation": "to you", "word_transliteration": "alayka"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0644\u0651\u064e\u0645\u064e", "word_translation": "And spoke", "word_transliteration": "wakallama"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "(to) Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u06e1\u0644\u0650\u064a\u0645\u0657\u0627", "word_translation": "(in a) conversation", "word_transliteration": "takliman"}]}