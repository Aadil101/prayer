{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0642\u064e\u0627\u0647\u0650\u0631\u064f \u0641\u064e\u0648\u06e1\u0642\u064e \u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6\u06d6 \u0648\u064e\u064a\u064f\u0631\u06e1\u0633\u0650\u0644\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u062d\u064e\u0641\u064e\u0638\u064e\u0629\u064b \u062d\u064e\u062a\u0651\u064e\u0649\u0670\u0653 \u0625\u0650\u0630\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e \u0623\u064e\u062d\u064e\u062f\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064f \u062a\u064e\u0648\u064e\u0641\u0651\u064e\u062a\u06e1\u0647\u064f \u0631\u064f\u0633\u064f\u0644\u064f\u0646\u064e\u0627 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064f\u0641\u064e\u0631\u0651\u0650\u0637\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0627\u0647\u0650\u0631\u064f", "word_translation": "(is) the Subjugator", "word_transliteration": "al-qahiru"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u06e1\u0642\u064e", "word_translation": "over", "word_transliteration": "fawqa"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6\u06d6", "word_translation": "His slaves", "word_transliteration": "ibadihi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0631\u06e1\u0633\u0650\u0644\u064f", "word_translation": "and He sends", "word_transliteration": "wayur'silu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "over you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u062d\u064e\u0641\u064e\u0638\u064e\u0629\u064b", "word_translation": "guardians", "word_transliteration": "hafazatan"}, {"word_arabic_uthmani": "\u062d\u064e\u062a\u0651\u064e\u0649\u0670\u0653", "word_translation": "until", "word_transliteration": "hatta"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "comes", "word_transliteration": "jaa"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u064e\u0643\u064f\u0645\u064f", "word_translation": "(to) anyone of you", "word_transliteration": "ahadakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064f", "word_translation": "the death", "word_transliteration": "al-mawtu"}, {"word_arabic_uthmani": "\u062a\u064e\u0648\u064e\u0641\u0651\u064e\u062a\u06e1\u0647\u064f", "word_translation": "take him", "word_transliteration": "tawaffathu"}, {"word_arabic_uthmani": "\u0631\u064f\u0633\u064f\u0644\u064f\u0646\u064e\u0627", "word_translation": "Our messengers", "word_transliteration": "rusuluna"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0641\u064e\u0631\u0651\u0650\u0637\u064f\u0648\u0646\u064e", "word_translation": "fail", "word_transliteration": "yufarrituna"}]}