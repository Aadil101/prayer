{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0631\u064e\u0621\u064e\u064a\u06e1\u062a\u064e \u0645\u064e\u0646\u0650 \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e \u0625\u0650\u0644\u064e\u0670\u0647\u064e\u0647\u064f\u06e5 \u0647\u064e\u0648\u064e\u0649\u0670\u0647\u064f \u0623\u064e\u0641\u064e\u0623\u064e\u0646\u062a\u064e \u062a\u064e\u0643\u064f\u0648\u0646\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0648\u064e\u0643\u0650\u064a\u0644\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0631\u064e\u0621\u064e\u064a\u06e1\u062a\u064e", "word_translation": "Have you seen", "word_transliteration": "ara-ayta"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u0650", "word_translation": "(one) who", "word_transliteration": "mani"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e", "word_translation": "takes", "word_transliteration": "ittakhadha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u064e\u0647\u064f\u06e5", "word_translation": "(as) his god", "word_transliteration": "ilahahu"}, {"word_arabic_uthmani": "\u0647\u064e\u0648\u064e\u0649\u0670\u0647\u064f", "word_translation": "his own desire", "word_transliteration": "hawahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0623\u064e\u0646\u062a\u064e", "word_translation": "Then would you", "word_transliteration": "afa-anta"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u064f\u0648\u0646\u064f", "word_translation": "be", "word_transliteration": "takunu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "over him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u0650\u064a\u0644\u064b\u0627", "word_translation": "a guardian", "word_transliteration": "wakilan"}]}