{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627\u0653 \u0625\u0650\u0630\u064e\u0627 \u0645\u064e\u0627 \u0671\u0628\u06e1\u062a\u064e\u0644\u064e\u0649\u0670\u0647\u064f \u0641\u064e\u0642\u064e\u062f\u064e\u0631\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0631\u0650\u0632\u06e1\u0642\u064e\u0647\u064f\u06e5 \u0641\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0631\u064e\u0628\u0651\u0650\u064a\u0653 \u0623\u064e\u0647\u064e\u0670\u0646\u064e\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627\u0653", "word_translation": "But", "word_transliteration": "wa-amma"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "does", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u062a\u064e\u0644\u064e\u0649\u0670\u0647\u064f", "word_translation": "He try him", "word_transliteration": "ib'talahu"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u064e\u0631\u064e", "word_translation": "and restricts", "word_transliteration": "faqadara"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "for him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0631\u0650\u0632\u06e1\u0642\u064e\u0647\u064f\u06e5", "word_translation": "his provision", "word_transliteration": "riz'qahu"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "then he says", "word_transliteration": "fayaqulu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a\u0653", "word_translation": "My Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u064e\u0670\u0646\u064e\u0646\u0650", "word_translation": "(has) humiliated me", "word_transliteration": "ahanani"}]}