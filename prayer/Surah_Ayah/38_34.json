{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0641\u064e\u062a\u064e\u0646\u0651\u064e\u0627 \u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e \u0648\u064e\u0623\u064e\u0644\u06e1\u0642\u064e\u064a\u06e1\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u0649\u0670 \u0643\u064f\u0631\u06e1\u0633\u0650\u064a\u0651\u0650\u0647\u0650\u06e6 \u062c\u064e\u0633\u064e\u062f\u0657\u0627 \u062b\u064f\u0645\u0651\u064e \u0623\u064e\u0646\u064e\u0627\u0628\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And certainly", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0646\u0651\u064e\u0627", "word_translation": "We tried", "word_transliteration": "fatanna"}, {"word_arabic_uthmani": "\u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e", "word_translation": "Sulaiman", "word_transliteration": "sulaymana"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0644\u06e1\u0642\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "and We placed", "word_transliteration": "wa-alqayna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0643\u064f\u0631\u06e1\u0633\u0650\u064a\u0651\u0650\u0647\u0650\u06e6", "word_translation": "his throne", "word_transliteration": "kur'siyyihi"}, {"word_arabic_uthmani": "\u062c\u064e\u0633\u064e\u062f\u0657\u0627", "word_translation": "a body", "word_transliteration": "jasadan"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627\u0628\u064e", "word_translation": "he turned", "word_transliteration": "anaba"}]}