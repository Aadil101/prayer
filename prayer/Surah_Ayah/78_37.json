{"ayahs": {"ayah_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627 \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650\u06d6 \u0644\u064e\u0627 \u064a\u064e\u0645\u06e1\u0644\u0650\u0643\u064f\u0648\u0646\u064e \u0645\u0650\u0646\u06e1\u0647\u064f \u062e\u0650\u0637\u064e\u0627\u0628\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650", "word_translation": "Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627", "word_translation": "(is) between both of them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650\u06d6", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmani"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u06e1\u0644\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "they have power", "word_transliteration": "yamlikuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f", "word_translation": "from Him", "word_transliteration": "min'hu"}, {"word_arabic_uthmani": "\u062e\u0650\u0637\u064e\u0627\u0628\u0657\u0627", "word_translation": "(to) address", "word_transliteration": "khitaban"}]}