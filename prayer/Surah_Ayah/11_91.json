{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u064a\u064e\u0670\u0634\u064f\u0639\u064e\u064a\u06e1\u0628\u064f \u0645\u064e\u0627 \u0646\u064e\u0641\u06e1\u0642\u064e\u0647\u064f \u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627 \u0645\u0651\u0650\u0645\u0651\u064e\u0627 \u062a\u064e\u0642\u064f\u0648\u0644\u064f \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0644\u064e\u0646\u064e\u0631\u064e\u0649\u0670\u0643\u064e \u0641\u0650\u064a\u0646\u064e\u0627 \u0636\u064e\u0639\u0650\u064a\u0641\u0657\u0627\u06d6 \u0648\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627 \u0631\u064e\u0647\u06e1\u0637\u064f\u0643\u064e \u0644\u064e\u0631\u064e\u062c\u064e\u0645\u06e1\u0646\u064e\u0670\u0643\u064e\u06d6 \u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u0628\u0650\u0639\u064e\u0632\u0650\u064a\u0632\u0656"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0634\u064f\u0639\u064e\u064a\u06e1\u0628\u064f", "word_translation": "O Shuaib", "word_transliteration": "yashu'aybu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0642\u064e\u0647\u064f", "word_translation": "we understand", "word_transliteration": "nafqahu"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627", "word_translation": "much", "word_transliteration": "kathiran"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "you say", "word_transliteration": "taqulu"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "and indeed, we", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0631\u064e\u0649\u0670\u0643\u064e", "word_translation": "surely [we] see you", "word_transliteration": "lanaraka"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0646\u064e\u0627", "word_translation": "among us", "word_transliteration": "fina"}, {"word_arabic_uthmani": "\u0636\u064e\u0639\u0650\u064a\u0641\u0657\u0627\u06d6", "word_translation": "weak", "word_transliteration": "da'ifan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "And if not", "word_transliteration": "walawla"}, {"word_arabic_uthmani": "\u0631\u064e\u0647\u06e1\u0637\u064f\u0643\u064e", "word_translation": "for your family", "word_transliteration": "rahtuka"}, {"word_arabic_uthmani": "\u0644\u064e\u0631\u064e\u062c\u064e\u0645\u06e1\u0646\u064e\u0670\u0643\u064e\u06d6", "word_translation": "surely we would have stoned you", "word_transliteration": "larajamnaka"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "and you are not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "and you are not", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "against us", "word_transliteration": "alayna"}, {"word_arabic_uthmani": "\u0628\u0650\u0639\u064e\u0632\u0650\u064a\u0632\u0656", "word_translation": "mighty", "word_transliteration": "bi'azizin"}]}