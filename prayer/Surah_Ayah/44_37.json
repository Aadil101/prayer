{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0647\u064f\u0645\u06e1 \u062e\u064e\u064a\u06e1\u0631\u064c \u0623\u064e\u0645\u06e1 \u0642\u064e\u0648\u06e1\u0645\u064f \u062a\u064f\u0628\u0651\u064e\u0639\u0656 \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1 \u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0645\u064f\u062c\u06e1\u0631\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Are they", "word_transliteration": "ahum"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u064c", "word_translation": "better", "word_transliteration": "khayrun"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064f", "word_translation": "(the) people", "word_transliteration": "qawmu"}, {"word_arabic_uthmani": "\u062a\u064f\u0628\u0651\u064e\u0639\u0656", "word_translation": "(of) Tubba", "word_transliteration": "tubba'in"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before them", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1", "word_translation": "before them", "word_transliteration": "qablihim"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1\u06da", "word_translation": "We destroyed them", "word_transliteration": "ahlaknahum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06