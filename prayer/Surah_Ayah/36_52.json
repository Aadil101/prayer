{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u064a\u064e\u0670\u0648\u064e\u064a\u06e1\u0644\u064e\u0646\u064e\u0627 \u0645\u064e\u0646\u06e2 \u0628\u064e\u0639\u064e\u062b\u064e\u0646\u064e\u0627 \u0645\u0650\u0646 \u0645\u0651\u064e\u0631\u06e1\u0642\u064e\u062f\u0650\u0646\u064e\u0627\u06dc\u06d7 \u0647\u064e\u0670\u0630\u064e\u0627 \u0645\u064e\u0627 \u0648\u064e\u0639\u064e\u062f\u064e \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f \u0648\u064e\u0635\u064e\u062f\u064e\u0642\u064e \u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They [will] say", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0648\u064e\u064a\u06e1\u0644\u064e\u0646\u064e\u0627", "word_translation": "O woe to us!", "word_transliteration": "yawaylana"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u06e2", "word_translation": "Who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u064e\u062b\u064e\u0646\u064e\u0627", "word_translation": "has raised us", "word_transliteration": "ba'athana"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0631\u06e1\u0642\u064e\u062f\u0650\u0646\u064e\u0627\u06dc\u06d7", "word_translation": "our sleeping place", "word_transliteration": "marqadina"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "This (is)", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u062f\u064e", "word_translation": "(had) promised", "word_transliteration": "wa'ada"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmanu"}, {"word_arabic_uthmani": "\u0648\u064e\u0635\u064e\u062f\u064e\u0642\u064e", "word_translation": "and told (the) truth", "word_transliteration": "wasadaqa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "the Messengers", "word_transliteration": "al-mur'saluna"}]}