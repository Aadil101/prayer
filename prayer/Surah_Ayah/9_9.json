{"ayahs": {"ayah_uthmani": "\u0671\u0634\u06e1\u062a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652 \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u062b\u064e\u0645\u064e\u0646\u0657\u0627 \u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627 \u0641\u064e\u0635\u064e\u062f\u0651\u064f\u0648\u0627\u0652 \u0639\u064e\u0646 \u0633\u064e\u0628\u0650\u064a\u0644\u0650\u0647\u0650\u06e6\u0653\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0633\u064e\u0627\u0653\u0621\u064e \u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0634\u06e1\u062a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652", "word_translation": "They exchange", "word_transliteration": "ish'taraw"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650", "word_translation": "[with] the Verses of Allah", "word_transliteration": "biayati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "[with] the Verses of Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u062b\u064e\u0645\u064e\u0646\u0657\u0627", "word_translation": "(for) a little price", "word_transliteration": "thamanan"}, {"word_arabic_uthmani": "\u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627", "word_translation": "(for) a little price", "word_transliteration": "qalilan"}, {"word_arabic_uthmani": "\u0641\u064e\u0635\u064e\u062f\u0651\u064f\u0648\u0627\u0652", "word_translation": "and they hinder (people)", "word_transliteration": "fasaddu"}, {"word_arabic_uthmani": "\u0639\u064e\u0646", "word_translation": "from", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u0650\u0647\u0650\u06e6\u0653\u06da", "word_translation": "His way", "word_transliteration": "sabilihi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Indeed", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0633\u064e\u0627\u0653\u0621\u064e", "word_translation": "evil", "word_transliteration": "saa"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "(is) what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they used to", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "do", "word_transliteration": "ya'maluna"}]}