{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646 \u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064d \u0625\u0650\u0644\u0651\u064e\u0627 \u0644\u0650\u064a\u064f\u0637\u064e\u0627\u0639\u064e \u0628\u0650\u0625\u0650\u0630\u06e1\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0648\u064e\u0644\u064e\u0648\u06e1 \u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0625\u0650\u0630 \u0638\u0651\u064e\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0647\u064f\u0645\u06e1 \u062c\u064e\u0627\u0653\u0621\u064f\u0648\u0643\u064e \u0641\u064e\u0671\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u064e\u0631\u064e \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064f \u0644\u064e\u0648\u064e\u062c\u064e\u062f\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u062a\u064e\u0648\u0651\u064e\u0627\u0628\u0657\u0627 \u0631\u0651\u064e\u062d\u0650\u064a\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We sent", "word_transliteration": "arsalna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064d", "word_translation": "Messenger", "word_transliteration": "rasulin"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u0637\u064e\u0627\u0639\u064e", "word_translation": "to be obeyed", "word_transliteration": "liyuta'a"}, {"word_arabic_uthmani": "\u0628\u0650\u0625\u0650\u0630\u06e1\u0646\u0650", "word_translation": "by (the) permission", "word_transliteration": "bi-idh'ni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "[that] they", "word_transliteration": "annahum"}, {"word_arabic_uthmani": "\u0625\u0650\u0630", "word_translation": "when", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0638\u0651\u064e\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652", "word_translation": "they wronged", "word_transliteration": "zalamu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0647\u064f\u0645\u06e1", "word_translation": "themselves", "word_transliteration": "anfusahum"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064f\u0648\u0643\u064e", "word_translation": "(had) come to you", "word_transliteration": "jauka"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "and asked forgiveness", "word_transliteration": "fa-is'taghfaru"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "(of) Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u064e\u0631\u064e", "word_translation": "and asked forgiveness", "word_transliteration": "wa-is'taghfara"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "for them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064f", "word_translation": "the Messenger", "word_transliteration": "al-rasulu"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u064e\u062c\u064e\u062f\u064f\u0648\u0627\u0652", "word_translation": "surely they would have found", "word_transliteration": "lawajadu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u062a\u064e\u0648\u0651\u064e\u0627\u0628\u0657\u0627", "word_translation": "Oft-Forgiving", "word_transliteration": "tawwaban"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u0657\u0627", "word_translation": "Most Merciful", "word_transliteration": "rahiman"}]}