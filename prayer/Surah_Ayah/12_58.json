{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062c\u064e\u0627\u0653\u0621\u064e \u0625\u0650\u062e\u06e1\u0648\u064e\u0629\u064f \u064a\u064f\u0648\u0633\u064f\u0641\u064e \u0641\u064e\u062f\u064e\u062e\u064e\u0644\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0641\u064e\u0639\u064e\u0631\u064e\u0641\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0647\u064f\u06e5 \u0645\u064f\u0646\u0643\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "And came", "word_transliteration": "wajaa"}, {"word_arabic_uthmani": "\u0625\u0650\u062e\u06e1\u0648\u064e\u0629\u064f", "word_translation": "(the) brothers", "word_transliteration": "ikh'watu"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0633\u064f\u0641\u064e", "word_translation": "(of) Yusuf", "word_transliteration": "yusufa"}, {"word_arabic_uthmani": "\u0641\u064e\u062f\u064e\u062e\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "and they entered", "word_transliteration": "fadakhalu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "upon him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0641\u064e\u0639\u064e\u0631\u064e\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and he recognized them", "word_transliteration": "fa'arafahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "but they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "knew him not", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u0646\u0643\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "knew him not", "word_transliteration": "munkiruna"}]}