{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u0625\u0650\u0646\u0651\u0650\u064a \u0644\u064e\u0627\u0653 \u0623\u064e\u0645\u06e1\u0644\u0650\u0643\u064f \u0644\u064e\u0643\u064f\u0645\u06e1 \u0636\u064e\u0631\u0651\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u0631\u064e\u0634\u064e\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0644\u064e\u0627\u0653", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0644\u0650\u0643\u064f", "word_translation": "possess", "word_transliteration": "amliku"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0636\u064e\u0631\u0651\u0657\u0627", "word_translation": "any harm", "word_transliteration": "darran"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0631\u064e\u0634\u064e\u062f\u0657\u0627", "word_translation": "right path", "word_transliteration": "rashadan"}]}