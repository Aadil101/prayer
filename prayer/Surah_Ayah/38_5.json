{"ayahs": {"ayah_uthmani": "\u0623\u064e\u062c\u064e\u0639\u064e\u0644\u064e \u0671\u0644\u06e1\u0623\u0653\u0644\u0650\u0647\u064e\u0629\u064e \u0625\u0650\u0644\u064e\u0670\u0647\u0657\u0627 \u0648\u064e\u0670\u062d\u0650\u062f\u064b\u0627\u06d6 \u0625\u0650\u0646\u0651\u064e \u0647\u064e\u0670\u0630\u064e\u0627 \u0644\u064e\u0634\u064e\u064a\u06e1\u0621\u064c \u0639\u064f\u062c\u064e\u0627\u0628\u065e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u062c\u064e\u0639\u064e\u0644\u064e", "word_translation": "Has he made", "word_transliteration": "aja'ala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u0644\u0650\u0647\u064e\u0629\u064e", "word_translation": "the gods", "word_transliteration": "al-alihata"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u0657\u0627", "word_translation": "(into) one god", "word_transliteration": "ilahan"}, {"word_arabic_uthmani": "\u0648\u064e\u0670\u062d\u0650\u062f\u064b\u0627\u06d6", "word_translation": "(into) one god", "word_transliteration": "wahidan"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "this", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0644\u064e\u0634\u064e\u064a\u06e1\u0621\u064c", "word_translation": "(is) certainly a thing", "word_transliteration": "lashayon"}, {"word_arabic_uthmani": "\u0639\u064f\u062c\u064e\u0627\u0628\u065e", "word_translation": "curious", "word_transliteration": "ujabun"}]}