{"ayahs": {"ayah_uthmani": "\u06de\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0641\u064e\u0627\u0644\u0650\u0642\u064f \u0671\u0644\u06e1\u062d\u064e\u0628\u0651\u0650 \u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0648\u064e\u0649\u0670\u06d6 \u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064f \u0671\u0644\u06e1\u062d\u064e\u064a\u0651\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064e\u064a\u0651\u0650\u062a\u0650 \u0648\u064e\u0645\u064f\u062e\u06e1\u0631\u0650\u062c\u064f \u0671\u0644\u06e1\u0645\u064e\u064a\u0651\u0650\u062a\u0650 \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u062d\u064e\u064a\u0651\u0650\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06d6 \u0641\u064e\u0623\u064e\u0646\u0651\u064e\u0649\u0670 \u062a\u064f\u0624\u06e1\u0641\u064e\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0641\u064e\u0627\u0644\u0650\u0642\u064f", "word_translation": "(is the) Cleaver", "word_transliteration": "faliqu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0628\u0651\u0650", "word_translation": "(of) the grain", "word_transliteration": "al-habi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0648\u064e\u0649\u0670\u06d6", "word_translation": "and the date-seed", "word_transliteration": "wal-nawa"}, {"word_arabic_uthmani": "\u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064f", "word_translation": "He brings forth", "word_transliteration": "yukh'riju"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u064a\u0651\u064e", "word_translation": "the living", "word_transliteration": "al-haya"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u064a\u0651\u0650\u062a\u0650", "word_translation": "the dead", "word_transliteration": "al-mayiti"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064f\u062e\u06e1\u0631\u0650\u062c\u064f", "word_translation": "and brings forth", "word_transliteration": "wamukh'riju"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u064a\u0651\u0650\u062a\u0650", "word_translation": "the dead", "word_transliteration": "al-mayiti"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u064a\u0651\u0650\u06da", "word_translation": "the living", "word_transliteration": "al-hayi"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u064f", "word_translation": "That", "word_transliteration": "dhalikumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06d6", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0646\u0651\u064e\u0649\u0670", "word_translation": "so how", "word_transliteration": "fa-anna"}, {"word_arabic_uthmani": "\u062a\u064f\u0624\u06e1\u0641\u064e\u0643\u064f\u0648\u0646\u064e", "word_translation": "are you deluded", "word_transliteration": "tu'fakuna"}]}