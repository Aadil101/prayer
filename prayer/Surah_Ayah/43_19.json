{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0647\u064f\u0645\u06e1 \u0639\u0650\u0628\u064e\u0670\u062f\u064f \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650 \u0625\u0650\u0646\u064e\u0670\u062b\u064b\u0627\u06da \u0623\u064e\u0634\u064e\u0647\u0650\u062f\u064f\u0648\u0627\u0652 \u062e\u064e\u0644\u06e1\u0642\u064e\u0647\u064f\u0645\u06e1\u06da \u0633\u064e\u062a\u064f\u0643\u06e1\u062a\u064e\u0628\u064f \u0634\u064e\u0647\u064e\u0670\u062f\u064e\u062a\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u064a\u064f\u0633\u06e1\u0640\u0654\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "And they made", "word_transliteration": "waja'alu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064e", "word_translation": "the Angels", "word_transliteration": "al-malaikata"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "themselves", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0670\u062f\u064f", "word_translation": "(are) slaves", "word_transliteration": "ibadu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650", "word_translation": "(of) the Most Gracious", "word_transliteration": "al-rahmani"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u064e\u0670\u062b\u064b\u0627\u06da", "word_translation": "females", "word_transliteration": "inathan"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u064e\u0647\u0650\u062f\u064f\u0648\u0627\u0652", "word_translation": "Did they witness", "word_transliteration": "ashahidu"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0642\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "their creation", "word_transliteration": "khalqahum"}, {"word_arabic_uthmani": "\u0633\u064e\u062a\u064f\u0643\u06e1\u062a\u064e\u0628\u064f", "word_translation": "Will be recorded", "word_transliteration": "satuk'tabu"}, {"word_arabic_uthmani": "\u0634\u064e\u0647\u064e\u0670\u062f\u064e\u062a\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their testimony", "word_transliteration": "shahadatuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0633\u06e1\u0640\u0654\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "and they will be questioned", "word_transliteration": "wayus'aluna"}]}