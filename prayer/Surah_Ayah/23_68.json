{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u062f\u0651\u064e\u0628\u0651\u064e\u0631\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u064e \u0623\u064e\u0645\u06e1 \u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645 \u0645\u0651\u064e\u0627 \u0644\u064e\u0645\u06e1 \u064a\u064e\u0623\u06e1\u062a\u0650 \u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0648\u0651\u064e\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Then, do not", "word_transliteration": "afalam"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u0651\u064e\u0628\u0651\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "they ponder", "word_transliteration": "yaddabbaru"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u064e", "word_translation": "the Word", "word_transliteration": "al-qawla"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645", "word_translation": "has come to them", "word_transliteration": "jaahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650", "word_translation": "(had) come", "word_transliteration": "yati"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u064f", "word_translation": "(to) their forefathers", "word_transliteration": "abaahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0648\u0651\u064e\u0644\u0650\u064a\u0646\u064e", "word_translation": "(to) their forefathers", "word_transliteration": "al-awalina"}]}