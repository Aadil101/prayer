{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0623\u064e\u0648\u06e1\u0633\u064e\u0637\u064f\u0647\u064f\u0645\u06e1 \u0623\u064e\u0644\u064e\u0645\u06e1 \u0623\u064e\u0642\u064f\u0644 \u0644\u0651\u064e\u0643\u064f\u0645\u06e1 \u0644\u064e\u0648\u06e1\u0644\u064e\u0627 \u062a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0633\u064e\u0637\u064f\u0647\u064f\u0645\u06e1", "word_translation": "(the) most moderate of them", "word_transliteration": "awsatuhum"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Did not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u0623\u064e\u0642\u064f\u0644", "word_translation": "I tell", "word_transliteration": "aqul"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "Why not", "word_transliteration": "lawla"}, {"word_arabic_uthmani": "\u062a\u064f\u0633\u064e\u0628\u0651\u0650\u062d\u064f\u0648\u0646\u064e", "word_translation": "you glorify (Allah)", "word_transliteration": "tusabbihuna"}]}