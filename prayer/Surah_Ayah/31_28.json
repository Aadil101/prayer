{"ayahs": {"ayah_uthmani": "\u0645\u0651\u064e\u0627 \u062e\u064e\u0644\u06e1\u0642\u064f\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u0628\u064e\u0639\u06e1\u062b\u064f\u0643\u064f\u0645\u06e1 \u0625\u0650\u0644\u0651\u064e\u0627 \u0643\u064e\u0646\u064e\u0641\u06e1\u0633\u0656 \u0648\u064e\u0670\u062d\u0650\u062f\u064e\u0629\u064d\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0633\u064e\u0645\u0650\u064a\u0639\u064f\u06e2 \u0628\u064e\u0635\u0650\u064a\u0631\u064c"}, "words": [{"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0642\u064f\u0643\u064f\u0645\u06e1", "word_translation": "(is) your creation", "word_transliteration": "khalqukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062b\u064f\u0643\u064f\u0645\u06e1", "word_translation": "your resurrection", "word_transliteration": "ba'thukum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0643\u064e\u0646\u064e\u0641\u06e1\u0633\u0656", "word_translation": "as a soul", "word_transliteration": "kanafsin"}, {"word_arabic_uthmani": "\u0648\u064e\u0670\u062d\u0650\u062f\u064e\u0629\u064d\u06da", "word_translation": "single", "word_transliteration": "wahidatin"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0633\u064e\u0645\u0650\u064a\u0639\u064f\u06e2", "word_translation": "(is) All-Hearer", "word_transliteration": "sami'un"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u0650\u064a\u0631\u064c"