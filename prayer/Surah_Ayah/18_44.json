{"ayahs": {"ayah_uthmani": "\u0647\u064f\u0646\u064e\u0627\u0644\u0650\u0643\u064e \u0671\u0644\u06e1\u0648\u064e\u0644\u064e\u0670\u064a\u064e\u0629\u064f \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650\u06da \u0647\u064f\u0648\u064e \u062e\u064e\u064a\u06e1\u0631\u065e \u062b\u064e\u0648\u064e\u0627\u0628\u0657\u0627 \u0648\u064e\u062e\u064e\u064a\u06e1\u0631\u064c \u0639\u064f\u0642\u06e1\u0628\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0647\u064f\u0646\u064e\u0627\u0644\u0650\u0643\u064e", "word_translation": "There", "word_transliteration": "hunalika"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0648\u064e\u0644\u064e\u0670\u064a\u064e\u0629\u064f", "word_translation": "the protection", "word_transliteration": "al-walayatu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "(is) from Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650\u06da", "word_translation": "the True", "word_transliteration": "al-haqi"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u065e", "word_translation": "(is the) best", "word_transliteration": "khayrun"}, {"word_arabic_uthmani": "\u062b\u064e\u0648\u064e\u0627\u0628\u0657\u0627", "word_translation": "(to) reward", "word_transliteration": "thawaban"}, {"word_arabic_uthmani": "\u0648\u064e\u062e\u064e\u064a\u06e1\u0631\u064c", "word_translation": "and (the) best", "word_transliteration": "wakhayrun"}, {"word_arabic_uthmani": "\u0639\u064f\u0642\u06e1\u0628\u0657\u0627", "word_translation": "(for) the final end", "word_transliteration": "uq'ban"}]}