{"ayahs": {"ayah_uthmani": "\u06de\u0641\u064e\u0645\u064e\u0627 \u0644\u064e\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e \u0641\u0650\u0626\u064e\u062a\u064e\u064a\u06e1\u0646\u0650 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u0631\u06e1\u0643\u064e\u0633\u064e\u0647\u064f\u0645 \u0628\u0650\u0645\u064e\u0627 \u0643\u064e\u0633\u064e\u0628\u064f\u0648\u0653\u0627\u0652\u06da \u0623\u064e\u062a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e \u0623\u064e\u0646 \u062a\u064e\u0647\u06e1\u062f\u064f\u0648\u0627\u0652 \u0645\u064e\u0646\u06e1 \u0623\u064e\u0636\u064e\u0644\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06d6 \u0648\u064e\u0645\u064e\u0646 \u064a\u064f\u0636\u06e1\u0644\u0650\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0641\u064e\u0644\u064e\u0646 \u062a\u064e\u062c\u0650\u062f\u064e \u0644\u064e\u0647\u064f\u06e5 \u0633\u064e\u0628\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u06de\u0641\u064e\u0645\u064e\u0627", "word_translation": "So what", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(is the matter) with you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "concerning", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0646\u064e\u0670\u0641\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "the hypocrites (that)", "word_transliteration": "al-munafiqina"}, {"word_arabic_uthmani": "\u0641\u0650\u0626\u064e\u062a\u064e\u064a\u06e1\u0646\u0650", "word_translation": "(you have become) two parties", "word_transliteration": "fi-atayni"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "While Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0643\u064e\u0633\u064e\u0647\u064f\u0645", "word_translation": "cast them back", "word_transliteration": "arkasahum"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "for what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0643\u064e\u0633\u064e\u0628\u064f\u0648\u0653\u0627\u0652\u06da", "word_translation": "they earned", "word_transliteration": "kasabu"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e", "word_translation": "Do you wish", "word_transliteration": "aturiduna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u0647\u06e1\u062f\u064f\u0648\u0627\u0652", "word_translation": "you guide", "word_transliteration": "tahdu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u06e1", "word_translation": "whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0623\u064e\u0636\u064e\u0644\u0651\u064e", "word_translation": "is let astray", "word_transliteration": "adalla"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06d6", "word_translation": "(by) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "And whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u064a\u064f\u0636\u06e1\u0644\u0650\u0644\u0650", "word_translation": "is let astray", "word_transliteration": "yud'lili"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(by) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0646", "word_translation": "then never", "word_transliteration": "falan"}, {"word_arabic_uthmani": "\u062a\u064e\u062c\u0650\u062f\u064e", "word_translation": "will you find", "word_transliteration": "tajida"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u0657\u0627", "word_translation": "a way", "word_transliteration": "sabilan"}]}