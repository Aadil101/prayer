{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0648\u0633\u064e\u0649\u0670 \u0648\u064e\u064a\u06e1\u0644\u064e\u0643\u064f\u0645\u06e1 \u0644\u064e\u0627 \u062a\u064e\u0641\u06e1\u062a\u064e\u0631\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0643\u064e\u0630\u0650\u0628\u0657\u0627 \u0641\u064e\u064a\u064f\u0633\u06e1\u062d\u0650\u062a\u064e\u0643\u064f\u0645 \u0628\u0650\u0639\u064e\u0630\u064e\u0627\u0628\u0656\u06d6 \u0648\u064e\u0642\u064e\u062f\u06e1 \u062e\u064e\u0627\u0628\u064e \u0645\u064e\u0646\u0650 \u0671\u0641\u06e1\u062a\u064e\u0631\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "to them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u06e1\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "Woe to you!", "word_transliteration": "waylakum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u06e1\u062a\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "invent", "word_transliteration": "taftaru"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "against", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0650\u0628\u0657\u0627", "word_translation": "a lie", "word_transliteration": "kadhiban"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064f\u0633\u06e1\u062d\u0650\u062a\u064e\u0643\u064