{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u064a\u064e\u0670\u0647\u064e\u0670\u0631\u064f\u0648\u0646\u064f \u0645\u064e\u0627 \u0645\u064e\u0646\u064e\u0639\u064e\u0643\u064e \u0625\u0650\u0630\u06e1 \u0631\u064e\u0623\u064e\u064a\u06e1\u062a\u064e\u0647\u064f\u0645\u06e1 \u0636\u064e\u0644\u0651\u064f\u0648\u0653\u0627\u0652"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0647\u064e\u0670\u0631\u064f\u0648\u0646\u064f", "word_translation": "O Harun!", "word_transliteration": "yaharunu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "What", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u064e\u0639\u064e\u0643\u064e", "word_translation": "prevented you", "word_transliteration": "mana'aka"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "when", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0631\u064e\u0623\u064e\u064a\u06e1\u062a\u064e\u0647\u064f\u0645\u06e1", "word_translation": "you saw them", "word_transliteration": "ra-aytahum"}, {"word_arabic_uthmani": "\u0636\u064e\u0644\u0651\u064f\u0648\u0653\u0627\u0652", "word_translation": "going astray", "word_transliteration": "dallu"}]}