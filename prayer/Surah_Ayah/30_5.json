{"ayahs": {"ayah_uthmani": "\u0628\u0650\u0646\u064e\u0635\u06e1\u0631\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u064a\u064e\u0646\u0635\u064f\u0631\u064f \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06d6 \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f \u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0628\u0650\u0646\u064e\u0635\u06e1\u0631\u0650", "word_translation": "With (the) help", "word_transliteration": "binasri"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u0635\u064f\u0631\u064f", "word_translation": "He helps", "word_transliteration": "yansuru"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06d6", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f", "word_translation": "(is) the All-Mighty", "word_transliteration": "al-'azizu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f", "word_translation": "the Most Merciful", "word_transliteration": "al-rahimu"}]}