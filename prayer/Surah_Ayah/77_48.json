{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u064e\u0627 \u0642\u0650\u064a\u0644\u064e \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0631\u06e1\u0643\u064e\u0639\u064f\u0648\u0627\u0652 \u0644\u064e\u0627 \u064a\u064e\u0631\u06e1\u0643\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u064e\u0627", "word_translation": "And when", "word_transliteration": "wa-idha"}, {"word_arabic_uthmani": "\u0642\u0650\u064a\u0644\u064e", "word_translation": "it is said", "word_transliteration": "qila"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "to them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0631\u06e1\u0643\u064e\u0639\u064f\u0648\u0627\u0652", "word_translation": "Bow", "word_transliteration": "ir'ka'u"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u0643\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "they bow", "word_transliteration": "yarka'una"}]}