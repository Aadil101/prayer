{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0671\u0636\u06e1\u0631\u0650\u0628\u06e1 \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u064e\u062b\u064e\u0644\u0657\u0627 \u0631\u0651\u064e\u062c\u064f\u0644\u064e\u064a\u06e1\u0646\u0650 \u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627 \u0644\u0650\u0623\u064e\u062d\u064e\u062f\u0650\u0647\u0650\u0645\u064e\u0627 \u062c\u064e\u0646\u0651\u064e\u062a\u064e\u064a\u06e1\u0646\u0650 \u0645\u0650\u0646\u06e1 \u0623\u064e\u0639\u06e1\u0646\u064e\u0670\u0628\u0656 \u0648\u064e\u062d\u064e\u0641\u064e\u0641\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u064e\u0627 \u0628\u0650\u0646\u064e\u062e\u06e1\u0644\u0656 \u0648\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627 \u0632\u064e\u0631\u06e1\u0639\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0671\u0636\u06e1\u0631\u0650\u0628\u06e1", "word_translation": "And set forth", "word_transliteration": "wa-id'rib"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "to them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u062b\u064e\u0644\u0657\u0627", "word_translation": "the example", "word_transliteration": "mathalan"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u062c\u064f\u0644\u064e\u064a\u06e1\u0646\u0650", "word_translation": "of two men:", "word_transliteration": "rajulayni"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We provided", "word_transliteration": "ja'alna"}, {"word_arabic_uthmani": "\u0644\u0650\u0623\u064e\u062d\u064e\u062f\u0650\u0647\u0650\u0645\u064e\u0627", "word_translation": "for one of them", "word_transliteration": "li-ahadihima"}, {"word_arabic_uthmani": "\u062c\u064e\u0646\u0651\u064e\u062a\u064e\u064a\u06e1\u0646\u0650", "word_translation": "two gardens", "word_transliteration": "jannatayni"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0646\u064e\u0670\u0628\u0656", "word_translation": "grapes", "word_transliteration": "a'nabin"}, {"word_arabic_uthmani": "\u0648\u064e\u062d\u064e\u0641\u064e\u0641\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u064e\u0627", "word_translation": "and We bordered them", "word_transliteration": "wahafafnahuma"}, {"word_arabic_uthmani": "\u0628\u0650\u0646\u064e\u062e\u06e1\u0644\u0656", "word_translation": "with date-palms", "word_transliteration": "binakhlin"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "and We placed", "word_transliteration": "waja'alna"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627", "word_translation": "between both of them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0632\u064e\u0631\u06e1\u0639\u0657\u0627", "word_translation": "crops", "word_transliteration": "zar'an"}]}