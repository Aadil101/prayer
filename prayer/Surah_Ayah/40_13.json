{"ayahs": {"ayah_uthmani": "\u0647\u064f\u0648\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u064a\u064f\u0631\u0650\u064a\u0643\u064f\u0645\u06e1 \u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0647\u0650\u06e6 \u0648\u064e\u064a\u064f\u0646\u064e\u0632\u0651\u0650\u0644\u064f \u0644\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650 \u0631\u0650\u0632\u06e1\u0642\u0657\u0627\u06da \u0648\u064e\u0645\u064e\u0627 \u064a\u064e\u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064e\u0646 \u064a\u064f\u0646\u0650\u064a\u0628\u064f"}, "words": [{"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u0643\u064f\u0645\u06e1", "word_translation": "shows you", "word_transliteration": "yurikum"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0647\u0650\u06e6", "word_translation": "His Signs", "word_transliteration": "ayatihi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0646\u064e\u0632\u0651\u0650\u0644\u064f", "word_translation": "and sends down", "word_transliteration": "wayunazzilu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650", "word_translation": "the sky", "word_transliteration": "al-samai"}, {"word_arabic_uthmani": "\u0631\u0650\u0632\u06e1\u0642\u0657\u0627\u06da", "word_translation": "provision", "word_transliteration": "riz'qan"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "But (does) not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0630\u064e\u0643\u0651\u064e\u0631\u064f", "word_translation": "take heed", "word_transliteration": "yatadhakkaru"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(one) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064f\u0646\u0650\u064a\u0628\u064f", "word_translation": "turns", "word_transliteration": "yunibu"}]}