{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0631\u0650\u0647\u064f\u0648\u0627\u0652 \u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u0632\u064e\u0644\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0641\u064e\u0623\u064e\u062d\u06e1\u0628\u064e\u0637\u064e \u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064e\u0647\u064f\u0645\u06e1"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "(is) because they", "word_transliteration": "bi-annahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0631\u0650\u0647\u064f\u0648\u0627\u0652", "word_translation": "hate", "word_transliteration": "karihu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0632\u064e\u0644\u064e", "word_translation": "Allah has revealed", "word_transliteration": "anzala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah has revealed", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u062d\u06e1\u0628\u064e\u0637\u064e", "word_translation": "so He has made worthless", "word_transliteration": "fa-ahbata"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their deeds", "word_transliteration": "a'malahum"}]}