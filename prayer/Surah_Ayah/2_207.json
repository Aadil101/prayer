{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0645\u064e\u0646 \u064a\u064e\u0634\u06e1\u0631\u0650\u064a \u0646\u064e\u0641\u06e1\u0633\u064e\u0647\u064f \u0671\u0628\u06e1\u062a\u0650\u063a\u064e\u0627\u0653\u0621\u064e \u0645\u064e\u0631\u06e1\u0636\u064e\u0627\u062a\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0631\u064e\u0621\u064f\u0648\u0641\u064f\u06e2 \u0628\u0650\u0671\u0644\u06e1\u0639\u0650\u0628\u064e\u0627\u062f\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u064e", "word_translation": "And of", "word_transliteration": "wamina"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "the people", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(is the one) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0631\u0650\u064a", "word_translation": "sells", "word_transliteration": "yashri"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0633\u064e\u0647\u064f", "word_translation": "his own self", "word_transliteration": "nafsahu"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u062a\u0650\u063a\u064e\u0627\u0653\u0621\u064e", "word_translation": "seeking", "word_transliteration": "ib'tighaa"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u06e1\u0636\u064e\u0627\u062a\u0650", "word_translation": "pleasure", "word_transliteration": "mardati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0631\u064e\u0621\u064f\u0648\u0641\u064f\u06e2", "word_translation": "(is) full of Kindness", "word_transliteration": "raufun"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0639\u0650\u0628\u064e\u0627\u062f\u0650", "word_translation": "to His servants", "word_transliteration": "bil-'ibadi"}]}