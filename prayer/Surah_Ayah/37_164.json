{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627  \u0645\u0650\u0646\u0651\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0644\u064e\u0647\u064f\u06e5 \u0645\u064e\u0642\u064e\u0627\u0645\u065e \u0645\u0651\u064e\u0639\u06e1\u0644\u064f\u0648\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627 ", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0651\u064e\u0627\u0653", "word_translation": "among us", "word_transliteration": "minna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0642\u064e\u0627\u0645\u065e", "word_translation": "(is) a position", "word_transliteration": "maqamun"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0639\u06e1\u0644\u064f\u0648\u0645\u065e", "word_translation": "known", "word_transliteration": "ma'lumun"}]}