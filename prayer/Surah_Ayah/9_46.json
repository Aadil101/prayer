{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0648\u06e1 \u0623\u064e\u0631\u064e\u0627\u062f\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u062e\u064f\u0631\u064f\u0648\u062c\u064e \u0644\u064e\u0623\u064e\u0639\u064e\u062f\u0651\u064f\u0648\u0627\u0652 \u0644\u064e\u0647\u064f\u06e5 \u0639\u064f\u062f\u0651\u064e\u0629\u0657 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646 \u0643\u064e\u0631\u0650\u0647\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0646\u06e2\u0628\u0650\u0639\u064e\u0627\u062b\u064e\u0647\u064f\u0645\u06e1 \u0641\u064e\u062b\u064e\u0628\u0651\u064e\u0637\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0642\u0650\u064a\u0644\u064e \u0671\u0642\u06e1\u0639\u064f\u062f\u064f\u0648\u0627\u0652 \u0645\u064e\u0639\u064e \u0671\u0644\u06e1\u0642\u064e\u0670\u0639\u0650\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u064e\u0627\u062f\u064f\u0648\u0627\u0652", "word_translation": "they had wished", "word_transliteration": "aradu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064f\u0631\u064f\u0648\u062c\u064e", "word_translation": "(to) go forth", "word_transliteration": "al-khuruja"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u064e\u0639\u064e\u062f\u0651\u064f\u0648\u0627\u0652", "word_translation": "surely they (would) have prepared", "word_transliteration": "la-a'addu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "for it", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0639\u064f\u062f\u0651\u064e\u0629\u0657", "word_translation": "(some) preparation", "word_transliteration": "uddatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646", "word_translation": "But", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u0643\u064e\u0631\u0650\u0647\u064e", "word_translation": "Allah disliked", "word_transliteration": "kariha"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah disliked", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0646\u06e2\u0628\u0650\u0639\u064e\u0627\u062b\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their being sent", "word_transliteration": "inbi'athahum"}, {"word_arabic_uthmani": "\u0641\u064e\u062b\u064e\u0628\u0651\u064e\u0637\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so He made them lag behind", "word_transliteration": "fathabbatahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u0650\u064a\u0644\u064e", "word_translation": "and it was said", "word_transliteration": "waqila"}, {"word_arabic_uthmani": "\u0671\u0642\u06e1\u0639\u064f\u062f\u064f\u0648\u0627\u0652", "word_translation": "Sit", "word_transliteration": "uq'udu"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e", "word_translation": "with", "word_transliteration": "ma'a"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0670\u0639\u0650\u062f\u0650\u064a\u0646\u064e", "word_translation": "those who sit", "word_transliteration": "al-qa'idina"}]}