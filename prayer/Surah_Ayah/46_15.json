": "\u0641\u0650\u064a", "word_translation": "among", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0630\u064f\u0631\u0651\u0650\u064a\u0651\u064e\u062a\u0650\u064a\u0653\u06d6", "word_translation": "my offspring", "word_transliteration": "dhurriyyati"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "indeed", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u062a\u064f\u0628\u06e1\u062a\u064f", "word_translation": "I turn", "word_transliteration": "tub'tu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e", "word_translation": "to You", "word_transliteration": "ilayka"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "and indeed I am", "word_transliteration": "wa-inni"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0633\u06e1\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "those who submit", "word_transliteration": "al-mus'limina"}]}