_transliteration": "wahamluhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u0650\u0635\u064e\u0670\u0644\u064f\u0647\u064f\u06e5", "word_translation": "and (the) weaning of him", "word_transliteration": "wafisaluhu"}, {"word_arabic_uthmani": "\u062b\u064e\u0644\u064e\u0670\u062b\u064f\u0648\u0646\u064e", "word_translation": "(is) thirty", "word_transliteration": "thalathuna"}, {"word_arabic_uthmani": "\u0634\u064e\u0647\u06e1\u0631\u064b\u0627\u06da", "word_translation": "month(s)", "word_transliteration": "shahran"}, {"word_arabic_uthmani": "\u062d\u064e\u062a\u0651\u064e\u0649\u0670\u0653", "word_translation": "until", "word_transliteration": "hatta"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u064e\u063a\u064e", "word_translation": "he reaches", "word_transliteration": "balagha"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u064f\u062f\u0651\u064e\u0647\u064f\u06e5", "word_translation": "his maturity", "word_transliteration": "ashuddahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u0644\u064e\u063a\u064e", "word_translation": "and reaches", "word_transliteration": "wabalagha"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0628\u064e\u0639\u0650\u064a\u0646\u064e", "word_translation": "forty", "word_transliteration": "arba'ina"}, {"word_arabic_uthmani": "\u0633\u064e\u0646\u064e\u0629\u0657", "word_translation": "year(s)", "word_transliteration": "sanatan"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "he says", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "My Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0632\u0650\u0639\u06e1\u0646\u0650\u064a\u0653", "word_translation": "grant me (the) power", "word_transliteration": "awzi'ni"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "wo