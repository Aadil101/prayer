{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u0650\u0641\u064f\u0648\u0647\u064f\u0645\u06e1\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645 \u0645\u0651\u064e\u0633\u06e1\u0640\u0654\u064f\u0648\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u0650\u0641\u064f\u0648\u0647\u064f\u0645\u06e1\u06d6", "word_translation": "And stop them", "word_transliteration": "waqifuhum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645", "word_translation": "indeed they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0633\u06e1\u0640\u0654\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "(are) to be questioned", "word_transliteration": "masuluna"}]}