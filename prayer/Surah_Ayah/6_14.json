{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u0623\u064e\u063a\u064e\u064a\u06e1\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0623\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u064f \u0648\u064e\u0644\u0650\u064a\u0651\u0657\u0627 \u0641\u064e\u0627\u0637\u0650\u0631\u0650 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0647\u064f\u0648\u064e \u064a\u064f\u0637\u06e1\u0639\u0650\u0645\u064f \u0648\u064e\u0644\u064e\u0627 \u064a\u064f\u0637\u06e1\u0639\u064e\u0645\u064f\u06d7 \u0642\u064f\u0644\u06e1 \u0625\u0650\u0646\u0651\u0650\u064a\u0653 \u0623\u064f\u0645\u0650\u0631\u06e1\u062a\u064f \u0623\u064e\u0646\u06e1 \u0623\u064e\u0643\u064f\u0648\u0646\u064e \u0623\u064e\u0648\u0651\u064e\u0644\u064e \u0645\u064e\u0646\u06e1 \u0623\u064e\u0633\u06e1\u0644\u064e\u0645\u064e\u06d6 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0643\u064f\u0648\u0646\u064e\u0646\u0651\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0634\u06e1\u0631\u0650\u0643\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0623\u064e\u063a\u064e\u064a\u06e1\u0631\u064e", "word_translation": "Is it other than", "word_transliteration": "aghayra"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u0651\u064e\u062e\u0650\u0630\u064f", "word_translation": "I (should) take", "word_transliteration": "attakhidhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u0651\u0657\u0627", "word_translation": "(as) a protector", "word_transliteration": "waliyyan"}, {"word_arabic_uthmani": "\u0641\u064e\u0627\u0637\u0650\u0631\u0650", "word_translation": "Creator", "word_transliteration": "fatiri"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "while (it is) He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u064a\u064f\u0637\u06e1\u0639\u0650\u0645\u064f", "word_translation": "Who feeds", "word_transliteration": "yut'imu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064f\u0637\u06e1\u0639\u064e\u0645\u064f\u06d7", "word_translation": "He is fed", "word_transliteration": "yut'amu"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0650\u0631\u06e1\u062a\u064f", "word_translation": "[I] am commanded", "word_transliteration": "umir'tu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u064f\u0648\u0646\u064e", "word_translation": "I be", "word_transliteration": "akuna"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u0651\u064e\u0644\u064e", "word_translation": "(the) first", "word_transliteration": "awwala"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u06e1", "word_translation": "who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u0644\u064e\u0645\u064e\u06d6", "word_translation": "submits (to Allah)", "word_transliteration": "aslama"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u064f\u0648\u0646\u064e\u0646\u0651\u064e", "word_translation": "be", "word_transliteration": "takunanna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0634\u06e1\u0631\u0650\u0643\u0650\u064a\u0646\u064e", "word_translation": "the polytheists", "word_transliteration": "al-mush'rikina"}]}