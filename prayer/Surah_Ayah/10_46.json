{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0645\u0651\u064e\u0627 \u0646\u064f\u0631\u0650\u064a\u064e\u0646\u0651\u064e\u0643\u064e \u0628\u064e\u0639\u06e1\u0636\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0646\u064e\u0639\u0650\u062f\u064f\u0647\u064f\u0645\u06e1 \u0623\u064e\u0648\u06e1 \u0646\u064e\u062a\u064e\u0648\u064e\u0641\u0651\u064e\u064a\u064e\u0646\u0651\u064e\u0643\u064e \u0641\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0646\u064e\u0627 \u0645\u064e\u0631\u06e1\u062c\u0650\u0639\u064f\u0647\u064f\u0645\u06e1 \u062b\u064f\u0645\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0634\u064e\u0647\u0650\u064a\u062f\u064c \u0639\u064e\u0644\u064e\u0649\u0670 \u0645\u064e\u0627 \u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0645\u0651\u064e\u0627", "word_translation": "And whether", "word_transliteration": "wa-imma"}, {"word_arabic_uthmani": "\u0646\u064f\u0631\u0650\u064a\u064e\u0646\u0651\u064e\u0643\u064e", "word_translation": "We show you", "word_transliteration": "nuriyannaka"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064e", "word_translation": "some", "word_transliteration": "ba'da"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(of) that which", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0646\u064e\u0639\u0650\u062f\u064f\u0647\u064f\u0645\u06e1", "word_translation": "We promised them", "word_transliteration": "na'iduhum"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0646\u064e\u062a\u064e\u0648\u064e\u0641\u0651\u064e\u064a\u064e\u0646\u0651\u064e\u0643\u064e", "word_translation": "We cause you to die", "word_transliteration": "natawaffayannaka"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "then to Us", "word_transliteration": "fa-ilayna"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u06e1\u062c\u0650\u0639\u064f\u0647\u064f\u0645\u06e1", "word_translation": "(is) their return", "word_transliteration": "marji'uhum"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0634\u064e\u0647\u0650\u064a\u062f\u064c", "word_translation": "(is) a Witness", "word_transliteration": "shahidun"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "they do", "word_transliteration": "yaf 'aluna"}]}