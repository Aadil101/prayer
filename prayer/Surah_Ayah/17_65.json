{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0639\u0650\u0628\u064e\u0627\u062f\u0650\u064a \u0644\u064e\u064a\u06e1\u0633\u064e \u0644\u064e\u0643\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u065e\u06da \u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0648\u064e\u0643\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u064a", "word_translation": "My slaves", "word_transliteration": "ibadi"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "not", "word_transliteration": "laysa"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e", "word_translation": "for you", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "over them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u065e\u06da", "word_translation": "any authority", "word_transliteration": "sul'tanun"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "And sufficient", "word_transliteration": "wakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "(is) your Lord", "word_transliteration": "birabbika"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u0650\u064a\u0644\u0657\u0627", "word_translation": "(as) a Guardian", "word_transliteration": "wakilan"}]}