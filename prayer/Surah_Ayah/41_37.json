{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1 \u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0647\u0650 \u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u064f \u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u064f \u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u064f \u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064f\u06da \u0644\u064e\u0627 \u062a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0627\u0652 \u0644\u0650\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u0650 \u0648\u064e\u0644\u064e\u0627 \u0644\u0650\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u0650 \u0648\u064e\u0671\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0627\u0652\u06e4 \u0644\u0650\u0644\u0651\u064e\u0647\u0650\u06e4 \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u062e\u064e\u0644\u064e\u0642\u064e\u0647\u064f\u0646\u0651\u064e \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u0625\u0650\u064a\u0651\u064e\u0627\u0647\u064f \u062a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1", "word_translation": "And of", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0647\u0650", "word_translation": "His Signs", "word_transliteration": "ayatihi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u064a\u06e1\u0644\u064f", "word_translation": "(are) the night", "word_transliteration": "al-laylu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0646\u0651\u064e\u0647\u064e\u0627\u0631\u064f", "word_translation": "and the day", "word_transliteration": "wal-naharu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u064f", "word_translation": "and the sun", "word_transliteration": "wal-shamsu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064f\u06da", "word_translation": "and the moon", "word_transliteration": "wal-qamaru"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0627\u0652", "word_translation": "prostrate", "word_transliteration": "tasjudu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u0650", "word_translation": "to the sun", "word_transliteration": "lilshamsi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u0650", "word_translation": "to the moon", "word_transliteration": "lil'qamari"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0627\u0652\u06e4", "word_translation": "but prostrate", "word_transliteration": "wa-us'judu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650\u06e4", "word_translation": "to Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064e\u0647\u064f\u0646\u0651\u064e", "word_translation": "created them", "word_transliteration": "khalaqahunna"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645\u06e1", "word_translation": "you", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0625\u0650\u064a\u0651\u064e\u0627\u0647\u064f", "word_translation": "Him alone", "word_transliteration": "iyyahu"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u064e", "word_translation": "worship", "word_transliteration": "ta'buduna"}]}