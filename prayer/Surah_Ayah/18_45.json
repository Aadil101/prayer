ord_translation": "And Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0642\u06e1\u062a\u064e\u062f\u0650\u0631\u064b\u0627", "word_translation": "(is) All Able", "word_transliteration": "muq'tadiran"}]}