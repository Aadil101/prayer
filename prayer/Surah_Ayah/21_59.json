{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0645\u064e\u0646 \u0641\u064e\u0639\u064e\u0644\u064e \u0647\u064e\u0670\u0630\u064e\u0627 \u0628\u0650\u0640\u0654\u064e\u0627\u0644\u0650\u0647\u064e\u062a\u0650\u0646\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u0645\u0650\u0646\u064e \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "Who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0641\u064e\u0639\u064e\u0644\u064e", "word_translation": "(has) done", "word_transliteration": "fa'ala"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "this", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u0644\u0650\u0647\u064e\u062a\u0650\u0646\u064e\u0627\u0653", "word_translation": "to our gods", "word_transliteration": "bialihatina"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed he", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u0650\u0646\u064e", "word_translation": "(is) of", "word_transliteration": "lamina"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimina"}]}