{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652 \u0643\u064e\u0645\u06e1 \u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0627 \u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0642\u064f\u0631\u064f\u0648\u0646\u0650 \u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0631\u06e1\u062c\u0650\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Do not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652", "word_translation": "they see", "word_transliteration": "yaraw"}, {"word_arabic_uthmani": "\u0643\u064e\u0645\u06e1", "word_translation": "how many", "word_transliteration": "kam"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0627", "word_translation": "We destroyed", "word_transliteration": "ahlakna"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645", "word_translation": "before them", "word_transliteration": "qablahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0631\u064f\u0648\u0646\u0650", "word_translation": "the generations", "word_transliteration": "al-quruni"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "That they", "word_transliteration": "annahum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "to them", "word_transliteration": "ilayhim"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "will not return", "word_transliteration": "la"}, {"word_arabic_uthmani":