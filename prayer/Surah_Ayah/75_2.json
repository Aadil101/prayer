{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627\u0653 \u0623\u064f\u0642\u06e1\u0633\u0650\u0645\u064f \u0628\u0650\u0671\u0644\u0646\u0651\u064e\u0641\u06e1\u0633\u0650 \u0671\u0644\u0644\u0651\u064e\u0648\u0651\u064e\u0627\u0645\u064e\u0629\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627\u0653", "word_translation": "And nay!", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0623\u064f\u0642\u06e1\u0633\u0650\u0645\u064f", "word_translation": "I swear", "word_transliteration": "uq'simu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0646\u0651\u064e\u0641\u06e1\u0633\u0650", "word_translation": "by the soul", "word_transliteration": "bil-nafsi"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0648\u0651\u064e\u0627\u0645\u064e\u0629\u0650", "word_translation": "self-accusing", "word_transliteration": "al-lawamati"}]}