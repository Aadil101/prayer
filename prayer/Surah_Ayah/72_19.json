{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u0645\u0651\u064e\u0627 \u0642\u064e\u0627\u0645\u064e \u0639\u064e\u0628\u06e1\u062f\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u064a\u064e\u062f\u06e1\u0639\u064f\u0648\u0647\u064f \u0643\u064e\u0627\u062f\u064f\u0648\u0627\u0652 \u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0646\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0644\u0650\u0628\u064e\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "And that", "word_transliteration": "wa-annahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "when", "word_transliteration": "lamma"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0645\u064e", "word_translation": "stood up", "word_transliteration": "qama"}, {"word_arabic_uthmani": "\u0639\u064e\u0628\u06e1\u062f\u064f", "word_translation": "(the) slave", "word_transliteration": "abdu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u06e1\u0639\u064f\u0648\u0647\u064f", "word_translation": "calling (upon) Him", "word_transliteration": "yad'uhu"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u062f\u064f\u0648\u0627\u0652", "word_translation": "they almost", "word_transliteration": "kadu"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0646\u064e", "word_translation": "became", "word_transliteration": "yakununa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "around him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0644\u0650\u0628\u064e\u062f\u0657\u0627", "word_translation": "a compacted mass", "word_transliteration": "libadan"}]}