{"ayahs": {"ayah_uthmani": "\u062e\u064e\u062a\u064e\u0645\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u0649\u0670 \u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0639\u064e\u0644\u064e\u0649\u0670 \u0633\u064e\u0645\u06e1\u0639\u0650\u0647\u0650\u0645\u06e1\u06d6 \u0648\u064e\u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u0650\u0647\u0650\u0645\u06e1 \u063a\u0650\u0634\u064e\u0670\u0648\u064e\u0629\u065e\u06d6 \u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064c \u0639\u064e\u0638\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u062e\u064e\u062a\u064e\u0645\u064e", "word_translation": "Has set a seal", "word_transliteration": "khatama"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their hearts", "word_transliteration": "qulubihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "and on", "word_transliteration": "wa'ala"}, {"word_arabic_uthmani": "\u0633\u064e\u0645\u06e1\u0639\u0650\u0647\u0650\u0645\u06e1\u06d6", "word_translation": "their hearing", "word_transliteration": "sam'ihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "and on", "word_transliteration": "wa'ala"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their vision", "word_transliteration": "absarihim"}, {"word_arabic_uthmani": "\u063a\u0650\u0634\u064e\u0670\u0648\u064e\u0629\u065e\u06d6", "word_translation": "(is) a veil", "word_transliteration": "ghishawatun"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "And for them", "word_transliteration": "walahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064c", "word_translation": "(is) a punishment", "word_transliteration": "adhabun"}, {"word_arabic_uthmani": "\u0639\u064e\u0638\u0650\u064a\u0645\u065e", "word_translation": "great", "word_transliteration": "azimun"}]}