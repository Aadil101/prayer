{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0627 \u0644\u064e\u0645\u064e\u0633\u06e1\u0646\u064e\u0627 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064e \u0641\u064e\u0648\u064e\u062c\u064e\u062f\u06e1\u0646\u064e\u0670\u0647\u064e\u0627 \u0645\u064f\u0644\u0650\u0626\u064e\u062a\u06e1 \u062d\u064e\u0631\u064e\u0633\u0657\u0627 \u0634\u064e\u062f\u0650\u064a\u062f\u0657\u0627 \u0648\u064e\u0634\u064f\u0647\u064f\u0628\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0627", "word_translation": "And that we", "word_transliteration": "wa-anna"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u064e\u0633\u06e1\u0646\u064e\u0627", "word_translation": "sought to touch", "word_transliteration": "lamasna"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064e", "word_translation": "the heaven", "word_transliteration": "al-samaa"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u064e\u062c\u064e\u062f\u06e1\u0646\u064e\u0670\u0647\u064e\u0627", "word_translation": "but we found it", "word_transliteration": "fawajadnaha"}, {"word_arabic_uthmani": "\u0645\u064f\u0644\u0650\u0626\u064e\u062a\u06e1", "word_translation": "filled (with)", "word_transliteration": "muli-at"}, {"word_arabic_uthmani": "\u062d\u064e\u0631\u064e\u0633\u0657\u0627", "word_translation": "guards", "word_transliteration": "harasan"}, {"word_arabic_uthmani": "\u0634\u064e\u062f\u0650\u064a\u062f\u0657\u0627", "word_translation": "severe", "word_transliteration": "shadidan"}, {"word_arabic_uthmani": "\u0648\u064e\u0634\u064f\u0647\u064f\u0628\u0657\u0627", "word_translation": "and flaming fires", "word_transliteration": "washuhuban"}]}