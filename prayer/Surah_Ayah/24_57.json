{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0627 \u062a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u064f\u0639\u06e1\u062c\u0650\u0632\u0650\u064a\u0646\u064e \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da \u0648\u064e\u0645\u064e\u0623\u06e1\u0648\u064e\u0649\u0670\u0647\u064f\u0645\u064f \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064f\u06d6 \u0648\u064e\u0644\u064e\u0628\u0650\u0626\u06e1\u0633\u064e \u0671\u0644\u06e1\u0645\u064e\u0635\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e", "word_translation": "think", "word_transliteration": "tahsabanna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0645\u064f\u0639\u06e1\u062c\u0650\u0632\u0650\u064a\u0646\u064e", "word_translation": "(can) escape", "word_transliteration": "mu'jizina"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0623\u06e1\u0648\u064e\u0649\u0670\u0647\u064f\u0645\u064f", "word_translation": "And their abode", "word_transliteration": "wamawahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064f\u06d6", "word_translation": "(will be) the Fire", "word_transliteration": "al-naru"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0628\u0650\u0626\u06e1\u0633\u064e", "word_translation": "and wretched is", "word_transliteration": "walabi'sa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0635\u0650\u064a\u0631\u064f", "word_translation": "the destination", "word_transliteration": "al-masiru"}]}