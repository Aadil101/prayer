{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e \u062a\u064f\u0628\u064e\u062f\u0651\u064e\u0644\u064f \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064f \u063a\u064e\u064a\u06e1\u0631\u064e \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u064f\u06d6 \u0648\u064e\u0628\u064e\u0631\u064e\u0632\u064f\u0648\u0627\u0652 \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u0648\u064e\u0670\u062d\u0650\u062f\u0650 \u0671\u0644\u06e1\u0642\u064e\u0647\u0651\u064e\u0627\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(On the) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u062a\u064f\u0628\u064e\u062f\u0651\u064e\u0644\u064f", "word_translation": "will be changed", "word_transliteration": "tubaddalu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064f", "word_translation": "the earth", "word_transliteration": "al-ardu"}, {"word_arabic_uthmani": "\u063a\u064e\u064a\u06e1\u0631\u064e", "word_translation": "(to) other (than)", "word_transliteration": "ghayra"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u064f\u06d6", "word_translation": "and the heavens", "word_transliteration": "wal-samawatu"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u0631\u064e\u0632\u064f\u0648\u0627\u0652", "word_translation": "and they will come forth", "word_transliteration": "wabarazu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "before Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0648\u064e\u0670\u062d\u0650\u062f\u0650", "word_translation": "the One", "word_transliteration": "al-wahidi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0647\u0651\u064e\u0627\u0631\u0650", "word_translation": "the Irresistible", "word_transliteration": "al-qahari"}]}