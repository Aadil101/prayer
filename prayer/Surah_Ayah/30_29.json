{"ayahs": {"ayah_uthmani": "\u0628\u064e\u0644\u0650 \u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0638\u064e\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0647\u06e1\u0648\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645 \u0628\u0650\u063a\u064e\u064a\u06e1\u0631\u0650 \u0639\u0650\u0644\u06e1\u0645\u0656\u06d6 \u0641\u064e\u0645\u064e\u0646 \u064a\u064e\u0647\u06e1\u062f\u0650\u064a \u0645\u064e\u0646\u06e1 \u0623\u064e\u0636\u064e\u0644\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06d6 \u0648\u064e\u0645\u064e\u0627 \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u0646\u0651\u064e\u0640\u0670\u0635\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0628\u064e\u0644\u0650", "word_translation": "Nay", "word_transliteration": "bali"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e", "word_translation": "follow", "word_transliteration": "ittaba'a"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0638\u064e\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652", "word_translation": "do wrong", "word_transliteration": "zalamu"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0648\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645", "word_translation": "their desires", "word_transliteration": "ahwaahum"}, {"word_arabic_uthmani": "\u0628\u0650\u063a\u064e\u064a\u06e1\u0631\u0650", "word_translation": "without", "word_transliteration": "bighayri"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u0656\u06d6", "word_translation": "knowledge", "word_transliteration": "il'min"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0646", "word_translation": "Then who", "word_transliteration": "faman"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062f\u0650\u064a", "word_translation": "(can) g