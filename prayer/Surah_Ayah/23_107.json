{"ayahs": {"ayah_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653 \u0623\u064e\u062e\u06e1\u0631\u0650\u062c\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u0641\u064e\u0625\u0650\u0646\u06e1 \u0639\u064f\u062f\u06e1\u0646\u064e\u0627 \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627 \u0638\u064e\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653", "word_translation": "Our Lord!", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u06e1\u0631\u0650\u062c\u06e1\u0646\u064e\u0627", "word_translation": "Bring us out", "word_transliteration": "akhrij'na"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "from it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u06e1", "word_translation": "then if", "word_transliteration": "fa-in"}, {"word_arabic_uthmani": "\u0639\u064f\u062f\u06e1\u0646\u064e\u0627", "word_translation": "we return", "word_transliteration": "ud'na"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "then indeed we", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u0638\u064e\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e", "word_translation": "(would be) wrongdoers", "word_transliteration": "zalimuna"}]}