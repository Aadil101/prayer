{"ayahs": {"ayah_uthmani": "\u0644\u0651\u0650\u0643\u064e\u064a\u06e1\u0644\u064e\u0627 \u062a\u064e\u0623\u06e1\u0633\u064e\u0648\u06e1\u0627\u0652 \u0639\u064e\u0644\u064e\u0649\u0670 \u0645\u064e\u0627 \u0641\u064e\u0627\u062a\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0641\u06e1\u0631\u064e\u062d\u064f\u0648\u0627\u0652 \u0628\u0650\u0645\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0643\u064f\u0645\u06e1\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0627 \u064a\u064f\u062d\u0650\u0628\u0651\u064f \u0643\u064f\u0644\u0651\u064e \u0645\u064f\u062e\u06e1\u062a\u064e\u0627\u0644\u0656 \u0641\u064e\u062e\u064f\u0648\u0631\u064d"}, "words": [{"word_arabic_uthmani": "\u0644\u0651\u0650\u0643\u064e\u064a\u06e1\u0644\u064e\u0627", "word_translation": "So that you may not", "word_transliteration": "likayla"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u0633\u064e\u0648\u06e1\u0627\u0652", "word_translation": "grieve", "word_transliteration": "tasaw"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u064e\u0627\u062a\u064e\u0643\u064f\u0645\u06e1", "word_translation": "has escaped you", "word_transliteration": "fatakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u06e1\u0631\u064e\u062d\u064f\u0648\u0627\u0652", "word_translation": "exult", "word_transliteration": "tafrahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627\u0653", "word_translation": "at what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0643\u064f\u0645\u06e1\u06d7", "word_translation": "He has given you", "word_transliteration": "atakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u0650\u0628\u0651\u064f", "word_translation": "love", "word_transliteration": "yuhibbu"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064e", "word_translation": "every", "word_transliteration": "kulla"}, {"word_arabic_uthmani": "\u0645\u064f\u062e\u06e1\u062a\u064e\u0627\u0644\u0656", "word_translation": "self-deluded", "word_transliteration": "mukh'talin"}, {"word_arabic_uthmani": "\u0641\u064e\u062e\u064f\u0648\u0631\u064d", "word_translation": "boaster", "word_transliteration": "fakhurin"}]}