{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062c\u064e\u064a\u06e1\u0646\u064e\u0627 \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0648\u064e\u0645\u064e\u0646 \u0645\u0651\u064e\u0639\u064e\u0647\u064f\u06e5\u0653 \u0623\u064e\u062c\u06e1\u0645\u064e\u0639\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062c\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "And We saved", "word_transliteration": "wa-anjayna"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "and who", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0639\u064e\u0647\u064f\u06e5\u0653", "word_translation": "(were) with him", "word_transliteration": "ma'ahu"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0645\u064e\u0639\u0650\u064a\u0646\u064e", "word_translation": "all", "word_transliteration": "ajma'ina"}]}