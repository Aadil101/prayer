{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0646\u064e\u0627 \u0646\u064e\u062a\u0651\u064e\u0628\u0650\u0639\u064f \u0671\u0644\u0633\u0651\u064e\u062d\u064e\u0631\u064e\u0629\u064e \u0625\u0650\u0646 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u063a\u064e\u0670\u0644\u0650\u0628\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0646\u064e\u0627", "word_translation": "That we may", "word_transliteration": "la'allana"}, {"word_arabic_uthmani": "\u0646\u064e\u062a\u0651\u064e\u0628\u0650\u0639\u064f", "word_translation": "follow", "word_transliteration": "nattabi'u"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u062d\u064e\u0631\u064e\u0629\u064e", "word_translation": "the magicians", "word_transliteration": "al-saharata"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they are", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "they are", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u0670\u0644\u0650\u0628\u0650\u064a\u0646\u064e", "word_translation": "the victorious", "word_transliteration": "al-ghalibina"}]}