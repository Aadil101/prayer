{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u062c\u064e\u0647\u0651\u064e\u0632\u064e\u0647\u064f\u0645 \u0628\u0650\u062c\u064e\u0647\u064e\u0627\u0632\u0650\u0647\u0650\u0645\u06e1 \u0642\u064e\u0627\u0644\u064e \u0671\u0626\u06e1\u062a\u064f\u0648\u0646\u0650\u064a \u0628\u0650\u0623\u064e\u062e\u0656 \u0644\u0651\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u06e1 \u0623\u064e\u0628\u0650\u064a\u0643\u064f\u0645\u06e1\u06da \u0623\u064e\u0644\u064e\u0627 \u062a\u064e\u0631\u064e\u0648\u06e1\u0646\u064e \u0623\u064e\u0646\u0651\u0650\u064a\u0653 \u0623\u064f\u0648\u0641\u0650\u064a \u0671\u0644\u06e1\u0643\u064e\u064a\u06e1\u0644\u064e \u0648\u064e\u0623\u064e\u0646\u064e\u0627\u06e0 \u062e\u064e\u064a\u06e1\u0631\u064f \u0671\u0644\u06e1\u0645\u064f\u0646\u0632\u0650\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "And when", "word_transliteration": "walamma"}, {"word_arabic_uthmani": "\u062c\u064e\u0647\u0651\u064e\u0632\u064e\u0647\u064f\u0645", "word_translation": "he had furnished them", "word_transliteration": "jahhazahum"}, {"word_arabic_uthmani": "\u0628\u0650\u062c\u064e\u0647\u064e\u0627\u0632\u0650\u0647\u0650\u0645\u06e1", "word_translation": "with their supplies", "word_transliteration": "bijahazihim"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "he said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0671\u0626\u06e1\u062a\u064f\u0648\u0646\u0650\u064a", "word_translation": "Bring to me", "word_transliteration": "i'tuni"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u062e\u0656", "word_translation": "a brother", "word_transliteration": "bi-akhin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0643\u064f\u0645", "word_translation": "of yours", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u0650\u064a\u0643\u064f\u0645\u06e1\u06da", "word_translation": "your father", "word_transliteration": "abikum"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627", "word_translation": "Do not", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e\u0648\u06e1\u0646\u064e", "word_translation": "you see", "word_transliteration": "tarawna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u0650\u064a\u0653", "word_translation": "that I", "word_transliteration": "anni"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0641\u0650\u064a", "word_translation": "[I] give full", "word_transliteration": "ufi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u064a\u06e1\u0644\u064e", "word_translation": "[the] measure", "word_transliteration": "al-kayla"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "and that I am", "word_transliteration": "wa-ana"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u064f", "word_translation": "(the) best", "word_transliteration": "khayru"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0646\u0632\u0650\u0644\u0650\u064a\u0646\u064e", "word_translation": "(of) the hosts", "word_transliteration": "al-munzilina"}]}