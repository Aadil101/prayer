{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0647\u064e\u0627\u062c\u064e\u0631\u064f\u0648\u0627\u0652 \u0641\u0650\u064a \u0633\u064e\u0628\u0650\u064a\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u062b\u064f\u0645\u0651\u064e \u0642\u064f\u062a\u0650\u0644\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0648\u06e1 \u0645\u064e\u0627\u062a\u064f\u0648\u0627\u0652 \u0644\u064e\u064a\u064e\u0631\u06e1\u0632\u064f\u0642\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0631\u0650\u0632\u06e1\u0642\u064b\u0627 \u062d\u064e\u0633\u064e\u0646\u0657\u0627\u06da \u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0647\u064f\u0648\u064e \u062e\u064e\u064a\u06e1\u0631\u064f \u0671\u0644\u0631\u0651\u064e\u0640\u0670\u0632\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "And those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0647\u064e\u0627\u062c\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "emigrated", "word_transliteration": "hajaru"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u0650", "word_translation": "(the) way", "word_transliteration": "sabili"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0642\u064f\u062a\u0650\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "were killed", "word_transliteration": "qutilu"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u062a\u064f\u0648\u0627\u0652", "word_translation": "died", "word_transliteration": "matu"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u064e\u0631\u06e1\u0632\u064f\u0642\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u064f", "word_translation": "surely will provide them", "word_transliteration": "layarzuqannahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0631\u0650\u0632\u06e1\u0642\u064b\u0627", "word_translation": "a provision", "word_transliteration": "riz'qan"}, {"word_arabic_uthmani": "\u062d\u064e\u0633\u064e\u0646\u0657\u0627\u06da", "word_translation": "good", "word_transliteration": "hasanan"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0648\u064e", "word_translation": "surely, He", "word_transliteration": "lahuwa"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u064f", "word_translation": "(is the) Best", "word_transliteration": "khayru"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0640\u0670\u0632\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "(of) the Providers", "word_transliteration": "al-raziqina"}]}