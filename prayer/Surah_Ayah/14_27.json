{"ayahs": {"ayah_uthmani": "\u064a\u064f\u062b\u064e\u0628\u0651\u0650\u062a\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0628\u0650\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u0650 \u0671\u0644\u062b\u0651\u064e\u0627\u0628\u0650\u062a\u0650 \u0641\u0650\u064a \u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u0650 \u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627 \u0648\u064e\u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650\u06d6 \u0648\u064e\u064a\u064f\u0636\u0650\u0644\u0651\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e\u06da \u0648\u064e\u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064e\u0627 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u062b\u064e\u0628\u0651\u0650\u062a\u064f", "word_translation": "Allah keeps firm", "word_transliteration": "yuthabbitu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah keeps firm", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u0650", "word_translation": "with the firm word", "word_transliteration": "bil-qawli"}, {"word_arabic_uthmani": "\u0671\u0644\u062b\u0651\u064e\u0627\u0628\u0650\u062a\u0650", "word_translation": "with the firm word", "word_transliteration": "al-thabiti"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u0650", "word_translation": "the life", "word_transliteration": "al-hayati"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627", "word_translation": "(of) the world", "word_transliteration": "al-dun'ya"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u0650\u064a", "word_translation": "and in", "word_transliteration": "wafi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650\u06d6", "word_translation": "the Hereafter", "word_transliteration": "al-akhirati"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0636\u0650\u0644\u0651\u064f", "word_translation": "And Allah lets go astray", "word_transliteration": "wayudillu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah lets go astray", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e\u06da", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimina"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f", "word_translation": "And Allah does", "word_transliteration": "wayaf 'alu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah does", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}]}