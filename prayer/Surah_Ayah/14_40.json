{"ayahs": {"ayah_uthmani": "\u0631\u064e\u0628\u0651\u0650 \u0671\u062c\u06e1\u0639\u064e\u0644\u06e1\u0646\u0650\u064a \u0645\u064f\u0642\u0650\u064a\u0645\u064e \u0671\u0644\u0635\u0651\u064e\u0644\u064e\u0648\u0670\u0629\u0650 \u0648\u064e\u0645\u0650\u0646 \u0630\u064f\u0631\u0651\u0650\u064a\u0651\u064e\u062a\u0650\u064a\u06da \u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627 \u0648\u064e\u062a\u064e\u0642\u064e\u0628\u0651\u064e\u0644\u06e1 \u062f\u064f\u0639\u064e\u0627\u0653\u0621\u0650"}, "words": [{"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "My Lord!", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0671\u062c\u06e1\u0639\u064e\u0644\u06e1\u0646\u0650\u064a", "word_translation": "Make me", "word_transliteration": "ij'alni"}, {"word_arabic_uthmani": "\u0645\u064f\u0642\u0650\u064a\u0645\u064e", "word_translation": "an establisher", "word_transliteration": "muqima"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0644\u064e\u0648\u0670\u0629\u0650", "word_translation": "(of) the prayer", "word_transliteration": "al-salati"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646", "word_translation": "and from", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u0630\u064f\u0631\u0651\u0650\u064a\u0651\u064e\u062a\u0650\u064a\u06da", "word_translation": "my offsprings", "word_transliteration": "dhurriyyati"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627", "word_translation": "Our Lord!", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0642\u064e\u0628\u0651\u064e\u0644\u06e1", "word_translation": "and accept", "word_transliteration": "wataqabbal"}, {"word_arabic_uthmani": "\u062f\u064f\u0639\u064e\u0627\u0653\u0621\u0650", "word_translation": "my prayer", "word_transliteration": "du'ai"}]}