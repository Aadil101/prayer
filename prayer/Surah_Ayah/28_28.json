{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u064e\u064a\u06e1\u0646\u0650\u064a \u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064e\u06d6 \u0623\u064e\u064a\u0651\u064e\u0645\u064e\u0627 \u0671\u0644\u06e1\u0623\u064e\u062c\u064e\u0644\u064e\u064a\u06e1\u0646\u0650 \u0642\u064e\u0636\u064e\u064a\u06e1\u062a\u064f \u0641\u064e\u0644\u064e\u0627 \u0639\u064f\u062f\u06e1\u0648\u064e\u0670\u0646\u064e \u0639\u064e\u0644\u064e\u064a\u0651\u064e\u06d6 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u0649\u0670 \u0645\u064e\u0627 \u0646\u064e\u0642\u064f\u0648\u0644\u064f \u0648\u064e\u0643\u0650\u064a\u0644\u065e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u0650\u064a", "word_translation": "(is) between me", "word_transliteration": "bayni"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u064a\u06e1\u0646\u064e\u0643\u064e\u06d6", "word_translation": "and between you", "word_transliteration": "wabaynaka"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u0651\u064e\u0645\u064e\u0627", "word_translation": "Whichever", "word_transliteration": "ayyama"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u062c\u064e\u0644\u064e\u064a\u06e1\u0646\u0650", "word_translation": "(of) the two terms", "word_transliteration": "al-ajalayni"}, {"word_arabic_uthmani": "\u0642\u064e\u0636\u064e\u064a\u06e1\u062a\u064f", "word_translation": "I complete", "word_transliteration": "qadaytu"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "then no", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u0639\u064f\u062f\u06e1\u0648\u064e\u0670\u0646\u064e", "word_translation": "injustice", "word_transliteration": "ud'wana"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u0651\u064e\u06d6", "word_translation": "to me", "word_transliteration": "alayya"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "and Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0646\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "we say", "word_transliteration": "naqulu"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u0650\u064a\u0644\u065e", "word_translation": "(is) a Witness", "word_transliteration": "wakilun"}]}