{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0647\u064f\u06e5 \u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6 \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0651\u064f \u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "To Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "(belong) whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0651\u064f", "word_translation": "(is) the Most High", "word_transliteration": "al-'aliyu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u064f", "word_translation": "the Most Great", "word_transliteration": "al-'azimu"}]}