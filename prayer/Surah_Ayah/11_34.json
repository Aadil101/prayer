{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0646\u0641\u064e\u0639\u064f\u0643\u064f\u0645\u06e1 \u0646\u064f\u0635\u06e1\u062d\u0650\u064a\u0653 \u0625\u0650\u0646\u06e1 \u0623\u064e\u0631\u064e\u062f\u062a\u0651\u064f \u0623\u064e\u0646\u06e1 \u0623\u064e\u0646\u0635\u064e\u062d\u064e \u0644\u064e\u0643\u064f\u0645\u06e1 \u0625\u0650\u0646 \u0643\u064e\u0627\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064f\u0631\u0650\u064a\u062f\u064f \u0623\u064e\u0646 \u064a\u064f\u063a\u06e1\u0648\u0650\u064a\u064e\u0643\u064f\u0645\u06e1\u06da \u0647\u064f\u0648\u064e \u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1 \u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (will) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u0641\u064e\u0639\u064f\u0643\u064f\u0645\u06e1", "word_translation": "benefit you", "word_transliteration": "yanfa'ukum"}, {"word_arabic_uthmani": "\u0646\u064f\u0635\u06e1\u062d\u0650\u064a\u0653", "word_translation": "my advice", "word_transliteration": "nus'hi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u06e1", "word_translation": "(even) if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u064e\u062f\u062a\u0651\u064f", "word_translation": "I wish", "word_transliteration": "aradttu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1", "word_translation": "to", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0635\u064e\u062d\u064e", "word_translation": "[I] advise", "word_transliteration": "ansaha"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "[to] you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "It was (Allah's)", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(It was) Allah's", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f", "word_translation": "will", "word_transliteration": "yuridu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "to", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u063a\u06e1\u0648\u0650\u064a\u064e\u0643\u064f\u0645\u06e1\u06da", "word_translation": "let you go astray", "word_transliteration": "yugh'wiyakum"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He (is)", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064f\u0645\u06e1", "word_translation": "your Lord", "word_transliteration": "rabbukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "and to Him", "word_transliteration": "wa-ilayhi"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "you will be returned", "word_transliteration": "tur'ja'una"}]}