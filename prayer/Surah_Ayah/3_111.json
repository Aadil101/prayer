{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0646 \u064a\u064e\u0636\u064f\u0631\u0651\u064f\u0648\u0643\u064f\u0645\u06e1 \u0625\u0650\u0644\u0651\u064e\u0627\u0653 \u0623\u064e\u0630\u0657\u0649\u06d6 \u0648\u064e\u0625\u0650\u0646 \u064a\u064f\u0642\u064e\u0670\u062a\u0650\u0644\u064f\u0648\u0643\u064f\u0645\u06e1 \u064a\u064f\u0648\u064e\u0644\u0651\u064f\u0648\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u062f\u06e1\u0628\u064e\u0627\u0631\u064e \u062b\u064f\u0645\u0651\u064e \u0644\u064e\u0627 \u064a\u064f\u0646\u0635\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0646", "word_translation": "Never", "word_transliteration": "lan"}, {"word_arabic_uthmani": "\u064a\u064e\u0636\u064f\u0631\u0651\u064f\u0648\u0643\u064f\u0645\u06e1", "word_translation": "will they harm you", "word_transliteration": "yadurrukum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627\u0653", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0623\u064e\u0630\u0657\u0649\u06d6", "word_translation": "a hurt", "word_transliteration": "adhan"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "And if", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u064a\u064f\u0642\u064e\u0670\u062a\u0650\u0644\u064f\u0648\u0643\u064f\u0645\u06e1", "word_translation": "they fight you", "word_transliteration": "yuqatilukum"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u064e\u0644\u0651\u064f\u0648\u0643\u064f\u0645\u064f", "word_translation": "they will turn (towards) you", "word_transliteration": "yuwallukumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u062f\u06e1\u0628\u064e\u0627\u0631\u064e", "word_translation": "the backs", "word_transliteration": "al-adbara"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "no