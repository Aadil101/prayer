{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646 \u0643\u064f\u0644\u0651\u065e \u0644\u0651\u064e\u0645\u0651\u064e\u0627 \u062c\u064e\u0645\u0650\u064a\u0639\u065e \u0644\u0651\u064e\u062f\u064e\u064a\u06e1\u0646\u064e\u0627 \u0645\u064f\u062d\u06e1\u0636\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "And surely", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u065e", "word_translation": "all", "word_transliteration": "kullun"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u0651\u064e\u0627", "word_translation": "then", "word_transliteration": "lamma"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0639\u065e", "word_translation": "together", "word_transliteration": "jami'un"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u062f\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "before Us", "word_transliteration": "ladayna"}, {"word_arabic_uthmani": "\u0645\u064f\u062d\u06e1\u0636\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "(will be) brought", "word_transliteration": "muh'daruna"}]}