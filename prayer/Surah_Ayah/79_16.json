{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0630\u06e1 \u0646\u064e\u0627\u062f\u064e\u0649\u0670\u0647\u064f \u0631\u064e\u0628\u0651\u064f\u0647\u064f\u06e5 \u0628\u0650\u0671\u0644\u06e1\u0648\u064e\u0627\u062f\u0650 \u0671\u0644\u06e1\u0645\u064f\u0642\u064e\u062f\u0651\u064e\u0633\u0650 \u0637\u064f\u0648\u064b\u0649"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "When", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u062f\u064e\u0649\u0670\u0647\u064f", "word_translation": "called him", "word_transliteration": "nadahu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0647\u064f\u06e5", "word_translation": "his Lord", "word_transliteration": "rabbuhu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0648\u064e\u0627\u062f\u0650", "word_translation": "in the valley", "word_transliteration": "bil-wadi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0642\u064e\u062f\u0651\u064e\u0633\u0650", "word_translation": "the sacred", "word_transliteration": "al-muqadasi"}, {"word_arabic_uthmani": "\u0637\u064f\u0648\u064b\u0649", "word_translation": "(of) Tuwa", "word_transliteration": "tuwan"}]}