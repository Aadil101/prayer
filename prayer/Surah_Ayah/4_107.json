{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064f\u062c\u064e\u0670\u062f\u0650\u0644\u06e1 \u0639\u064e\u0646\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u062e\u06e1\u062a\u064e\u0627\u0646\u064f\u0648\u0646\u064e \u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0647\u064f\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0627 \u064a\u064f\u062d\u0650\u0628\u0651\u064f \u0645\u064e\u0646 \u0643\u064e\u0627\u0646\u064e \u062e\u064e\u0648\u0651\u064e\u0627\u0646\u064b\u0627 \u0623\u064e\u062b\u0650\u064a\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064f\u062c\u064e\u0670\u062f\u0650\u0644\u06e1", "word_translation": "argue", "word_transliteration": "tujadil"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0650", "word_translation": "for", "word_transliteration": "ani"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u062a\u064e\u0627\u0646\u064f\u0648\u0646\u064e", "word_translation": "deceive", "word_transliteration": "yakhtanuna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "themselves", "word_transliteration": "anfusahum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u0650\u0628\u0651\u064f", "word_translation": "love", "word_transl