{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0645\u06e1\u0631\u064f\u0646\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0648\u064e\u0670\u062d\u0650\u062f\u064e\u0629\u065e \u0643\u064e\u0644\u064e\u0645\u06e1\u062d\u0650\u06ed \u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u0635\u064e\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0631\u064f\u0646\u064e\u0627\u0653", "word_translation": "(is) Our Command", "word_transliteration": "amruna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0648\u064e\u0670\u062d\u0650\u062f\u064e\u0629\u065e", "word_translation": "one", "word_transliteration": "wahidatun"}, {"word_arabic_uthmani": "\u0643\u064e\u0644\u064e\u0645\u06e1\u062d\u0650\u06ed", "word_translation": "like the twinkling", "word_transliteration": "kalamhin"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u0635\u064e\u0631\u0650", "word_translation": "(of) the eye", "word_transliteration": "bil-basari"}]}