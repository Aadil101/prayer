{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0645\u0651\u064e\u0627 \u062a\u064e\u062e\u064e\u0627\u0641\u064e\u0646\u0651\u064e \u0645\u0650\u0646 \u0642\u064e\u0648\u06e1\u0645\u064d \u062e\u0650\u064a\u064e\u0627\u0646\u064e\u0629\u0657 \u0641\u064e\u0671\u0646\u06e2\u0628\u0650\u0630\u06e1 \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0639\u064e\u0644\u064e\u0649\u0670 \u0633\u064e\u0648\u064e\u0627\u0653\u0621\u064d\u06da \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0627 \u064a\u064f\u062d\u0650\u0628\u0651\u064f \u0671\u0644\u06e1\u062e\u064e\u0627\u0653\u0626\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0645\u0651\u064e\u0627", "word_translation": "And if", "word_transliteration": "wa-imma"}, {"word_arabic_uthmani": "\u062a\u064e\u062e\u064e\u0627\u0641\u064e\u0646\u0651\u064e", "word_translation": "you fear", "word_transliteration": "takhafanna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064d", "word_translation": "a people", "word_transliteration": "qawmin"}, {"word_arabic_uthmani": "\u062e\u0650\u064a\u064e\u0627\u0646\u064e\u0629\u0657", "word_translation": "betrayal", "word_transliteration": "khiyanatan"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0646\u06e2\u0628\u0650\u0630\u06e1", "word_translation": "throw back", "word_transliteration": "fa-inbidh"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "to them", "word_transliteration": "ilayhim"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0633\u064e\u0648\u064e\u0627\u0653\u0621\u064d\u06da", "word_translation": "equal (terms)", "word_transliteration": "sawain"}, {"word_arabic_uthmani": "\u0625\u