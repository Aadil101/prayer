n": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u0627\u0631\u0650", "word_translation": "the cave", "word_transliteration": "al-ghari"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "when", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "he said", "word_transliteration": "yaqulu"}, {"word_arabic_uthmani": "\u0644\u0650\u0635\u064e\u0670\u062d\u0650\u0628\u0650\u0647\u0650\u06e6", "word_translation": "to his companion", "word_transliteration": "lisahibihi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u06e1\u0632\u064e\u0646\u06e1", "word_translation": "grieve", "word_transliteration": "tahzan"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e\u0646\u064e\u0627\u06d6", "word_translation": "(is) with us", "word_transliteration": "ma'ana"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0646\u0632\u064e\u0644\u064e", "word_translation": "Then Allah sent down", "word_transliteration": "fa-anzala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Then Allah sent down", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0633\u064e\u0643\u0650\u064a\u0646\u064e\u062a\u064e\u0647\u064f\u06e5", "word_translation": "His tranquility", "word_transliteration": "sakinatahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "upon him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u064a\u0651\u064e\u062f\u064e\u0647\u064f\u06e5", "word_translation": "and supported him", "word_transliteration": "wa-ayyadahu"}, {"word_arabic_uthmani": "\u0628\u0650\u062c\u064f\u0646\u064f\u0648\u062f\u0656", "word_translation": "with forces", "word_transliteration": "bijunudin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u06e1", "word_translation": "which you did not see", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e\u0648\u06e1\u0647\u064e\u0627", "word_translation": "which you did not see", "word_transliteration": "tarawha"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u064e", "word_translation": "and made", "word_transliteration": "waja'ala"}, {"word_arabic_uthmani": "\u0643\u064e\u0644\u0650\u0645\u064e\u0629\u064e", "word_translation": "(the) word", "word_transliteration": "kalimata"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064f\u0641\u06e1\u0644\u064e\u0649\u0670\u06d7", "word_translation": "the lowest", "word_transliteration": "al-suf'la"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0644\u0650\u0645\u064e\u0629\u064f", "word_translation": "while (the) Word", "word_transliteration": "wakalimatu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0647\u0650\u064a\u064e", "word_translation": "it (is)", "word_transliteration": "hiya"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064f\u0644\u06e1\u064a\u064e\u0627\u06d7", "word_translation": "the highest", "word_transliteration": "al-'ul'ya"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0632\u0650\u064a\u0632\u064c", "word_translation": "(is) All-Mighty", "word_transliteration": "azizun"}, {"word_arabic_uthmani": "\u062d\u064e\u0643\u0650\u064a\u0645\u064c", "word_translation": "All-Wise", "word_transliteration": "hakimun"}]}