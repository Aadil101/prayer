{"ayahs": {"ayah_uthmani": "\u0643\u064e\u0644\u0651\u064e\u0627\u06da \u0633\u064e\u064a\u064e\u0643\u06e1\u0641\u064f\u0631\u064f\u0648\u0646\u064e \u0628\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u064e\u062a\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0646\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0636\u0650\u062f\u0651\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0643\u064e\u0644\u0651\u064e\u0627\u06da", "word_translation": "Nay", "word_transliteration": "kalla"}, {"word_arabic_uthmani": "\u0633\u064e\u064a\u064e\u0643\u06e1\u0641\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "they will deny", "word_transliteration": "sayakfuruna"}, {"word_arabic_uthmani": "\u0628\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u064e\u062a\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their worship (of them)", "word_transliteration": "bi'ibadatihim"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0646\u064e", "word_translation": "and they will be", "word_transliteration": "wayakununa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "against them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0636\u0650\u062f\u0651\u064b\u0627", "word_translation": "opponents", "word_transliteration": "diddan"}]}