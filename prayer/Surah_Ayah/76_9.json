{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0646\u064f\u0637\u06e1\u0639\u0650\u0645\u064f\u0643\u064f\u0645\u06e1 \u0644\u0650\u0648\u064e\u062c\u06e1\u0647\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0644\u064e\u0627 \u0646\u064f\u0631\u0650\u064a\u062f\u064f \u0645\u0650\u0646\u0643\u064f\u0645\u06e1 \u062c\u064e\u0632\u064e\u0627\u0653\u0621\u0657 \u0648\u064e\u0644\u064e\u0627 \u0634\u064f\u0643\u064f\u0648\u0631\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0646\u064f\u0637\u06e1\u0639\u0650\u0645\u064f\u0643\u064f\u0645\u06e1", "word_translation": "we feed you", "word_transliteration": "nut'imukum"}, {"word_arabic_uthmani": "\u0644\u0650\u0648\u064e\u062c\u06e1\u0647\u0650", "word_translation": "for (the) Countenance", "word_transliteration": "liwajhi"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0646\u064f\u0631\u0650\u064a\u062f\u064f", "word_translation": "we desire", "word_transliteration": "nuridu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0643\u064f\u0645\u06e1", "word_translation": "from you", "word_transliteration": "minkum"}, {"word_arabic_uthmani": "\u062c\u064e\u0632\u064e\u0627\u0653\u0621\u0657", "word_translation": "any reward", "word_transliteration": "jazaan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0634\u064f\u0643\u064f\u0648\u0631\u064b\u0627", "word_translation": "thanks", "word_transliteration": "shukuran"}]}