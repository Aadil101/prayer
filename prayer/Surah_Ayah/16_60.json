{"ayahs": {"ayah_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0644\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u0628\u0650\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650 \u0645\u064e\u062b\u064e\u0644\u064f \u0671\u0644\u0633\u0651\u064e\u0648\u06e1\u0621\u0650\u06d6 \u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u0645\u064e\u062b\u064e\u0644\u064f \u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0644\u064e\u0649\u0670\u06da \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "For those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "believe", "word_transliteration": "yu'minuna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u0650", "word_translation": "in the Hereafter", "word_transliteration": "bil-akhirati"}, {"word_arabic_uthmani": "\u0645\u064e\u062b\u064e\u0644\u064f", "word_translation": "(is) a similitude", "word_transliteration": "mathalu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0648\u06e1\u0621\u0650\u06d6", "word_translation": "(of) the evil", "word_transliteration": "al-sawi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "and for Allah", "word_transliteration": "walillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u062b\u064e\u0644\u064f", "word_translation": "(is) the similitude", "word_transliteration": "al-mathalu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u0644\u064e\u0649\u0670\u06da", "word_translation": "the Highest", "word_transliteration": "al-a'la"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f", "word_translation": "(is) the All-Mighty", "word_transliteration": "al-'azizu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f", "word_translation": "All-Wise", "word_transliteration": "al-hakimu"}]}