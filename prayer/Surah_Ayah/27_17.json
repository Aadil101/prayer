{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062d\u064f\u0634\u0650\u0631\u064e \u0644\u0650\u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e \u062c\u064f\u0646\u064f\u0648\u062f\u064f\u0647\u064f\u06e5 \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u0650 \u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u0650 \u0648\u064e\u0671\u0644\u0637\u0651\u064e\u064a\u06e1\u0631\u0650 \u0641\u064e\u0647\u064f\u0645\u06e1 \u064a\u064f\u0648\u0632\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062d\u064f\u0634\u0650\u0631\u064e", "word_translation": "And were gathered", "word_transliteration": "wahushira"}, {"word_arabic_uthmani": "\u0644\u0650\u0633\u064f\u0644\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u064e", "word_translation": "for Sulaiman", "word_transliteration": "lisulaymana"}, {"word_arabic_uthmani": "\u062c\u064f\u0646\u064f\u0648\u062f\u064f\u0647\u064f\u06e5", "word_translation": "his hosts", "word_transliteration": "junuduhu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u0650", "word_translation": "jinn", "word_transliteration": "al-jini"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u0650", "word_translation": "and the men", "word_transliteration": "wal-insi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0637\u0651\u064e\u064a\u06e1\u0631\u0650", "word_translation": "and the birds", "word_transliteration": "wal-tayri"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and they", "word_transliteration": "fahum"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0632\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "(were) set in rows", "word_transliteration": "yuza'una"}]}