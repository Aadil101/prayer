{"ayahs": {"ayah_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u0646 \u0646\u0651\u064f\u0628\u064e\u062f\u0651\u0650\u0644\u064e \u0623\u064e\u0645\u06e1\u062b\u064e\u0670\u0644\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0646\u064f\u0646\u0634\u0650\u0626\u064e\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a \u0645\u064e\u0627 \u0644\u064e\u0627 \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "In", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0646\u0651\u064f\u0628\u064e\u062f\u0651\u0650\u0644\u064e", "word_translation": "We (will) change", "word_transliteration": "nubaddila"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u062b\u064e\u0670\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your likeness[es]", "word_transliteration": "amthalakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064f\u0646\u0634\u0650\u0626\u064e\u0643\u064f\u0645\u06e1", "word_translation": "and produce you", "word_transliteration": "wanunshi-akum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "you know", "word_transliteration": "ta'lamuna"}]}