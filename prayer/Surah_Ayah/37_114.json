{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0645\u064e\u0646\u064e\u0646\u0651\u064e\u0627 \u0639\u064e\u0644\u064e\u0649\u0670 \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0648\u064e\u0647\u064e\u0670\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And verily", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u064e\u0646\u0651\u064e\u0627", "word_translation": "We conferred Favor", "word_transliteration": "mananna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "upon", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064e\u0670\u0631\u064f\u0648\u0646\u064e", "word_translation": "and Harun", "word_transliteration": "waharuna"}]}