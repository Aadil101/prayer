{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0633\u0651\u064e\u0644\u064e\u0670\u0645\u064f \u0639\u064e\u0644\u064e\u064a\u0651\u064e \u064a\u064e\u0648\u06e1\u0645\u064e \u0648\u064f\u0644\u0650\u062f\u062a\u0651\u064f \u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e \u0623\u064e\u0645\u064f\u0648\u062a\u064f \u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e \u0623\u064f\u0628\u06e1\u0639\u064e\u062b\u064f \u062d\u064e\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0633\u0651\u064e\u0644\u064e\u0670\u0645\u064f", "word_translation": "And peace (be)", "word_transliteration": "wal-salamu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u0651\u064e", "word_translation": "on me", "word_transliteration": "alayya"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(the) day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u0648\u064f\u0644\u0650\u062f\u062a\u0651\u064f", "word_translation": "I was born", "word_transliteration": "wulidttu"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "and (the) day", "word_transliteration": "wayawma"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u064f\u0648\u062a\u064f", "word_translation": "I will die", "word_transliteration": "amutu"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "and (the) Day", "word_transliteration": "wayawma"}, {"word_arabic_uthmani": "\u0623\u064f\u0628\u06e1\u0639\u064e\u062b\u064f", "word_translation": "I will be raised", "word_transliteration": "ub'athu"}, {"word_arabic_uthmani": "\u062d\u064e\u064a\u0651\u0657\u0627", "word_translation": "alive", "word_transliteration": "hayyan"}]}