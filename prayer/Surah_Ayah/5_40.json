{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1 \u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u06e1 \u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0647\u064f\u06e5 \u0645\u064f\u0644\u06e1\u0643\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0648\u064e\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f \u0644\u0650\u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u0649\u0670 \u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0642\u064e\u062f\u0650\u064a\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Do not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u06e1", "word_translation": "you know", "word_transliteration": "ta'lam"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e", "word_translation": "that", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "to Him (belongs)", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "the dominion", "word_transliteration": "mul'ku"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064f", "word_translation": "He punishes", "word_transliteration": "yu'adhibu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f", "word_translation": "and He forgives", "word_transliteration": "wayaghfiru"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0646", "word_translation": "[to] whom", "word_transliteration": "liman"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06d7", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "(is) on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0650\u064a\u0631\u065e", "word_translation": "All-Powerful", "word_transliteration": "qadirun"}]}