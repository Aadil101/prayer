{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0647\u064f\u06e5 \u0645\u064f\u0644\u06e1\u0643\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6 \u064a\u064f\u062d\u06e1\u064a\u0650\u06e6 \u0648\u064e\u064a\u064f\u0645\u0650\u064a\u062a\u064f\u06da \u0648\u064e\u0645\u064e\u0627 \u0644\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0645\u0650\u0646 \u0648\u064e\u0644\u0650\u064a\u0651\u0656 \u0648\u064e\u0644\u064e\u0627 \u0646\u064e\u0635\u0650\u064a\u0631\u0656"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "to Him (belongs)", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "the dominion", "word_transliteration": "mul'ku"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u06e1\u064a\u0650\u06e6", "word_translation": "He gives life", "word_transliteration": "yuh'yi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0645\u0650\u064a\u062a\u064f\u06da", "word_translation": "and He causes death", "word_transliteration": "wayumitu"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "besides Allah", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides Allah", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "besides Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u064a\u0651\u0656", "word_translation": "protector", "word_transliteration": "waliyyin"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u0650\u064a\u0631\u0656", "word_translation": "any helper", "word_transliteration": "nasirin"}]}