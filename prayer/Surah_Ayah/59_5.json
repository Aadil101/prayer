{"ayahs": {"ayah_uthmani": "\u0645\u064e\u0627 \u0642\u064e\u0637\u064e\u0639\u06e1\u062a\u064f\u0645 \u0645\u0651\u0650\u0646 \u0644\u0651\u0650\u064a\u0646\u064e\u0629\u064d \u0623\u064e\u0648\u06e1 \u062a\u064e\u0631\u064e\u0643\u06e1\u062a\u064f\u0645\u064f\u0648\u0647\u064e\u0627 \u0642\u064e\u0627\u0653\u0626\u0650\u0645\u064e\u0629\u064b \u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064f\u0635\u064f\u0648\u0644\u0650\u0647\u064e\u0627 \u0641\u064e\u0628\u0650\u0625\u0650\u0630\u06e1\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0644\u0650\u064a\u064f\u062e\u06e1\u0632\u0650\u064a\u064e \u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0642\u064e\u0637\u064e\u0639\u06e1\u062a\u064f\u0645", "word_translation": "you cut down", "word_transliteration": "qata'tum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u064a\u0646\u064e\u0629\u064d", "word_translation": "(the) palm-trees", "word_transliteration": "linatin"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e\u0643\u06e1\u062a\u064f\u0645\u064f\u0648\u0647\u064e\u0627", "word_translation": "you left them", "word_transliteration": "taraktumuha"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0653\u0626\u0650\u0645\u064e\u0629\u064b", "word_translation": "standing", "word_transliteration": "qaimatan"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064f\u0635\u064f\u0648\u0644\u0650\u0647\u064e\u0627", "word_translation": "their roots", "word_transliteration": "usuliha"}, {"word_arabi