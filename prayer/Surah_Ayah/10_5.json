{"ayahs": {"ayah_uthmani": "\u0647\u064f\u0648\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u062c\u064e\u0639\u064e\u0644\u064e \u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u064e \u0636\u0650\u064a\u064e\u0627\u0653\u0621\u0657 \u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064e \u0646\u064f\u0648\u0631\u0657\u0627 \u0648\u064e\u0642\u064e\u062f\u0651\u064e\u0631\u064e\u0647\u064f\u06e5 \u0645\u064e\u0646\u064e\u0627\u0632\u0650\u0644\u064e \u0644\u0650\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0627\u0652 \u0639\u064e\u062f\u064e\u062f\u064e \u0671\u0644\u0633\u0651\u0650\u0646\u0650\u064a\u0646\u064e \u0648\u064e\u0671\u0644\u06e1\u062d\u0650\u0633\u064e\u0627\u0628\u064e\u06da \u0645\u064e\u0627 \u062e\u064e\u0644\u064e\u0642\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650\u06da \u064a\u064f\u0641\u064e\u0635\u0651\u0650\u0644\u064f \u0671\u0644\u06e1\u0623\u0653\u064a\u064e\u0670\u062a\u0650 \u0644\u0650\u0642\u064e\u0648\u06e1\u0645\u0656 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u064e", "word_translation": "made", "word_transliteration": "ja'ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u064e", "word_translation": "the sun", "word_transliteration": "al-shamsa"}, {"word_arabic_uthmani": "\u0636\u0650\u064a\u064e\u0627\u0653\u0621\u0657", "word_translation": "a shining light", "word_transliteration": "diyaan"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064e", "word_translation": "and the moon", "word_transliteration": "wal-qamara"}, {"word_arabic_uthmani": "\u0646\u064f\u0648\u0631\u0657\u0627", "word_translation": "a reflected light", "word_transliteration": "nuran"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u0651\u064e\u0631\u064e\u0647\u064f\u06e5", "word_translation": "and determined for it", "word_transliteration": "waqaddarahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u064e\u0627\u0632\u0650\u0644\u064e", "word_translation": "phases", "word_transliteration": "manazila"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0627\u0652", "word_translation": "that you may know", "word_transliteration": "lita'lamu"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064e\u062f\u064e", "word_translation": "(the) number", "word_transliteration": "adada"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "(of) the years", "word_transliteration": "al-sinina"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u062d\u0650\u0633\u064e\u0627\u0628\u064e\u06da", "word_translation": "and the count (of time)", "word_transliteration": "wal-hisaba"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064e", "word_translation": "created", "word_transliteration": "khalaqa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650\u06da", "word_translation": "in truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u064a\u064f\u0641\u064e\u0635\u0651\u0650\u0644\u064f", "word_translation": "He explains", "word_transliteration": "yufassilu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u064a\u064e\u0670\u062a\u0650", "word_translation": "the Signs", "word_transliteration": "al-ayati"}, {"word_arabic_uthmani": "\u0644\u0650\u0642\u064e\u0648\u06e1\u0645\u0656", "word_translation": "for a people", "word_transliteration": "liqawmin"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "(who) know", "word_transliteration": "ya'lamuna"}]}