{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0631\u0650\u064a\u062d\u0657\u0627 \u0635\u064e\u0631\u06e1\u0635\u064e\u0631\u0657\u0627 \u0641\u0650\u064a \u064a\u064e\u0648\u06e1\u0645\u0650 \u0646\u064e\u062d\u06e1\u0633\u0656 \u0645\u0651\u064f\u0633\u06e1\u062a\u064e\u0645\u0650\u0631\u0651\u0656"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653", "word_translation": "Indeed We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "[We] sent", "word_transliteration": "arsalna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "upon them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0631\u0650\u064a\u062d\u0657\u0627", "word_translation": "a wind", "word_transliteration": "rihan"}, {"word_arabic_uthmani": "\u0635\u064e\u0631\u06e1\u0635\u064e\u0631\u0657\u0627", "word_translation": "furious", "word_transliteration": "sarsaran"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "on", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u0650", "word_translation": "a day", "word_transliteration": "yawmi"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0633\u0656", "word_translation": "(of) misfortune", "word_transliteration": "nahsin"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0633\u06e1\u062a\u064e\u0645\u0650\u0631\u0651\u0656", "word_translation": "continuous", "word_transliteration": "mus'tamirrin"}]}