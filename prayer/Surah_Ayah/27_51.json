{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0671\u0646\u0638\u064f\u0631\u06e1 \u0643\u064e\u064a\u06e1\u0641\u064e \u0643\u064e\u0627\u0646\u064e \u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f \u0645\u064e\u0643\u06e1\u0631\u0650\u0647\u0650\u0645\u06e1 \u0623\u064e\u0646\u0651\u064e\u0627 \u062f\u064e\u0645\u0651\u064e\u0631\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064f\u0645\u06e1 \u0623\u064e\u062c\u06e1\u0645\u064e\u0639\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0671\u0646\u0638\u064f\u0631\u06e1", "word_translation": "Then see", "word_transliteration": "fa-unzur"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "how", "word_transliteration": "kayfa"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "was", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f", "word_translation": "(the) end", "word_transliteration": "aqibatu"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u06e1\u0631\u0650\u0647\u0650\u0645\u06e1", "word_translation": "(of) their plot", "word_transliteration": "makrihim"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0627", "word_translation": "that We", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u062f\u064e\u0645\u0651\u064e\u0631\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "destroyed them", "word_transliteration": "dammarnahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and their people", "word_transliteration": "waqawmahum"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0645\u064e\u0639\u0650\u064a\u0646\u064e", "word_translation": "all", "word_transliteration": "ajma'ina"}]}