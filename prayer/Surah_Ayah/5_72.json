{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0642\u064e\u062f\u06e1 \u0643\u064e\u0641\u064e\u0631\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0645\u064e\u0633\u0650\u064a\u062d\u064f \u0671\u0628\u06e1\u0646\u064f \u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064e\u06d6 \u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u06e1\u0645\u064e\u0633\u0650\u064a\u062d\u064f \u064a\u064e\u0670\u0628\u064e\u0646\u0650\u064a\u0653 \u0625\u0650\u0633\u06e1\u0631\u064e\u0640\u0670\u0653\u0621\u0650\u064a\u0644\u064e \u0671\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0631\u064e\u0628\u0651\u0650\u064a \u0648\u064e\u0631\u064e\u0628\u0651\u064e\u0643\u064f\u0645\u06e1\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0645\u064e\u0646 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u06e1 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0641\u064e\u0642\u064e\u062f\u06e1 \u062d\u064e\u0631\u0651\u064e\u0645\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e \u0648\u064e\u0645\u064e\u0623\u06e1\u0648\u064e\u0649\u0670\u0647\u064f \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064f\u06d6 \u0648\u064e\u0645\u064e\u0627 \u0644\u0650\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u06e1 \u0623\u064e\u0646\u0635\u064e\u0627\u0631\u0656"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "Certainly", "word_transliteration": "laqad"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064e", "word_translation": "disbelieved", "word_transliteration": "kafara"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "say", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0633\u0650\u064a\u062d\u064f", "word_translation": "(is) the Messiah", "word_transliteration": "al-masihu"}, {"word_arabic_uthmani": "\u0671\u0628\u06e1\u0646\u064f", "word_translation": "son", "word_transliteration": "ub'nu"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064e\u06d6", "word_translation": "(of) Maryam", "word_transliteration": "maryama"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "While said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0633\u0650\u064a\u062d\u064f", "word_translation": "the Messiah", "word_transliteration": "al-masihu"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0628\u064e\u0646\u0650\u064a\u0653", "word_translation": "O Children", "word_transliteration": "yabani"}, {"word_arabic_uthmani": "\u0625\u0650\u0633\u06e1\u0631\u064e\u0640\u0670\u0653\u0621\u0650\u064a\u0644\u064e", "word_translation": "(of) Israel", "word_transliteration": "is'raila"}, {"word_arabic_uthmani": "\u0671\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0627\u0652", "word_translation": "Worship", "word_transliteration": "u'budu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a", "word_translation": "my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0628\u0651\u064e\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "and your Lord", "word_transliteration": "warabbakum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed, he", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u06e1", "word_translation": "associates partners", "word_transliteration": "yush'rik"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "with Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u06e1", "word_translation": "then surely", "word_transliteration": "faqad"}, {"word_arabic_uthmani": "\u062d\u064e\u0631\u0651\u064e\u0645\u064e", "word_translation": "(has) forbidden", "word_transliteration": "harrama"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "for him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e", "word_translation": "Paradise", "word_transliteration": "al-janata"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0623\u06e1\u0648\u064e\u0649\u0670\u0647\u064f", "word_translation": "and his abode", "word_transliteration": "wamawahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064f\u06d6", "word_translation": "(will be) the Fire", "word_transliteration": "al-naru"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "for the wrongdoers", "word_transliteration": "lilzzalimina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0635\u064e\u0627\u0631\u0656", "word_translation": "(any) helpers", "word_transliteration": "ansarin"}]}