{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0643\u064e\u0645\u06e1 \u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0627 \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0642\u064f\u0631\u064f\u0648\u0646\u0650 \u0645\u0650\u0646\u06e2 \u0628\u064e\u0639\u06e1\u062f\u0650 \u0646\u064f\u0648\u062d\u0656\u06d7 \u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0628\u0650\u0630\u064f\u0646\u064f\u0648\u0628\u0650 \u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6 \u062e\u064e\u0628\u0650\u064a\u0631\u064e\u06e2\u0627 \u0628\u064e\u0635\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0645\u06e1", "word_translation": "And how many", "word_transliteration": "wakam"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0627", "word_translation": "We destroyed", "word_transliteration": "ahlakna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0631\u064f\u0648\u0646\u0650", "word_translation": "the generations", "word_transliteration": "al-quruni"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e2", "word_translation": "after", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u0650", "word_translation": "after", "word_transliteration": "ba'di"}, {"word_arabic_uthmani": "\u0646\u064f\u0648\u062d\u0656\u06d7", "word_translation": "Nuh", "word_transliteration": "nuhin"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "And sufficient", "word_transliteration": "wakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "(is) your Lord", "word_transliteration": "birabbika"}, {"word_arabic_uthmani": "\u0628\u0650\u0630\u064f\u0646\u064f\u0648\u0628\u0650", "word_translation": "concerning the sins", "word_transliteration": "bidhunubi"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6", "word_translation": "(of) His servants", "word_transliteration": "ibadihi"}, {"word_arabic_uthmani": "\u062e\u064e\u0628\u0650\u064a\u0631\u064e\u06e2\u0627", "word_translation": "All-Aware", "word_transliteration": "khabiran"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u0650\u064a\u0631\u0657\u0627", "word_translation": "All-Seer", "word_transliteration": "basiran"}]}