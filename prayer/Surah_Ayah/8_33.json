{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u0650\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0623\u064e\u0646\u062a\u064e \u0641\u0650\u064a\u0647\u0650\u0645\u06e1\u06da \u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "But not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(for) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064e\u0647\u064f\u0645\u06e1", "word_translation": "that He punishes them", "word_transliteration": "liyu'adhibahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062a\u064e", "word_translation": "while you", "word_transliteration": "wa-anta"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650\u0645\u06e1\u06da", "word_translation": "(are) among them", "word_transliteration": "fihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064e\u0647\u064f\u0645\u06e1", "word_translation": "the One Who punishes them", "word_transliteration": "mu'adhibahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "seek forgiveness", "word_transliteration": "yastaghfiruna"}]}