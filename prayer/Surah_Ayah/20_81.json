{"ayahs": {"ayah_uthmani": "\u0643\u064f\u0644\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650 \u0645\u064e\u0627 \u0631\u064e\u0632\u064e\u0642\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0637\u06e1\u063a\u064e\u0648\u06e1\u0627\u0652 \u0641\u0650\u064a\u0647\u0650 \u0641\u064e\u064a\u064e\u062d\u0650\u0644\u0651\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u063a\u064e\u0636\u064e\u0628\u0650\u064a\u06d6 \u0648\u064e\u0645\u064e\u0646 \u064a\u064e\u062d\u06e1\u0644\u0650\u0644\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u063a\u064e\u0636\u064e\u0628\u0650\u064a \u0641\u064e\u0642\u064e\u062f\u06e1 \u0647\u064e\u0648\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0643\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "Eat", "word_transliteration": "kulu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0637\u064e\u064a\u0651\u0650\u0628\u064e\u0670\u062a\u0650", "word_translation": "(the) good things", "word_transliteration": "tayyibati"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "which", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0631\u064e\u0632\u064e\u0642\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1", "word_translation": "We have provided you", "word_transliteration": "razaqnakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0637\u06e1\u063a\u064e\u0648\u06e1\u0627\u0652", "word_translation": "transgress", "word_transliteration": "tatghaw"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650", "word_translation": "therein", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064e\u062d\u0650\u0644\u0651\u064e", "word_translation": "lest should descend", "word_transliteration": "fayahilla"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "upon you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u063a\u064e\u0636\u064e\u0628\u0650\u064a\u06d6", "word_translation": "My Anger", "word_transliteration": "ghadabi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "And whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0644\u0650\u0644\u06e1", "word_translation": "on whom descends", "word_transliteration": "yahlil"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "on whom descends", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u063a\u064e\u0636\u064e\u0628\u0650\u064a", "word_translation": "My Anger", "word_transliteration": "ghadabi"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u06e1", "word_translation": "indeed", "word_transliteration": "faqad"}, {"word_arabic_uthmani": "\u0647\u064e\u0648\u064e\u0649\u0670", "word_translation": "he (has) perished", "word_transliteration": "hawa"}]}