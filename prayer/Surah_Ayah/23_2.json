{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0635\u064e\u0644\u064e\u0627\u062a\u0650\u0647\u0650\u0645\u06e1 \u062e\u064e\u0670\u0634\u0650\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "Those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "[they]", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "during", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0635\u064e\u0644\u064e\u0627\u062a\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their prayers", "word_transliteration": "salatihim"}, {"word_arabic_uthmani": "\u062e\u064e\u0670\u0634\u0650\u0639\u064f\u0648\u0646\u064e", "word_translation": "(are) humbly submissive", "word_transliteration": "khashi'una"}]}