ion": "generations", "word_transliteration": "qarnan"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062e\u064e\u0631\u0650\u064a\u0646\u064e", "word_translation": "other", "word_transliteration": "akharina"}]}