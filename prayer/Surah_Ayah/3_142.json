{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1 \u062d\u064e\u0633\u0650\u0628\u06e1\u062a\u064f\u0645\u06e1 \u0623\u064e\u0646 \u062a\u064e\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e \u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u062c\u064e\u0670\u0647\u064e\u062f\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u0643\u064f\u0645\u06e1 \u0648\u064e\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064e \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0628\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u062d\u064e\u0633\u0650\u0628\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "do you think", "word_transliteration": "hasib'tum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "you will enter", "word_transliteration": "tadkhulu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e", "word_translation": "Paradise", "word_transliteration": "al-janata"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "while has not yet", "word_transliteration": "walamma"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u0650", "word_translation": "made evident", "word_transliteration": "ya'lami"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u062c\u064e\u0670\u0647\u064e\u062f\u064f\u0648\u0627\u0652", "word_translation": "strove hard", "word_transliteration": "jahadu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0643\u064f\u0645\u06e1", "word_translation": "among you", "word_transliteration": "minkum"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064e", "word_translation": "and made evident", "word_transliteration": "waya'lama"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0628\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the steadfast", "word_transliteration": "al-sabirina"}]}