{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0646 \u0644\u0651\u064e\u0645\u06e1 \u062a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0627\u0652 \u0648\u064e\u0644\u064e\u0646 \u062a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0627\u0652 \u0641\u064e\u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652 \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064e \u0671\u0644\u0651\u064e\u062a\u0650\u064a \u0648\u064e\u0642\u064f\u0648\u062f\u064f\u0647\u064e\u0627 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f \u0648\u064e\u0671\u0644\u06e1\u062d\u0650\u062c\u064e\u0627\u0631\u064e\u0629\u064f\u06d6 \u0623\u064f\u0639\u0650\u062f\u0651\u064e\u062a\u06e1 \u0644\u0650\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646", "word_translation": "But if", "word_transliteration": "fa-in"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "you do", "word_transliteration": "taf 'alu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0646", "word_translation": "and never", "word_transliteration": "walan"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u06e1\u0639\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "will you do", "word_transliteration": "taf 'alu"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "then fear", "word_transliteration": "fa-ittaqu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064e", "word_translation": "the Fire", "word_transliteration": "al-nara"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u062a\u0650\u064a", "word_translation": "whose", "word_transliteration": "allati"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064f\u0648\u062f\u064f\u0647\u064e\u0627", "word_translation": "[its] fuel", "word_transliteration": "waquduha"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u064f", "word_translation": "(is) [the] men", "word_transliteration": "al-nasu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u062d\u0650\u062c\u064e\u0627\u0631\u064e\u0629\u064f\u06d6", "word_translation": "and [the] stones", "word_transliteration": "wal-hijaratu"}, {"word_arabic_uthmani": "\u0623\u064f\u0639\u0650\u062f\u0651\u064e\u062a\u06e1", "word_translation": "prepared", "word_transliteration": "u'iddat"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "for the disbelievers", "word_transliteration": "lil'kafirina"}]}