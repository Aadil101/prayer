{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0671\u0646\u0642\u064e\u0644\u064e\u0628\u064f\u0648\u0627\u0652 \u0628\u0650\u0646\u0650\u0639\u06e1\u0645\u064e\u0629\u0656 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0641\u064e\u0636\u06e1\u0644\u0656 \u0644\u0651\u064e\u0645\u06e1 \u064a\u064e\u0645\u06e1\u0633\u064e\u0633\u06e1\u0647\u064f\u0645\u06e1 \u0633\u064f\u0648\u0653\u0621\u065e \u0648\u064e\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064f\u0648\u0627\u0652 \u0631\u0650\u0636\u06e1\u0648\u064e\u0670\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0630\u064f\u0648 \u0641\u064e\u0636\u06e1\u0644\u064d \u0639\u064e\u0638\u0650\u064a\u0645\u064d"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0671\u0646\u0642\u064e\u0644\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "So they returned", "word_transliteration": "fa-inqalabu"}, {"word_arabic_uthmani": "\u0628\u0650\u0646\u0650\u0639\u06e1\u0645\u064e\u0629\u0656", "word_translation": "with (the) Favor", "word_transliteration": "bini'matin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u064e\u0636\u06e1\u0644\u0656", "word_translation": "and Bounty", "word_transliteration": "wafadlin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u06e1\u0633\u064e\u0633\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "touched them", "word_transliteration": "yamsashum"}, {"word_arabic_uthmani": "\u0633\u064f\u0648\u0653\u0621\u065e", "word_translation": "any harm", "word_transliteration": "suon"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064f\u0648\u0627\u0652", "word_translation": "And they followed", "word_transliteration": "wa-ittaba'u"}, {"word_arabic_uthmani": "\u0631\u0650\u0636\u06e1\u0648\u064e\u0670\u0646\u064e", "word_translation": "(the) pleasure", "word_transliteration": "rid'wana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d7", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "and Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0630\u064f\u0648", "word_translation": "(is) Possessor", "word_transliteration": "dhu"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u064d", "word_translation": "(of) Bounty", "word_transliteration": "fadlin"}, {"word_arabic_uthmani": "\u0639\u064e\u0638\u0650\u064a\u0645\u064d", "word_translation": "great", "word_transliteration": "azimin"}]}