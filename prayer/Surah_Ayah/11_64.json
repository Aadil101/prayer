{"ayahs": {"ayah_uthmani": "\u0648\u064e\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0647\u064e\u0670\u0630\u0650\u0647\u0650\u06e6 \u0646\u064e\u0627\u0642\u064e\u0629\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0644\u064e\u0643\u064f\u0645\u06e1 \u0621\u064e\u0627\u064a\u064e\u0629\u0657\u06d6 \u0641\u064e\u0630\u064e\u0631\u064f\u0648\u0647\u064e\u0627 \u062a\u064e\u0623\u06e1\u0643\u064f\u0644\u06e1 \u0641\u0650\u064a\u0653 \u0623\u064e\u0631\u06e1\u0636\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0645\u064e\u0633\u0651\u064f\u0648\u0647\u064e\u0627 \u0628\u0650\u0633\u064f\u0648\u0653\u0621\u0656 \u0641\u064e\u064a\u064e\u0623\u06e1\u062e\u064f\u0630\u064e\u0643\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u065e \u0642\u064e\u0631\u0650\u064a\u0628\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "And O my people!", "word_transliteration": "wayaqawmi"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u0650\u0647\u0650\u06e6", "word_translation": "This", "word_transliteration": "hadhihi"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u0642\u064e\u0629\u064f", "word_translation": "she-camel", "word_transliteration": "naqatu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(is) for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0629\u0657\u06d6", "word_translation": "a Sign", "word_transliteration": "ayatan"}, {"word_arabic_uthmani": "\u0641\u064e\u0630\u064e\u0631\u064f\u0648\u0647\u064e\u0627", "word_translation": "so leave her", "word_transliteration": "fadharuha"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u0643\u064f\u0644\u06e1", "word_translation": "to eat", "word_transliteration": "takul"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0653", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "ardi"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06d6", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0645\u064e\u0633\u0651\u064f\u0648\u0647\u064e\u0627", "word_translation": "touch her", "word_transliteration": "tamassuha"}, {"word_arabic_uthmani": "\u0628\u0650\u0633\u064f\u0648\u0653\u0621\u0656", "word_translation": "with harm", "word_transliteration": "bisuin"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064e\u0623\u06e1\u062e\u064f\u0630\u064e\u0643\u064f\u0645\u06e1", "word_translation": "lest will seize you", "word_transliteration": "fayakhudhakum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u065e", "word_translation": "a punishment", "word_transliteration": "adhabun"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u0650\u064a\u0628\u065e", "word_translation": "impending", "word_transliteration": "qaribun"}]}