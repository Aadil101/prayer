{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1 \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0644\u06e1\u0643\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u06d6 \u0641\u064e\u0644\u06e1\u064a\u064e\u0631\u06e1\u062a\u064e\u0642\u064f\u0648\u0627\u0652 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0633\u06e1\u0628\u064e\u0670\u0628\u0650"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0644\u06e1\u0643\u064f", "word_translation": "(is the) dominion", "word_transliteration": "mul'ku"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627\u06d6", "word_translation": "(is) between them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u06e1\u064a\u064e\u0631\u06e1\u062a\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "Then let them ascend", "word_transliteration": "falyartaqu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "by", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0633\u06e1\u0628\u064e\u0670\u0628\u0650", "word_translation": "the means", "word_transliteration": "al-asbabi"}]}