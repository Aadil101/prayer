{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064e\u0646 \u062a\u064e\u0627\u0628\u064e \u0648\u064e\u0621\u064e\u0627\u0645\u064e\u0646\u064e \u0648\u064e\u0639\u064e\u0645\u0650\u0644\u064e \u0635\u064e\u0670\u0644\u0650\u062d\u0657\u0627 \u0641\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u064a\u064e\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0646\u064e \u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e \u0648\u064e\u0644\u064e\u0627 \u064a\u064f\u0638\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e \u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "Except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(one) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u062a\u064e\u0627\u0628\u064e", "word_translation": "repented", "word_transliteration": "taba"}, {"word_arabic_uthmani": "\u0648\u064e\u0621\u064e\u0627\u0645\u064e\u0646\u064e", "word_translation": "and believed", "word_transliteration": "waamana"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0645\u0650\u0644\u064e", "word_translation": "and did", "word_transliteration": "wa'amila"}, {"word_arabic_uthmani": "\u0635\u064e\u0670\u0644\u0650\u062d\u0657\u0627", "word_translation": "good (deeds)", "word_transliteration": "salihan"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Then those", "word_transliteration": "fa-ulaika"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0646\u064e", "word_translation": "will enter", "word_transliteration": "yadkhuluna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0646\u0651\u064e\u0629\u064e", "word_translation": "Paradise", "word_transliteration": "al-janata"}, {"word_arabic_uthmani": "\u0648