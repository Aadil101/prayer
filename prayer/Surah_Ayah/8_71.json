{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646 \u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0627\u0652 \u062e\u0650\u064a\u064e\u0627\u0646\u064e\u062a\u064e\u0643\u064e \u0641\u064e\u0642\u064e\u062f\u06e1 \u062e\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u064f \u0641\u064e\u0623\u064e\u0645\u06e1\u0643\u064e\u0646\u064e \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u0650\u064a\u0645\u064c \u062d\u064e\u0643\u0650\u064a\u0645\u064c"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "But if", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0627\u0652", "word_translation": "they intend", "word_transliteration": "yuridu"}, {"word_arabic_uthmani": "\u062e\u0650\u064a\u064e\u0627\u0646\u064e\u062a\u064e\u0643\u064e", "word_translation": "(to) betray you", "word_transliteration": "khiyanataka"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u06e1", "word_translation": "certainly", "word_transliteration": "faqad"}, {"word_arabic_uthmani": "\u062e\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they have betrayed", "word_transliteration": "khanu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f", "word_translation": "before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0645\u06e1\u0643\u064e\u0646\u064e", "word_translation": "So He gave (you) power", "word_transliteration": "fa-amkana"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1\u06d7", "word_translation": "over them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u064c", "word_translation": "(is) All-Knower", "word_transliteration": "alimun"}, {"word_arabic_uthmani": "\u062d\u064e\u0643\u0650\u064a\u0645\u064c", "word_translation": "All-Wise", "word_transliteration": "hakimun"}]}