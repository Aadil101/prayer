{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0623\u064e\u0646\u06e2\u0628\u064e\u062a\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0646\u064e\u0628\u064e\u0627\u062a\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e2\u0628\u064e\u062a\u064e\u0643\u064f\u0645", "word_translation": "has caused you to grow", "word_transliteration": "anbatakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0646\u064e\u0628\u064e\u0627\u062a\u0657\u0627", "word_translation": "(as) a growth", "word_transliteration": "nabatan"}]}