{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0671\u0647\u06e1\u0628\u0650\u0637\u064e\u0627 \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u062c\u064e\u0645\u0650\u064a\u0639\u064e\u06e2\u0627\u06d6 \u0628\u064e\u0639\u06e1\u0636\u064f\u0643\u064f\u0645\u06e1 \u0644\u0650\u0628\u064e\u0639\u06e1\u0636\u064d \u0639\u064e\u062f\u064f\u0648\u0651\u065e\u06d6 \u0641\u064e\u0625\u0650\u0645\u0651\u064e\u0627 \u064a\u064e\u0623\u06e1\u062a\u0650\u064a\u064e\u0646\u0651\u064e\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u0651\u0650\u064a \u0647\u064f\u062f\u0657\u0649 \u0641\u064e\u0645\u064e\u0646\u0650 \u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e \u0647\u064f\u062f\u064e\u0627\u064a\u064e \u0641\u064e\u0644\u064e\u0627 \u064a\u064e\u0636\u0650\u0644\u0651\u064f \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0634\u06e1\u0642\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0671\u0647\u06e1\u0628\u0650\u0637\u064e\u0627", "word_translation": "Go down", "word_transliteration": "ih'bita"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "from it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u062c\u064e\u0645\u0650\u064a\u0639\u064e\u06e2\u0627\u06d6", "word_translation": "all", "word_transliteration": "jami'an"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064f\u0643\u064f\u0645\u06e1", "word_translation": "some of you", "word_transliteration": "ba'dukum"}, {"word_arabic_uthmani": "\u0644\u0650\u0628\u064e\u0639\u06e1\u0636\u064d", "word_translation": "to others", "word_transliteration": "liba'din"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u064f\u0648\u0651\u065e\u06d6", "word_translation": "(as) enemy", "word_transliteration": "aduwwun"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0645\u0651\u064e\u0627", "word_translation": "Then if", "word_transliteration": "fa-imma"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650\u064a\u064e\u0646\u0651\u064e\u0643\u064f\u0645", "word_translation": "comes to you", "word_transliteration": "yatiyannakum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u0651\u0650\u064a", "word_translation": "from Me", "word_transliteration": "minni"}, {"word_arabic_uthmani": "\u0647\u064f\u062f\u0657\u0649", "word_translation": "guidance", "word_transliteration": "hudan"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0646\u0650", "word_translation": "then whoever", "word_transliteration": "famani"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e", "word_translation": "follows", "word_transliteration": "ittaba'a"}, {"word_arabic_uthmani": "\u0647\u064f\u062f\u064e\u0627\u064a\u064e", "word_translation": "My guidance", "word_transliteration": "hudaya"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "then not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u064a\u064e\u0636\u0650\u0644\u0651\u064f", "word_translation": "he will go astray", "word_transliteration": "yadillu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0642\u064e\u0649\u0670", "word_translation": "suffer", "word_transliteration": "yashqa"}]}