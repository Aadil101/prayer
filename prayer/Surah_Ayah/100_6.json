{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e\u0670\u0646\u064e \u0644\u0650\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u06e6 \u0644\u064e\u0643\u064e\u0646\u064f\u0648\u062f\u065e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e\u0670\u0646\u064e", "word_translation": "mankind", "word_transliteration": "al-insana"}, {"word_arabic_uthmani": "\u0644\u0650\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u06e6", "word_translation": "to his Lord", "word_transliteration": "lirabbihi"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e\u0646\u064f\u0648\u062f\u065e", "word_translation": "(is) surely ungrateful", "word_transliteration": "lakanudun"}]}