{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064f\u0628\u064e\u0634\u0651\u0650\u0631\u0657\u0627 \u0648\u064e\u0646\u064e\u0630\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e", "word_translation": "We sent you", "word_transliteration": "arsalnaka"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u064f\u0628\u064e\u0634\u0651\u0650\u0631\u0657\u0627", "word_translation": "(as) a bearer of glad tidings", "word_transliteration": "mubashiran"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u0630\u0650\u064a\u0631\u0657\u0627", "word_translation": "and a warner", "word_transliteration": "wanadhiran"}]}