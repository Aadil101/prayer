{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0648\u064e\u0642\u064e\u0649\u0670\u0647\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0633\u064e\u064a\u0651\u0650\u0640\u0654\u064e\u0627\u062a\u0650 \u0645\u064e\u0627 \u0645\u064e\u0643\u064e\u0631\u064f\u0648\u0627\u0652\u06d6 \u0648\u064e\u062d\u064e\u0627\u0642\u064e \u0628\u0650\u0640\u0654\u064e\u0627\u0644\u0650 \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0633\u064f\u0648\u0653\u0621\u064f \u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u0650"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0648\u064e\u0642\u064e\u0649\u0670\u0647\u064f", "word_translation": "So Allah protected him", "word_transliteration": "fawaqahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "So Allah protected him", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0633\u064e\u064a\u0651\u0650\u0640\u0654\u064e\u0627\u062a\u0650", "word_translation": "(from the) evils", "word_transliteration": "sayyiati"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "that", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0645\u064e\u0643\u064e\u0631\u064f\u0648\u0627\u0652\u06d6", "word_translation": "they plotted", "word_transliteration": "makaru"}, {"word_arabic_uthmani": "\u0648\u064e\u062d\u064e\u0627\u0642\u064e", "word_translation": "and enveloped", "word_transliteration": "wahaqa"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u0644\u0650", "word_translation": "(the) people", "word_transliteration": "biali"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "(of) Firaun", "word_transliteration": "fir'awna"}, {"word_arabic_uthmani": "\u0633\u064f\u0648\u0653\u0621\u064f", "word_translation": "(the) worst", "word_transliteration": "suu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u0650", "word_translation": "punishment"