{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0644\u064e\u0627 \u062a\u064e\u0631\u06e1\u0641\u064e\u0639\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0635\u06e1\u0648\u064e\u0670\u062a\u064e\u0643\u064f\u0645\u06e1 \u0641\u064e\u0648\u06e1\u0642\u064e \u0635\u064e\u0648\u06e1\u062a\u0650 \u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u0650 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u062c\u06e1\u0647\u064e\u0631\u064f\u0648\u0627\u0652 \u0644\u064e\u0647\u064f\u06e5 \u0628\u0650\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u0650 \u0643\u064e\u062c\u064e\u0647\u06e1\u0631\u0650 \u0628\u064e\u0639\u06e1\u0636\u0650\u0643\u064f\u0645\u06e1 \u0644\u0650\u0628\u064e\u0639\u06e1\u0636\u064d \u0623\u064e\u0646 \u062a\u064e\u062d\u06e1\u0628\u064e\u0637\u064e \u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064f\u0643\u064f\u0645\u06e1 \u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u0644\u064e\u0627 \u062a\u064e\u0634\u06e1\u0639\u064f\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O (you who)", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(O) you who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u06e1\u0641\u064e\u0639\u064f\u0648\u0653\u0627\u0652", "word_translation": "raise", "word_transliteration": "tarfa'u"}, {"word_arabic_uthmani": "\u0623\u064e\u0635\u06e1\u0648\u064e\u0670\u062a\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your voices", "word_transliteration": "aswatakum"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u06e1\u0642\u064e", "word_translation": "above", "word_transliteration": "fawqa"}, {"word_arabic_uthmani": "\u0635\u064e\u0648\u06e1\u062a\u0650", "word_translation": "(the) voice", "word_transliteration": "sawti"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u0650", "word_translation": "(of) the Prophet", "word_transliteration": "al-nabiyi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u062c\u06e1\u0647\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "be loud", "word_transliteration": "tajharu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "to him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0642\u064e\u0648\u06e1\u0644\u0650", "word_translation": "in speech", "word_transliteration": "bil-qawli"}, {"word_arabic_uthmani": "\u0643\u064e\u062c\u064e\u0647\u06e1\u0631\u0650", "word_translation": "like (the) loudness", "word_transliteration": "kajahri"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u0650\u0643\u064f\u0645\u06e1", "word_translation": "(of) some of you", "word_transliteration": "ba'dikum"}, {"word_arabic_uthmani": "\u0644\u0650\u0628\u064e\u0639\u06e1\u0636\u064d", "word_translation": "to others", "word_transliteration": "liba'din"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "lest", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u06e1\u0628\u064e\u0637\u064e", "word_translation": "become worthless", "word_transliteration": "tahbata"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064f\u0643\u064f\u0645\u06e1", "word_translation": "your deeds", "word_transliteration": "a'malukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "while you", "word_transliteration": "wa-antum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0634\u06e1\u0639\u064f\u0631\u064f\u0648\u0646\u064e", "word_translation": "perceive", "word_transliteration": "tash'uruna"}]}