{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062c\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u0633\u0651\u064e\u062d\u064e\u0631\u064e\u0629\u064f \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u0651\u064e \u0644\u064e\u0646\u064e\u0627 \u0644\u064e\u0623\u064e\u062c\u06e1\u0631\u064b\u0627 \u0625\u0650\u0646 \u0643\u064f\u0646\u0651\u064e\u0627 \u0646\u064e\u062d\u06e1\u0646\u064f \u0671\u0644\u06e1\u063a\u064e\u0670\u0644\u0650\u0628\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "So came", "word_transliteration": "wajaa"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u062d\u064e\u0631\u064e\u0629\u064f", "word_translation": "the magicians", "word_transliteration": "al-saharatu"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "(to) Firaun", "word_transliteration": "fir'awna"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0627", "word_translation": "for us", "word_transliteration": "lana"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u064e\u062c\u06e1\u0631\u064b\u0627", "word_translation": "surely (will be) a reward", "word_transliteration": "la-ajran"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "we are", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "[we]", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u0670\u0644\u0650\u0628\u0650\u064a\u0646\u064e", "word_translation": "the victors", "word_transliteration": "al-ghalibina"}]}