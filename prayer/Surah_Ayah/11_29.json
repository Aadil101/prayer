{"ayahs": {"ayah_uthmani": "\u0648\u064e\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0644\u064e\u0627\u0653 \u0623\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064f\u0643\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0645\u064e\u0627\u0644\u064b\u0627\u06d6 \u0625\u0650\u0646\u06e1 \u0623\u064e\u062c\u06e1\u0631\u0650\u064a\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da \u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u064e\u0627\u06e0 \u0628\u0650\u0637\u064e\u0627\u0631\u0650\u062f\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0653\u0627\u0652\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0644\u064e\u0670\u0642\u064f\u0648\u0627\u0652 \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u0650\u064a\u0653 \u0623\u064e\u0631\u064e\u0649\u0670\u0643\u064f\u0645\u06e1 \u0642\u064e\u0648\u06e1\u0645\u0657\u0627 \u062a\u064e\u062c\u06e1\u0647\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "And O my people!", "word_transliteration": "wayaqawmi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627\u0653", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064f\u0643\u064f\u0645\u06e1", "word_translation": "I ask (of) you", "word_transliteration": "asalukum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "for it", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0644\u064b\u0627\u06d6", "word_translation": "any wealth", "word_transliteration": "malan"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u06e1", "word_translation": "Not", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0631\u0650\u064a\u064e", "word_translation": "(is) my reward", "word_transliteration": "ajriya"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "from", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650\u06da", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "I am", "word_transliteration": "ana"}, {"word_arabic_uthmani": "\u0628\u0650\u0637\u064e\u0627\u0631\u0650\u062f\u0650", "word_translation": "going to drive away", "word_transliteration": "bitaridi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0653\u0627\u0652\u06da", "word_translation": "believed", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645", "word_translation": "Indeed they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0644\u064e\u0670\u0642\u064f\u0648\u0627\u0652", "word_translation": "(will) be meeting", "word_transliteration": "mulaqu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their Lord", "word_transliteration": "rabbihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "but I", "word_transliteration": "walakinni"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u064e\u0649\u0670\u0643\u064f\u0645\u06e1", "word_translation": "see you", "word_transliteration": "arakum"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0657\u0627", "word_translation": "(are) a people", "word_transliteration": "qawman"}, {"word_arabic_uthmani": "\u062a\u064e\u062c\u06e1\u0647\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "ignorant", "word_transliteration": "tajhaluna"}]}