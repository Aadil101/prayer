{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0628\u064e\u062a\u0650 \u0625\u0650\u0646\u0651\u0650\u064a \u0642\u064e\u062f\u06e1 \u062c\u064e\u0627\u0653\u0621\u064e\u0646\u0650\u064a \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0639\u0650\u0644\u06e1\u0645\u0650 \u0645\u064e\u0627 \u0644\u064e\u0645\u06e1 \u064a\u064e\u0623\u06e1\u062a\u0650\u0643\u064e \u0641\u064e\u0671\u062a\u0651\u064e\u0628\u0650\u0639\u06e1\u0646\u0650\u064a\u0653 \u0623\u064e\u0647\u06e1\u062f\u0650\u0643\u064e \u0635\u0650\u0631\u064e\u0670\u0637\u0657\u0627 \u0633\u064e\u0648\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0628\u064e\u062a\u0650", "word_translation": "O my father!", "word_transliteration": "yaabati"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed [I]", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "verily", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0646\u0650\u064a", "word_translation": "(has) come to me", "word_transliteration": "jaani"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0644\u06e1\u0645\u0650", "word_translation": "the knowledge", "word_transliteration": "al-'il'mi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650\u0643\u064e", "word_translation": "came to you", "word_transliteration": "yatika"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u062a\u0651\u064e\u0628\u0650\u0639\u06e1\u0646\u0650\u064a\u0653", "word_translation": "so follow me", "word_transliteration": "fa-ittabi'ni"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u062f\u0650\u0643\u064e", "word_translation": "I will guide you", "word_transliteration": "ahdika"}, {"word_arabic_uthmani": "\u0635\u0650\u0631\u064e\u0670\u0637\u0657\u0627", "word_translation": "(to) the path", "word_transliteration": "siratan"}, {"word_arabic_uthmani": "\u0633\u064e\u0648\u0650\u064a\u0651\u0657\u0627", "word_translation": "even", "word_transliteration": "sawiyyan"}]}