{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u0650\u0643\u064f\u0644\u0651\u0656 \u062f\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u065e \u0645\u0651\u0650\u0645\u0651\u064e\u0627 \u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627\u0652\u06da \u0648\u064e\u0645\u064e\u0627 \u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0628\u0650\u063a\u064e\u0670\u0641\u0650\u0644\u064d \u0639\u064e\u0645\u0651\u064e\u0627 \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u0643\u064f\u0644\u0651\u0656", "word_translation": "And for all", "word_transliteration": "walikullin"}, {"word_arabic_uthmani": "\u062f\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u065e", "word_translation": "(will be) degrees", "word_transliteration": "darajatun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0627", "word_translation": "for what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0650\u0644\u064f\u0648\u0627\u0652\u06da", "word_translation": "they did", "word_transliteration": "amilu"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "(is) your Lord", "word_transliteration": "rabbuka"}, {"word_arabic_uthmani": "\u0628\u0650\u063a\u064e\u0670\u0641\u0650\u0644\u064d", "word_translation": "unaware", "word_transliteration": "bighafilin"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "about what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "they do", "word_transliteration": "ya'maluna"}]}