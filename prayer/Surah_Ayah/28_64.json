{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u0650\u064a\u0644\u064e \u0671\u062f\u06e1\u0639\u064f\u0648\u0627\u0652 \u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1 \u0641\u064e\u062f\u064e\u0639\u064e\u0648\u06e1\u0647\u064f\u0645\u06e1 \u0641\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0633\u06e1\u062a\u064e\u062c\u0650\u064a\u0628\u064f\u0648\u0627\u0652 \u0644\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u0631\u064e\u0623\u064e\u0648\u064f\u0627\u0652 \u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u064e\u06da \u0644\u064e\u0648\u06e1 \u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064e\u0647\u06e1\u062a\u064e\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u0650\u064a\u0644\u064e", "word_translation": "And it will be said", "word_transliteration": "waqila"}, {"word_arabic_uthmani": "\u0671\u062f\u06e1\u0639\u064f\u0648\u0627\u0652", "word_translation": "Call", "word_transliteration": "id'u"}, {"word_arabic_uthmani": "\u0634\u064f\u0631\u064e\u0643\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your partners", "word_transliteration": "shurakaakum"}, {"word_arabic_uthmani": "\u0641\u064e\u062f\u064e\u0639\u064e\u0648\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "And they will call them", "word_transliteration": "fada'awhum"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u06e1", "word_translation": "but not", "word_transliteration": "falam"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u062c\u0650\u064a\u0628\u064f\u0648\u0627\u0652", "word_translation": "they will respond", "word_transliteration": "yastajibu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "to them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0623\u064e\u0648\u064f\u0627\u0652", "word_translation": "and they will see", "word_transliteration": "wara-awu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u064e\u06da", "word_translation": "the punishment", "word_transliteration": "al-'adhaba"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1", "word_translation": "If only", "word_transliteration": "law"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "[that] they", "word_transliteration": "annahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "had been", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062a\u064e\u062f\u064f\u0648\u0646\u064e", "word_translation": "guided!", "word_transliteration": "yahtaduna"}]}