{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0643\u064f \u064a\u064e\u0646\u0641\u064e\u0639\u064f\u0647\u064f\u0645\u06e1 \u0625\u0650\u064a\u0645\u064e\u0670\u0646\u064f\u0647\u064f\u0645\u06e1 \u0644\u064e\u0645\u0651\u064e\u0627 \u0631\u064e\u0623\u064e\u0648\u06e1\u0627\u0652 \u0628\u064e\u0623\u06e1\u0633\u064e\u0646\u064e\u0627\u06d6 \u0633\u064f\u0646\u0651\u064e\u062a\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u0651\u064e\u062a\u0650\u064a \u0642\u064e\u062f\u06e1 \u062e\u064e\u0644\u064e\u062a\u06e1 \u0641\u0650\u064a \u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6\u06d6 \u0648\u064e\u062e\u064e\u0633\u0650\u0631\u064e \u0647\u064f\u0646\u064e\u0627\u0644\u0650\u0643\u064e \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u06e1", "word_translation": "But did not", "word_transliteration": "falam"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f", "word_translation": "But did not", "word_transliteration": "yaku"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u0641\u064e\u0639\u064f\u0647\u064f\u0645\u06e1", "word_translation": "benefit them", "word_transliteration": "yanfa'uhum"}, {"word_arabic_uthmani": "\u0625\u0650\u064a\u0645\u064e\u0670\u0646\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their faith", "word_transliteration": "imanuhum"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "when", "word_transliteration": "lamma"}, {"word_arabic_uthmani": "\u0631\u064e\u0623\u064e\u0648\u06e1\u0627\u0652", "word_translation": "they saw", "word_transliteration": "ra-aw"}, {"word_arabic_uthmani": "\u0628\u064e\u0623\u06e1\u0633\u064e\u0646\u064e\u0627\u06d6", "word_translation": "Our punishment", "word_transliteration": "basana"}, {"word_arabic_uthmani": "\u0633\u064f\u0646\u0651\u064e\u062a\u064e", "word_translation": "(Such is the) Way", "word_transliteration": "sunnata"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u062a\u0650\u064a", "word_translation": "which", "word_transliteration": "allati"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "(has) indeed", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u062a\u06e1", "word_translation": "preceded", "word_transliteration": "khalat"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "among", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6\u06d6", "word_translation": "His slaves", "word_transliteration": "ibadihi"}, {"word_arabic_uthmani": "\u0648\u064e\u062e\u064e\u0633\u0650\u0631\u064e", "word_translation": "And are lost", "word_transliteration": "wakhasira"}, {"word_arabic_uthmani": "\u0647\u064f\u0646\u064e\u0627\u0644\u0650\u0643\u064e", "word_translation": "there", "word_transliteration": "hunalika"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "the disbelievers", "word_transliteration": "al-kafiruna"}]}