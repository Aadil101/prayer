{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0649\u0670 \u0631\u064e\u0628\u0651\u0650\u0646\u064e\u0627 \u0644\u064e\u0645\u064f\u0646\u0642\u064e\u0644\u0650\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0627\u0653", "word_translation": "And indeed we", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0646\u064e\u0627", "word_translation": "our Lord", "word_transliteration": "rabbina"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u064f\u0646\u0642\u064e\u0644\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "will surely return", "word_transliteration": "lamunqalibuna"}]}