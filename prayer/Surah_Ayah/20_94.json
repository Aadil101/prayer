{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u064a\u064e\u0628\u06e1\u0646\u064e\u0624\u064f\u0645\u0651\u064e \u0644\u064e\u0627 \u062a\u064e\u0623\u06e1\u062e\u064f\u0630\u06e1 \u0628\u0650\u0644\u0650\u062d\u06e1\u064a\u064e\u062a\u0650\u064a \u0648\u064e\u0644\u064e\u0627 \u0628\u0650\u0631\u064e\u0623\u06e1\u0633\u0650\u064a\u0653\u06d6 \u0625\u0650\u0646\u0651\u0650\u064a \u062e\u064e\u0634\u0650\u064a\u062a\u064f \u0623\u064e\u0646 \u062a\u064e\u0642\u064f\u0648\u0644\u064e \u0641\u064e\u0631\u0651\u064e\u0642\u06e1\u062a\u064e \u0628\u064e\u064a\u06e1\u0646\u064e \u0628\u064e\u0646\u0650\u064a\u0653 \u0625\u0650\u0633\u06e1\u0631\u064e\u0640\u0670\u0653\u0621\u0650\u064a\u0644\u064e \u0648\u064e\u0644\u064e\u0645\u06e1 \u062a\u064e\u0631\u06e1\u0642\u064f\u0628\u06e1 \u0642\u064e\u0648\u06e1\u0644\u0650\u064a"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u064a\u064e\u0628\u06e1\u0646\u064e\u0624\u064f\u0645\u0651\u064e", "word_translation": "O son of my mother", "word_transliteration": "yabna-umma"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u062e\u064f\u0630\u06e1", "word_translation": "seize (me)", "word_transliteration": "takhudh"}, {"word_arabic_uthmani": "\u0628\u0650\u0644\u0650\u062d\u06e1\u064a\u064e\u062a\u0650\u064a", "word_translation": "by my beard", "word_transliteration": "bilih'yati"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u0623\u06e1\u0633\u0650\u064a\u0653\u06d6", "word_translation": "by my head", "word_transliteration": "birasi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed, I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u062e\u064e\u0634\u0650\u064a\u062a\u064f", "word_translation": "[I] feared", "word_transliteration": "khashitu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u064f\u0648\u0644\u064e", "word_translation": "you would say", "word_transliteration": "taqula"}, {"word_arabic_uthmani": "\u0641\u064e\u0631\u0651\u064e\u0642\u06e1\u062a\u064e", "word_translation": "You caused division", "word_transliteration": "farraqta"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e", "word_translation": "between", "word_transliteration": "bayna"}, {"word_arabic_uthmani": "\u0628\u064e\u0646\u0650\u064a\u0653", "word_translation": "(the) Children of Israel", "word_transliteration": "bani"}, {"word_arabic_uthmani": "\u0625\u0650\u0633\u06e1\u0631\u064e\u0640\u0670\u0653\u0621\u0650\u064a\u0644\u064e", "word_translation": "(the) Children of Israel", "word_transliteration": "is'raila"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "and not", "word_transliteration": "walam"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u06e1\u0642\u064f\u0628\u06e1", "word_translation": "you respect", "word_transliteration": "tarqub"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0644\u0650\u064a", "word_translation": "my word", "word_transliteration": "qawli"}]}