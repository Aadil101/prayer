{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064f\u0631\u06e1\u0633\u0650\u0644\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u062d\u064e\u0670\u0641\u0650\u0638\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "But not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064f\u0631\u06e1\u0633\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "they had been sent", "word_transliteration": "ur'silu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "over them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u062d\u064e\u0670\u0641\u0650\u0638\u0650\u064a\u0646\u064e", "word_translation": "(as) guardians", "word_transliteration": "hafizina"}]}