{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0627 \u0644\u064e\u0627 \u0646\u064e\u062f\u06e1\u0631\u0650\u064a\u0653 \u0623\u064e\u0634\u064e\u0631\u0651\u064c \u0623\u064f\u0631\u0650\u064a\u062f\u064e \u0628\u0650\u0645\u064e\u0646 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0623\u064e\u0645\u06e1 \u0623\u064e\u0631\u064e\u0627\u062f\u064e \u0628\u0650\u0647\u0650\u0645\u06e1 \u0631\u064e\u0628\u0651\u064f\u0647\u064f\u0645\u06e1 \u0631\u064e\u0634\u064e\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0627", "word_translation": "And that we", "word_transliteration": "wa-anna"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0646\u064e\u062f\u06e1\u0631\u0650\u064a\u0653", "word_translation": "we know", "word_transliteration": "nadri"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u064e\u0631\u0651\u064c", "word_translation": "whether evil", "word_transliteration": "asharrun"}, {"word_arabic_uthmani": "\u0623\u064f\u0631\u0650\u064a\u062f\u064e", "word_translation": "is intended", "word_transliteration": "urida"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0646", "word_translation": "for (those) who", "word_transliteration": "biman"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(are) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u064e\u0627\u062f\u064e", "word_translation": "intends", "word_transliteration": "arada"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u0645\u06e1", "word_translation": "for them", "word_transliteration": "bihim"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their Lord", "word_transliteration": "rabbuhum"}, {"word_arabic_uthmani": "\u0631\u064e\u0634\u064e\u062f\u0657\u0627", "word_translation": "a right path", "word_transliteration": "rashadan"}]}