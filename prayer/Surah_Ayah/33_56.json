{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u062a\u064e\u0647\u064f\u06e5 \u064a\u064f\u0635\u064e\u0644\u0651\u064f\u0648\u0646\u064e \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u0650\u06da \u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0635\u064e\u0644\u0651\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0648\u064e\u0633\u064e\u0644\u0651\u0650\u0645\u064f\u0648\u0627\u0652 \u062a\u064e\u0633\u06e1\u0644\u0650\u064a\u0645\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u062a\u064e\u0647\u064f\u06e5", "word_translation": "and His Angels", "word_transliteration": "wamalaikatahu"}, {"word_arabic_uthmani": "\u064a\u064f\u0635\u064e\u0644\u0651\u064f\u0648\u0646\u064e", "word_translation": "send blessings", "word_transliteration": "yusalluna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "upon", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u0650\u06da", "word_translation": "the Prophet", "word_transliteration": "al-nabiyi"}, {"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O you who believe", "word_transliteration": "yaayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "O you who believe", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "O you who believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0635\u064e\u0644\u0651\u064f\u0648\u0627\u0652", "word_translation": "Send blessings", "word_transliteration": "sallu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "on him", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0648\u064e\u0633\u064e\u0644\u0651\u0650\u0645\u064f\u0648\u0627\u0652", "word_translation": "and greet him", "word_transliteration": "wasallimu"}, {"word_arabic_uthmani": "\u062a\u064e\u0633\u06e1\u0644\u0650\u064a\u0645\u064b\u0627", "word_translation": "(with) greetings", "word_transliteration": "tasliman"}]}