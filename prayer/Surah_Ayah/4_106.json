{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u064e\u06d6 \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0643\u064e\u0627\u0646\u064e \u063a\u064e\u0641\u064f\u0648\u0631\u0657\u0627 \u0631\u0651\u064e\u062d\u0650\u064a\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u0650", "word_translation": "And seek forgiveness", "word_transliteration": "wa-is'taghfiri"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e\u06d6", "word_translation": "(of) Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u063a\u064e\u0641\u064f\u0648\u0631\u0657\u0627", "word_translation": "Oft-Forgiving", "word_transliteration": "ghafuran"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u0657\u0627", "word_translation": "Most Merciful", "word_transliteration": "rahiman"}]}