{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0631\u064e\u062d\u06e1\u0645\u064e\u0629\u0657 \u0644\u0651\u0650\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e", "word_translation": "We have sent you", "word_transliteration": "arsalnaka"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0631\u064e\u062d\u06e1\u0645\u064e\u0629\u0657", "word_translation": "(as) a mercy", "word_transliteration": "rahmatan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "for the worlds", "word_transliteration": "lil'alamina"}]}