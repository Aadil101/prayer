{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0648\u06e1 \u0646\u064e\u0634\u064e\u0627\u0653\u0621\u064f \u0644\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f \u062d\u064f\u0637\u064e\u0670\u0645\u0657\u0627 \u0641\u064e\u0638\u064e\u0644\u06e1\u062a\u064f\u0645\u06e1 \u062a\u064e\u0641\u064e\u0643\u0651\u064e\u0647\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1", "word_translation": "If", "word_transliteration": "law"}, {"word_arabic_uthmani": "\u0646\u064e\u0634\u064e\u0627\u0653\u0621\u064f", "word_translation": "We willed", "word_transliteration": "nashau"}, {"word_arabic_uthmani": "\u0644\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f", "word_translation": "We (would) surely make it", "word_transliteration": "laja'alnahu"}, {"word_arabic_uthmani": "\u062d\u064f\u0637\u064e\u0670\u0645\u0657\u0627", "word_translation": "debris", "word_transliteration": "hutaman"}, {"word_arabic_uthmani": "\u0641\u064e\u0638\u064e\u0644\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "then you would remain", "word_transliteration": "fazaltum"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u064e\u0643\u0651\u064e\u0647\u064f\u0648\u0646\u064e", "word_translation": "wondering", "word_transliteration": "tafakkahuna"}]}