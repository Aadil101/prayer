{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653 \u0623\u064e\u0644\u06e1\u0642\u064e\u0648\u06e1\u0627\u0652 \u0642\u064e\u0627\u0644\u064e \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0645\u064e\u0627 \u062c\u0650\u0626\u06e1\u062a\u064f\u0645 \u0628\u0650\u0647\u0650 \u0671\u0644\u0633\u0651\u0650\u062d\u06e1\u0631\u064f\u06d6 \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0633\u064e\u064a\u064f\u0628\u06e1\u0637\u0650\u0644\u064f\u0647\u064f\u06e5\u0653 \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0627 \u064a\u064f\u0635\u06e1\u0644\u0650\u062d\u064f \u0639\u064e\u0645\u064e\u0644\u064e \u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653", "word_translation": "Then when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u06e1\u0642\u064e\u0648\u06e1\u0627\u0652", "word_translation": "they (had) thrown", "word_transliteration": "alqaw"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Musa said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa said", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "What", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062c\u0650\u0626\u06e1\u062a\u064f\u0645", "word_translation": "you have brought", "word_transliteration": "ji'tum"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650", "word_translation": "[it]", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u0650\u062d\u06e1\u0631\u064f\u06d6", "word_translation": "(is) the magic", "word_transliteration": "al-sih'ru"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_tra