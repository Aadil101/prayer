\u064e\u0623\u064f\u0648\u064e\u0670\u0631\u0650\u064a\u064e", "word_translation": "and hide", "word_transliteration": "fa-uwariya"}, {"word_arabic_uthmani": "\u0633\u064e\u0648\u06e1\u0621\u064e\u0629\u064e", "word_translation": "(the) dead body", "word_transliteration": "sawata"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u0650\u064a\u06d6", "word_translation": "(of) my brother", "word_transliteration": "akhi"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0635\u06e1\u0628\u064e\u062d\u064e", "word_translation": "Then he became", "word_transliteration": "fa-asbaha"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0640\u0670\u062f\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the regretful", "word_transliteration": "al-nadimina"}]}