{"ayahs": {"ayah_uthmani": "\u0631\u064e\u0628\u0651\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627 \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f \u0671\u0644\u06e1\u063a\u064e\u0641\u0651\u064e\u0640\u0670\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f", "word_translation": "Lord", "word_transliteration": "rabbu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627", "word_translation": "(is) between them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f", "word_translation": "the All-Mighty", "word_transliteration": "al-'azizu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u0641\u0651\u064e\u0640\u0670\u0631\u064f", "word_translation": "the Oft-Forgiving", "word_transliteration": "al-ghafaru"}]}