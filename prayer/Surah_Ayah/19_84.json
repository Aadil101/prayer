{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u0639\u06e1\u062c\u064e\u0644\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1\u06d6 \u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0646\u064e\u0639\u064f\u062f\u0651\u064f \u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u062f\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "So (do) not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u062c\u064e\u0644\u06e1", "word_translation": "make haste", "word_transliteration": "ta'jal"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1\u06d6", "word_translation": "against them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0646\u064e\u0639\u064f\u062f\u0651\u064f", "word_translation": "We count", "word_transliteration": "na'uddu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0639\u064e\u062f\u0651\u0657\u0627", "word_translation": "a number", "word_transliteration": "addan"}]}