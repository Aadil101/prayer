{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u064a\u064e\u0670\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653 \u0625\u0650\u0646\u0651\u064e\u0627 \u0644\u064e\u0646 \u0646\u0651\u064e\u062f\u06e1\u062e\u064f\u0644\u064e\u0647\u064e\u0627\u0653 \u0623\u064e\u0628\u064e\u062f\u0657\u0627 \u0645\u0651\u064e\u0627 \u062f\u064e\u0627\u0645\u064f\u0648\u0627\u0652 \u0641\u0650\u064a\u0647\u064e\u0627 \u0641\u064e\u0671\u0630\u06e1\u0647\u064e\u0628\u06e1 \u0623\u064e\u0646\u062a\u064e \u0648\u064e\u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0641\u064e\u0642\u064e\u0670\u062a\u0650\u0644\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e\u0627 \u0647\u064e\u0670\u0647\u064f\u0646\u064e\u0627 \u0642\u064e\u0670\u0639\u0650\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653", "word_translation": "O Musa", "word_transliteration": "yamusa"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed, we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0644\u064e\u0646", "word_translation": "never", "word_transliteration": "lan"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u062f\u06e1\u062e\u064f\u0644\u064e\u0647\u064e\u0627\u0653", "word_translation": "will enter it", "word_transliteration": "nadkhulaha"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u064e\u062f\u0657\u0627", "word_translation": "ever", "word_transliteration": "abadan"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "for", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0645\u064f\u0648\u0627\u0652", "word_translation": "as long as they are", "word_transliteration": "damu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "in it", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0630\u06e1\u0647\u064e\u0628\u06e1", "word_translation": "So go", "word_transliteration": "fa-idh'hab"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "you", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "and your Lord", "word_transliteration": "warabbuka"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u0670\u062a\u0650\u0644\u064e\u0627\u0653", "word_translation": "and you both fight", "word_transliteration": "faqatila"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed, we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0647\u064f\u0646\u064e\u0627", "word_translation": "are [here]", "word_transliteration": "hahuna"}, {"word_arabic_uthmani": "\u0642\u064e\u0670\u0639\u0650\u062f\u064f\u0648\u0646\u064e", "word_translation": "sitting", "word_transliteration": "qa'iduna"}]}