{"ayahs": {"ayah_uthmani": "\u064a\u064f\u0648\u0641\u064f\u0648\u0646\u064e \u0628\u0650\u0671\u0644\u0646\u0651\u064e\u0630\u06e1\u0631\u0650 \u0648\u064e\u064a\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0646\u064e \u064a\u064e\u0648\u06e1\u0645\u0657\u0627 \u0643\u064e\u0627\u0646\u064e \u0634\u064e\u0631\u0651\u064f\u0647\u064f\u06e5 \u0645\u064f\u0633\u06e1\u062a\u064e\u0637\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u0648\u0641\u064f\u0648\u0646\u064e", "word_translation": "They fulfill", "word_transliteration": "yufuna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0646\u0651\u064e\u0630\u06e1\u0631\u0650", "word_translation": "the vows", "word_transliteration": "bil-nadhri"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0646\u064e", "word_translation": "and fear", "word_transliteration": "wayakhafuna"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u0657\u0627", "word_translation": "a Day", "word_transliteration": "yawman"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "(which) is", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0634\u064e\u0631\u0651\u064f\u0647\u064f\u06e5", "word_translation": "its evil", "word_transliteration": "sharruhu"}, {"word_arabic_uthmani": "\u0645\u064f\u0633\u06e1\u062a\u064e\u0637\u0650\u064a\u0631\u0657\u0627", "word_translation": "widespread", "word_transliteration": "mus'tatiran"}]}