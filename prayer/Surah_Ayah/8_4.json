{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u062d\u064e\u0642\u0651\u0657\u0627\u06da \u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u062f\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u064c \u0639\u0650\u0646\u062f\u064e \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0645\u064e\u063a\u06e1\u0641\u0650\u0631\u064e\u0629\u065e \u0648\u064e\u0631\u0650\u0632\u06e1\u0642\u065e \u0643\u064e\u0631\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "they are", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "the believers", "word_transliteration": "al-mu'minuna"}, {"word_arabic_uthmani": "\u062d\u064e\u0642\u0651\u0657\u0627\u06da", "word_translation": "(in) truth", "word_transliteration": "haqqan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "For them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u062f\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u064c", "word_translation": "(are) ranks", "word_transliteration": "darajatun"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "with", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their Lord", "word_transliteration": "rabbihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u063a\u06e1\u0641\u0650\u0631\u064e\u0629\u065e", "word_translation": "and forgiveness", "word_transliteration": "wamaghfiratun"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u0650\u0632\u06e1\u0642\u065e", "word_translation": "and a provision", "word_transliteration": "wariz'qun"}, {"word_arabic_uthmani": "\u0643\u064e\u0631\u0650\u064a\u0645\u065e", "word_translation": "noble", "word_transliteration": "karimun"}]}