{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1 \u0631\u064f\u0633\u064f\u0644\u064f\u0646\u064e\u0627 \u0644\u064f\u0648\u0637\u0657\u0627 \u0633\u0650\u064a\u0653\u0621\u064e \u0628\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0636\u064e\u0627\u0642\u064e \u0628\u0650\u0647\u0650\u0645\u06e1 \u0630\u064e\u0631\u06e1\u0639\u0657\u0627 \u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0647\u064e\u0670\u0630\u064e\u0627 \u064a\u064e\u0648\u06e1\u0645\u064c \u0639\u064e\u0635\u0650\u064a\u0628\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "And when", "word_transliteration": "walamma"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u062a\u06e1", "word_translation": "came", "word_transliteration": "jaat"}, {"word_arabic_uthmani": "\u0631\u064f\u0633\u064f\u0644\u064f\u0646\u064e\u0627", "word_translation": "Our messengers", "word_transliteration": "rusuluna"}, {"word_arabic_uthmani": "\u0644\u064f\u0648\u0637\u0657\u0627", "word_translation": "(to) Lut", "word_transliteration": "lutan"}, {"word_arabic_uthmani": "\u0633\u0650\u064a\u0653\u0621\u064e", "word_translation": "he was distressed", "word_transliteration": "sia"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u0645\u06e1", "word_translation": "for them", "word_transliteration": "bihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0636\u064e\u0627\u0642\u064e", "word_translation": "and felt straitened", "word_transliteration": "wadaqa"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u0645\u06e1", "word_translation": "for them", "word_transliteration": "bihim"}, {"word_arabic_uthmani": "\u0630\u064e\u0631\u06e1\u0639\u0657\u0627", "word_translation": "(and) uneasy", "word_transliteration": "dhar'an"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "and said", "word_transliteration": "waqala"}, {"word_arabic_uthman