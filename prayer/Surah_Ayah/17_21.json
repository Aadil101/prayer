{"ayahs": {"ayah_uthmani": "\u0671\u0646\u0638\u064f\u0631\u06e1 \u0643\u064e\u064a\u06e1\u0641\u064e \u0641\u064e\u0636\u0651\u064e\u0644\u06e1\u0646\u064e\u0627 \u0628\u064e\u0639\u06e1\u0636\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u0649\u0670 \u0628\u064e\u0639\u06e1\u0636\u0656\u06da \u0648\u064e\u0644\u064e\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u064f \u0623\u064e\u0643\u06e1\u0628\u064e\u0631\u064f \u062f\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u0656 \u0648\u064e\u0623\u064e\u0643\u06e1\u0628\u064e\u0631\u064f \u062a\u064e\u0641\u06e1\u0636\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0671\u0646\u0638\u064f\u0631\u06e1", "word_translation": "See", "word_transliteration": "unzur"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "how", "word_transliteration": "kayfa"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u0651\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We preferred", "word_transliteration": "faddalna"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064e\u0647\u064f\u0645\u06e1", "word_translation": "some of them", "word_transliteration": "ba'dahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u0656\u06da", "word_translation": "others", "word_transliteration": "ba'din"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u064f", "word_translation": "And surely the Hereafter", "word_transliteration": "walalakhiratu"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u0628\u064e\u0631\u064f", "word_translation": "(is) greater", "word_transliteration": "akbaru"}, {"word_arabic_uthmani": "\u062f\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u0656", "word_translation": "(in) degrees", "word_transliteration": "darajatin"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0643\u06e1\u0628\u064e\u0631\u064f", "word_translation": "and greater", "word_transliteration": "wa-akbaru"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u06e1\u0636\u0650\u064a\u0644\u0657\u0627", "word_translation": "(in) excellence", "word_transliteration": "tafdilan"}]}