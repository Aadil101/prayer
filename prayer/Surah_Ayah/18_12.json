{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u0628\u064e\u0639\u064e\u062b\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0644\u0650\u0646\u064e\u0639\u06e1\u0644\u064e\u0645\u064e \u0623\u064e\u064a\u0651\u064f \u0671\u0644\u06e1\u062d\u0650\u0632\u06e1\u0628\u064e\u064a\u06e1\u0646\u0650 \u0623\u064e\u062d\u06e1\u0635\u064e\u0649\u0670 \u0644\u0650\u0645\u064e\u0627 \u0644\u064e\u0628\u0650\u062b\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0645\u064e\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u064e\u062b\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "We raised them up", "word_transliteration": "ba'athnahum"}, {"word_arabic_uthmani": "\u0644\u0650\u0646\u064e\u0639\u06e1\u0644\u064e\u0645\u064e", "word_translation": "that We make evident", "word_transliteration": "lina'lama"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u0651\u064f", "word_translation": "which", "word_transliteration": "ayyu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u0650\u0632\u06e1\u0628\u064e\u064a\u06e1\u0646\u0650", "word_translation": "(of) the two parties", "word_transliteration": "al-hiz'bayni"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u06e1\u0635\u064e\u0649\u0670", "word_translation": "best calculated", "word_transliteration": "ahsa"}, {"word_arabic_uthmani": "\u0644\u0650\u0645\u064e\u0627", "word_translation": "for what", "word_transliteration": "lima"}, {"word_arabic_uthmani": "\u0644\u064e\u0628\u0650\u062b\u064f\u0648\u0653\u0627\u0652", "word_translation": "(they had) remained", "word_transliteration": "labithu"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u064e\u062f\u0657\u0627", "word_translation": "(in) time", "word_transliteration": "amadan"}]}