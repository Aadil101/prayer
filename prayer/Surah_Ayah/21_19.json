{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u06e5 \u0645\u064e\u0646 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da \u0648\u064e\u0645\u064e\u0646\u06e1 \u0639\u0650\u0646\u062f\u064e\u0647\u064f\u06e5 \u0644\u064e\u0627 \u064a\u064e\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e \u0639\u064e\u0646\u06e1 \u0639\u0650\u0628\u064e\u0627\u062f\u064e\u062a\u0650\u0647\u0650\u06e6 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0633\u06e1\u062a\u064e\u062d\u06e1\u0633\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0647\u064f\u06e5", "word_translation": "And to Him (belongs)", "word_transliteration": "walahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whoever", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06da", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646\u06e1", "word_translation": "And (those) who", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e\u0647\u064f\u06e5", "word_translation": "(are) near Him", "word_transliteration": "indahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "they are arrogant", "word_transliteration": "yastakbiruna"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1", "word_translation": "to", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u064e\u062a\u0650\u0647\u0650\u06e6", "word_translation": "worship Him", "word_transliteration": "ibadatihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u062d\u06e1\u0633\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "they tire", "word_transliteration": "yastahsiruna"}]}