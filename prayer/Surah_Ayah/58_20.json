{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064f\u062d\u064e\u0627\u0653\u062f\u0651\u064f\u0648\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5\u0653 \u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0630\u064e\u0644\u0651\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u064e\u0627\u0653\u062f\u0651\u064f\u0648\u0646\u064e", "word_translation": "oppose", "word_transliteration": "yuhadduna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5\u0653", "word_translation": "and His Messenger", "word_transliteration": "warasulahu"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(will be) among", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0630\u064e\u0644\u0651\u0650\u064a\u0646\u064e", "word_translation": "the most humiliated", "word_transliteration": "al-adhalina"}]}