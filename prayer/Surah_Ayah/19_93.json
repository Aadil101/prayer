{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646 \u0643\u064f\u0644\u0651\u064f \u0645\u064e\u0646 \u0641\u0650\u064a \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0625\u0650\u0644\u0651\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u0650\u064a \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650 \u0639\u064e\u0628\u06e1\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "Not", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064f", "word_translation": "all", "word_transliteration": "kullu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(are) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627\u0653", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u0650\u064a", "word_translation": "(will) come", "word_transliteration": "ati"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650", "word_translation": "(to) the Most Gracious", "word_transliteration": "al-rahmani"}, {"word_arabic_uthmani": "\u0639\u064e\u0628\u06e1\u062f\u0657\u0627", "word_translation": "(as) a slave", "word_transliteration": "abdan"}]}