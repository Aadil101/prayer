n": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u06e1", "word_translation": "[and] but", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0647\u064f\u0645\u06e1", "word_translation": "themselves", "word_transliteration": "anfusahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0638\u06e1\u0644\u0650\u0645\u064f\u0648\u0646\u064e", "word_translation": "they wronged", "word_transliteration": "yazlimuna"}]}