{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u062c\u064e\u0639\u064e\u0644\u064e\u0643\u064f\u0645\u06e1 \u062e\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0641\u064e \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0648\u064e\u0631\u064e\u0641\u064e\u0639\u064e \u0628\u064e\u0639\u06e1\u0636\u064e\u0643\u064f\u0645\u06e1 \u0641\u064e\u0648\u06e1\u0642\u064e \u0628\u064e\u0639\u06e1\u0636\u0656 \u062f\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u0656 \u0644\u0651\u0650\u064a\u064e\u0628\u06e1\u0644\u064f\u0648\u064e\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a \u0645\u064e\u0627\u0653 \u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0643\u064f\u0645\u06e1\u06d7 \u0625\u0650\u0646\u0651\u064e \u0631\u064e\u0628\u0651\u064e\u0643\u064e \u0633\u064e\u0631\u0650\u064a\u0639\u064f \u0671\u0644\u06e1\u0639\u0650\u0642\u064e\u0627\u0628\u0650 \u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u063a\u064e\u0641\u064f\u0648\u0631\u065e \u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f\u06e2"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(is) the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(has) made you", "word_transliteration": "ja'alakum"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0641\u064e", "word_translation": "successors", "word_transliteration": "khalaifa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "(of) the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0641\u064e\u0639\u064e", "word_translation": "and raised", "word_transliteration": "warafa'a"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064e\u0643\u064f\u0645\u06e1", "word_translation": "some of you", "word_transliteration": "ba'dakum"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u06e1\u0642\u064e", "word_translation": "above", "word_transliteration": "fawqa"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u0656", "word_translation": "others", "word_transliteration": "ba'din"}, {"word_arabic_uthmani": "\u062f\u064e\u0631\u064e\u062c\u064e\u0670\u062a\u0656", "word_translation": "(in) ranks", "word_transliteration": "darajatin"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u064a\u064e\u0628\u06e1\u0644\u064f\u0648\u064e\u0643\u064f\u0645\u06e1", "word_translation": "so that He may test you", "word_transliteration": "liyabluwakum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u062a\u064e\u0649\u0670\u0643\u064f\u0645\u06e1\u06d7", "word_translation": "He has given you", "word_transliteration": "atakum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbaka"}, {"word_arabic_uthmani": "\u0633\u064e\u0631\u0650\u064a\u0639\u064f", "word_translation": "(is) swift", "word_transliteration": "sari'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0642\u064e\u0627\u0628\u0650", "word_translation": "(in) the punishment", "word_transliteration": "al-'iqabi"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "and indeed He (is)", "word_transliteration": "wa-innahu"}, {"word_arabic_uthmani": "\u0644\u064e\u063a\u064e\u0641\u064f\u0648\u0631\u065e", "word_translation": "[certainly] Oft-Forgiving", "word_transliteration": "laghafurun"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f\u06e2", "word_translation": "Most Merciful", "word_transliteration": "rahimun"}]}