{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0644\u064e\u0643\u064f\u0645\u06e1 \u0644\u064e\u0627 \u062a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064f \u064a\u064e\u062f\u06e1\u0639\u064f\u0648\u0643\u064f\u0645\u06e1 \u0644\u0650\u062a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0627\u0652 \u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0643\u064f\u0645\u06e1 \u0648\u064e\u0642\u064e\u062f\u06e1 \u0623\u064e\u062e\u064e\u0630\u064e \u0645\u0650\u064a\u062b\u064e\u0670\u0642\u064e\u0643\u064f\u0645\u06e1 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645 \u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "(is) for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(that) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "you believe", "word_transliteration": "tu'minuna"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "in Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064f", "word_translation": "while the Messenger", "word_transliteration": "wal-rasulu"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u06e1\u0639\u064f\u0648\u0643\u064f\u0645\u06e1", "word_translation": "calls you", "word_transliteration": "yad'ukum"}, {"word_arabic_uthmani": "\u0644\u0650\u062a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0627\u0652", "word_translation": "that you believe", "word_transliteration": "litu'minu"}, {"word_arabic_uthmani": "\u0628\u0650\u0631\u064e\u0628\u0651\u0650\u0643\u064f\u0645\u06e1", "word_translation": "in your Lord", "word_transliteration": "birabbikum"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1", "word_translation": "and indeed", "word_transliteration": "waqad"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u064e\u0630\u064e", "word_translation": "He has taken", "word_transliteration": "akhadha"}, {"word_arabic_uthmani": "\u0645\u0650\u064a\u062b\u064e\u0670\u0642\u064e\u0643\u064f\u0645\u06e1", "word_translation": "your covenant", "word_transliteration": "mithaqakum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645", "word_translation": "you are", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "believers", "word_transliteration": "mu'minina"}]}