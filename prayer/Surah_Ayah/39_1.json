{"ayahs": {"ayah_uthmani": "\u062a\u064e\u0646\u0632\u0650\u064a\u0644\u064f \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u0645\u0650\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u0650 \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u062a\u064e\u0646\u0632\u0650\u064a\u0644\u064f", "word_translation": "(The) revelation", "word_transliteration": "tanzilu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "(of) the Book", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "(is) from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u0650", "word_translation": "the All-Mighty", "word_transliteration": "al-'azizi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u0650", "word_translation": "the All-Wise", "word_transliteration": "al-hakimi"}]}