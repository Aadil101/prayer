{"ayahs": {"ayah_uthmani": "\u062e\u064f\u0634\u0651\u064e\u0639\u064b\u0627 \u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064f\u0647\u064f\u0645\u06e1 \u064a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0646\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u064e\u062c\u06e1\u062f\u064e\u0627\u062b\u0650 \u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u062c\u064e\u0631\u064e\u0627\u062f\u065e \u0645\u0651\u064f\u0646\u062a\u064e\u0634\u0650\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u062e\u064f\u0634\u0651\u064e\u0639\u064b\u0627", "word_translation": "(Will be) humbled", "word_transliteration": "khusha'an"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their eyes", "word_transliteration": "absaruhum"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0646\u064e", "word_translation": "they will come forth", "word_transliteration": "yakhrujuna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u062c\u06e1\u062f\u064e\u0627\u062b\u0650", "word_translation": "the graves", "word_transliteration": "al-ajdathi"}, {"word_arabic_uthmani": "\u0643\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "as if they (were)", "word_transliteration": "ka-annahum"}, {"word_arabic_uthmani": "\u062c\u064e\u0631\u064e\u0627\u062f\u065e", "word_translation": "locusts", "word_transliteration": "jaradun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0646\u062a\u064e\u0634\u0650\u0631\u065e", "word_translation": "spreading", "word_transliteration": "muntashirun"}]}