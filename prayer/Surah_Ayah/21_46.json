{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0626\u0650\u0646 \u0645\u0651\u064e\u0633\u0651\u064e\u062a\u06e1\u0647\u064f\u0645\u06e1 \u0646\u064e\u0641\u06e1\u062d\u064e\u0629\u065e \u0645\u0651\u0650\u0646\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u0650 \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0644\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0646\u0651\u064e \u064a\u064e\u0670\u0648\u064e\u064a\u06e1\u0644\u064e\u0646\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e\u0627 \u0643\u064f\u0646\u0651\u064e\u0627 \u0638\u064e\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0626\u0650\u0646", "word_translation": "And if", "word_transliteration": "wala-in"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0633\u0651\u064e\u062a\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "touches them", "word_transliteration": "massathum"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u062d\u064e\u0629\u065e", "word_translation": "a whiff", "word_transliteration": "nafhatun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u0650", "word_translation": "(the) punishment", "word_transliteration": "adhabi"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "(of) your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0646\u0651\u064e", "word_translation": "surely they will say", "word_transliteration": "layaqulunna"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0648\u064e\u064a\u06e1\u0644\u064e\u0646\u064e\u0627\u0653", "word_translation": "O woe to us!", "word_transliteration": "yawaylana"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u0651\u064e\u0627", "word_translation": "[we] were", "word_transliteration": "kunna"}, {"word_arabic_uthmani": "\u0638\u064e\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "wrongdoers", "word_transliteration": "zalimina"}]}