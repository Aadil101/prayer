{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0644\u064e\u0626\u0650\u0646 \u0644\u0651\u064e\u0645\u06e1 \u062a\u064e\u0646\u062a\u064e\u0647\u0650 \u064a\u064e\u0670\u0646\u064f\u0648\u062d\u064f \u0644\u064e\u062a\u064e\u0643\u064f\u0648\u0646\u064e\u0646\u0651\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064e\u0631\u06e1\u062c\u064f\u0648\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0644\u064e\u0626\u0650\u0646", "word_translation": "If", "word_transliteration": "la-in"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u062a\u064e\u0646\u062a\u064e\u0647\u0650", "word_translation": "you desist", "word_transliteration": "tantahi"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0646\u064f\u0648\u062d\u064f", "word_translation": "O Nuh!", "word_transliteration": "yanuhu"}, {"word_arabic_uthmani": "\u0644\u064e\u062a\u064e\u0643\u064f\u0648\u0646\u064e\u0646\u0651\u064e", "word_translation": "Surely you will be", "word_transliteration": "latakunanna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0631\u06e1\u062c\u064f\u0648\u0645\u0650\u064a\u0646\u064e", "word_translation": "those who are stoned", "word_transliteration": "al-marjumina"}]}