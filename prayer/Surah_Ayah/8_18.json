{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u06e1 \u0648\u064e\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0645\u064f\u0648\u0647\u0650\u0646\u064f \u0643\u064e\u064a\u06e1\u062f\u0650 \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064f\u0645\u06e1", "word_translation": "That (is the case)", "word_transliteration": "dhalikum"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e", "word_translation": "and that", "word_transliteration": "wa-anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah (is)", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0647\u0650\u0646\u064f", "word_translation": "one who makes weak", "word_transliteration": "muhinu"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u062f\u0650", "word_translation": "(the) plan", "word_transliteration": "kaydi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(of) the disbelievers", "word_transliteration": "al-kafirina"}]}