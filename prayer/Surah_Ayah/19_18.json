{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e\u062a\u06e1 \u0625\u0650\u0646\u0651\u0650\u064a\u0653 \u0623\u064e\u0639\u064f\u0648\u0630\u064f \u0628\u0650\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650 \u0645\u0650\u0646\u0643\u064e \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064e \u062a\u064e\u0642\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e\u062a\u06e1", "word_translation": "She said", "word_transliteration": "qalat"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u064f\u0648\u0630\u064f", "word_translation": "[I] seek refuge", "word_transliteration": "a'udhu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650", "word_translation": "with the Most Gracious", "word_transliteration": "bil-rahmani"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0643\u064e", "word_translation": "from you", "word_transliteration": "minka"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064e", "word_translation": "you are", "word_transliteration": "kunta"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u0650\u064a\u0651\u0657\u0627", "word_translation": "God fearing", "word_transliteration": "taqiyyan"}]}