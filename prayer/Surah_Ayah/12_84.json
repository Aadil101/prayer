{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0649\u0670 \u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0648\u064e\u0642\u064e\u0627\u0644\u064e \u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0633\u064e\u0641\u064e\u0649\u0670 \u0639\u064e\u0644\u064e\u0649\u0670 \u064a\u064f\u0648\u0633\u064f\u0641\u064e \u0648\u064e\u0671\u0628\u06e1\u064a\u064e\u0636\u0651\u064e\u062a\u06e1 \u0639\u064e\u064a\u06e1\u0646\u064e\u0627\u0647\u064f \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u062d\u064f\u0632\u06e1\u0646\u0650 \u0641\u064e\u0647\u064f\u0648\u064e \u0643\u064e\u0638\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0649\u0670", "word_translation": "And he turned away", "word_transliteration": "watawalla"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "from them", "word_transliteration": "anhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "and said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0633\u064e\u0641\u064e\u0649\u0670", "word_translation": "Alas my grief", "word_transliteration": "yaasafa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0633\u064f\u0641\u064e", "word_translation": "Yusuf!", "word_transliteration": "yusufa"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0628\u06e1\u064a\u064e\u0636\u0651\u064e\u062a\u06e1", "word_translation": "And became white", "word_transliteration": "wa-ib'yaddat"}, {"word_arabic_uthmani": "\u0639\u064e\u064a\u06e1\u0646\u064e\u0627\u0647\u064f", "word_translation": "his eyes", "word_transliteration": "aynahu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064f\u0632\u06e1\u0646\u0650", "word_translation": "the grief", "word_transliteration": "al-huz'ni"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0648\u064e", "word_translation": "and he (was)", "word_transliteration": "fahuwa"}, {"word_arabic_uthmani": "\u0643\u064e\u0638\u0650\u064a\u0645\u065e", "word_translation": "a suppressor", "word_transliteration": "kazimun"}]}