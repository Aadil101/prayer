{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064e \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627 \u0628\u064e\u0670\u0637\u0650\u0644\u0657\u0627\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0638\u064e\u0646\u0651\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652\u06da \u0641\u064e\u0648\u064e\u064a\u06e1\u0644\u065e \u0644\u0651\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627", "word_translation": "We created", "word_transliteration": "khalaqna"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064e", "word_translation": "the heaven", "word_transliteration": "al-samaa"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "and the earth", "word_transliteration": "wal-arda"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and whatever", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627", "word_translation": "(is) between them", "word_transliteration": "baynahuma"}, {"word_arabic_uthmani": "\u0628\u064e\u0670\u0637\u0650\u0644\u0657\u0627\u06da", "word_translation": "without purpose", "word_transliteration": "batilan"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0638\u064e\u0646\u0651\u064f", "word_translation": "(is the) assumption", "word_transliteration": "zannu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652\u06da", "word_translation": "disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0641\u064e\u0648\u064e\u064a\u06e1\u0644\u065e", "word_translation": "So woe", "word_transliteration": "fawaylun"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "to those", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "who disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650", "word_translation": "the Fire", "word_transliteration": "al-nari"}]}