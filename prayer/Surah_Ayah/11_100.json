{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0645\u0650\u0646\u06e1 \u0623\u064e\u0646\u06e2\u0628\u064e\u0627\u0653\u0621\u0650 \u0671\u0644\u06e1\u0642\u064f\u0631\u064e\u0649\u0670 \u0646\u064e\u0642\u064f\u0635\u0651\u064f\u0647\u064f\u06e5 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e\u06d6 \u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u0642\u064e\u0627\u0653\u0626\u0650\u0645\u065e \u0648\u064e\u062d\u064e\u0635\u0650\u064a\u062f\u065e"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "(is) from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e2\u0628\u064e\u0627\u0653\u0621\u0650", "word_translation": "(the) news", "word_transliteration": "anbai"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0631\u064e\u0649\u0670", "word_translation": "(of) the cities", "word_transliteration": "al-qura"}, {"word_arabic_uthmani": "\u0646\u064e\u0642\u064f\u0635\u0651\u064f\u0647\u064f\u06e5", "word_translation": "(which) We relate", "word_transliteration": "naqussuhu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064e\u06d6", "word_translation": "to you", "word_transliteration": "alayka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "of them", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0653\u0626\u0650\u0645\u065e", "word_translation": "some are standing", "word_transliteration": "qaimun"}, {"word_arabic_uthmani": "\u0648\u064e\u062d\u064e\u0635\u0650\u064a\u062f\u065e", "word_translation": "and (some) mown", "word_transliteration": "wahasidun"}]}