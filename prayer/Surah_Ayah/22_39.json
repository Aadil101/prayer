{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0630\u0650\u0646\u064e \u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064f\u0642\u064e\u0670\u062a\u064e\u0644\u064f\u0648\u0646\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0638\u064f\u0644\u0650\u0645\u064f\u0648\u0627\u0652\u06da \u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0646\u064e\u0635\u06e1\u0631\u0650\u0647\u0650\u0645\u06e1 \u0644\u064e\u0642\u064e\u062f\u0650\u064a\u0631\u064c"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0630\u0650\u0646\u064e", "word_translation": "Permission is given", "word_transliteration": "udhina"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "to those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u064a\u064f\u0642\u064e\u0670\u062a\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "are being fought", "word_transliteration": "yuqataluna"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "because they", "word_transliteration": "bi-annahum"}, {"word_arabic_uthmani": "\u0638\u064f\u0644\u0650\u0645\u064f\u0648\u0627\u0652\u06da", "word_translation": "were wronged", "word_transliteration": "zulimu"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "for", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u06e1\u0631\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their victory", "word_transliteration": "nasrihim"}, {"word_arabic_uthmani": "\u0644\u064e\u0642\u064e\u062f\u0650\u064a\u0631\u064c", "word_translation": "(is) surely Able", "word_transliteration": "laqadirun"}]}