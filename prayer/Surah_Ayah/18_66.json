{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0644\u064e\u0647\u064f\u06e5 \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0647\u064e\u0644\u06e1 \u0623\u064e\u062a\u0651\u064e\u0628\u0650\u0639\u064f\u0643\u064e \u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u0646 \u062a\u064f\u0639\u064e\u0644\u0651\u0650\u0645\u064e\u0646\u0650 \u0645\u0650\u0645\u0651\u064e\u0627 \u0639\u064f\u0644\u0651\u0650\u0645\u06e1\u062a\u064e \u0631\u064f\u0634\u06e1\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "to him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0647\u064e\u0644\u06e1", "word_translation": "May", "word_transliteration": "hal"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u0651\u064e\u0628\u0650\u0639\u064f\u0643\u064e", "word_translation": "I follow you", "word_transliteration": "attabi'uka"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064f\u0639\u064e\u0644\u0651\u0650\u0645\u064e\u0646\u0650", "word_translation": "you teach me", "word_transliteration": "tu'allimani"}, {"word_arabic_uthmani": "\u0645\u0650\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u0639\u064f\u0644\u0651\u0650\u0645\u06e1\u062a\u064e", "word_translation": "you have been taught", "word_transliteration": "ullim'ta"}, {"word_arabic_uthmani": "\u0631\u064f\u0634\u06e1\u062f\u0657\u0627", "word_translation": "(of) right guidance", "word_transliteration": "rush'dan"}]}