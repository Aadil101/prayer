{"ayahs": {"ayah_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0643\u064e\u0641\u064e\u0631\u064e\u0629\u064f \u0671\u0644\u06e1\u0641\u064e\u062c\u064e\u0631\u064e\u0629\u064f"}, "words": [{"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "[they]", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0641\u064e\u0631\u064e\u0629\u064f", "word_translation": "(are) the disbelievers", "word_transliteration": "al-kafaratu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u062c\u064e\u0631\u064e\u0629\u064f", "word_translation": "the wicked ones", "word_transliteration": "al-fajaratu"}]}