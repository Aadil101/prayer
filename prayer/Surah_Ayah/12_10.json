{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0642\u064e\u0627\u0653\u0626\u0650\u0644\u065e \u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u062a\u064e\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0627\u0652 \u064a\u064f\u0648\u0633\u064f\u0641\u064e \u0648\u064e\u0623\u064e\u0644\u06e1\u0642\u064f\u0648\u0647\u064f \u0641\u0650\u064a \u063a\u064e\u064a\u064e\u0670\u0628\u064e\u062a\u0650 \u0671\u0644\u06e1\u062c\u064f\u0628\u0651\u0650 \u064a\u064e\u0644\u06e1\u062a\u064e\u0642\u0650\u0637\u06e1\u0647\u064f \u0628\u064e\u0639\u06e1\u0636\u064f \u0671\u0644\u0633\u0651\u064e\u064a\u0651\u064e\u0627\u0631\u064e\u0629\u0650 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u0641\u064e\u0670\u0639\u0650\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0653\u0626\u0650\u0644\u065e", "word_translation": "a speaker", "word_transliteration": "qailun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "among them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(Do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "kill", "word_transliteration": "taqtulu"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0633\u064f\u0641\u064e", "word_translation": "Yusuf", "word_transliteration": "yusufa"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0644\u06e1\u0642\u064f\u0648\u0647\u064f", "word_translation": "but throw him", "word_transliteration": "wa-alquhu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u063a\u064e\u064a\u064e\u0670\u0628\u064e\u062a\u0650", "word_translation": "th