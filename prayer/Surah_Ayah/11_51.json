{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0644\u064e\u0627\u0653 \u0623\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064f\u0643\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650 \u0623\u064e\u062c\u06e1\u0631\u064b\u0627\u06d6 \u0625\u0650\u0646\u06e1 \u0623\u064e\u062c\u06e1\u0631\u0650\u064a\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0641\u064e\u0637\u064e\u0631\u064e\u0646\u0650\u064a\u0653\u06da \u0623\u064e\u0641\u064e\u0644\u064e\u0627 \u062a\u064e\u0639\u06e1\u0642\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "O my people!", "word_transliteration": "yaqawmi"}, {"word_arabic_uthmani": "\u0644\u064e\u0627\u0653", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u0640\u0654\u064e\u0644\u064f\u0643\u064f\u0645\u06e1", "word_translation": "I ask you", "word_transliteration": "asalukum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "for it", "word_transliteration": "alayhi"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0631\u064b\u0627\u06d6", "word_translation": "any reward", "word_transliteration": "ajran"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u06e1", "word_translation": "Not", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u06e1\u0631\u0650\u064a\u064e", "word_translation": "(is) my reward", "word_transliteration": "ajriya"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "from", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "the One Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0641\u064e\u0637\u064e\u0631\u064e\u0646\u0650\u064a\u0653\u06da", "word_translation": "created me", "word_transliteration": "fatarani"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0627", "word_translation": "Then will not", "word_transliteration": "afala"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0642\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "you use reason", "word_transliteration": "ta'qiluna"}]}