{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1 \u0634\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064e\u0627\u0653 \u0623\u064e\u0634\u06e1\u0631\u064e\u0643\u064f\u0648\u0627\u0652\u06d7 \u0648\u064e\u0645\u064e\u0627 \u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u062d\u064e\u0641\u0650\u064a\u0638\u0657\u0627\u06d6 \u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645 \u0628\u0650\u0648\u064e\u0643\u0650\u064a\u0644\u0656"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0634\u064e\u0627\u0653\u0621\u064e", "word_translation": "(had) willed", "word_transliteration": "shaa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "not (they would have)", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064e\u0634\u06e1\u0631\u064e\u0643\u064f\u0648\u0627\u0652\u06d7", "word_translation": "associated partners (with Him)", "word_transliteration": "ashraku"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0643\u064e", "word_translation": "We have made you", "word_transliteration": "ja'alnaka"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "over them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u062d\u064e\u0641\u0650\u064a\u0638\u0657\u0627\u06d6", "word_translation": "a guardian", "word_transliteration": "hafizan"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "you", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645", "word_translation": "(are) over them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0628\u0650\u0648\u064e\u0643\u0650\u064a\u0644\u0656", "word_translation": "a manager", "word_transliteration": "biwakilin"}]}