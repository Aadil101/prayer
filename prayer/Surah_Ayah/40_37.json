{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0633\u06e1\u0628\u064e\u0670\u0628\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0641\u064e\u0623\u064e\u0637\u0651\u064e\u0644\u0650\u0639\u064e \u0625\u0650\u0644\u064e\u0649\u0670\u0653 \u0625\u0650\u0644\u064e\u0670\u0647\u0650 \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a \u0644\u064e\u0623\u064e\u0638\u064f\u0646\u0651\u064f\u0647\u064f\u06e5 \u0643\u064e\u0670\u0630\u0650\u0628\u0657\u0627\u06da \u0648\u064e\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0632\u064f\u064a\u0651\u0650\u0646\u064e \u0644\u0650\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0633\u064f\u0648\u0653\u0621\u064f \u0639\u064e\u0645\u064e\u0644\u0650\u0647\u0650\u06e6 \u0648\u064e\u0635\u064f\u062f\u0651\u064e \u0639\u064e\u0646\u0650 \u0671\u0644\u0633\u0651\u064e\u0628\u0650\u064a\u0644\u0650\u06da \u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u064a\u06e1\u062f\u064f \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0641\u0650\u064a \u062a\u064e\u0628\u064e\u0627\u0628\u0656"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0633\u06e1\u0628\u064e\u0670\u0628\u064e", "word_translation": "(The) ways", "word_transliteration": "asbaba"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(to) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0637\u0651\u064e\u0644\u0650\u0639\u064e", "word_translation": "so I may look", "word_transliteration": "fa-attali'a"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670\u0653", "word_translation": "at", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0670\u0647\u0650", "word_translation": "(the) God", "word_transliteration": "ilahi"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "(of) Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "and indeed I", "word_transliteration": "wa-inni"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u064e\u0638\u064f\u0646\u0651\u064f\u0647\u064f\u06e5", "word_translation": "[I] surely think him", "word_transliteration": "la-azunnuhu"}, {"word_arabic_uthmani": "\u0643\u064e\u0670\u0630\u0650\u0628\u0657\u0627\u06da", "word_translation": "(to be) a liar", "word_transliteration": "kadhiban"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "And thus", "word_transliteration": "wakadhalika"}, {"word_arabic_uthmani": "\u0632\u064f\u064a\u0651\u0650\u0646\u064e", "word_translation": "was made fair-seeming", "word_transliteration": "zuyyina"}, {"word_arabic_uthmani": "\u0644\u0650\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "to Firaun", "word_transliteration": "lifir'awna"}, {"word_arabic_uthmani": "\u0633\u064f\u0648\u0653\u0621\u064f", "word_translation": "(the) evil", "word_transliteration": "suu"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u064e\u0644\u0650\u0647\u0650\u06e6", "word_translation": "(of) his deed", "word_transliteration": "amalihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0635\u064f\u062f\u0651\u064e", "word_translation": "and he was averted", "word_transliteration": "wasudda"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0650", "word_translation": "from", "word_transliteration": "ani"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0628\u0650\u064a\u0644\u0650\u06da", "word_translation": "the way", "word_transliteration": "al-sabili"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u062f\u064f", "word_translation": "(was the) plot", "word_transliteration": "kaydu"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "(of) Firaun", "word_transliteration": "fir'awna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u062a\u064e\u0628\u064e\u0627\u0628\u0656", "word_translation": "ruin", "word_transliteration": "tababin"}]}