{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0639\u064e\u0630\u064e\u0627\u0628\u064e \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0644\u064e\u0648\u064e\u0670\u0642\u0650\u0639\u065e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064e", "word_translation": "(the) punishment", "word_transliteration": "adhaba"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "(of) your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u064e\u0670\u0642\u0650\u0639\u065e", "word_translation": "(will) surely occur", "word_transliteration": "lawaqi'un"}]}