{"ayahs": {"ayah_uthmani": "\u0642\u064e\u062f\u06e1 \u0641\u064e\u0631\u064e\u0636\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0643\u064f\u0645\u06e1 \u062a\u064e\u062d\u0650\u0644\u0651\u064e\u0629\u064e \u0623\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u0650\u0643\u064f\u0645\u06e1\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0645\u064e\u0648\u06e1\u0644\u064e\u0649\u0670\u0643\u064f\u0645\u06e1\u06d6 \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u062f\u06e1", "word_translation": "Indeed", "word_transliteration": "qad"}, {"word_arabic_uthmani": "\u0641\u064e\u0631\u064e\u0636\u064e", "word_translation": "has ordained", "word_transliteration": "farada"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u0650\u0644\u0651\u064e\u0629\u064e", "word_translation": "(the) dissolution", "word_transliteration": "tahillata"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u0645\u064e\u0670\u0646\u0650\u0643\u064f\u0645\u06e1\u06da", "word_translation": "(of) your oaths", "word_transliteration": "aymanikum"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0648\u06e1\u0644\u064e\u0649\u0670\u0643\u064f\u0645\u06e1\u06d6", "word_translation": "(is) your Protector", "word_transliteration": "mawlakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "(is) the All-Knower", "word_transliteration": "al-'alimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f", "word_translation": "the All-Wise", "word_transliteration": "al-hakimu"}]}