{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0637\u0651\u064e\u0644\u064e\u0639\u064e \u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u064e \u0623\u064e\u0645\u0650 \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650 \u0639\u064e\u0647\u06e1\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0637\u0651\u064e\u0644\u064e\u0639\u064e", "word_translation": "Has he looked", "word_transliteration": "attala'a"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u064e", "word_translation": "(into) the unseen", "word_transliteration": "al-ghayba"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u0650", "word_translation": "or", "word_transliteration": "ami"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e", "word_translation": "has he taken", "word_transliteration": "ittakhadha"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "from", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmani"}, {"word_arabic_uthmani": "\u0639\u064e\u0647\u06e1\u062f\u0657\u0627", "word_translation": "a promise", "word_transliteration": "ahdan"}]}