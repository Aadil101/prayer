{"ayahs": {"ayah_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064e\u062a\u06e1 \u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1 \u0642\u064e\u0648\u06e1\u0645\u064f \u0646\u064f\u0648\u062d\u0656 \u0648\u064e\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064f \u0671\u0644\u0631\u0651\u064e\u0633\u0651\u0650 \u0648\u064e\u062b\u064e\u0645\u064f\u0648\u062f\u064f"}, "words": [{"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064e\u062a\u06e1", "word_translation": "Denied", "word_transliteration": "kadhabat"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "before them", "word_transliteration": "qablahum"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064f", "word_translation": "(the) people", "word_transliteration": "qawmu"}, {"word_arabic_uthmani": "\u0646\u064f\u0648\u062d\u0656", "word_translation": "(of) Nuh", "word_transliteration": "nuhin"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064f", "word_translation": "and (the) companions", "word_transliteration": "wa-ashabu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0633\u0651\u0650", "word_translation": "(of) Ar-Raas", "word_transliteration": "al-rasi"}, {"word_arabic_uthmani": "\u0648\u064e\u062b\u064e\u0645\u064f\u0648\u062f\u064f", "word_translation": "and Thamud", "word_transliteration": "wathamudu"}]}