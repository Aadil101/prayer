{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0623\u064e\u0644\u06e1\u0642\u064f\u0648\u0627\u0652\u06d6 \u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653 \u0623\u064e\u0644\u06e1\u0642\u064e\u0648\u06e1\u0627\u0652 \u0633\u064e\u062d\u064e\u0631\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0639\u06e1\u064a\u064f\u0646\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u0631\u06e1\u0647\u064e\u0628\u064f\u0648\u0647\u064f\u0645\u06e1 \u0648\u064e\u062c\u064e\u0627\u0653\u0621\u064f\u0648 \u0628\u0650\u0633\u0650\u062d\u06e1\u0631\u064d \u0639\u064e\u0638\u0650\u064a\u0645\u0656"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u06e1\u0642\u064f\u0648\u0627\u0652\u06d6", "word_translation": "Throw", "word_transliteration": "alqu"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653", "word_translation": "Then when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u06e1\u0642\u064e\u0648\u06e1\u0627\u0652", "word_translation": "they threw", "word_transliteration": "alqaw"}, {"word_arabic_uthmani": "\u0633\u064e\u062d\u064e\u0631\u064f\u0648\u0653\u0627\u0652", "word_translation": "they bewitched", "word_transliteration": "saharu"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u064a\u064f\u0646\u064e", "word_translation": "(the) eyes", "word_transliteration": "a'yuna"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "(of) the people", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0633\u06e1\u062a\u064e\u0631\u06e1\u0647\u064e\u0628\u064f\u0648\u0647\u064f\u0645\u06e1", "word_translation": "and terrified them", "word_transliteration": "wa-is'tarhabuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0627\u0653\u0621\u064f\u0648", "word_translation": "and came (up)", "word_transliteration": "wajau"}, {"word_arabic_uthmani": "\u0628\u0650\u0633\u0650\u062d\u06e1\u0631\u064d", "word_translation": "with a magic", "word_transliteration": "bisih'rin"}, {"word_arabic_uthmani": "\u0639\u064e\u0638\u0650\u064a\u0645\u0656", "word_translation": "great", "word_transliteration": "azimin"}]}