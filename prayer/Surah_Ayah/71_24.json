{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1 \u0623\u064e\u0636\u064e\u0644\u0651\u064f\u0648\u0627\u0652 \u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627\u06d6 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0632\u0650\u062f\u0650 \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0636\u064e\u0644\u064e\u0670\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And indeed", "word_transliteration": "waqad"}, {"word_arabic_uthmani": "\u0623\u064e\u0636\u064e\u0644\u0651\u064f\u0648\u0627\u0652", "word_translation": "they have led astray", "word_transliteration": "adallu"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627\u06d6", "word_translation": "many", "word_transliteration": "kathiran"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0632\u0650\u062f\u0650", "word_translation": "increase", "word_transliteration": "tazidi"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimina"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0636\u064e\u0644\u064e\u0670\u0644\u0657\u0627", "word_translation": "(in) error", "word_transliteration": "dalalan"}]}