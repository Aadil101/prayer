{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0647\u064e\u0670\u0630\u0650\u0647\u0650 \u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u064f \u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627 \u0645\u064e\u062a\u064e\u0670\u0639\u065e \u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u064e \u0647\u0650\u064a\u064e \u062f\u064e\u0627\u0631\u064f \u0671\u0644\u06e1\u0642\u064e\u0631\u064e\u0627\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "O my people!", "word_transliteration": "yaqawmi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u0650\u0647\u0650", "word_translation": "this", "word_transliteration": "hadhihi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u064a\u064e\u0648\u0670\u0629\u064f", "word_translation": "the life", "word_transliteration": "al-hayatu"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627", "word_translation": "(of) the world", "word_transliteration": "al-dun'ya"}, {"word_arabic_uthmani": "\u0645\u064e\u062a\u064e\u0670\u0639\u065e", "word_translation": "(is) enjoyment", "word_transliteration": "mata'un"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "and indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064e\u0629\u064e", "word_translation": "the Hereafter", "word_transliteration": "al-akhirata"}, {"word_arabic_uthmani": "\u0647\u0650\u064a\u064e", "word_translation": "it", "word_transliteration": "hiya"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0631\u064f", "word_translation": "(is the) home", "word_transliteration": "daru"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0631\u064e\u0627\u0631\u0650", "word_translation": "(of) settlement", "word_transliteration": "al-qarari"}]}