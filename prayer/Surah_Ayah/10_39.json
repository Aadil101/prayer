{"ayahs": {"ayah_uthmani": "\u0628\u064e\u0644\u06e1 \u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652 \u0628\u0650\u0645\u064e\u0627 \u0644\u064e\u0645\u06e1 \u064a\u064f\u062d\u0650\u064a\u0637\u064f\u0648\u0627\u0652 \u0628\u0650\u0639\u0650\u0644\u06e1\u0645\u0650\u0647\u0650\u06e6 \u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u064a\u064e\u0623\u06e1\u062a\u0650\u0647\u0650\u0645\u06e1 \u062a\u064e\u0623\u06e1\u0648\u0650\u064a\u0644\u064f\u0647\u064f\u06e5\u06da \u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0643\u064e\u0630\u0651\u064e\u0628\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1\u06d6 \u0641\u064e\u0671\u0646\u0638\u064f\u0631\u06e1 \u0643\u064e\u064a\u06e1\u0641\u064e \u0643\u064e\u0627\u0646\u064e \u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "they denied", "word_transliteration": "kadhabu"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u0650\u064a\u0637\u064f\u0648\u0627\u0652", "word_translation": "they could encompass", "word_transliteration": "yuhitu"}, {"word_arabic_uthmani": "\u0628\u0650\u0639\u0650\u0644\u06e1\u0645\u0650\u0647\u0650\u06e6", "word_translation": "(of) its knowledge", "word_transliteration": "bi'il'mihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "and not", "word_transliteration": "walamma"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650\u0647\u0650\u0645\u06e1", "word_translation": "has come (to) them", "word_transliteration": "yatihim"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u0648\u0650\u064a\u0644\u064f\u0647\u064f\u06e5\u06da", "word_translation": "its interpretation", "word_transliteration": "tawiluhu"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "Thus", "word_transliteration": "kadhalika"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064e", "word_translation": "denied", "word_transliteration": "kadhaba"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before them", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1\u06d6", "word_translation": "before them", "word_transliteration": "qablihim"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0646\u0638\u064f\u0631\u06e1", "word_translation": "then see", "word_transliteration": "fa-unzur"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "how", "word_transliteration": "kayfa"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "was", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f", "word_translation": "(the) end", "word_transliteration": "aqibatu"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the wrongdoers", "word_transliteration": "al-zalimina"}]}