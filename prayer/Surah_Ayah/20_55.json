{"ayahs": {"ayah_uthmani": "\u06de\u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1 \u0648\u064e\u0641\u0650\u064a\u0647\u064e\u0627 \u0646\u064f\u0639\u0650\u064a\u062f\u064f\u0643\u064f\u0645\u06e1 \u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064e\u0627 \u0646\u064f\u062e\u06e1\u0631\u0650\u062c\u064f\u0643\u064f\u0645\u06e1 \u062a\u064e\u0627\u0631\u064e\u0629\u064b \u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u06de\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "From it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1", "word_translation": "We created you", "word_transliteration": "khalaqnakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "and in it", "word_transliteration": "wafiha"}, {"word_arabic_uthmani": "\u0646\u064f\u0639\u0650\u064a\u062f\u064f\u0643\u064f\u0645\u06e1", "word_translation": "We will return you", "word_transliteration": "nu'idukum"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "and from it", "word_transliteration": "wamin'ha"}, {"word_arabic_uthmani": "\u0646\u064f\u062e\u06e1\u0631\u0650\u062c\u064f\u0643\u064f\u0645\u06e1", "word_translation": "We will bring you out", "word_transliteration": "nukh'rijukum"}, {"word_arabic_uthmani": "\u062a\u064e\u0627\u0631\u064e\u0629\u064b", "word_translation": "time", "word_transliteration": "taratan"}, {"word_arabic_uthmani": "\u0623\u064f\u062e\u06e1\u0631\u064e\u0649\u0670", "word_translation": "another", "word_transliteration": "ukh'ra"}]}