{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0645\u064e\u0648\u06e1\u0644\u064e\u0649 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0648\u064e\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e \u0644\u064e\u0627 \u0645\u064e\u0648\u06e1\u0644\u064e\u0649\u0670 \u0644\u064e\u0647\u064f\u0645\u06e1"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e", "word_translation": "(is) because", "word_transliteration": "bi-anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0645\u064e\u0648\u06e1\u0644\u064e\u0649", "word_translation": "(is the) Protector", "word_transliteration": "mawla"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e", "word_translation": "and because", "word_transliteration": "wa-anna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the disbelievers", "word_transliteration": "al-kafirina"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(there is) no", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0645\u064e\u0648\u06e1\u0644\u064e\u0649\u0670", "word_translation": "protector", "word_transliteration": "mawla"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}]}