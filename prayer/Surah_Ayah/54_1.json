{"ayahs": {"ayah_uthmani": "\u0671\u0642\u06e1\u062a\u064e\u0631\u064e\u0628\u064e\u062a\u0650 \u0671\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u064f \u0648\u064e\u0671\u0646\u0634\u064e\u0642\u0651\u064e \u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0671\u0642\u06e1\u062a\u064e\u0631\u064e\u0628\u064e\u062a\u0650", "word_translation": "Has come near", "word_transliteration": "iq'tarabati"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0627\u0639\u064e\u0629\u064f", "word_translation": "the Hour", "word_transliteration": "al-sa'atu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0646\u0634\u064e\u0642\u0651\u064e", "word_translation": "and has split", "word_transliteration": "wa-inshaqqa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0645\u064e\u0631\u064f", "word_translation": "the moon", "word_transliteration": "al-qamaru"}]}