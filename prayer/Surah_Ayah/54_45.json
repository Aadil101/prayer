{"ayahs": {"ayah_uthmani": "\u0633\u064e\u064a\u064f\u0647\u06e1\u0632\u064e\u0645\u064f \u0671\u0644\u06e1\u062c\u064e\u0645\u06e1\u0639\u064f \u0648\u064e\u064a\u064f\u0648\u064e\u0644\u0651\u064f\u0648\u0646\u064e \u0671\u0644\u062f\u0651\u064f\u0628\u064f\u0631\u064e"}, "words": [{"word_arabic_uthmani": "\u0633\u064e\u064a\u064f\u0647\u06e1\u0632\u064e\u0645\u064f", "word_translation": "Soon will be defeated", "word_transliteration": "sayuh'zamu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0645\u06e1\u0639\u064f", "word_translation": "(their) assembly", "word_transliteration": "al-jam'u"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0648\u064e\u0644\u0651\u064f\u0648\u0646\u064e", "word_translation": "and they will turn", "word_transliteration": "wayuwalluna"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0628\u064f\u0631\u064e", "word_translation": "(their) backs", "word_transliteration": "al-dubura"}]}