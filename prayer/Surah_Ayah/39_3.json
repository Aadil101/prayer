{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0627 \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0671\u0644\u062f\u0651\u0650\u064a\u0646\u064f \u0671\u0644\u06e1\u062e\u064e\u0627\u0644\u0650\u0635\u064f\u06da \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6\u0653 \u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e \u0645\u064e\u0627 \u0646\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0647\u064f\u0645\u06e1 \u0625\u0650\u0644\u0651\u064e\u0627 \u0644\u0650\u064a\u064f\u0642\u064e\u0631\u0651\u0650\u0628\u064f\u0648\u0646\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0632\u064f\u0644\u06e1\u0641\u064e\u0649\u0670\u0653 \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u064a\u064e\u062d\u06e1\u0643\u064f\u0645\u064f \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0645\u064e\u0627 \u0647\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u0650 \u064a\u064e\u062e\u06e1\u062a\u064e\u0644\u0650\u0641\u064f\u0648\u0646\u064e\u06d7 \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0627 \u064a\u064e\u0647\u06e1\u062f\u0650\u064a \u0645\u064e\u0646\u06e1 \u0647\u064f\u0648\u064e \u0643\u064e\u0670\u0630\u0650\u0628\u065e \u0643\u064e\u0641\u0651\u064e\u0627\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627", "word_translation": "Unquestionably", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "for Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u0650\u064a\u0646\u064f", "word_translation": "(is) the religion", "word_transliteration": "al-dinu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0627\u0644\u0650\u0635\u064f\u06da", "word_translation": "the pure", "word_transliteration": "al-khalisu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "And those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652", "word_translation": "take", "word_transliteration": "ittakhadhu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "besides Him", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650\u0647\u0650\u06e6\u0653", "word_translation": "besides Him", "word_transliteration": "dunihi"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u0650\u064a\u064e\u0627\u0653\u0621\u064e", "word_translation": "protectors", "word_transliteration": "awliyaa"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0646\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0647\u064f\u0645\u06e1", "word_translation": "we worship them", "word_transliteration": "na'buduhum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u0642\u064e\u0631\u0651\u0650\u0628\u064f\u0648\u0646\u064e\u0627\u0653", "word_translation": "that they may bring us near", "word_transliteration": "liyuqarribuna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0632\u064f\u0644\u06e1\u0641\u064e\u0649\u0670\u0653", "word_translation": "(in) position", "word_transliteration": "zul'fa"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0643\u064f\u0645\u064f", "word_translation": "will judge", "word_transliteration": "yahkumu"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1", "word_translation": "between them", "word_transliteration": "baynahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650", "word_translation": "[in it]", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u062a\u064e\u0644\u0650\u0641\u064f\u0648\u0646\u064e\u06d7", "word_translation": "differ", "word_transliteration": "yakhtalifuna"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062f\u0650\u064a", "word_translation": "guide", "word_transliteration": "yahdi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u06e1", "word_translation": "(one) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "[he]", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0643\u064e\u0670\u0630\u0650\u0628\u065e", "word_translation": "(is) a liar", "word_transliteration": "kadhibun"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u0651\u064e\u0627\u0631\u065e", "word_translation": "and a disbeliever", "word_transliteration": "kaffarun"}]}