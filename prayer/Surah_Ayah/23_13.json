{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f \u0646\u064f\u0637\u06e1\u0641\u064e\u0629\u0657 \u0641\u0650\u064a \u0642\u064e\u0631\u064e\u0627\u0631\u0656 \u0645\u0651\u064e\u0643\u0650\u064a\u0646\u0656"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f", "word_translation": "We placed him", "word_transliteration": "ja'alnahu"}, {"word_arabic_uthmani": "\u0646\u064f\u0637\u06e1\u0641\u064e\u0629\u0657", "word_translation": "(as) a semen-drop", "word_transliteration": "nut'fatan"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u064e\u0627\u0631\u0656", "word_translation": "a resting place", "word_transliteration": "qararin"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0643\u0650\u064a\u0646\u0656", "word_translation": "firm", "word_transliteration": "makinin"}]}