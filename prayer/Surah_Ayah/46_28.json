{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627 \u0646\u064e\u0635\u064e\u0631\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0642\u064f\u0631\u06e1\u0628\u064e\u0627\u0646\u064b\u0627 \u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u064e\u06e2\u06d6 \u0628\u064e\u0644\u06e1 \u0636\u064e\u0644\u0651\u064f\u0648\u0627\u0652 \u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1\u06da \u0648\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0625\u0650\u0641\u06e1\u0643\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064e\u0641\u06e1\u062a\u064e\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0648\u06e1\u0644\u064e\u0627", "word_translation": "Then why (did) not", "word_transliteration": "falawla"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u064e\u0631\u064e\u0647\u064f\u0645\u064f", "word_translation": "help them", "word_transliteration": "nasarahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those whom", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0627\u0652", "word_translation": "they had taken", "word_transliteration": "ittakhadhu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "besides", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0642\u064f\u0631\u06e1\u0628\u064e\u0627\u0646\u064b\u0627", "word_translation": "gods as a way of approach", "word_transliteration": "qur'banan"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u064e\u06e2\u06d6", "word_translation": "gods as a way of approach", "word_transliteration": "alihatan"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0636\u064e\u0644\u0651\u064f\u0648\u0627\u0652", "word_translation": "they were lost", "word_transliteration": "dallu"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f\u0645\u06e1\u06da", "word_translation": "from them", "word_transliteration": "anhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "And that", "word_transliteration": "wadhalika"}, {"word_arabic_uthmani": "\u0625\u0650\u0641\u06e1\u0643\u064f\u0647\u064f\u0645\u06e1", "word_translation": "(was) their falsehood", "word_transliteration": "if'kuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they were", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064e\u0641\u06e1\u062a\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "inventing", "word_transliteration": "yaftaruna"}]}