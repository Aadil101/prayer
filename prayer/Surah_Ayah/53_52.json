{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064e \u0646\u064f\u0648\u062d\u0656 \u0645\u0651\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u064f\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0647\u064f\u0645\u06e1 \u0623\u064e\u0638\u06e1\u0644\u064e\u0645\u064e \u0648\u064e\u0623\u064e\u0637\u06e1\u063a\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064e", "word_translation": "And (the) people", "word_transliteration": "waqawma"}, {"word_arabic_uthmani": "\u0646\u064f\u0648\u062d\u0656", "word_translation": "(of) Nuh", "word_transliteration": "nuhin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f\u06d6", "word_translation": "before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Indeed, they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they were", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "they were", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0623\u064e\u0638\u06e1\u0644\u064e\u0645\u064e", "word_translation": "more unjust", "word_transliteration": "azlama"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0637\u06e1\u063a\u064e\u0649\u0670", "word_translation": "and more rebellious", "word_transliteration": "wa-atgha"}]}