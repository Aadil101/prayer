{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0648\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0644\u06e1\u0628\u0650\u0633\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u064a\u0645\u064e\u0670\u0646\u064e\u0647\u064f\u0645 \u0628\u0650\u0638\u064f\u0644\u06e1\u0645\u064d \u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0646\u064f \u0648\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0647\u06e1\u062a\u064e\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "Those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believed", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0645\u06e1", "word_translation": "and (did) not", "word_transliteration": "walam"}, {"word_arabic_uthmani": "\u064a\u064e\u0644\u06e1\u0628\u0650\u0633\u064f\u0648\u0653\u0627\u0652", "word_translation": "mix", "word_transliteration": "yalbisu"}, {"word_arabic_uthmani": "\u0625\u0650\u064a\u0645\u064e\u0670\u0646\u064e\u0647\u064f\u0645", "word_translation": "their belief", "word_transliteration": "imanahum"}, {"word_arabic_uthmani": "\u0628\u0650\u0638\u064f\u0644\u06e1\u0645\u064d", "word_translation": "with wrong", "word_transliteration": "bizul'min"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "for them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0646\u064f", "word_translation": "(is) the security"