{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u064a\u064e\u062c\u06e1\u0645\u064e\u0639\u064f \u0628\u064e\u064a\u06e1\u0646\u064e\u0646\u064e\u0627 \u0631\u064e\u0628\u0651\u064f\u0646\u064e\u0627 \u062b\u064f\u0645\u0651\u064e \u064a\u064e\u0641\u06e1\u062a\u064e\u062d\u064f \u0628\u064e\u064a\u06e1\u0646\u064e\u0646\u064e\u0627 \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0641\u064e\u062a\u0651\u064e\u0627\u062d\u064f \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u064a\u064e\u062c\u06e1\u0645\u064e\u0639\u064f", "word_translation": "Will gather", "word_transliteration": "yajma'u"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0646\u064e\u0627", "word_translation": "us together", "word_transliteration": "baynana"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0646\u064e\u0627", "word_translation": "our Lord", "word_transliteration": "rabbuna"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u064a\u064e\u0641\u06e1\u062a\u064e\u062d\u064f", "word_translation": "He will judge", "word_transliteration": "yaftahu"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0646\u064e\u0627", "word_translation": "between us", "word_transliteration": "baynana"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650", "word_translation": "in truth", "word_transliteration": "bil-haqi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "And He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u062a\u0651\u064e\u0627\u062d\u064f", "word_translation": "(is) the Judge", "word_transliteration": "al-fatahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "the All-Knowing", "word_transliteration": "al-'alimu"}]}