{"ayahs": {"ayah_uthmani": "\u0621\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u0623\u064e\u0646\u0632\u064e\u0644\u06e1\u062a\u064f\u0645\u064f\u0648\u0647\u064f \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0632\u06e1\u0646\u0650 \u0623\u064e\u0645\u06e1 \u0646\u064e\u062d\u06e1\u0646\u064f \u0671\u0644\u06e1\u0645\u064f\u0646\u0632\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0621\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "Is it you", "word_transliteration": "a-antum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0632\u064e\u0644\u06e1\u062a\u064f\u0645\u064f\u0648\u0647\u064f", "word_translation": "who send it down", "word_transliteration": "anzaltumuhu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0632\u06e1\u0646\u0650", "word_translation": "the rain clouds", "word_transliteration": "al-muz'ni"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "We", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0646\u0632\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "(are) the Ones to send", "word_transliteration": "al-munziluna"}]}