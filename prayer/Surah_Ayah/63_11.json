{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0646 \u064a\u064f\u0624\u064e\u062e\u0651\u0650\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0646\u064e\u0641\u06e1\u0633\u064b\u0627 \u0625\u0650\u0630\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e \u0623\u064e\u062c\u064e\u0644\u064f\u0647\u064e\u0627\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u062e\u064e\u0628\u0650\u064a\u0631\u064f\u06e2 \u0628\u0650\u0645\u064e\u0627 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0646", "word_translation": "But never", "word_transliteration": "walan"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u064e\u062e\u0651\u0650\u0631\u064e", "word_translation": "will (be) delayed", "word_transliteration": "yu-akhira"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(by) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u06e1\u0633\u064b\u0627", "word_translation": "a soul", "word_transliteration": "nafsan"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "has come", "word_transliteration": "jaa"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u064e\u0644\u064f\u0647\u064e\u0627\u06da", "word_translation": "its term", "word_transliteration": "ajaluha"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u062e\u064e\u0628\u0650\u064a\u0631\u064f\u06e2", "word_translation": "(is) All-Aware", "word_transliteration": "khabirun"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644