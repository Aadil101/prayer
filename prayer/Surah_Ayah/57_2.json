{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0647\u064f\u06e5 \u0645\u064f\u0644\u06e1\u0643\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6 \u064a\u064f\u062d\u06e1\u064a\u0650\u06e6 \u0648\u064e\u064a\u064f\u0645\u0650\u064a\u062a\u064f\u06d6 \u0648\u064e\u0647\u064f\u0648\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0642\u064e\u062f\u0650\u064a\u0631\u064c"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "For Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0645\u064f\u0644\u06e1\u0643\u064f", "word_translation": "(is the) dominion", "word_transliteration": "mul'ku"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650\u06d6", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u06e1\u064a\u0650\u06e6", "word_translation": "He gives life", "word_transliteration": "yuh'yi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0645\u0650\u064a\u062a\u064f\u06d6", "word_translation": "and causes death", "word_transliteration": "wayumitu"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "(is) over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "all", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "things", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0650\u064a\u0631\u064c", "word_translation": "All-Powerful", "word_transliteration": "qadirun"}]}