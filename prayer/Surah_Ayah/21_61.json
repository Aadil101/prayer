{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0641\u064e\u0623\u06e1\u062a\u064f\u0648\u0627\u0652 \u0628\u0650\u0647\u0650\u06e6 \u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u0639\u06e1\u064a\u064f\u0646\u0650 \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0634\u06e1\u0647\u064e\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u06e1\u062a\u064f\u0648\u0627\u0652", "word_translation": "Then bring", "word_transliteration": "fatu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "before", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u064a\u064f\u0646\u0650", "word_translation": "(the) eyes", "word_transliteration": "a'yuni"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "(of) the people", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so that they may", "word_transliteration": "la'allahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0647\u064e\u062f\u064f\u0648\u0646\u064e", "word_translation": "bear witness", "word_transliteration": "yashhaduna"}]}