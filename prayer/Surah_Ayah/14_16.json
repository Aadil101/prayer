{"ayahs": {"ayah_uthmani": "\u0645\u0651\u0650\u0646 \u0648\u064e\u0631\u064e\u0627\u0653\u0626\u0650\u0647\u0650\u06e6 \u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064f \u0648\u064e\u064a\u064f\u0633\u06e1\u0642\u064e\u0649\u0670 \u0645\u0650\u0646 \u0645\u0651\u064e\u0627\u0653\u0621\u0656 \u0635\u064e\u062f\u0650\u064a\u062f\u0656"}, "words": [{"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "Ahead of him", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0627\u0653\u0626\u0650\u0647\u0650\u06e6", "word_translation": "Ahead of him", "word_transliteration": "waraihi"}, {"word_arabic_uthmani": "\u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064f", "word_translation": "(is) Hell", "word_transliteration": "jahannamu"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0633\u06e1\u0642\u064e\u0649\u0670", "word_translation": "and he will be made to drink", "word_transliteration": "wayus'qa"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627\u0653\u0621\u0656", "word_translation": "water", "word_transliteration": "main"}, {"word_arabic_uthmani": "\u0635\u064e\u062f\u0650\u064a\u062f\u0656", "word_translation": "purulent", "word_transliteration": "sadidin"}]}