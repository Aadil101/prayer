{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1 \u062e\u064e\u0644\u064e\u0642\u064f\u0648\u0627\u0652 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e\u06da \u0628\u064e\u0644 \u0644\u0651\u064e\u0627 \u064a\u064f\u0648\u0642\u0650\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "(did) they create", "word_transliteration": "khalaqu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e\u06da", "word_translation": "and the earth", "word_transliteration": "wal-arda"}, {"word_arabic_uthmani": "\u0628\u064e\u0644", "word_translation": "Nay", "word_transliteration": "bal"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0642\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "they are certain", "word_transliteration": "yuqinuna"}]}