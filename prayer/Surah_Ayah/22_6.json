{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0628\u0650\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f \u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5 \u064a\u064f\u062d\u06e1\u064a\u0650 \u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e\u0649\u0670 \u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5 \u0639\u064e\u0644\u064e\u0649\u0670 \u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0642\u064e\u062f\u0650\u064a\u0631\u065e"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0646\u0651\u064e", "word_translation": "(is) because", "word_transliteration": "bi-anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f", "word_translation": "(is) the Truth", "word_transliteration": "al-haqu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "And that He", "word_transliteration": "wa-annahu"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u06e1\u064a\u0650", "word_translation": "[He] gives life", "word_transliteration": "yuh'yi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e\u0649\u0670", "word_translation": "(to) the dead", "word_transliteration": "al-mawta"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "and that He", "word_transliteration": "wa-annahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "(is) over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0650\u064a\u0631\u065e", "word_translation": "All-Powerful", "word_transliteration": "qadirun"}]}