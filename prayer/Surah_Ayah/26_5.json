{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u064a\u064e\u0623\u06e1\u062a\u0650\u064a\u0647\u0650\u0645 \u0645\u0651\u0650\u0646 \u0630\u0650\u0643\u06e1\u0631\u0656 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650 \u0645\u064f\u062d\u06e1\u062f\u064e\u062b\u064d \u0625\u0650\u0644\u0651\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0639\u064e\u0646\u06e1\u0647\u064f \u0645\u064f\u0639\u06e1\u0631\u0650\u0636\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And (does) not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650\u064a\u0647\u0650\u0645", "word_translation": "come to them", "word_transliteration": "yatihim"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0630\u0650\u0643\u06e1\u0631\u0656", "word_translation": "reminder", "word_transliteration": "dhik'rin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u0650", "word_translation": "the Most Gracious", "word_transliteration": "al-rahmani"}, {"word_arabic_uthmani": "\u0645\u064f\u062d\u06e1\u062f\u064e\u062b\u064d", "word_translation": "new", "word_transliteration": "muh'dathin"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f", "word_translation": "from it", "word_transliteration": "anhu"}, {"word_arabic_uthmani": "\u0645\u064f\u0639\u06e1\u0631\u0650\u0636\u0650\u064a\u0646\u064e", "word