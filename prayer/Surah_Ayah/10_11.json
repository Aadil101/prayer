{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0648\u06e1 \u064a\u064f\u0639\u064e\u062c\u0651\u0650\u0644\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u0650\u0644\u0646\u0651\u064e\u0627\u0633\u0650 \u0671\u0644\u0634\u0651\u064e\u0631\u0651\u064e \u0671\u0633\u06e1\u062a\u0650\u0639\u06e1\u062c\u064e\u0627\u0644\u064e\u0647\u064f\u0645 \u0628\u0650\u0671\u0644\u06e1\u062e\u064e\u064a\u06e1\u0631\u0650 \u0644\u064e\u0642\u064f\u0636\u0650\u064a\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0623\u064e\u062c\u064e\u0644\u064f\u0647\u064f\u0645\u06e1\u06d6 \u0641\u064e\u0646\u064e\u0630\u064e\u0631\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0644\u064e\u0627 \u064a\u064e\u0631\u06e1\u062c\u064f\u0648\u0646\u064e \u0644\u0650\u0642\u064e\u0627\u0653\u0621\u064e\u0646\u064e\u0627 \u0641\u0650\u064a \u0637\u064f\u063a\u06e1\u064a\u064e\u0670\u0646\u0650\u0647\u0650\u0645\u06e1 \u064a\u064e\u0639\u06e1\u0645\u064e\u0647\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u064a\u064f\u0639\u064e\u062c\u0651\u0650\u0644\u064f", "word_translation": "hastens", "word_transliteration": "yu'ajjilu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(by) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0646\u0651\u064e\u0627\u0633\u0650", "word_translation": "for the mankind", "word_transliteration": "lilnnasi"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u0631\u0651\u064e", "word_translation": "the evil", "word_transliteration": "al-shara"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u0650\u0639\u06e1\u062c\u064e\u0627\u0644\u064e\u0647\u064f\u0645", "word_translation": "(as) He hastens for them", "word_transliteration": "is'ti'jalahum"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062e\u064e\u064a\u06e1\u0631\u0650", "word_translation": "the good", "word_transliteration": "bil-khayri"}, {"word_arabic_uthmani": "\u0644\u064e\u0642\u064f\u0636\u0650\u064a\u064e", "word_translation": "surely, would have been decreed", "word_transliteration": "laqudiya"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "for them", "word_transliteration": "ilayhim"}, {"word_arabic_uthmani": "\u0623\u064e\u062c\u064e\u0644\u064f\u0647\u064f\u0645\u06e1\u06d6", "word_translation": "their term", "word_transliteration": "ajaluhum"}, {"word_arabic_uthmani": "\u0641\u064e\u0646\u064e\u0630\u064e\u0631\u064f", "word_translation": "But We leave", "word_transliteration": "fanadharu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(do) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u062c\u064f\u0648\u0646\u064e", "word_translation": "expect", "word_transliteration": "yarjuna"}, {"word_arabic_uthmani": "\u0644\u0650\u0642\u064e\u0627\u0653\u0621\u064e\u0646\u064e\u0627", "word_translation": "the meeting with Us", "word_transliteration": "liqaana"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0637\u064f\u063a\u06e1\u064a\u064e\u0670\u0646\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their transgression", "word_transliteration": "tugh'yanihim"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0647\u064f\u0648\u0646\u064e", "word_translation": "wandering blindly", "word_transliteration": "ya'mahuna"}]}