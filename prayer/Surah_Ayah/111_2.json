{"ayahs": {"ayah_uthmani": "\u0645\u064e\u0627\u0653 \u0623\u064e\u063a\u06e1\u0646\u064e\u0649\u0670 \u0639\u064e\u0646\u06e1\u0647\u064f \u0645\u064e\u0627\u0644\u064f\u0647\u064f\u06e5 \u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u0633\u064e\u0628\u064e"}, "words": [{"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064e\u063a\u06e1\u0646\u064e\u0649\u0670", "word_translation": "(will) avail", "word_transliteration": "aghna"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f", "word_translation": "him", "word_transliteration": "anhu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0644\u064f\u0647\u064f\u06e5", "word_translation": "his wealth", "word_transliteration": "maluhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064e\u0633\u064e\u0628\u064e", "word_translation": "he earned", "word_transliteration": "kasaba"}]}