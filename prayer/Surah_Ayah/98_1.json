{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0645\u06e1 \u064a\u064e\u0643\u064f\u0646\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u06e1 \u0623\u064e\u0647\u06e1\u0644\u0650 \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0634\u06e1\u0631\u0650\u0643\u0650\u064a\u0646\u064e \u0645\u064f\u0646\u0641\u064e\u0643\u0651\u0650\u064a\u0646\u064e \u062d\u064e\u062a\u0651\u064e\u0649\u0670 \u062a\u064e\u0623\u06e1\u062a\u0650\u064a\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0628\u064e\u064a\u0651\u0650\u0646\u064e\u0629\u064f"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0645\u06e1", "word_translation": "Not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u064f\u0646\u0650", "word_translation": "were", "word_transliteration": "yakuni"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u0650", "word_translation": "(the) People", "word_transliteration": "ahli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "of the Book", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064f\u0634\u06e1\u0631\u0650\u0643\u0650\u064a\u0646\u064e", "word_translation": "and the polytheists", "word_transliteration": "wal-mush'rikina"}, {"word_arabic_uthmani": "\u0645\u064f\u0646\u0641\u064e\u0643\u0651\u0650\u064a\u0646\u064e", "word_translation": "to be abandoned", "word_transliterat