{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1 \u0642\u064e\u0627\u0644\u064e\u062a \u0637\u0651\u064e\u0627\u0653\u0626\u0650\u0641\u064e\u0629\u065e \u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0647\u06e1\u0644\u064e \u064a\u064e\u062b\u06e1\u0631\u0650\u0628\u064e \u0644\u064e\u0627 \u0645\u064f\u0642\u064e\u0627\u0645\u064e \u0644\u064e\u0643\u064f\u0645\u06e1 \u0641\u064e\u0671\u0631\u06e1\u062c\u0650\u0639\u064f\u0648\u0627\u0652\u06da \u0648\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u0640\u0654\u06e1\u0630\u0650\u0646\u064f \u0641\u064e\u0631\u0650\u064a\u0642\u065e \u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u064f \u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u064e \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u0625\u0650\u0646\u0651\u064e \u0628\u064f\u064a\u064f\u0648\u062a\u064e\u0646\u064e\u0627 \u0639\u064e\u0648\u06e1\u0631\u064e\u0629\u065e \u0648\u064e\u0645\u064e\u0627 \u0647\u0650\u064a\u064e \u0628\u0650\u0639\u064e\u0648\u06e1\u0631\u064e\u0629\u064d\u06d6 \u0625\u0650\u0646 \u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0641\u0650\u0631\u064e\u0627\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1", "word_translation": "And when", "word_transliteration": "wa-idh"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e\u062a", "word_translation": "said", "word_transliteration": "qalat"}, {"word_arabic_uthmani": "\u0637\u0651\u064e\u0627\u0653\u0626\u0650\u0641\u064e\u0629\u065e", "word_translation": "a party", "word_transliteration": "taifatun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "of them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064e\u0647\u06e1\u0644\u064e", "word_translation": "O People", "word_transliteration": "yaahla"}, {"word_arabic_uthmani": "\u064a\u064e\u062b\u06e1\u0631\u0650\u0628\u064e", "word_translation": "(of) Yathrib!", "word_transliteration": "yathriba"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "No", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0645\u064f\u0642\u064e\u0627\u0645\u064e", "word_translation": "stand", "word_transliteration": "muqama"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0631\u06e1\u062c\u0650\u0639\u064f\u0648\u0627\u0652\u06da", "word_translation": "so return", "word_transliteration": "fa-ir'ji'u"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u0640\u0654\u06e1\u0630\u0650\u0646\u064f", "word_translation": "And asked permission", "word_transliteration": "wayastadhinu"}, {"word_arabic_uthmani": "\u0641\u064e\u0631\u0650\u064a\u0642\u065e", "word_translation": "a group", "word_transliteration": "fariqun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u064f", "word_translation": "of them", "word_transliteration": "min'humu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0628\u0650\u064a\u0651\u064e", "word_translation": "(from) the Prophet", "word_transliteration": "al-nabiya"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "saying", "word_transliteration": "yaquluna"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0628\u064f\u064a\u064f\u0648\u062a\u064e\u0646\u064e\u0627", "word_translation": "our houses", "word_transliteration": "buyutana"}, {"word_arabic_uthmani": "\u0639\u064e\u0648\u06e1\u0631\u064e\u0629\u065e", "word_translation": "(are) exposed", "word_transliteration": "awratun"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0647\u0650\u064a\u064e", "word_translation": "they", "word_transliteration": "hiya"}, {"word_arabic_uthmani": "\u0628\u0650\u0639\u064e\u0648\u06e1\u0631\u064e\u0629\u064d\u06d6", "word_translation": "(were) exposed", "word_transliteration": "bi'awratin"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "Not", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f\u0648\u0646\u064e", "word_translation": "they wished", "word_transliteration": "yuriduna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0641\u0650\u0631\u064e\u0627\u0631\u0657\u0627", "word_translation": "to flee", "word_transliteration": "firaran"}]}