{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0630\u064e\u0627 \u0631\u064e\u0643\u0650\u0628\u064f\u0648\u0627\u0652 \u0641\u0650\u064a \u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u0650 \u062f\u064e\u0639\u064e\u0648\u064f\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0645\u064f\u062e\u06e1\u0644\u0650\u0635\u0650\u064a\u0646\u064e \u0644\u064e\u0647\u064f \u0671\u0644\u062f\u0651\u0650\u064a\u0646\u064e \u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627 \u0646\u064e\u062c\u0651\u064e\u0649\u0670\u0647\u064f\u0645\u06e1 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u06e1\u0628\u064e\u0631\u0651\u0650 \u0625\u0650\u0630\u064e\u0627 \u0647\u064f\u0645\u06e1 \u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0630\u064e\u0627", "word_translation": "And when", "word_transliteration": "fa-idha"}, {"word_arabic_uthmani": "\u0631\u064e\u0643\u0650\u0628\u064f\u0648\u0627\u0652", "word_translation": "they embark", "word_transliteration": "rakibu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "[in]", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u0650", "word_translation": "the ship", "word_transliteration": "al-ful'ki"}, {"word_arabic_uthmani": "\u062f\u064e\u0639\u064e\u0648\u064f\u0627\u0652", "word_translation": "they call", "word_transliteration": "da'awu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0645\u064f\u062e\u06e1\u0644\u0650\u0635\u0650\u064a\u0646\u064e", "word_translation": "(being) sincere", "word_transliteration": "mukh'lisina"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f", "word_translation": "to Him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u0650\u064a\u0646\u064e", "word_translation": "(in) the religion", "word_transliteration": "al-dina"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627", "word_translation": "But when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u0646\u064e\u062c\u0651\u064e\u0649\u0670\u0647\u064f\u0645\u06e1", "word_translation": "He delivers them", "word_transliteration": "najjahum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0631\u0651\u0650", "word_translation": "the land", "word_transliteration": "al-bari"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "behold", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u064a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "associate partners (with Him)", "word_transliteration": "yush'rikuna"}]}