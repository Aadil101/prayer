{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0647\u064f\u0648\u064e \u064a\u064e\u0642\u06e1\u0628\u064e\u0644\u064f \u0671\u0644\u062a\u0651\u064e\u0648\u06e1\u0628\u064e\u0629\u064e \u0639\u064e\u0646\u06e1 \u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u0650\u06e6 \u0648\u064e\u064a\u064e\u0623\u06e1\u062e\u064f\u0630\u064f \u0671\u0644\u0635\u0651\u064e\u062f\u064e\u0642\u064e\u0670\u062a\u0650 \u0648\u064e\u0623\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0647\u064f\u0648\u064e \u0671\u0644\u062a\u0651\u064e\u0648\u0651\u064e\u0627\u0628\u064f \u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Do not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652", "word_translation": "they know", "word_transliteration": "ya'lamu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e", "word_translation": "that", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "(is) He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u06e1\u0628\u064e\u0644\u064f", "word_translation": "(Who) accepts", "word_transliteration": "yaqbalu"}, {"word_arabic_uthmani": "\u0671\u0644\u062a\u0651\u064e\u0648\u06e1\u0628\u064e\u0629\u064e", "word_translation": "the repentance", "word_transliteration": "al-tawbata"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1", "word_translation": "from", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0647\u065