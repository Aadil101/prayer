{"ayahs": {"ayah_uthmani": "\u06de\u0641\u064e\u0640\u0654\u064e\u0627\u0645\u064e\u0646\u064e \u0644\u064e\u0647\u064f\u06e5 \u0644\u064f\u0648\u0637\u065e\u06d8 \u0648\u064e\u0642\u064e\u0627\u0644\u064e \u0625\u0650\u0646\u0651\u0650\u064a \u0645\u064f\u0647\u064e\u0627\u062c\u0650\u0631\u064c \u0625\u0650\u0644\u064e\u0649\u0670 \u0631\u064e\u0628\u0651\u0650\u064a\u0653\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u06de\u0641\u064e\u0640\u0654\u064e\u0627\u0645\u064e\u0646\u064e", "word_translation": "And believed", "word_transliteration": "faamana"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "[in] him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u0644\u064f\u0648\u0637\u065e\u06d8", "word_translation": "Lut", "word_transliteration": "lutun"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "and he said", "word_transliteration": "waqala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I (am)", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0645\u064f\u0647\u064e\u0627\u062c\u0650\u0631\u064c", "word_translation": "emigrating", "word_transliteration": "muhajirun"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a\u0653\u06d6", "word_translation": "my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "[He] (is)", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f", "word_translation": "the All-Mighty", "word_transliteration": "al-'azizu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f", "word_translation": "the All-Wise", "word_transliteration": "al-hakimu"}]}