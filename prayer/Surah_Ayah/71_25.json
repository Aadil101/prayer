{"ayahs": {"ayah_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0627 \u062e\u064e\u0637\u0650\u064a\u0653\u0640\u0654\u064e\u0670\u062a\u0650\u0647\u0650\u0645\u06e1 \u0623\u064f\u063a\u06e1\u0631\u0650\u0642\u064f\u0648\u0627\u0652 \u0641\u064e\u0623\u064f\u062f\u06e1\u062e\u0650\u0644\u064f\u0648\u0627\u0652 \u0646\u064e\u0627\u0631\u0657\u0627 \u0641\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u062c\u0650\u062f\u064f\u0648\u0627\u0652 \u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0623\u064e\u0646\u0635\u064e\u0627\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0627", "word_translation": "Because of", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u062e\u064e\u0637\u0650\u064a\u0653\u0640\u0654\u064e\u0670\u062a\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their sins", "word_transliteration": "khatiatihim"}, {"word_arabic_uthmani": "\u0623\u064f\u063a\u06e1\u0631\u0650\u0642\u064f\u0648\u0627\u0652", "word_translation": "they were drowned", "word_transliteration": "ugh'riqu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064f\u062f\u06e1\u062e\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "then made to enter", "word_transliteration": "fa-ud'khilu"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u0631\u0657\u0627", "word_translation": "(the) Fire", "word_transliteration": "naran"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u06e1", "word_translation": "and not", "word_transliteration": "falam"}, {"word_arabic_uthmani": "\u064a\u064e\u062c\u0650\u062f\u064f\u0648\u0627\u0652", "word_translation": "they found", "word_transliteration": "yajidu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "for themselves", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "besides", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0635\u064e\u0627\u0631\u0657\u0627", "word_translation": "any helpers", "word_transliteration": "ansaran"}]}