{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653 \u0623\u064e\u062a\u064e\u0649\u0670\u0647\u064e\u0627 \u0646\u064f\u0648\u062f\u0650\u064a\u064e \u0645\u0650\u0646 \u0634\u064e\u0670\u0637\u0650\u064a\u0655\u0650 \u0671\u0644\u06e1\u0648\u064e\u0627\u062f\u0650 \u0671\u0644\u06e1\u0623\u064e\u064a\u06e1\u0645\u064e\u0646\u0650 \u0641\u0650\u064a \u0671\u0644\u06e1\u0628\u064f\u0642\u06e1\u0639\u064e\u0629\u0650 \u0671\u0644\u06e1\u0645\u064f\u0628\u064e\u0670\u0631\u064e\u0643\u064e\u0629\u0650 \u0645\u0650\u0646\u064e \u0671\u0644\u0634\u0651\u064e\u062c\u064e\u0631\u064e\u0629\u0650 \u0623\u064e\u0646 \u064a\u064e\u0670\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653 \u0625\u0650\u0646\u0651\u0650\u064a\u0653 \u0623\u064e\u0646\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0631\u064e\u0628\u0651\u064f \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0645\u0651\u064e\u0627\u0653", "word_translation": "But when", "word_transliteration": "falamma"}, {"word_arabic_uthmani": "\u0623\u064e\u062a\u064e\u0649\u0670\u0647\u064e\u0627", "word_translation": "he came (to) it", "word_transliteration": "ataha"}, {"word_arabic_uthmani": "\u0646\u064f\u0648\u062f\u0650\u064a\u064e", "word_translation": "he was called", "word_transliteration": "nudiya"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u064e\u0670\u0637\u0650\u064a\u0655\u0650", "word_translation": "(the) side", "word_transliteration": "shati-i"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0648\u064e\u0627\u062f\u0650", "word_translation": "(of) the valley", "word_transliteration": "al-wadi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u064a\u06e1\u0645\u064e\u0646\u0650", "word_translation": "the right", "word_transliteration": "al-aymani"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064f\u0642\u06e1\u0639\u064e\u0629\u0650", "word_translation": "the place even", "word_transliteration": "al-buq'ati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0628\u064e\u0670\u0631\u064e\u0643\u064e\u0629\u0650", "word_translation": "blessed", "word_transliteration": "al-mubarakati"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u062c\u064e\u0631\u064e\u0629\u0650", "word_translation": "the tree", "word_transliteration": "al-shajarati"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0645\u064f\u0648\u0633\u064e\u0649\u0670\u0653", "word_translation": "O Musa!", "word_transliteration": "yamusa"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "Indeed", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627", "word_translation": "I Am", "word_transliteration": "ana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f", "word_translation": "(the) Lord", "word_transliteration": "rabbu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the worlds", "word_transliteration": "al-'alamina"}]}