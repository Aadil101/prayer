ation": "yatu"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0631\u06e1\u0628\u064e\u0639\u064e\u0629\u0650", "word_translation": "four", "word_transliteration": "bi-arba'ati"}, {"word_arabic_uthmani": "\u0634\u064f\u0647\u064e\u062f\u064e\u0627\u0653\u0621\u064e", "word_translation": "witnesses", "word_transliteration": "shuhadaa"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u062c\u06e1\u0644\u0650\u062f\u064f\u0648\u0647\u064f\u0645\u06e1", "word_translation": "then flog them", "word_transliteration": "fa-ij'liduhum"}, {"word_arabic_uthmani": "\u062b\u064e\u0645\u064e\u0670\u0646\u0650\u064a\u0646\u064e", "word_translation": "(with) eighty", "word_transliteration": "thamanina"}, {"word_arabic_uthmani": "\u062c\u064e\u0644\u06e1\u062f\u064e\u0629\u0657", "word_translation": "lashe(s)", "word_transliteration": "jaldatan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u06e1\u0628\u064e\u0644\u064f\u0648\u0627\u0652", "word_translation": "accept", "word_transliteration": "taqbalu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0634\u064e\u0647\u064e\u0670\u062f\u064e\u0629\u064b", "word_translation": "testimony", "word_transliteration": "shahadatan"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u064e\u062f\u0657\u0627\u06da", "word_translation": "ever", "word_transliteration": "abadan"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "And those", "word_transliteration": "wa-ulaika"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "they", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "(are) the defiantly d