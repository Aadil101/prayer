{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u062e\u064e\u0644\u064e\u0642\u06e1\u062a\u064f \u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u064e \u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0644\u0650\u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u06e1\u062a\u064f", "word_translation": "I have created", "word_transliteration": "khalaqtu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u064e", "word_translation": "the jinn", "word_transliteration": "al-jina"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u064e", "word_translation": "and the mankind", "word_transliteration": "wal-insa"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0646\u0650", "word_translation": "that they worship Me", "word_transliteration": "liya'buduni"}]}