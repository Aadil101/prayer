{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0646 \u0644\u0651\u064e\u0645\u06e1 \u062a\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u0650\u064a \u0628\u0650\u0647\u0650\u06e6 \u0641\u064e\u0644\u064e\u0627 \u0643\u064e\u064a\u06e1\u0644\u064e \u0644\u064e\u0643\u064f\u0645\u06e1 \u0639\u0650\u0646\u062f\u0650\u064a \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0642\u06e1\u0631\u064e\u0628\u064f\u0648\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646", "word_translation": "But if", "word_transliteration": "fa-in"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0645\u06e1", "word_translation": "not", "word_transliteration": "lam"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u062a\u064f\u0648\u0646\u0650\u064a", "word_translation": "you bring him to me", "word_transliteration": "tatuni"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "you bring him to me", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "then (there will be) no", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0644\u064e", "word_translation": "measure", "word_transliteration": "kayla"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u0650\u064a", "word_translation": "from me", "word_transliteration": "indi"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u06e1\u0631\u064e\u0628\u064f\u0648\u0646\u0650", "word_translation": "you will come near me", "word_transliteration": "taqrabuni"}]}