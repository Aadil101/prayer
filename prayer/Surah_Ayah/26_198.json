{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1 \u0646\u064e\u0632\u0651\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f \u0639\u064e\u0644\u064e\u0649\u0670 \u0628\u064e\u0639\u06e1\u0636\u0650 \u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u062c\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0646\u064e\u0632\u0651\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f", "word_translation": "We (had) revealed it", "word_transliteration": "nazzalnahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u0650", "word_translation": "any", "word_transliteration": "ba'di"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0639\u06e1\u062c\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the non-Arabs", "word_transliteration": "al-a'jamina"}]}