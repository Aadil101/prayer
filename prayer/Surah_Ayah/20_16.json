{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0644\u064e\u0627 \u064a\u064e\u0635\u064f\u062f\u0651\u064e\u0646\u0651\u064e\u0643\u064e \u0639\u064e\u0646\u06e1\u0647\u064e\u0627 \u0645\u064e\u0646 \u0644\u0651\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f \u0628\u0650\u0647\u064e\u0627 \u0648\u064e\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e \u0647\u064e\u0648\u064e\u0649\u0670\u0647\u064f \u0641\u064e\u062a\u064e\u0631\u06e1\u062f\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "So (do) not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u064a\u064e\u0635\u064f\u062f\u0651\u064e\u0646\u0651\u064e\u0643\u064e", "word_translation": "(let) avert you", "word_transliteration": "yasuddannaka"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064e\u0627", "word_translation": "from it", "word_transliteration": "anha"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(one) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "(does) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f", "word_translation": "believe", "word_transliteration": "yu'minu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u064e\u0627", "word_translation": "in it", "word_transliteration": "biha"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u0628\u064e\u0639\u064e", "word_translation": "and follows", "word_transliteration": "wa-ittaba'a"}, {"word_arabic_uthmani": "\u0647\u064e\u0648\u064e\u0649\u0670\u0647\u064f", "word_translation": "his desires", "word_transliteration": "hawahu"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0631\u06e1\u062f\u064e\u0649\u0670", "word_translation": "lest you perish", "word_transliteration": "fatarda"}]}