{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0645\u064e\u0646 \u064a\u064f\u0633\u06e1\u0644\u0650\u0645\u06e1 \u0648\u064e\u062c\u06e1\u0647\u064e\u0647\u064f\u06e5\u0653 \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0647\u064f\u0648\u064e \u0645\u064f\u062d\u06e1\u0633\u0650\u0646\u065e \u0641\u064e\u0642\u064e\u062f\u0650 \u0671\u0633\u06e1\u062a\u064e\u0645\u06e1\u0633\u064e\u0643\u064e \u0628\u0650\u0671\u0644\u06e1\u0639\u064f\u0631\u06e1\u0648\u064e\u0629\u0650 \u0671\u0644\u06e1\u0648\u064f\u062b\u06e1\u0642\u064e\u0649\u0670\u06d7 \u0648\u064e\u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f \u0671\u0644\u06e1\u0623\u064f\u0645\u064f\u0648\u0631\u0650"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0645\u064e\u0646", "word_translation": "And whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u064a\u064f\u0633\u06e1\u0644\u0650\u0645\u06e1", "word_translation": "submits", "word_transliteration": "yus'lim"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u06e1\u0647\u064e\u0647\u064f\u06e5\u0653", "word_translation": "his face", "word_transliteration": "wajhahu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "while he", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0645\u064f\u062d\u06e1\u0633\u0650\u0646\u065e", "word_translation": "(is) a good- doer", "word_transliteration": "muh'sinun"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u0650", "word_translation": "then indeed", "word_transliteration": "faqadi"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0645\u06e1\u0633\u064e\u0643\u064e", "word_translation": "he has grasped", "word_transliteration": "is'tamsaka"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0639\u064f\u0631\u06e1\u0648\u064e\u0629\u0650", "word_translation": "the handhold", "word_transliteration": "bil-'ur'wati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0648\u064f\u062b\u06e1\u0642\u064e\u0649\u0670\u06d7", "word_translation": "the most trustworthy", "word_transliteration": "al-wuth'qa"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u0649", "word_translation": "And to", "word_transliteration": "wa-ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f", "word_translation": "(is the) end", "word_transliteration": "aqibatu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064f\u0645\u064f\u0648\u0631\u0650", "word_translation": "(of) the matters", "word_transliteration": "al-umuri"}]}