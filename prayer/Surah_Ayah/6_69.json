{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u062a\u0651\u064e\u0642\u064f\u0648\u0646\u064e \u0645\u0650\u0646\u06e1 \u062d\u0650\u0633\u064e\u0627\u0628\u0650\u0647\u0650\u0645 \u0645\u0651\u0650\u0646 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646 \u0630\u0650\u0643\u06e1\u0631\u064e\u0649\u0670 \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u062a\u0651\u064e\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "(is) on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u0651\u064e\u0642\u064f\u0648\u0646\u064e", "word_translation": "fear (Allah)", "word_transliteration": "yattaquna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062d\u0650\u0633\u064e\u0627\u0628\u0650\u0647\u0650\u0645", "word_translation": "their account", "word_transliteration": "hisabihim"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "[of]", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "anything", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646", "word_translation": "but", "word_transliteration": "walakin"}, {"word_arabic_uthmani": "\u0630\u0650\u0643\u06e1\u0631\u064e\u0649\u0670", "word_translation": "(for) reminder", "word_transliteration": "dhik'ra"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\