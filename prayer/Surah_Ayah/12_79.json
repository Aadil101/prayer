{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0645\u064e\u0639\u064e\u0627\u0630\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0623\u064e\u0646 \u0646\u0651\u064e\u0623\u06e1\u062e\u064f\u0630\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064e\u0646 \u0648\u064e\u062c\u064e\u062f\u06e1\u0646\u064e\u0627 \u0645\u064e\u062a\u064e\u0670\u0639\u064e\u0646\u064e\u0627 \u0639\u0650\u0646\u062f\u064e\u0647\u064f\u06e5\u0653 \u0625\u0650\u0646\u0651\u064e\u0627\u0653 \u0625\u0650\u0630\u0657\u0627 \u0644\u0651\u064e\u0638\u064e\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e\u0627\u0630\u064e", "word_translation": "Allah forbid", "word_transliteration": "ma'adha"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah forbid", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0623\u06e1\u062e\u064f\u0630\u064e", "word_translation": "we take", "word_transliteration": "nakhudha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(one) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u062f\u06e1\u0646\u064e\u0627", "word_translation": "we found", "word_transliteration": "wajadna"}, {"word_arabic_uthmani": "\u0645\u064e\u062a\u064e\u0670\u0639\u064e\u0646\u064e\u0627", "word_translation": "our possession", "word_transliteration": "mata'ana"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e\u0647\u064f\u06e5\u0653", "word_translation": "with him", "word_transliteration": "indahu"}, {"word_arabic_uthmani": "\u