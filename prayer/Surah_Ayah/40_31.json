{"ayahs": {"ayah_uthmani": "\u0645\u0650\u062b\u06e1\u0644\u064e \u062f\u064e\u0623\u06e1\u0628\u0650 \u0642\u064e\u0648\u06e1\u0645\u0650 \u0646\u064f\u0648\u062d\u0656 \u0648\u064e\u0639\u064e\u0627\u062f\u0656 \u0648\u064e\u062b\u064e\u0645\u064f\u0648\u062f\u064e \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u06e2 \u0628\u064e\u0639\u06e1\u062f\u0650\u0647\u0650\u0645\u06e1\u06da \u0648\u064e\u0645\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064f\u0631\u0650\u064a\u062f\u064f \u0638\u064f\u0644\u06e1\u0645\u0657\u0627 \u0644\u0651\u0650\u0644\u06e1\u0639\u0650\u0628\u064e\u0627\u062f\u0650"}, "words": [{"word_arabic_uthmani": "\u0645\u0650\u062b\u06e1\u0644\u064e", "word_translation": "Like", "word_transliteration": "mith'la"}, {"word_arabic_uthmani": "\u062f\u064e\u0623\u06e1\u0628\u0650", "word_translation": "(the) plight", "word_transliteration": "dabi"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "(of the) people", "word_transliteration": "qawmi"}, {"word_arabic_uthmani": "\u0646\u064f\u0648\u062d\u0656", "word_translation": "(of) Nuh", "word_transliteration": "nuhin"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0627\u062f\u0656", "word_translation": "and Aad", "word_transliteration": "wa'adin"}, {"word_arabic_uthmani": "\u0648\u064e\u062b\u064e\u0645\u064f\u0648\u062f\u064e", "word_translation": "and Thamud", "word_transliteration": "wathamuda"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e2", "word_translation": "after them", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u0650\u0647\u0650\u0645\u06e1\u06da", "word_translation": "after them", "word_transliteration": "ba'dihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word