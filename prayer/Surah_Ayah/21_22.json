{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0648\u06e1 \u0643\u064e\u0627\u0646\u064e \u0641\u0650\u064a\u0647\u0650\u0645\u064e\u0627\u0653 \u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u064c \u0625\u0650\u0644\u0651\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0641\u064e\u0633\u064e\u062f\u064e\u062a\u064e\u0627\u06da \u0641\u064e\u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0631\u064e\u0628\u0651\u0650 \u0671\u0644\u06e1\u0639\u064e\u0631\u06e1\u0634\u0650 \u0639\u064e\u0645\u0651\u064e\u0627 \u064a\u064e\u0635\u0650\u0641\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0648\u06e1", "word_translation": "If", "word_transliteration": "law"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "(there) were", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650\u0645\u064e\u0627\u0653", "word_translation": "in both of them", "word_transliteration": "fihima"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u0650\u0647\u064e\u0629\u064c", "word_translation": "gods", "word_transliteration": "alihatun"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "besides", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0641\u064e\u0633\u064e\u062f\u064e\u062a\u064e\u0627\u06da", "word_translation": "surely they (would) have been ruined", "word_transliteration": "lafasadata"}, {"word_arabic_uthmani": "\u0641\u064e\u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e", "word_translation": "So glorified", "word_transliteration": "fasub'hana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(is) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0631\u06e1\u0634\u0650", "word_translation": "(of) the Throne", "word_transliteration": "al-'arshi"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "(above) what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u064a\u064e\u0635\u0650\u0641\u064f\u0648\u0646\u064e", "word_translation": "they attribute", "word_transliteration": "yasifuna"}]}