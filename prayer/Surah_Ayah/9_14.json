{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0670\u062a\u0650\u0644\u064f\u0648\u0647\u064f\u0645\u06e1 \u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u06e1\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u0650\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0643\u064f\u0645\u06e1 \u0648\u064e\u064a\u064f\u062e\u06e1\u0632\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u064a\u064e\u0646\u0635\u064f\u0631\u06e1\u0643\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0648\u064e\u064a\u064e\u0634\u06e1\u0641\u0650 \u0635\u064f\u062f\u064f\u0648\u0631\u064e \u0642\u064e\u0648\u06e1\u0645\u0656 \u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0670\u062a\u0650\u0644\u064f\u0648\u0647\u064f\u0645\u06e1", "word_translation": "Fight them", "word_transliteration": "qatiluhum"}, {"word_arabic_uthmani": "\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u06e1\u0647\u064f\u0645\u064f", "word_translation": "Allah will punish them", "word_transliteration": "yu'adhib'humu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah will punish them", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0643\u064f\u0645\u06e1", "word_translation": "by your hands", "word_transliteration": "bi-aydikum"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u062e\u06e1\u0632\u0650\u0647\u0650\u0645\u06e1", "word_translation": "and disgrace them", "word_transliteration": "wayukh'zihim"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0646\u0635\u064f\u0631\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "and give you victory", "word_transliteration": "wayansur'kum"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "over them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0634\u06e1\u0641\u0650", "word_translation": "and will heal", "word_transliteration": "wayashfi"}, {"word_arabic_uthmani": "\u0635\u064f\u062f\u064f\u0648\u0631\u064e", "word_translation": "(the) breasts", "word_transliteration": "sudura"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0656", "word_translation": "(of) a people", "word_transliteration": "qawmin"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "(who are) believers", "word_transliteration": "mu'minina"}]}