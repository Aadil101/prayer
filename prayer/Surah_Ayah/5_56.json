{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0646 \u064a\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5 \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0641\u064e\u0625\u0650\u0646\u0651\u064e \u062d\u0650\u0632\u06e1\u0628\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u063a\u064e\u0670\u0644\u0650\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "And whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e", "word_translation": "takes as an ally", "word_transliteration": "yatawalla"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0633\u064f\u0648\u0644\u064e\u0647\u064f\u06e5", "word_translation": "and His Messenger", "word_transliteration": "warasulahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "then indeed", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u062d\u0650\u0632\u06e1\u0628\u064e", "word_translation": "(the) party", "word_transliteration": "hiz'ba"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "they", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u0670\u0644\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "(are) the victorious", "word_transliteration": "al-ghalibuna"}]}