{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064e\u0637\u0651\u064e\u0639\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0623\u064f\u0645\u064e\u0645\u0657\u0627\u06d6 \u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u064f \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064f\u0648\u0646\u064e \u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u062f\u064f\u0648\u0646\u064e \u0630\u064e\u0670\u0644\u0650\u0643\u064e\u06d6 \u0648\u064e\u0628\u064e\u0644\u064e\u0648\u06e1\u0646\u064e\u0670\u0647\u064f\u0645 \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0633\u064e\u0646\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0640\u0654\u064e\u0627\u062a\u0650 \u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0631\u06e1\u062c\u0650\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0637\u0651\u064e\u0639\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "And We divided them", "word_transliteration": "waqatta'nahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u064e\u0645\u0657\u0627\u06d6", "word_translation": "(as) nations", "word_transliteration": "umaman"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u064f", "word_translation": "Among them", "word_transliteration": "min'humu"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0644\u0650\u062d\u064f\u0648\u0646\u064e", "word_translation": "(are) the righteous", "word_transliteration": "al-salihuna"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "and among them", "word_transliteration": "wamin'hum"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u064e", "word_translation": "(are) other than", "word_transliteration": "duna"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e\u06d6", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0648\u064e\u0628\u064e\u0644\u064e\u0648\u06e1\u0646\u064e\u0670\u0647\u064f\u0645", "word_translation": "And We tested them", "word_transliteration": "wabalawnahum"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0633\u064e\u0646\u064e\u0670\u062a\u0650", "word_translation": "with the good", "word_transliteration": "bil-hasanati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0640\u0654\u064e\u0627\u062a\u0650", "word_translation": "and the bad", "word_transliteration": "wal-sayiati"}, {"word_arabic_uthmani": "\u0644\u064e\u0639\u064e\u0644\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so that they may", "word_transliteration": "la'allahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u062c\u0650\u0639\u064f\u0648\u0646\u064e", "word_translation": "return", "word_transliteration": "yarji'una"}]}