{"ayahs": {"ayah_uthmani": "\u06de\u0623\u064e\u0644\u064e\u0645\u06e1 \u062a\u064e\u0631\u064e \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0646\u064e\u0627\u0641\u064e\u0642\u064f\u0648\u0627\u0652 \u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u0644\u0650\u0625\u0650\u062e\u06e1\u0648\u064e\u0670\u0646\u0650\u0647\u0650\u0645\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u06e1 \u0623\u064e\u0647\u06e1\u0644\u0650 \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u0644\u064e\u0626\u0650\u0646\u06e1 \u0623\u064f\u062e\u06e1\u0631\u0650\u062c\u06e1\u062a\u064f\u0645\u06e1 \u0644\u064e\u0646\u064e\u062e\u06e1\u0631\u064f\u062c\u064e\u0646\u0651\u064e \u0645\u064e\u0639\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u0646\u064f\u0637\u0650\u064a\u0639\u064f \u0641\u0650\u064a\u0643\u064f\u0645\u06e1 \u0623\u064e\u062d\u064e\u062f\u064b\u0627 \u0623\u064e\u0628\u064e\u062f\u0657\u0627 \u0648\u064e\u0625\u0650\u0646 \u0642\u064f\u0648\u062a\u0650\u0644\u06e1\u062a\u064f\u0645\u06e1 \u0644\u064e\u0646\u064e\u0646\u0635\u064f\u0631\u064e\u0646\u0651\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064e\u0634\u06e1\u0647\u064e\u062f\u064f \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0643\u064e\u0670\u0630\u0650\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Do not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e", "word_translation": "you see", "word_transliteration": "tara"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "[to]", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0646\u064e\u0627\u0641\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "(were) hypocrites", "word_transliteration": "nafaqu"}, {"word_arabic_uthmani": "\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "saying", "word_transliteration": "yaquluna"}, {"word_arabic_uthmani": "\u0644\u0650\u0625\u0650\u062e\u06e1\u0648\u064e\u0670\u0646\u0650\u0647\u0650\u0645\u064f", "word_translation": "to their brothers", "word_transliteration": "li-ikh'wanihimu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "among", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0647\u06e1\u0644\u0650", "word_translation": "the People", "word_transliteration": "ahli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "(of) the Scripture", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u0644\u064e\u0626\u0650\u0646\u06e1", "word_translation": "If", "word_transliteration": "la-in"}, {"word_arabic_uthmani": "\u0623\u064f\u062e\u06e1\u0631\u0650\u062c\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "you are expelled", "word_transliteration": "ukh'rij'tum"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u062e\u06e1\u0631\u064f\u062c\u064e\u0646\u0651\u064e", "word_translation": "surely we will leave", "word_transliteration": "lanakhrujanna"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e\u0643\u064f\u0645\u06e1", "word_translation": "with you", "word_transliteration": "ma'akum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0646\u064f\u0637\u0650\u064a\u0639\u064f", "word_translation": "we will obey", "word_transliteration": "nuti'u"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0643\u064f\u0645\u06e1", "word_translation": "concerning you", "word_transliteration": "fikum"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u064b\u0627", "word_translation": "anyone", "word_transliteration": "ahadan"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u064e\u062f\u0657\u0627", "word_translation": "ever", "word_transliteration": "abadan"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "and if", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0642\u064f\u0648\u062a\u0650\u0644\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "you are fought", "word_transliteration": "qutil'tum"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0646\u0635\u064f\u0631\u064e\u0646\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "certainly we will help you", "word_transliteration": "lanansurannakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u06e1\u0647\u064e\u062f\u064f", "word_translation": "bears witness", "word_transliteration": "yashhadu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "that they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e\u0670\u0630\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "(are) surely liars", "word_transliteration": "lakadhibuna"}]}