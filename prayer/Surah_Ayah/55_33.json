{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0670\u0645\u064e\u0639\u06e1\u0634\u064e\u0631\u064e \u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u0650 \u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u0650 \u0625\u0650\u0646\u0650 \u0671\u0633\u06e1\u062a\u064e\u0637\u064e\u0639\u06e1\u062a\u064f\u0645\u06e1 \u0623\u064e\u0646 \u062a\u064e\u0646\u0641\u064f\u0630\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u06e1 \u0623\u064e\u0642\u06e1\u0637\u064e\u0627\u0631\u0650 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0641\u064e\u0671\u0646\u0641\u064f\u0630\u064f\u0648\u0627\u0652\u06da \u0644\u064e\u0627 \u062a\u064e\u0646\u0641\u064f\u0630\u064f\u0648\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u0650\u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u0656"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0670\u0645\u064e\u0639\u06e1\u0634\u064e\u0631\u064e", "word_translation": "O assembly", "word_transliteration": "yama'shara"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u0650\u0646\u0651\u0650", "word_translation": "(of) the jinn", "word_transliteration": "al-jini"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0625\u0650\u0646\u0633\u0650", "word_translation": "and the men!", "word_transliteration": "wal-insi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0650", "word_translation": "If", "word_transliteration": "ini"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0637\u064e\u0639\u06e1\u062a\u064f\u0645\u06e1", "word_translation": "you are able", "word_transliteration": "is'tata'tum"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "to", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u0646\u0641\u064f\u0630\u064f\u0648\u0627\u0652", "word_translation": "pass beyond", "word_transliteration": "tanfudhu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "[of]", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0642\u06e1\u0637\u064e\u0627\u0631\u0650", "word_translation": "(the) regions", "word_transliteration": "aqtari"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0646\u0641\u064f\u0630\u064f\u0648\u0627\u0652\u06da", "word_translation": "then pass", "word_transliteration": "fa-unfudhu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062a\u064e\u0646\u0641\u064f\u0630\u064f\u0648\u0646\u064e", "word_translation": "you (can) pass", "word_transliteration": "tanfudhuna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u0650\u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u0656", "word_translation": "by authority", "word_transliteration": "bisul'tanin"}]}