{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062d\u064e\u0633\u0650\u0628\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0644\u0651\u064e\u0627 \u062a\u064e\u0643\u064f\u0648\u0646\u064e \u0641\u0650\u062a\u06e1\u0646\u064e\u0629\u065e \u0641\u064e\u0639\u064e\u0645\u064f\u0648\u0627\u0652 \u0648\u064e\u0635\u064e\u0645\u0651\u064f\u0648\u0627\u0652 \u062b\u064f\u0645\u0651\u064e \u062a\u064e\u0627\u0628\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u062b\u064f\u0645\u0651\u064e \u0639\u064e\u0645\u064f\u0648\u0627\u0652 \u0648\u064e\u0635\u064e\u0645\u0651\u064f\u0648\u0627\u0652 \u0643\u064e\u062b\u0650\u064a\u0631\u065e \u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0628\u064e\u0635\u0650\u064a\u0631\u064f\u06e2 \u0628\u0650\u0645\u064e\u0627 \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062d\u064e\u0633\u0650\u0628\u064f\u0648\u0653\u0627\u0652", "word_translation": "And they thought", "word_transliteration": "wahasibu"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0651\u064e\u0627", "word_translation": "that not", "word_transliteration": "alla"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u064f\u0648\u0646\u064e", "word_translation": "will be (for them)", "word_transliteration": "takuna"}, {"word_arabic_uthmani": "\u0641\u0650\u062a\u06e1\u0646\u064e\u0629\u065e", "word_translation": "a trial", "word_transliteration": "fit'natun"}, {"word_arabic_uthmani": "\u0641\u064e\u0639\u064e\u0645\u064f\u0648\u0627\u0652", "word_translation": "so they became blind", "word_transliteration": "fa'amu"}, {"word_arabic_uthmani": "\u0648\u064e\u0635\u064e\u0645\u0651\u064f\u0648\u0627\u0652", "word_translation": "and they became deaf", "word_transliteration": "wasammu"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u062a\u064e\u0627\u0628\u064e", "word_translation": "turned", "word_transliteration": "taba"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "to them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then (again)", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u064f\u0648\u0627\u0652", "word_translation": "they became blind", "word_transliteration": "amu"}, {"word_arabic_uthmani": "\u0648\u064e\u0635\u064e\u0645\u0651\u064f\u0648\u0627\u0652", "word_translation": "and they became deaf", "word_transliteration": "wasammu"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u065e", "word_translation": "many", "word_transliteration": "kathirun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1\u06da", "word_translation": "of them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0628\u064e\u0635\u0650\u064a\u0631\u064f\u06e2", "word_translation": "(is) All-Seer", "word_transliteration": "basirun"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "they do", "word_transliteration": "ya'maluna"}]}