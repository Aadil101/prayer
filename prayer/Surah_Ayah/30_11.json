{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u064a\u064e\u0628\u06e1\u062f\u064e\u0624\u064f\u0627\u0652 \u0671\u0644\u06e1\u062e\u064e\u0644\u06e1\u0642\u064e \u062b\u064f\u0645\u0651\u064e \u064a\u064f\u0639\u0650\u064a\u062f\u064f\u0647\u064f\u06e5 \u062b\u064f\u0645\u0651\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u064a\u064e\u0628\u06e1\u062f\u064e\u0624\u064f\u0627\u0652", "word_translation": "originates", "word_transliteration": "yabda-u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0644\u06e1\u0642\u064e", "word_translation": "the creation", "word_transliteration": "al-khalqa"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u064a\u064f\u0639\u0650\u064a\u062f\u064f\u0647\u064f\u06e5", "word_translation": "He repeats it", "word_transliteration": "yu'iduhu"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "to Him", "word_transliteration": "ilayhi"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "you will be returned", "word_transliteration": "tur'ja'una"}]}