{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627 \u0632\u064e\u064a\u0651\u064e\u0646\u0651\u064e\u0627 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627 \u0628\u0650\u0632\u0650\u064a\u0646\u064e\u0629\u064d \u0671\u0644\u06e1\u0643\u064e\u0648\u064e\u0627\u0643\u0650\u0628\u0650"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627", "word_translation": "Indeed We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0632\u064e\u064a\u0651\u064e\u0646\u0651\u064e\u0627", "word_translation": "[We] adorned", "word_transliteration": "zayyanna"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064e", "word_translation": "the heaven", "word_transliteration": "al-samaa"}, {"word_arabic_uthmani": "\u0671\u0644\u062f\u0651\u064f\u0646\u06e1\u064a\u064e\u0627", "word_translation": "nearest", "word_transliteration": "al-dun'ya"}, {"word_arabic_uthmani": "\u0628\u0650\u0632\u0650\u064a\u0646\u064e\u0629\u064d", "word_translation": "with an adornment", "word_transliteration": "bizinatin"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0648\u064e\u0627\u0643\u0650\u0628\u0650", "word_translation": "(of) the stars", "word_transliteration": "al-kawakibi"}]}