{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646 \u0645\u0651\u0650\u0646 \u0634\u064e\u064a\u06e1\u0621\u064d \u0625\u0650\u0644\u0651\u064e\u0627 \u0639\u0650\u0646\u062f\u064e\u0646\u064e\u0627 \u062e\u064e\u0632\u064e\u0627\u0653\u0626\u0650\u0646\u064f\u0647\u064f\u06e5 \u0648\u064e\u0645\u064e\u0627 \u0646\u064f\u0646\u064e\u0632\u0651\u0650\u0644\u064f\u0647\u064f\u06e5\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u0650\u0642\u064e\u062f\u064e\u0631\u0656 \u0645\u0651\u064e\u0639\u06e1\u0644\u064f\u0648\u0645\u0656"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "And not", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "(is) any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u064d", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e\u0646\u064e\u0627", "word_translation": "with Us", "word_transliteration": "indana"}, {"word_arabic_uthmani": "\u062e\u064e\u0632\u064e\u0627\u0653\u0626\u0650\u0646\u064f\u0647\u064f\u06e5", "word_translation": "(are) its treasures", "word_transliteration": "khazainuhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0646\u064f\u0646\u064e\u0632\u0651\u0650\u0644\u064f\u0647\u064f\u06e5\u0653", "word_translation": "We send it down", "word_transliteration": "nunazziluhu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u0650\u0642\u064e\u062f\u064e\u0631\u0656", "word_translation": "in a measure", "word_transliteration": "biqadarin"}, {"word_arabic_uthmani": "\u0645\u0651\u