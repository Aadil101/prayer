{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0624\u064f\u0627\u0652 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6 \u0645\u064e\u0627 \u0647\u064e\u0670\u0630\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u064e\u0634\u064e\u0631\u065e \u0645\u0651\u0650\u062b\u06e1\u0644\u064f\u0643\u064f\u0645\u06e1 \u064a\u064f\u0631\u0650\u064a\u062f\u064f \u0623\u064e\u0646 \u064a\u064e\u062a\u064e\u0641\u064e\u0636\u0651\u064e\u0644\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0648\u06e1 \u0634\u064e\u0627\u0653\u0621\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0644\u064e\u0623\u064e\u0646\u0632\u064e\u0644\u064e \u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u0657 \u0645\u0651\u064e\u0627 \u0633\u064e\u0645\u0650\u0639\u06e1\u0646\u064e\u0627 \u0628\u0650\u0647\u064e\u0670\u0630\u064e\u0627 \u0641\u0650\u064a\u0653 \u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0626\u0650\u0646\u064e\u0627 \u0671\u0644\u06e1\u0623\u064e\u0648\u0651\u064e\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "But said", "word_transliteration": "faqala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0624\u064f\u0627\u0652", "word_translation": "the chiefs", "word_transliteration": "al-mala-u"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "among", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6", "word_translation": "his people", "word_transliteration": "qawmihi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "This is not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u064e\u0627\u0653", "word_translation": "This is not", "word_transliteration": "hadha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "but", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0628\u064e\u0634\u064e\u0631\u065e", "word_translation": "a man", "word_transliteration": "basharun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u062b\u06e1\u0644\u064f\u0643\u064f\u0645\u06e1", "word_translation": "like you", "word_transliteration": "mith'lukum"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f", "word_translation": "he wishes", "word_transliteration": "yuridu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "to", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0641\u064e\u0636\u0651\u064e\u0644\u064e", "word_translation": "assert (his) superiority", "word_transliteration": "yatafaddala"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "over you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "and if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0634\u064e\u0627\u0653\u0621\u064e", "word_translation": "Allah had willed", "word_transliteration": "shaa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah had willed", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u064e\u0646\u0632\u064e\u0644\u064e", "word_translation": "surely He (would have) sent down", "word_transliteration": "la-anzala"}, {"word_arabic_uthmani": "\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u0657", "word_translation": "Angels", "word_transliteration": "malaikatan"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0633\u064e\u0645\u0650\u0639\u06e1\u0646\u064e\u0627", "word_translation": "we heard", "word_transliteration": "sami'na"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u064e\u0670\u0630\u064e\u0627", "word_translation": "of this", "word_transliteration": "bihadha"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0653", "word_translation": "from", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0626\u0650\u0646\u064e\u0627", "word_translation": "our forefathers", "word_transliteration": "abaina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0648\u0651\u064e\u0644\u0650\u064a\u0646\u064e", "word_translation": "our forefathers", "word_transliteration": "al-awalina"}]}