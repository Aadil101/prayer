on": "afabini'mati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "of Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u064a\u064e\u062c\u06e1\u062d\u064e\u062f\u064f\u0648\u0646\u064e", "word_translation": "they reject", "word_transliteration": "yajhaduna"}]}