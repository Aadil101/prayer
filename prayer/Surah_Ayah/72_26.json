{"ayahs": {"ayah_uthmani": "\u0639\u064e\u0670\u0644\u0650\u0645\u064f \u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650 \u0641\u064e\u0644\u064e\u0627 \u064a\u064f\u0638\u06e1\u0647\u0650\u0631\u064f \u0639\u064e\u0644\u064e\u0649\u0670 \u063a\u064e\u064a\u06e1\u0628\u0650\u0647\u0650\u06e6\u0653 \u0623\u064e\u062d\u064e\u062f\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0639\u064e\u0670\u0644\u0650\u0645\u064f", "word_translation": "(The) All-Knower", "word_transliteration": "alimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650", "word_translation": "(of) the unseen", "word_transliteration": "al-ghaybi"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0627", "word_translation": "so not", "word_transliteration": "fala"}, {"word_arabic_uthmani": "\u064a\u064f\u0638\u06e1\u0647\u0650\u0631\u064f", "word_translation": "He reveals", "word_transliteration": "yuz'hiru"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "from", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u063a\u064e\u064a\u06e1\u0628\u0650\u0647\u0650\u06e6\u0653", "word_translation": "His unseen", "word_transliteration": "ghaybihi"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u064b\u0627", "word_translation": "(to) anyone", "word_transliteration": "ahadan"}]}