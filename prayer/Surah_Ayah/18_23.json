{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0642\u064f\u0648\u0644\u064e\u0646\u0651\u064e \u0644\u0650\u0634\u064e\u0627\u0652\u064a\u06e1\u0621\u064d \u0625\u0650\u0646\u0651\u0650\u064a \u0641\u064e\u0627\u0639\u0650\u0644\u065e \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u063a\u064e\u062f\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u064f\u0648\u0644\u064e\u0646\u0651\u064e", "word_translation": "say", "word_transliteration": "taqulanna"}, {"word_arabic_uthmani": "\u0644\u0650\u0634\u064e\u0627\u0652\u064a\u06e1\u0621\u064d", "word_translation": "of anything", "word_transliteration": "lishay'in"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0641\u064e\u0627\u0639\u0650\u0644\u065e", "word_translation": "will do", "word_transliteration": "fa'ilun"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "that", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u063a\u064e\u062f\u064b\u0627", "word_translation": "tomorrow", "word_transliteration": "ghadan"}]}