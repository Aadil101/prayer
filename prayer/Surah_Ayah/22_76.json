{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e \u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1 \u0648\u064e\u0645\u064e\u0627 \u062e\u064e\u0644\u06e1\u0641\u064e\u0647\u064f\u0645\u06e1\u06da \u0648\u064e\u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f \u0671\u0644\u06e1\u0623\u064f\u0645\u064f\u0648\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "He knows", "word_transliteration": "ya'lamu"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e", "word_translation": "(is) before them", "word_transliteration": "bayna"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "(is) before them", "word_transliteration": "aydihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0641\u064e\u0647\u064f\u0645\u06e1\u06da", "word_translation": "(is) after them", "word_transliteration": "khalfahum"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0644\u064e\u0649", "word_translation": "And to", "word_transliteration": "wa-ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f", "word_translation": "return", "word_transliteration": "tur'ja'u"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064f\u0645\u064f\u0648\u0631\u064f", "word_translation": "all the matters", "word_transliteration": "al-umuru"}]}