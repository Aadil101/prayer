{"ayahs": {"ayah_uthmani": "\u06de\u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0645\u0650\u0646 \u0634\u0650\u064a\u0639\u064e\u062a\u0650\u0647\u0650\u06e6 \u0644\u064e\u0625\u0650\u0628\u06e1\u0631\u064e\u0670\u0647\u0650\u064a\u0645\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "And indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "among", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0634\u0650\u064a\u0639\u064e\u062a\u0650\u0647\u0650\u06e6", "word_translation": "his kind", "word_transliteration": "shi'atihi"}, {"word_arabic_uthmani": "\u0644\u064e\u0625\u0650\u0628\u06e1\u0631\u064e\u0670\u0647\u0650\u064a\u0645\u064e", "word_translation": "(was) surely Ibrahim", "word_transliteration": "la-ib'rahima"}]}