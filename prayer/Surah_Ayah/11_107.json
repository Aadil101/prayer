{"ayahs": {"ayah_uthmani": "\u062e\u064e\u0670\u0644\u0650\u062f\u0650\u064a\u0646\u064e \u0641\u0650\u064a\u0647\u064e\u0627 \u0645\u064e\u0627 \u062f\u064e\u0627\u0645\u064e\u062a\u0650 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u064f \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064f \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064e\u0627 \u0634\u064e\u0627\u0653\u0621\u064e \u0631\u064e\u0628\u0651\u064f\u0643\u064e\u06da \u0625\u0650\u0646\u0651\u064e \u0631\u064e\u0628\u0651\u064e\u0643\u064e \u0641\u064e\u0639\u0651\u064e\u0627\u0644\u065e \u0644\u0651\u0650\u0645\u064e\u0627 \u064a\u064f\u0631\u0650\u064a\u062f\u064f"}, "words": [{"word_arabic_uthmani": "\u062e\u064e\u0670\u0644\u0650\u062f\u0650\u064a\u0646\u064e", "word_translation": "(Will be) abiding", "word_transliteration": "khalidina"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "as long as remain", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062f\u064e\u0627\u0645\u064e\u062a\u0650", "word_translation": "as long as remain", "word_transliteration": "damati"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u064f", "word_translation": "the heavens", "word_transliteration": "al-samawatu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064f", "word_translation": "and the earth", "word_transliteration": "wal-ardu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what your Lord wills", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0634\u064e\u0627\u0653\u0621\u064e", "word_translation": "what your Lord wills", "word_transliteration": "shaa"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064e\u06da", "word_translation": "what your Lord wills", "word_transliteration": "rabbuka"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0643\u064e", "word_translation": "your Lord", "word_transliteration": "rabbaka"}, {"word_arabic_uthmani": "\u0641\u064e\u0639\u0651\u064e\u0627\u0644\u065e", "word_translation": "(is) All-Accomplisher", "word_transliteration": "fa'alun"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "lima"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u0650\u064a\u062f\u064f", "word_translation": "He intends", "word_transliteration": "yuridu"}]}