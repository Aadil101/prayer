{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0631\u064e\u062c\u064f\u0644\u064e\u0627\u0646\u0650 \u0645\u0650\u0646\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0646\u064e \u0623\u064e\u0646\u06e1\u0639\u064e\u0645\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064e\u0627 \u0671\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f \u0671\u0644\u06e1\u0628\u064e\u0627\u0628\u064e \u0641\u064e\u0625\u0650\u0630\u064e\u0627 \u062f\u064e\u062e\u064e\u0644\u06e1\u062a\u064f\u0645\u064f\u0648\u0647\u064f \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0643\u064f\u0645\u06e1 \u063a\u064e\u0670\u0644\u0650\u0628\u064f\u0648\u0646\u064e\u06da \u0648\u064e\u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0641\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645 \u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0631\u064e\u062c\u064f\u0644\u064e\u0627\u0646\u0650", "word_translation": "two men", "word_transliteration": "rajulani"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u064e\u0627\u0641\u064f\u0648\u0646\u064e", "word_translation": "feared (Allah)", "word_transliteration": "yakhafuna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u06e1\u0639\u064e\u0645\u064e", "word_translation": "(had) favored", "word_transliteration": "an'ama"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064e\u0627", "word_translation": "[on] both of them", "word_transliteration": "alayhima"}, {"word_arabic_uthmani": "\u0671\u062f\u06e1\u062e\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "Enter", "word_transliteration": "ud'khulu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f", "word_translation": "upon them", "word_transliteration": "alayhimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u0627\u0628\u064e", "word_translation": "(through) the gate", "word_transliteration": "al-baba"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0630\u064e\u0627", "word_translation": "then when", "word_transliteration": "fa-idha"}, {"word_arabic_uthmani": "\u062f\u064e\u062e\u064e\u0644\u06e1\u062a\u064f\u0645\u064f\u0648\u0647\u064f", "word_translation": "you have entered it", "word_transliteration": "dakhaltumuhu"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0643\u064f\u0645\u06e1", "word_translation": "then indeed, you (will be)", "word_transliteration": "fa-innakum"}, {"word_arabic_uthmani": "\u063a\u064e\u0670\u0644\u0650\u0628\u064f\u0648\u0646\u064e\u06da", "word_translation": "victorious", "word_transliteration": "ghalibuna"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0644\u064e\u0649", "word_translation": "And upon", "word_transliteration": "wa'ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0648\u064e\u0643\u0651\u064e\u0644\u064f\u0648\u0653\u0627\u0652", "word_translation": "then put your trust", "word_transliteration": "fatawakkalu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f\u0645", "word_translation": "you are", "word_transliteration": "kuntum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0624\u06e1\u0645\u0650\u0646\u0650\u064a\u0646\u064e", "word_translation": "believers", "word_transliteration": "mu'minina"}]}