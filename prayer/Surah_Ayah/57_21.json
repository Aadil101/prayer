{"ayahs": {"ayah_uthmani": "\u0633\u064e\u0627\u0628\u0650\u0642\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0644\u064e\u0649\u0670 \u0645\u064e\u063a\u06e1\u0641\u0650\u0631\u064e\u0629\u0656 \u0645\u0651\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064f\u0645\u06e1 \u0648\u064e\u062c\u064e\u0646\u0651\u064e\u0629\u064d \u0639\u064e\u0631\u06e1\u0636\u064f\u0647\u064e\u0627 \u0643\u064e\u0639\u064e\u0631\u06e1\u0636\u0650 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0623\u064f\u0639\u0650\u062f\u0651\u064e\u062a\u06e1 \u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0631\u064f\u0633\u064f\u0644\u0650\u0647\u0650\u06e6\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u0641\u064e\u0636\u06e1\u0644\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u064a\u064f\u0624\u06e1\u062a\u0650\u064a\u0647\u0650 \u0645\u064e\u0646 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0630\u064f\u0648 \u0671\u0644\u06e1\u0641\u064e\u0636\u06e1\u0644\u0650 \u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0633\u064e\u0627\u0628\u0650\u0642\u064f\u0648\u0653\u0627\u0652", "word_translation": "Race", "word_transliteration": "sabiqu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0645\u064e\u063a\u06e1\u0641\u0650\u0631\u064e\u0629\u0656", "word_translation": "(the) forgiveness", "word_transliteration": "maghfiratin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650\u0643\u064f\u0645\u06e1", "word_translation": "your Lord", "word_transliteration": "rabbikum"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0646\u0651\u064e\u0629\u064d", "word_translation": "and a Garden", "word_transliteration": "wajannatin"}, {"word_arabic_uthmani": "\u0639\u064e\u0631\u06e1\u0636\u064f\u0647\u064e\u0627", "word_translation": "its width", "word_transliteration": "arduha"}, {"word_arabic_uthmani": "\u0643\u064e\u0639\u064e\u0631\u06e1\u0636\u0650", "word_translation": "(is) like (the) width", "word_transliteration": "ka'ardi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650", "word_translation": "(of) the heaven", "word_transliteration": "al-samai"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0623\u064f\u0639\u0650\u062f\u0651\u064e\u062a\u06e1", "word_translation": "prepared", "word_transliteration": "u'iddat"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "for those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believe", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "in Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064f\u0633\u064f\u0644\u0650\u0647\u0650\u06e6\u06da", "word_translation": "and His Messengers", "word_transliteration": "warusulihi"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0641\u064e\u0636\u06e1\u0644\u064f", "word_translation": "(is the) Bounty", "word_transliteration": "fadlu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u062a\u0650\u064a\u0647\u0650", "word_translation": "He gives", "word_transliteration": "yu'tihi"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(to) whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u06da", "word_translation": "He wills", "word_transliteration": "yashau"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0630\u064f\u0648", "word_translation": "(is) the Possessor of Bounty", "word_transliteration": "dhu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064e\u0636\u06e1\u0644\u0650", "word_translation": "(is) the Possessor of Bounty", "word_transliteration": "al-fadli"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0638\u0650\u064a\u0645\u0650", "word_translation": "the Great", "word_transliteration": "al-'azimi"}]}