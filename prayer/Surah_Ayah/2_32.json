{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0643\u064e \u0644\u064e\u0627 \u0639\u0650\u0644\u06e1\u0645\u064e \u0644\u064e\u0646\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064e\u0627 \u0639\u064e\u0644\u0651\u064e\u0645\u06e1\u062a\u064e\u0646\u064e\u0627\u0653\u06d6 \u0625\u0650\u0646\u0651\u064e\u0643\u064e \u0623\u064e\u0646\u062a\u064e \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f \u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0633\u064f\u0628\u06e1\u062d\u064e\u0670\u0646\u064e\u0643\u064e", "word_translation": "Glory be to You", "word_transliteration": "sub'hanaka"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "No", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u064e", "word_translation": "knowledge", "word_transliteration": "il'ma"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u0627\u0653", "word_translation": "(is) for us", "word_transliteration": "lana"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0651\u064e\u0645\u06e1\u062a\u064e\u0646\u064e\u0627\u0653\u06d6", "word_translation": "You have taught us", "word_transliteration": "allamtana"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "Indeed You", "word_transliteration": "innaka"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "You", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "(are) the All-Knowing", "word_transliteration": "al-'alimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0643\u0650\u064a\u0645\u064f", "word_translation": "the All-Wise", "word_transliteration": "al-hakimu"}]}