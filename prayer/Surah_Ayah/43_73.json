{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u064e\u0627 \u0641\u064e\u0670\u0643\u0650\u0647\u064e\u0629\u065e \u0643\u064e\u062b\u0650\u064a\u0631\u064e\u0629\u065e \u0645\u0651\u0650\u0646\u06e1\u0647\u064e\u0627 \u062a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "For you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0641\u064e\u0670\u0643\u0650\u0647\u064e\u0629\u065e", "word_translation": "(are) fruits", "word_transliteration": "fakihatun"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u064e\u0629\u065e", "word_translation": "abundant", "word_transliteration": "kathiratun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064e\u0627", "word_translation": "from it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0646\u064e", "word_translation": "you will eat", "word_transliteration": "takuluna"}]}