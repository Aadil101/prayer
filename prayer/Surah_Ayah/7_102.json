{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0648\u064e\u062c\u064e\u062f\u06e1\u0646\u064e\u0627 \u0644\u0650\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u0650\u0647\u0650\u0645 \u0645\u0651\u0650\u0646\u06e1 \u0639\u064e\u0647\u06e1\u062f\u0656\u06d6 \u0648\u064e\u0625\u0650\u0646 \u0648\u064e\u062c\u064e\u062f\u06e1\u0646\u064e\u0627\u0653 \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0641\u064e\u0670\u0633\u0650\u0642\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u062f\u06e1\u0646\u064e\u0627", "word_translation": "We found", "word_transliteration": "wajadna"}, {"word_arabic_uthmani": "\u0644\u0650\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u0650\u0647\u0650\u0645", "word_translation": "for most of them", "word_transliteration": "li-aktharihim"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "[of]", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u064e\u0647\u06e1\u062f\u0656\u06d6", "word_translation": "(any) covenant", "word_transliteration": "ahdin"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "But", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u062f\u06e1\u0646\u064e\u0627\u0653", "word_translation": "We found", "word_transliteration": "wajadna"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1", "word_translation": "most of them", "word_transliteration": "aktharahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0641\u064e\u0670\u0633\u0650\u0642\u0650\u064a\u0646\u064e", "word_translation": "certainly defiantly disobedient", "word_transliteration": "lafasiqina"}]}