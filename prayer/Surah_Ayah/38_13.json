{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062b\u064e\u0645\u064f\u0648\u062f\u064f \u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064f \u0644\u064f\u0648\u0637\u0656 \u0648\u064e\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064f \u0644\u06e1\u0640\u0654\u064e\u064a\u06e1\u0643\u064e\u0629\u0650\u06da \u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0671\u0644\u06e1\u0623\u064e\u062d\u06e1\u0632\u064e\u0627\u0628\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062b\u064e\u0645\u064f\u0648\u062f\u064f", "word_translation": "And Thamud", "word_transliteration": "wathamudu"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064f", "word_translation": "and (the) people", "word_transliteration": "waqawmu"}, {"word_arabic_uthmani": "\u0644\u064f\u0648\u0637\u0656", "word_translation": "(of) Lut", "word_transliteration": "lutin"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0635\u06e1\u062d\u064e\u0670\u0628\u064f", "word_translation": "and (the) companions", "word_transliteration": "wa-ashabu"}, {"word_arabic_uthmani": "\u0644\u06e1\u0640\u0654\u064e\u064a\u06e1\u0643\u064e\u0629\u0650\u06da", "word_translation": "(of) the wood", "word_transliteration": "al'aykati"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u062d\u06e1\u0632\u064e\u0627\u0628\u064f", "word_translation": "(were) the companies", "word_transliteration": "al-ahzabu"}]}