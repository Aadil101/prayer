{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0644\u064e\u0626\u0650\u0646\u06e1 \u0623\u064e\u0643\u064e\u0644\u064e\u0647\u064f \u0671\u0644\u0630\u0651\u0650\u0626\u06e1\u0628\u064f \u0648\u064e\u0646\u064e\u062d\u06e1\u0646\u064f \u0639\u064f\u0635\u06e1\u0628\u064e\u0629\u064c \u0625\u0650\u0646\u0651\u064e\u0627\u0653 \u0625\u0650\u0630\u0657\u0627 \u0644\u0651\u064e\u062e\u064e\u0670\u0633\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0644\u064e\u0626\u0650\u0646\u06e1", "word_translation": "If", "word_transliteration": "la-in"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u064e\u0644\u064e\u0647\u064f", "word_translation": "eats him", "word_transliteration": "akalahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0630\u0651\u0650\u0626\u06e1\u0628\u064f", "word_translation": "the wolf", "word_transliteration": "al-dhi'bu"}, {"word_arabic_uthmani": "\u0648\u064e\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "while we", "word_transliteration": "wanahnu"}, {"word_arabic_uthmani": "\u0639\u064f\u0635\u06e1\u0628\u064e\u0629\u064c", "word_translation": "(are) a group", "word_transliteration": "us'batun"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653", "word_translation": "indeed we", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u0657\u0627", "word_translation": "then", "word_transliteration": "idhan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u062e\u064e\u0670\u0633\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "surely (would be) losers", "word_transliteration": "lakhasiruna"}]}