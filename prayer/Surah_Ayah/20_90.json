{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0642\u064e\u0627\u0644\u064e \u0644\u064e\u0647\u064f\u0645\u06e1 \u0647\u064e\u0670\u0631\u064f\u0648\u0646\u064f \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u064f \u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0641\u064f\u062a\u0650\u0646\u062a\u064f\u0645 \u0628\u0650\u0647\u0650\u06e6\u06d6 \u0648\u064e\u0625\u0650\u0646\u0651\u064e \u0631\u064e\u0628\u0651\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f \u0641\u064e\u0671\u062a\u0651\u064e\u0628\u0650\u0639\u064f\u0648\u0646\u0650\u064a \u0648\u064e\u0623\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0645\u06e1\u0631\u0650\u064a"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And verily", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "(had) said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "to them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0631\u064f\u0648\u0646\u064f", "word_translation": "Harun", "word_transliteration": "harunu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f", "word_translation": "before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "O my people", "word_transliteration": "yaqawmi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0641\u064f\u062a\u0650\u0646\u062a\u064f\u0645", "word_translation": "you are being tested", "word_transliteration": "futintum"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6\u06d6", "word_translation": "by it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "and indeed", "word_transliteration": "wa-inna"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0643\u064f\u0645\u064f", "word_translation": "your Lord", "word_transliteration": "rabbakumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u062d\u06e1\u0645\u064e\u0670\u0646\u064f", "word_translation": "(is) the Most Gracious", "word_transliteration": "al-rahmanu"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u062a\u0651\u064e\u0628\u0650\u0639\u064f\u0648\u0646\u0650\u064a", "word_translation": "so follow me", "word_transliteration": "fa-ittabi'uni"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0653\u0627\u0652", "word_translation": "and obey", "word_transliteration": "wa-ati'u"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0631\u0650\u064a", "word_translation": "my order", "word_transliteration": "amri"}]}