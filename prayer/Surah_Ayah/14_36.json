{"ayahs": {"ayah_uthmani": "\u0631\u064e\u0628\u0651\u0650 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0646\u0651\u064e \u0623\u064e\u0636\u06e1\u0644\u064e\u0644\u06e1\u0646\u064e \u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650\u06d6 \u0641\u064e\u0645\u064e\u0646 \u062a\u064e\u0628\u0650\u0639\u064e\u0646\u0650\u064a \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0645\u0650\u0646\u0651\u0650\u064a\u06d6 \u0648\u064e\u0645\u064e\u0646\u06e1 \u0639\u064e\u0635\u064e\u0627\u0646\u0650\u064a \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0643\u064e \u063a\u064e\u0641\u064f\u0648\u0631\u065e \u0631\u0651\u064e\u062d\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650", "word_translation": "My Lord!", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0646\u0651\u064e", "word_translation": "Indeed they", "word_transliteration": "innahunna"}, {"word_arabic_uthmani": "\u0623\u064e\u0636\u06e1\u0644\u064e\u0644\u06e1\u0646\u064e", "word_translation": "have led astray", "word_transliteration": "adlalna"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627", "word_translation": "many", "word_transliteration": "kathiran"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "among", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0633\u0650\u06d6", "word_translation": "the mankind", "word_transliteration": "al-nasi"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0646", "word_translation": "So whoever", "word_transliteration": "faman"}, {"word_arabic_uthmani": "\u062a\u064e\u0628\u0650\u0639\u064e\u0646\u0650\u064a", "word_translation": "follows me", "word_transliteration": "tabi'ani"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "then indeed he", "word_transliteration": "fa-innahu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0651\u0650\u064a\u06d6", "word_translation": "(is) of me", "word_transliteration": "minni"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646\u06e1", "word_translation": "and whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u0639\u064e\u0635\u064e\u0627\u0646\u0650\u064a", "word_translation": "disobeys me", "word_transliteration": "asani"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "then indeed You", "word_transliteration": "fa-innaka"}, {"word_arabic_uthmani": "\u063a\u064e\u0641\u064f\u0648\u0631\u065e", "word_translation": "(are) Oft-Forgiving", "word_transliteration": "ghafurun"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u065e", "word_translation": "Most Merciful", "word_transliteration": "rahimun"}]}