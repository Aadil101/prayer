{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062c\u064e\u062f\u062a\u0651\u064f\u0647\u064e\u0627 \u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064e\u0627 \u064a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0646\u064e \u0644\u0650\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u0650 \u0645\u0650\u0646 \u062f\u064f\u0648\u0646\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0632\u064e\u064a\u0651\u064e\u0646\u064e \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064f \u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064e\u0647\u064f\u0645\u06e1 \u0641\u064e\u0635\u064e\u062f\u0651\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0646\u0650 \u0671\u0644\u0633\u0651\u064e\u0628\u0650\u064a\u0644\u0650 \u0641\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0647\u06e1\u062a\u064e\u062f\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u062f\u062a\u0651\u064f\u0647\u064e\u0627", "word_translation": "And I found her", "word_transliteration": "wajadttuha"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0648\u06e1\u0645\u064e\u0647\u064e\u0627", "word_translation": "and her people", "word_transliteration": "waqawmaha"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062c\u064f\u062f\u064f\u0648\u0646\u064e", "word_translation": "prostrating", "word_transliteration": "yasjuduna"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0634\u0651\u064e\u0645\u06e1\u0633\u0650", "word_translation": "to the sun", "word_transliteration": "lilshamsi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "instead of Allah", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062f\u064f\u0648\u0646\u0650", "word_translation": "instead of Allah", "word_transliteration": "duni"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "instead of Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0632\u064e\u064a\u0651\u064e\u0646\u064e", "word_translation": "and has made fair-seeming", "word_transliteration": "wazayyana"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "to them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u06e1\u0637\u064e\u0670\u0646\u064f", "word_translation": "the Shaitaan", "word_transliteration": "al-shaytanu"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0645\u064e\u0670\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "their deeds", "word_transliteration": "a'malahum"}, {"word_arabic_uthmani": "\u0641\u064e\u0635\u064e\u062f\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "and averted them", "word_transliteration": "fasaddahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u0650", "word_translation": "from", "word_transliteration": "ani"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0628\u0650\u064a\u0644\u0650", "word_translation": "the Way", "word_transliteration": "al-sabili"}, {"word_arabic_uthmani": "\u0641\u064e\u0647\u064f\u0645\u06e1", "word_translation": "so they", "word_transliteration": "fahum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(are) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062a\u064e\u062f\u064f\u0648\u0646\u064e", "word_translation": "guided", "word_transliteration": "yahtaduna"}]}