{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u064a\u06e1\u0633\u064e\u062a\u0650 \u0671\u0644\u062a\u0651\u064e\u0648\u06e1\u0628\u064e\u0629\u064f \u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e \u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0640\u0654\u064e\u0627\u062a\u0650 \u062d\u064e\u062a\u0651\u064e\u0649\u0670\u0653 \u0625\u0650\u0630\u064e\u0627 \u062d\u064e\u0636\u064e\u0631\u064e \u0623\u064e\u062d\u064e\u062f\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064f \u0642\u064e\u0627\u0644\u064e \u0625\u0650\u0646\u0651\u0650\u064a \u062a\u064f\u0628\u06e1\u062a\u064f \u0671\u0644\u06e1\u0640\u0654\u064e\u0670\u0646\u064e \u0648\u064e\u0644\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u0645\u064f\u0648\u062a\u064f\u0648\u0646\u064e \u0648\u064e\u0647\u064f\u0645\u06e1 \u0643\u064f\u0641\u0651\u064e\u0627\u0631\u064c\u06da \u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0623\u064e\u0639\u06e1\u062a\u064e\u062f\u06e1\u0646\u064e\u0627 \u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064b\u0627 \u0623\u064e\u0644\u0650\u064a\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u064a\u06e1\u0633\u064e\u062a\u0650", "word_translation": "And not", "word_transliteration": "walaysati"}, {"word_arabic_uthmani": "\u0671\u0644\u062a\u0651\u064e\u0648\u06e1\u0628\u064e\u0629\u064f", "word_translation": "(is) the acceptance of repentance", "word_transliteration": "al-tawbatu"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "for those who", "word_transliteration": "lilladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "do", "word_transliteration": "ya'maluna"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u064a\u0651\u0650\u0640\u0654\u064e\u0627\u062a\u0650", "word_translation": "the evil deeds", "word_transliteration": "al-sayiati"}, {"word_arabic_uthmani": "\u062d\u064e\u062a\u0651\u064e\u0649\u0670\u0653", "word_translation": "until", "word_transliteration": "hatta"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u062d\u064e\u0636\u064e\u0631\u064e", "word_translation": "approaches", "word_transliteration": "hadara"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u064e\u0647\u064f\u0645\u064f", "word_translation": "one of them", "word_transliteration": "ahadahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064f", "word_translation": "[the] death", "word_transliteration": "al-mawtu"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "he says", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u062a\u064f\u0628\u06e1\u062a\u064f", "word_translation": "repent", "word_transliteration": "tub'tu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0640\u0654\u064e\u0670\u0646\u064e", "word_translation": "now", "word_transliteration": "al-ana"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u064f\u0648\u062a\u064f\u0648\u0646\u064e", "word_translation": "die", "word_transliteration": "yamutuna"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0643\u064f\u0641\u0651\u064e\u0627\u0631\u064c\u06da", "word_translation": "(are) disbelievers", "word_transliteration": "kuffarun"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "Those", "word_transliteration": "ulaika"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u062a\u064e\u062f\u06e1\u0646\u064e\u0627", "word_translation": "We have prepared", "word_transliteration": "a'tadna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064b\u0627", "word_translation": "a punishment", "word_transliteration": "adhaban"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0650\u064a\u0645\u0657\u0627", "word_translation": "painful", "word_transliteration": "aliman"}]}