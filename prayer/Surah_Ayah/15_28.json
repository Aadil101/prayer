{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1 \u0642\u064e\u0627\u0644\u064e \u0631\u064e\u0628\u0651\u064f\u0643\u064e \u0644\u0650\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u0650 \u0625\u0650\u0646\u0651\u0650\u064a \u062e\u064e\u0670\u0644\u0650\u0642\u064f\u06e2 \u0628\u064e\u0634\u064e\u0631\u0657\u0627 \u0645\u0651\u0650\u0646 \u0635\u064e\u0644\u06e1\u0635\u064e\u0670\u0644\u0656 \u0645\u0651\u0650\u0646\u06e1 \u062d\u064e\u0645\u064e\u0625\u0656 \u0645\u0651\u064e\u0633\u06e1\u0646\u064f\u0648\u0646\u0656"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1", "word_translation": "And when", "word_transliteration": "wa-idh"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "your Lord said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064f\u0643\u064e", "word_translation": "your Lord said", "word_transliteration": "rabbuka"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u0650", "word_translation": "to the Angels", "word_transliteration": "lil'malaikati"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed, I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u062e\u064e\u0670\u0644\u0650\u0642\u064f\u06e2", "word_translation": "(will) create", "word_transliteration": "khaliqun"}, {"word_arabic_uthmani": "\u0628\u064e\u0634\u064e\u0631\u0657\u0627", "word_translation": "a human being", "word_transliteration": "basharan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "(out) of", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0635\u064e\u0644\u06e1\u0635\u064e\u0670\u0644\u0656", "word_translation": "clay", "word_transliteration": "salsalin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration