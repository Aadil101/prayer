{"ayahs": {"ayah_uthmani": "\u0641\u064e\u062e\u064e\u0631\u064e\u062c\u064e \u0639\u064e\u0644\u064e\u0649\u0670 \u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6 \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u0650\u062d\u06e1\u0631\u064e\u0627\u0628\u0650 \u0641\u064e\u0623\u064e\u0648\u06e1\u062d\u064e\u0649\u0670\u0653 \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0623\u064e\u0646 \u0633\u064e\u0628\u0651\u0650\u062d\u064f\u0648\u0627\u0652 \u0628\u064f\u0643\u06e1\u0631\u064e\u0629\u0657 \u0648\u064e\u0639\u064e\u0634\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u062e\u064e\u0631\u064e\u062c\u064e", "word_translation": "Then he came out", "word_transliteration": "fakharaja"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6", "word_translation": "his people", "word_transliteration": "qawmihi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u0650\u062d\u06e1\u0631\u064e\u0627\u0628\u0650", "word_translation": "the prayer chamber", "word_transliteration": "al-mih'rabi"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0648\u06e1\u062d\u064e\u0649\u0670\u0653", "word_translation": "and he signaled", "word_transliteration": "fa-awha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "to them", "word_transliteration": "ilayhim"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "to", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0651\u0650\u062d\u064f\u0648\u0627\u0652", "word_translation": "glorify (Allah)", "word_transliteration": "sabbihu"}, {"word_arabic_uthmani": "\u0628\u064f\u0643\u06e1\u0631\u064e\u0629\u0657", "word_translation": "(in) the morning", "word_transliteration": "buk'ratan"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0634\u0650\u064a\u0651\u0657\u0627", "word_translation": "and (in) the evening", "word_transliteration": "wa'ashiyyan"}]}