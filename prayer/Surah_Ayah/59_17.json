{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0643\u064e\u0627\u0646\u064e \u0639\u064e\u0670\u0642\u0650\u0628\u064e\u062a\u064e\u0647\u064f\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u064e\u0627 \u0641\u0650\u064a \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650 \u062e\u064e\u0670\u0644\u0650\u062f\u064e\u064a\u06e1\u0646\u0650 \u0641\u0650\u064a\u0647\u064e\u0627\u06da \u0648\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u062c\u064e\u0632\u064e\u0640\u0670\u0653\u0624\u064f\u0627\u0652 \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "So will be", "word_transliteration": "fakana"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u062a\u064e\u0647\u064f\u0645\u064e\u0627\u0653", "word_translation": "(the) end of both of them", "word_transliteration": "aqibatahuma"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645\u064e\u0627", "word_translation": "that they will", "word_transliteration": "annahuma"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(be) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u0650", "word_translation": "the Fire", "word_transliteration": "al-nari"}, {"word_arabic_uthmani": "\u062e\u064e\u0670\u0644\u0650\u062f\u064e\u064a\u06e1\u0646\u0650", "word_translation": "abiding forever", "word_transliteration": "khalidayni"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627\u06da", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0648\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "And that", "word_transliteration": "wadhalika"}, {"word_arabic_uthmani": "\u062c\u064e\u0632\u064e\u0640\u0670\u0653\u0624\u064f\u0627\u0652", "word_translation": "(is the) recompense", "word_transliteration": "jazau"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the wrongdoers", "word_transliteration": "al-zalimina"}]}