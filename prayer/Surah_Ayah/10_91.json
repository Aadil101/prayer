{"ayahs": {"ayah_uthmani": "\u0621\u064e\u0627\u0653\u0644\u06e1\u0640\u0654\u064e\u0670\u0646\u064e \u0648\u064e\u0642\u064e\u062f\u06e1 \u0639\u064e\u0635\u064e\u064a\u06e1\u062a\u064e \u0642\u064e\u0628\u06e1\u0644\u064f \u0648\u064e\u0643\u064f\u0646\u062a\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0621\u064e\u0627\u0653\u0644\u06e1\u0640\u0654\u064e\u0670\u0646\u064e", "word_translation": "Now", "word_transliteration": "al'ana"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And verily", "word_transliteration": "waqad"}, {"word_arabic_uthmani": "\u0639\u064e\u0635\u064e\u064a\u06e1\u062a\u064e", "word_translation": "you (had) disobeyed", "word_transliteration": "asayta"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f", "word_translation": "before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064f\u0646\u062a\u064e", "word_translation": "and you were", "word_transliteration": "wakunta"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0633\u0650\u062f\u0650\u064a\u0646\u064e", "word_translation": "the corrupters", "word_transliteration": "al-muf'sidina"}]}