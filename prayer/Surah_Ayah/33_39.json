{"ayahs": {"ayah_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064f\u0628\u064e\u0644\u0651\u0650\u063a\u064f\u0648\u0646\u064e \u0631\u0650\u0633\u064e\u0670\u0644\u064e\u0670\u062a\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u064a\u064e\u062e\u06e1\u0634\u064e\u0648\u06e1\u0646\u064e\u0647\u064f\u06e5 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u062e\u06e1\u0634\u064e\u0648\u06e1\u0646\u064e \u0623\u064e\u062d\u064e\u062f\u064b\u0627 \u0625\u0650\u0644\u0651\u064e\u0627 \u0671\u0644\u0644\u0651\u064e\u0647\u064e\u06d7 \u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u062d\u064e\u0633\u0650\u064a\u0628\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "Those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064f\u0628\u064e\u0644\u0651\u0650\u063a\u064f\u0648\u0646\u064e", "word_translation": "convey", "word_transliteration": "yuballighuna"}, {"word_arabic_uthmani": "\u0631\u0650\u0633\u064e\u0670\u0644\u064e\u0670\u062a\u0650", "word_translation": "(the) Messages", "word_transliteration": "risalati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062e\u06e1\u0634\u064e\u0648\u06e1\u0646\u064e\u0647\u064f\u06e5", "word_translation": "and fear Him", "word_transliteration": "wayakhshawnahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0634\u064e\u0648\u06e1\u0646\u064e", "word_translation": "fear", "word_transliteration": "yakhshawna"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u062f\u064b\u0627", "word_translation": "anyone", "word_transliteration": "ahadan"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e\u06d7", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "And sufficient is Allah", "word_transliteration": "wakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "And sufficient is Allah", "word_transliteration": "bil-lahi"}, {"word_arabic_uthmani": "\u062d\u064e\u0633\u0650\u064a\u0628\u0657\u0627", "word_translation": "(as) a Reckoner", "word_transliteration": "hasiban"}]}