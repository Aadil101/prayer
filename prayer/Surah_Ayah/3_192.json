{"ayahs": {"ayah_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e\u0643\u064e \u0645\u064e\u0646 \u062a\u064f\u062f\u06e1\u062e\u0650\u0644\u0650 \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064e \u0641\u064e\u0642\u064e\u062f\u06e1 \u0623\u064e\u062e\u06e1\u0632\u064e\u064a\u06e1\u062a\u064e\u0647\u064f\u06e5\u06d6 \u0648\u064e\u0645\u064e\u0627 \u0644\u0650\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e \u0645\u0650\u0646\u06e1 \u0623\u064e\u0646\u0635\u064e\u0627\u0631\u0656"}, "words": [{"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0646\u064e\u0627\u0653", "word_translation": "Our Lord", "word_transliteration": "rabbana"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "indeed [You]", "word_transliteration": "innaka"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "whom", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u062a\u064f\u062f\u06e1\u062e\u0650\u0644\u0650", "word_translation": "You admit", "word_transliteration": "tud'khili"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064e", "word_translation": "(to) the Fire", "word_transliteration": "al-nara"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u062f\u06e1", "word_translation": "then surely", "word_transliteration": "faqad"}, {"word_arabic_uthmani": "\u0623\u064e\u062e\u06e1\u0632\u064e\u064a\u06e1\u062a\u064e\u0647\u064f\u06e5\u06d6", "word_translation": "You (have) disgraced him", "word_transliteration": "akhzaytahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "for the wrongdoers", "word_transliteration": "lilzzalimina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "(are) any", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0635\u064e\u0627\u0631\u0656", "word_translation": "helpers", "word_transliteration": "ansarin"}]}