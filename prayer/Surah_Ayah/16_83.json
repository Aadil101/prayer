{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0639\u06e1\u0631\u0650\u0641\u064f\u0648\u0646\u064e \u0646\u0650\u0639\u06e1\u0645\u064e\u062a\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u062b\u064f\u0645\u0651\u064e \u064a\u064f\u0646\u0643\u0650\u0631\u064f\u0648\u0646\u064e\u0647\u064e\u0627 \u0648\u064e\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0631\u0650\u0641\u064f\u0648\u0646\u064e", "word_translation": "They recognize", "word_transliteration": "ya'rifuna"}, {"word_arabic_uthmani": "\u0646\u0650\u0639\u06e1\u0645\u064e\u062a\u064e", "word_translation": "(the) Favor", "word_transliteration": "ni'mata"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u064a\u064f\u0646\u0643\u0650\u0631\u064f\u0648\u0646\u064e\u0647\u064e\u0627", "word_translation": "they deny it", "word_transliteration": "yunkirunaha"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645\u064f", "word_translation": "And most of them", "word_transliteration": "wa-aktharuhumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0670\u0641\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "(are) the disbelievers", "word_transliteration": "al-kafiruna"}]}