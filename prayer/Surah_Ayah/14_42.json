{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u063a\u064e\u0670\u0641\u0650\u0644\u064b\u0627 \u0639\u064e\u0645\u0651\u064e\u0627 \u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f \u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e\u06da \u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u064a\u064f\u0624\u064e\u062e\u0651\u0650\u0631\u064f\u0647\u064f\u0645\u06e1 \u0644\u0650\u064a\u064e\u0648\u06e1\u0645\u0656 \u062a\u064e\u0634\u06e1\u062e\u064e\u0635\u064f \u0641\u0650\u064a\u0647\u0650 \u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e", "word_translation": "think", "word_transliteration": "tahsabanna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "(that) Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u063a\u064e\u0670\u0641\u0650\u0644\u064b\u0627", "word_translation": "(is) unaware", "word_transliteration": "ghafilan"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "amma"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f", "word_translation": "do", "word_transliteration": "ya'malu"}, {"word_arabic_uthmani": "\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e\u06da", "word_translation": "the wrongdoers", "word_transliteration": "al-zalimuna"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u064e\u062e\u0651\u0650\u0631\u064f\u0647\u064f\u0645\u06e1", "word_translation": "He gives them respite", "word_transliteration": "yu-akhiruhum"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064e\u0648\u06e1\u0645\u0656", "word_translation": "to a Day", "word_transliteration": "liyawmin"}, {"word_arabic_uthmani": "\u062a\u064e\u0634\u06e1\u062e\u064e\u0635\u064f", "word_translation": "will stare", "word_transliteration": "tashkhasu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u0650", "word_translation": "in it", "word_transliteration": "fihi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0628\u06e1\u0635\u064e\u0670\u0631\u064f", "word_translation": "the eyes", "word_transliteration": "al-absaru"}]}