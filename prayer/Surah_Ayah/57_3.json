{"ayahs": {"ayah_uthmani": "\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0623\u064e\u0648\u0651\u064e\u0644\u064f \u0648\u064e\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064f \u0648\u064e\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0647\u0650\u0631\u064f \u0648\u064e\u0671\u0644\u06e1\u0628\u064e\u0627\u0637\u0650\u0646\u064f\u06d6 \u0648\u064e\u0647\u064f\u0648\u064e \u0628\u0650\u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u064d \u0639\u064e\u0644\u0650\u064a\u0645\u064c"}, "words": [{"word_arabic_uthmani": "\u0647\u064f\u0648\u064e", "word_translation": "He", "word_transliteration": "huwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0648\u0651\u064e\u0644\u064f", "word_translation": "(is) the First", "word_transliteration": "al-awalu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u0653\u062e\u0650\u0631\u064f", "word_translation": "and the Last", "word_transliteration": "wal-akhiru"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0638\u0651\u064e\u0640\u0670\u0647\u0650\u0631\u064f", "word_translation": "and the Apparent", "word_transliteration": "wal-zahiru"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0628\u064e\u0627\u0637\u0650\u0646\u064f\u06d6", "word_translation": "and the Unapparent", "word_transliteration": "wal-batinu"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064f\u0644\u0651\u0650", "word_translation": "(is) of every", "word_transliteration": "bikulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u064d", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u064c", "word_translation": "All-Knower", "word_transliteration": "alimun"}]}