{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650 \u0623\u064e\u0631\u064e\u0647\u06e1\u0637\u0650\u064a\u0653 \u0623\u064e\u0639\u064e\u0632\u0651\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u06e1\u062a\u064f\u0645\u064f\u0648\u0647\u064f \u0648\u064e\u0631\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1 \u0638\u0650\u0647\u06e1\u0631\u0650\u064a\u0651\u064b\u0627\u06d6 \u0625\u0650\u0646\u0651\u064e \u0631\u064e\u0628\u0651\u0650\u064a \u0628\u0650\u0645\u064e\u0627 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e \u0645\u064f\u062d\u0650\u064a\u0637\u065e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0642\u064e\u0648\u06e1\u0645\u0650", "word_translation": "O my people!", "word_transliteration": "yaqawmi"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u064e\u0647\u06e1\u0637\u0650\u064a\u0653", "word_translation": "Is my family", "word_transliteration": "arahti"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u064e\u0632\u0651\u064f", "word_translation": "mightier", "word_transliteration": "a'azzu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645", "word_translation": "on you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "than", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u06e1\u062a\u064f\u0645\u064f\u0648\u0647\u064f", "word_translation": "And you have taken Him", "word_transliteration": "wa-ittakhadhtumuhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0627\u0653\u0621\u064e\u0643\u064f\u0645\u06e1", "word_translation": "behind your", "word_transliteration": "waraakum"}, {"word_arabic_uthmani": "\u0638\u0650\u0647\u06e1\u0631\u0650\u064a\u0651\u064b\u0627\u06d6", "word_translation": "backs", "word_transliteration": "zih'riyyan"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u064a", "word_translation": "my Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "of what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "you do", "word_transliteration": "ta'maluna"}, {"word_arabic_uthmani": "\u0645\u064f\u062d\u0650\u064a\u0637\u065e", "word_translation": "(is) All-Encompassing", "word_transliteration": "muhitun"}]}