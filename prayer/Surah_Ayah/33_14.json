{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1 \u062f\u064f\u062e\u0650\u0644\u064e\u062a\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645 \u0645\u0651\u0650\u0646\u06e1 \u0623\u064e\u0642\u06e1\u0637\u064e\u0627\u0631\u0650\u0647\u064e\u0627 \u062b\u064f\u0645\u0651\u064e \u0633\u064f\u0626\u0650\u0644\u064f\u0648\u0627\u0652 \u0671\u0644\u06e1\u0641\u0650\u062a\u06e1\u0646\u064e\u0629\u064e \u0644\u064e\u0623\u0653\u062a\u064e\u0648\u06e1\u0647\u064e\u0627 \u0648\u064e\u0645\u064e\u0627 \u062a\u064e\u0644\u064e\u0628\u0651\u064e\u062b\u064f\u0648\u0627\u0652 \u0628\u0650\u0647\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u064a\u064e\u0633\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u062f\u064f\u062e\u0650\u0644\u064e\u062a\u06e1", "word_translation": "had been entered", "word_transliteration": "dukhilat"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645", "word_translation": "upon them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0623\u064e\u0642\u06e1\u0637\u064e\u0627\u0631\u0650\u0647\u064e\u0627", "word_translation": "all its sides", "word_transliteration": "aqtariha"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0633\u064f\u0626\u0650\u0644\u064f\u0648\u0627\u0652", "word_translation": "they had been asked", "word_transliteration": "su-ilu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u0650\u062a\u06e1\u0646\u064e\u0629\u064e", "word_translation": "the treachery", "word_transliteration": "al-fit'nata"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u0653\u062a\u064e\u0648\u06e1\u0647\u064e\u0627", "word_translation": "they (would) have certainly done it", "word_transliteration": "laatawha"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u062a\u064e\u0644\u064e\u0628\u0651\u064e\u062b\u064f\u0648\u0627\u0652", "word_translation": "they (would) have hesitated", "word_transliteration": "talabbathu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u064e\u0627\u0653", "word_translation": "over it", "word_transliteration": "biha"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u0650\u064a\u0631\u0657\u0627", "word_translation": "a little", "word_transliteration": "yasiran"}]}