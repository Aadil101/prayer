{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646 \u0646\u0651\u064e\u0642\u064f\u0648\u0644\u064f \u0625\u0650\u0644\u0651\u064e\u0627 \u0671\u0639\u06e1\u062a\u064e\u0631\u064e\u0649\u0670\u0643\u064e \u0628\u064e\u0639\u06e1\u0636\u064f \u0621\u064e\u0627\u0644\u0650\u0647\u064e\u062a\u0650\u0646\u064e\u0627 \u0628\u0650\u0633\u064f\u0648\u0653\u0621\u0656\u06d7 \u0642\u064e\u0627\u0644\u064e \u0625\u0650\u0646\u0651\u0650\u064a\u0653 \u0623\u064f\u0634\u06e1\u0647\u0650\u062f\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0671\u0634\u06e1\u0647\u064e\u062f\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0646\u0651\u0650\u064a \u0628\u064e\u0631\u0650\u064a\u0653\u0621\u065e \u0645\u0651\u0650\u0645\u0651\u064e\u0627 \u062a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "Not", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "we say", "word_transliteration": "naqulu"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except (that)", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0671\u0639\u06e1\u062a\u064e\u0631\u064e\u0649\u0670\u0643\u064e", "word_translation": "have seized you", "word_transliteration": "i'taraka"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064f", "word_translation": "some", "word_transliteration": "ba'du"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0644\u0650\u0647\u064e\u062a\u0650\u0646\u064e\u0627", "word_translation": "(of) our gods", "word_transliteration": "alihatina"}, {"word_arabic_uthmani": "\u0628\u0650\u0633\u064f\u0648\u0653\u0621\u0656\u06d7", "word_translation": "with evil", "word_transliteration": "bisuin"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a\u0653", "word_translation": "Indeed, I", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0623\u064f\u0634\u06e1\u0647\u0650\u062f\u064f", "word_translation": "[I] call Allah to witness", "word_transliteration": "ush'hidu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "[I] call Allah to witness", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0634\u06e1\u0647\u064e\u062f\u064f\u0648\u0653\u0627\u0652", "word_translation": "and (you) bear witness", "word_transliteration": "wa-ish'hadu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u0650\u064a", "word_translation": "that I am", "word_transliteration": "anni"}, {"word_arabic_uthmani": "\u0628\u064e\u0631\u0650\u064a\u0653\u0621\u065e", "word_translation": "innocent", "word_transliteration": "barion"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u062a\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "you associate", "word_transliteration": "tush'rikuna"}]}