{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u064f\u0647\u064f\u06e5 \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0648\u06e1\u0646\u064e\u0647\u064f\u06e5 \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0647\u064f\u0645 \u0628\u0650\u0647\u0650\u06e6 \u0645\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0633\u064f\u0644\u06e1\u0637\u064e\u0670\u0646\u064f\u0647\u064f\u06e5", "word_translation": "his authority", "word_transliteration": "sul'tanuhu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "(is) over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0648\u06e1\u0646\u064e\u0647\u064f\u06e5", "word_translation": "take him as an ally", "word_transliteration": "yatawallawnahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0647\u064f\u0645", "word_translation": "[they]", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with Him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0645\u064f\u0634\u06e1\u0631\u0650\u0643\u064f\u0648\u0646\u064e", "word_translation": "associate partners", "word_transliteration": "mush'rikuna"}]}