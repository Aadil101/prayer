{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u0644\u064e\u0646\u064e\u062d\u06e1\u0646\u064f \u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f \u0628\u0650\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0647\u064f\u0645\u06e1 \u0623\u064e\u0648\u06e1\u0644\u064e\u0649\u0670 \u0628\u0650\u0647\u064e\u0627 \u0635\u0650\u0644\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0644\u064e\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "surely We", "word_transliteration": "lanahnu"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0644\u064e\u0645\u064f", "word_translation": "know best", "word_transliteration": "a'lamu"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "[of] those who", "word_transliteration": "bi-alladhina"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "[they]", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1\u0644\u064e\u0649\u0670", "word_translation": "(are) most worthy", "word_transliteration": "awla"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "biha"}, {"word_arabic_uthmani": "\u0635\u0650\u0644\u0650\u064a\u0651\u0657\u0627", "word_translation": "(of) being burnt", "word_transliteration": "siliyyan"}]}