{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645 \u0645\u0651\u064e\u0646\u06e1 \u0621\u064e\u0627\u0645\u064e\u0646\u064e \u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645 \u0645\u0651\u064e\u0646 \u0635\u064e\u062f\u0651\u064e \u0639\u064e\u0646\u06e1\u0647\u064f\u06da \u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670 \u0628\u0650\u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e \u0633\u064e\u0639\u0650\u064a\u0631\u064b\u0627"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645", "word_translation": "Then of them", "word_transliteration": "famin'hum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646\u06e1", "word_translation": "(are some) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064e", "word_translation": "believed", "word_transliteration": "amana"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "in him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1\u0647\u064f\u0645", "word_translation": "and of them", "word_transliteration": "wamin'hum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0646", "word_translation": "(are some) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0635\u064e\u062f\u0651\u064e", "word_translation": "turned away", "word_transliteration": "sadda"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f\u06da", "word_translation": "from him", "word_transliteration": "anhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0641\u064e\u0649\u0670", "word_translation": "and sufficient", "word_transliteration": "wakafa"}, {"word_arabic_uthmani": "\u0628\u0650\u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e", "word_translation": "(is) Hell", "word_transliteration": "bijahannama"}, {"word_arabic_uthmani": "\u0633\u064e\u0639\u0650\u064a\u0631\u064b\u0627", "word_tr