{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e \u0644\u0650\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e \u0639\u0650\u0646\u062f\u064e \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1 \u062c\u064e\u0646\u0651\u064e\u0640\u0670\u062a\u0650 \u0671\u0644\u0646\u0651\u064e\u0639\u0650\u064a\u0645\u0650"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u06e1\u0645\u064f\u062a\u0651\u064e\u0642\u0650\u064a\u0646\u064e", "word_translation": "for the righteous", "word_transliteration": "lil'muttaqina"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "with", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their Lord", "word_transliteration": "rabbihim"}, {"word_arabic_uthmani": "\u062c\u064e\u0646\u0651\u064e\u0640\u0670\u062a\u0650", "word_translation": "(are) Gardens", "word_transliteration": "jannati"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0639\u0650\u064a\u0645\u0650", "word_translation": "(of) Delight", "word_transliteration": "al-na'imi"}]}