{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0627 \u064a\u064e\u062a\u064e\u062f\u064e\u0628\u0651\u064e\u0631\u064f\u0648\u0646\u064e \u0671\u0644\u06e1\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064e\u06da \u0648\u064e\u0644\u064e\u0648\u06e1 \u0643\u064e\u0627\u0646\u064e \u0645\u0650\u0646\u06e1 \u0639\u0650\u0646\u062f\u0650 \u063a\u064e\u064a\u06e1\u0631\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0644\u064e\u0648\u064e\u062c\u064e\u062f\u064f\u0648\u0627\u0652 \u0641\u0650\u064a\u0647\u0650 \u0671\u062e\u06e1\u062a\u0650\u0644\u064e\u0670\u0641\u0657\u0627 \u0643\u064e\u062b\u0650\u064a\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0627", "word_translation": "Then (do) not", "word_transliteration": "afala"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u062f\u064e\u0628\u0651\u064e\u0631\u064f\u0648\u0646\u064e", "word_translation": "they ponder", "word_transliteration": "yatadabbaruna"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u064e\u06da", "word_translation": "(on) the Quran", "word_transliteration": "al-qur'ana"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0648\u06e1", "word_translation": "And if", "word_transliteration": "walaw"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "it had (been)", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "(of)", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u0650", "word_translation": "from", "word_transliteration": "indi"}, {"word_arabic_uthmani": "\u063a\u064e\u064a\u06e1\u0631\u0650", "word_translation": "other than", "word_transliteration": "ghayri"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0644\u064e\u0648\u064e\u062c\u064e\u062