{"ayahs": {"ayah_uthmani": "\u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u064e \u0646\u064e\u062e\u06e1\u062a\u0650\u0645\u064f \u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u0641\u06e1\u0648\u064e\u0670\u0647\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u062a\u064f\u0643\u064e\u0644\u0651\u0650\u0645\u064f\u0646\u064e\u0627\u0653 \u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1 \u0648\u064e\u062a\u064e\u0634\u06e1\u0647\u064e\u062f\u064f \u0623\u064e\u0631\u06e1\u062c\u064f\u0644\u064f\u0647\u064f\u0645 \u0628\u0650\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u064a\u064e\u0643\u06e1\u0633\u0650\u0628\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0644\u06e1\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "This Day", "word_transliteration": "al-yawma"}, {"word_arabic_uthmani": "\u0646\u064e\u062e\u06e1\u062a\u0650\u0645\u064f", "word_translation": "We will seal", "word_transliteration": "nakhtimu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "[on]", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u06e1\u0648\u064e\u0670\u0647\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their mouths", "word_transliteration": "afwahihim"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064f\u0643\u064e\u0644\u0651\u0650\u0645\u064f\u0646\u064e\u0627\u0653", "word_translation": "and will speak to Us", "word_transliteration": "watukallimuna"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "their hands", "word_transliteration": "aydihim"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0634\u06e1\u0647\u064e\u062f\u064f", "word_translation": "and will bear witness", "word_transliteration": "watashhadu"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u062c\u064f\u0644\u064f\u0647\u064f\u0645", "word_translation": "their feet", "word_transliteration": "arjuluhum"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0627", "word_translation": "about what", "word_transliteration": "bima"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "they used (to)", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u06e1\u0633\u0650\u0628\u064f\u0648\u0646\u064e", "word_translation": "earn", "word_transliteration": "yaksibuna"}]}