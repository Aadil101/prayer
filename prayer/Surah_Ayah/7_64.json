{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0647\u064f \u0641\u064e\u0623\u064e\u0646\u062c\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u064e\u0639\u064e\u0647\u064f\u06e5 \u0641\u0650\u064a \u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u0650 \u0648\u064e\u0623\u064e\u063a\u06e1\u0631\u064e\u0642\u06e1\u0646\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652 \u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627\u0653\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0642\u064e\u0648\u06e1\u0645\u064b\u0627 \u0639\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0647\u064f", "word_translation": "But they denied him", "word_transliteration": "fakadhabuhu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0646\u062c\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f", "word_translation": "so We saved him", "word_transliteration": "fa-anjaynahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e\u0647\u064f\u06e5", "word_translation": "(were) with him", "word_transliteration": "ma'ahu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u0650", "word_translation": "the ship", "word_transliteration": "al-ful'ki"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u063a\u06e1\u0631\u064e\u0642\u06e1\u0646\u064e\u0627", "word_translation": "And We drowned", "word_transliteration": "wa-aghraqna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "denied", "word_transliteration": "kadhabu"}, {"word_arabic_uthmani": "\u0628\u0650\u0640\u0654\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0646\u064e\u0627\u0653\u06da", "word_translation": "Our Verses", "word_transliteration": "biayatina"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Indeed they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "were", "word_transliteration": "kanu"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064b\u0627", "word_translation": "a people", "word_transliteration": "qawman"}, {"word_arabic_uthmani": "\u0639\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "blind", "word_transliteration": "amina"}]}