{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0646\u0650 \u0671\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0648\u064e\u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0647\u064f \u0648\u064e\u0623\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0646\u0650", "word_translation": "That", "word_transliteration": "ani"}, {"word_arabic_uthmani": "\u0671\u0639\u06e1\u0628\u064f\u062f\u064f\u0648\u0627\u0652", "word_translation": "Worship", "word_transliteration": "u'budu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u0642\u064f\u0648\u0647\u064f", "word_translation": "and fear Him", "word_transliteration": "wa-ittaquhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0637\u0650\u064a\u0639\u064f\u0648\u0646\u0650", "word_translation": "and obey me", "word_transliteration": "wa-ati'uni"}]}