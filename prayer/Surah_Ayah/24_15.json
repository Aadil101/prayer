{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0630\u06e1 \u062a\u064e\u0644\u064e\u0642\u0651\u064e\u0648\u06e1\u0646\u064e\u0647\u064f\u06e5 \u0628\u0650\u0623\u064e\u0644\u06e1\u0633\u0650\u0646\u064e\u062a\u0650\u0643\u064f\u0645\u06e1 \u0648\u064e\u062a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e \u0628\u0650\u0623\u064e\u0641\u06e1\u0648\u064e\u0627\u0647\u0650\u0643\u064f\u0645 \u0645\u0651\u064e\u0627 \u0644\u064e\u064a\u06e1\u0633\u064e \u0644\u064e\u0643\u064f\u0645 \u0628\u0650\u0647\u0650\u06e6 \u0639\u0650\u0644\u06e1\u0645\u065e \u0648\u064e\u062a\u064e\u062d\u06e1\u0633\u064e\u0628\u064f\u0648\u0646\u064e\u0647\u064f\u06e5 \u0647\u064e\u064a\u0651\u0650\u0646\u0657\u0627 \u0648\u064e\u0647\u064f\u0648\u064e \u0639\u0650\u0646\u062f\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0639\u064e\u0638\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0630\u06e1", "word_translation": "When", "word_transliteration": "idh"}, {"word_arabic_uthmani": "\u062a\u064e\u0644\u064e\u0642\u0651\u064e\u0648\u06e1\u0646\u064e\u0647\u064f\u06e5", "word_translation": "you received it", "word_transliteration": "talaqqawnahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0644\u06e1\u0633\u0650\u0646\u064e\u062a\u0650\u0643\u064f\u0645\u06e1", "word_translation": "with your tongues", "word_transliteration": "bi-alsinatikum"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e", "word_translation": "and you said", "word_transliteration": "wataquluna"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0641\u06e1\u0648\u064e\u0627\u0647\u0650\u0643\u064f\u0645", "word_translation": "with your mouths", "word_transliteration": "bi-afwahikum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "not", "word_transliteration": "laysa"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "of it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u065e", "word_translation": "any knowledge", "word_transliteration": "il'mun"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u062d\u06e1\u0633\u064e\u0628\u064f\u0648\u0646\u064e\u0647\u064f\u06e5", "word_translation": "and you thought it", "word_transliteration": "watahsabunahu"}, {"word_arabic_uthmani": "\u0647\u064e\u064a\u0651\u0650\u0646\u0657\u0627", "word_translation": "(was) insignificant", "word_transliteration": "hayyinan"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "while it", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e", "word_translation": "(was) near Allah", "word_transliteration": "inda"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(was) near Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0639\u064e\u0638\u0650\u064a\u0645\u065e", "word_translation": "great", "word_transliteration": "azimun"}]}