{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e \u0642\u064e\u0648\u06e1\u0645\u064f \u0645\u064f\u0648\u0633\u064e\u0649\u0670 \u0645\u0650\u0646\u06e2 \u0628\u064e\u0639\u06e1\u062f\u0650\u0647\u0650\u06e6 \u0645\u0650\u0646\u06e1 \u062d\u064f\u0644\u0650\u064a\u0651\u0650\u0647\u0650\u0645\u06e1 \u0639\u0650\u062c\u06e1\u0644\u0657\u0627 \u062c\u064e\u0633\u064e\u062f\u0657\u0627 \u0644\u0651\u064e\u0647\u064f\u06e5 \u062e\u064f\u0648\u064e\u0627\u0631\u064c\u06da \u0623\u064e\u0644\u064e\u0645\u06e1 \u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652 \u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5 \u0644\u064e\u0627 \u064a\u064f\u0643\u064e\u0644\u0651\u0650\u0645\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u0647\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1 \u0633\u064e\u0628\u0650\u064a\u0644\u064b\u0627\u06d8 \u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0647\u064f \u0648\u064e\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0638\u064e\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064e", "word_translation": "And took", "word_transliteration": "wa-ittakhadha"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064f", "word_translation": "(the) people", "word_transliteration": "qawmu"}, {"word_arabic_uthmani": "\u0645\u064f\u0648\u0633\u064e\u0649\u0670", "word_translation": "(of) Musa", "word_transliteration": "musa"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e2", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u0650\u0647\u0650\u06e6", "word_translation": "after him", "word_transliteration": "ba'dihi"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062d\u064f\u0644\u0650\u064a\u0651\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their ornaments", "word_transliteration": "huliyyihim"}, {"word_arabic_uthmani": "\u0639\u0650\u062c\u06e1\u0644\u0657\u0627", "word_translation": "a calf", "word_transliteration": "ij'lan"}, {"word_arabic_uthmani": "\u062c\u064e\u0633\u064e\u062f\u0657\u0627", "word_translation": "an image", "word_transliteration": "jasadan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0647\u064f\u06e5", "word_translation": "[for] it", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u062e\u064f\u0648\u064e\u0627\u0631\u064c\u06da", "word_translation": "(had) a lowing sound", "word_transliteration": "khuwarun"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Did not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u064e\u0648\u06e1\u0627\u0652", "word_translation": "they see", "word_transliteration": "yaraw"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "that it", "word_transliteration": "annahu"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "(could) not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0643\u064e\u0644\u0651\u0650\u0645\u064f\u0647\u064f\u0645\u06e1", "word_translation": "speak to them", "word_transliteration": "yukallimuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u0647\u06e1\u062f\u0650\u064a\u0647\u0650\u0645\u06e1", "word_translation": "guide them", "word_transliteration": "yahdihim"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u0650\u064a\u0644\u064b\u0627\u06d8", "word_translation": "(to) a way", "word_transliteration": "sabilan"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0647\u064f", "word_translation": "They took it (for worship)", "word_transliteration": "ittakhadhuhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652", "word_translation": "and they were", "word_transliteration": "wakanu"}, {"word_arabic_uthmani": "\u0638\u064e\u0670\u0644\u0650\u0645\u0650\u064a\u0646\u064e", "word_translation": "wrongdoers", "word_transliteration": "zalimina"}]}