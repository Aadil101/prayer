{"ayahs": {"ayah_uthmani": "\u062b\u064f\u0645\u0651\u064e \u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627 \u0631\u064f\u0633\u064f\u0644\u064e\u0646\u064e\u0627 \u062a\u064e\u062a\u06e1\u0631\u064e\u0627\u06d6 \u0643\u064f\u0644\u0651\u064e \u0645\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e \u0623\u064f\u0645\u0651\u064e\u0629\u0657 \u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064f\u0647\u064e\u0627 \u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0647\u064f\u06d6 \u0641\u064e\u0623\u064e\u062a\u06e1\u0628\u064e\u0639\u06e1\u0646\u064e\u0627 \u0628\u064e\u0639\u06e1\u0636\u064e\u0647\u064f\u0645 \u0628\u064e\u0639\u06e1\u0636\u0657\u0627 \u0648\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0623\u064e\u062d\u064e\u0627\u062f\u0650\u064a\u062b\u064e\u06da \u0641\u064e\u0628\u064f\u0639\u06e1\u062f\u0657\u0627 \u0644\u0651\u0650\u0642\u064e\u0648\u06e1\u0645\u0656 \u0644\u0651\u064e\u0627 \u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0633\u064e\u0644\u06e1\u0646\u064e\u0627", "word_translation": "We sent", "word_transliteration": "arsalna"}, {"word_arabic_uthmani": "\u0631\u064f\u0633\u064f\u0644\u064e\u0646\u064e\u0627", "word_translation": "Our Messengers", "word_transliteration": "rusulana"}, {"word_arabic_uthmani": "\u062a\u064e\u062a\u06e1\u0631\u064e\u0627\u06d6", "word_translation": "(in) succession", "word_transliteration": "tatra"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u064e", "word_translation": "Every time", "word_transliteration": "kulla"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Every time", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e", "word_translation": "came", "word_transliteration": "jaa"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u064e\u0629\u0657", "word_translation": "(to) a nation", "word_transliteration": "ummatan"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064f\u0647\u064e\u0627", "word_translation": "its Messenger", "word_transliteration": "rasuluha"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u0651\u064e\u0628\u064f\u0648\u0647\u064f\u06d6", "word_translation": "they denied him", "word_transliteration": "kadhabuhu"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u062a\u06e1\u0628\u064e\u0639\u06e1\u0646\u064e\u0627", "word_translation": "so We made (them) follow", "word_transliteration": "fa-atba'na"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u064e\u0647\u064f\u0645", "word_translation": "some of them", "word_transliteration": "ba'dahum"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u0636\u0657\u0627", "word_translation": "others", "word_transliteration": "ba'dan"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1", "word_translation": "and We made them", "word_transliteration": "waja'alnahum"}, {"word_arabic_uthmani": "\u0623\u064e\u062d\u064e\u0627\u062f\u0650\u064a\u062b\u064e\u06da", "word_translation": "narrations", "word_transliteration": "ahaditha"}, {"word_arabic_uthmani": "\u0641\u064e\u0628\u064f\u0639\u06e1\u062f\u0657\u0627", "word_translation": "So away", "word_transliteration": "fabu'dan"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0642\u064e\u0648\u06e1\u0645\u0656", "word_translation": "with a people", "word_transliteration": "liqawmin"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e", "word_translation": "they believe", "word_transliteration": "yu'minuna"}]}