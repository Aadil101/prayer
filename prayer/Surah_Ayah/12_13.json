{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0625\u0650\u0646\u0651\u0650\u064a \u0644\u064e\u064a\u064e\u062d\u06e1\u0632\u064f\u0646\u064f\u0646\u0650\u064a\u0653 \u0623\u064e\u0646 \u062a\u064e\u0630\u06e1\u0647\u064e\u0628\u064f\u0648\u0627\u0652 \u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u0623\u064e\u062e\u064e\u0627\u0641\u064f \u0623\u064e\u0646 \u064a\u064e\u0623\u06e1\u0643\u064f\u0644\u064e\u0647\u064f \u0671\u0644\u0630\u0651\u0650\u0626\u06e1\u0628\u064f \u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u0639\u064e\u0646\u06e1\u0647\u064f \u063a\u064e\u0670\u0641\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed, [I]", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u064e\u062d\u06e1\u0632\u064f\u0646\u064f\u0646\u0650\u064a\u0653", "word_translation": "it surely saddens me", "word_transliteration": "layahzununi"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u0630\u06e1\u0647\u064e\u0628\u064f\u0648\u0627\u0652", "word_translation": "you should take him", "word_transliteration": "tadhhabu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "you should take him", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u062e\u064e\u0627\u0641\u064f", "word_translation": "and I fear", "word_transliteration": "wa-akhafu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u0643\u064f\u0644\u064e\u0647\u064f", "word_translation": "would eat him", "word_transliteration": "yakulahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0630\u0651\u0650\u0626\u06e1\u0628\u064f", "word_translation": "a wolf", "word_transliteration": "al-dhi'bu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "while you", "word_transliteration": "wa-antum"}, {"word_arabic_uthmani": "\u0639\u064e\u0646\u06e1\u0647\u064f", "word_translation": "of him", "word_transliteration": "anhu"}, {"word_arabic_uthmani": "\u063a\u064e\u0670\u0641\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "(are) unaware", "word_transliteration": "ghafiluna"}]}