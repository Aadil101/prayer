{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652 \u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u062a\u064e\u0641\u064e\u0631\u0651\u064e\u0642\u064f\u0648\u0627\u0652 \u0648\u064e\u0671\u062e\u06e1\u062a\u064e\u0644\u064e\u0641\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u06e2 \u0628\u064e\u0639\u06e1\u062f\u0650 \u0645\u064e\u0627 \u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0628\u064e\u064a\u0651\u0650\u0646\u064e\u0670\u062a\u064f\u06da \u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064c \u0639\u064e\u0638\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652", "word_translation": "be", "word_transliteration": "takunu"}, {"word_arabic_uthmani": "\u0643\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "like those who", "word_transliteration": "ka-alladhina"}, {"word_arabic_uthmani": "\u062a\u064e\u0641\u064e\u0631\u0651\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "became divided", "word_transliteration": "tafarraqu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u062e\u06e1\u062a\u064e\u0644\u064e\u0641\u064f\u0648\u0627\u0652", "word_translation": "and differed", "word_transliteration": "wa-ikh'talafu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e2", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u0650", "word_translation": "after", "word_transliteration": "ba'di"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u062c\u064e\u0627\u0653\u0621\u064e\u0647\u064f\u0645\u064f", "word_translation": "came to them", "word_transliteration": "jaahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0628\u064e\u064a\u0651\u0650\u0646\u064e\u0670\u062a\u064f\u06da", "word_translation": "the clear proofs", "word_transliteration": "al-bayinatu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "And those", "word_transliteration": "wa-ulaika"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064c", "word_translation": "(is) a punishment", "word_transliteration": "adhabun"}, {"word_arabic_uthmani": "\u0639\u064e\u0638\u0650\u064a\u0645\u065e", "word_translation": "great", "word_transliteration": "azimun"}]}