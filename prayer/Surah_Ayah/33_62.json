{"ayahs": {"ayah_uthmani": "\u0633\u064f\u0646\u0651\u064e\u0629\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0641\u0650\u064a \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u062e\u064e\u0644\u064e\u0648\u06e1\u0627\u0652 \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u064f\u06d6 \u0648\u064e\u0644\u064e\u0646 \u062a\u064e\u062c\u0650\u062f\u064e \u0644\u0650\u0633\u064f\u0646\u0651\u064e\u0629\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u062a\u064e\u0628\u06e1\u062f\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0633\u064f\u0646\u0651\u064e\u0629\u064e", "word_translation": "(Such is the) Way", "word_transliteration": "sunnata"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "with", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0648\u06e1\u0627\u0652", "word_translation": "passed away", "word_transliteration": "khalaw"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f\u06d6", "word_translation": "before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0646", "word_translation": "and never", "word_transliteration": "walan"}, {"word_arabic_uthmani": "\u062a\u064e\u062c\u0650\u062f\u064e", "word_translation": "you will find", "word_transliteration": "tajida"}, {"word_arabic_uthmani": "\u0644\u0650\u0633\u064f\u0646\u0651\u064e\u0629\u0650", "word_translation": "in (the) Way", "word_transliteration": "lisunnati"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_t