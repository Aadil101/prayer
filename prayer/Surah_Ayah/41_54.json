{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0641\u0650\u064a \u0645\u0650\u0631\u06e1\u064a\u064e\u0629\u0656 \u0645\u0651\u0650\u0646 \u0644\u0651\u0650\u0642\u064e\u0627\u0653\u0621\u0650 \u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1\u06d7 \u0623\u064e\u0644\u064e\u0627\u0653 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0628\u0650\u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0645\u0651\u064f\u062d\u0650\u064a\u0637\u064f\u06e2"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627\u0653", "word_translation": "Unquestionably", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "they", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(are) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0645\u0650\u0631\u06e1\u064a\u064e\u0629\u0656", "word_translation": "doubt", "word_transliteration": "mir'yatin"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646", "word_translation": "about", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0642\u064e\u0627\u0653\u0621\u0650", "word_translation": "(the) meeting", "word_transliteration": "liqai"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0647\u0650\u0645\u06e1\u06d7", "word_translation": "(with) their Lord", "word_transliteration": "rabbihim"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0627\u0653", "word_translation": "Unquestionably", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "indeed He", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064f\u0644\u0651\u0650", "word_translation": "(is) of all", "word_transliteration": "bikulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u062