{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0633\u06e1\u0645\u064e\u0670\u0639\u0650\u064a\u0644\u064e \u0648\u064e\u0625\u0650\u062f\u06e1\u0631\u0650\u064a\u0633\u064e \u0648\u064e\u0630\u064e\u0627 \u0671\u0644\u06e1\u0643\u0650\u0641\u06e1\u0644\u0650\u06d6 \u0643\u064f\u0644\u0651\u065e \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0628\u0650\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0633\u06e1\u0645\u064e\u0670\u0639\u0650\u064a\u0644\u064e", "word_translation": "And Ismail", "word_transliteration": "wa-is'ma'ila"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u062f\u06e1\u0631\u0650\u064a\u0633\u064e", "word_translation": "and Idris", "word_transliteration": "wa-id'risa"}, {"word_arabic_uthmani": "\u0648\u064e\u0630\u064e\u0627", "word_translation": "and Dhul-Kifl", "word_transliteration": "wadha"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u0641\u06e1\u0644\u0650\u06d6", "word_translation": "and Dhul-Kifl", "word_transliteration": "al-kif'li"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u065e", "word_translation": "all", "word_transliteration": "kullun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "(were) of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064e\u0640\u0670\u0628\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "the patient ones", "word_transliteration": "al-sabirina"}]}