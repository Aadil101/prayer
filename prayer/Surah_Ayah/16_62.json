{"ayahs": {"ayah_uthmani": "\u0648\u064e\u064a\u064e\u062c\u06e1\u0639\u064e\u0644\u064f\u0648\u0646\u064e \u0644\u0650\u0644\u0651\u064e\u0647\u0650 \u0645\u064e\u0627 \u064a\u064e\u0643\u06e1\u0631\u064e\u0647\u064f\u0648\u0646\u064e\u06da \u0648\u064e\u062a\u064e\u0635\u0650\u0641\u064f \u0623\u064e\u0644\u06e1\u0633\u0650\u0646\u064e\u062a\u064f\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0643\u064e\u0630\u0650\u0628\u064e \u0623\u064e\u0646\u0651\u064e \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u062d\u064f\u0633\u06e1\u0646\u064e\u0649\u0670\u06da \u0644\u064e\u0627 \u062c\u064e\u0631\u064e\u0645\u064e \u0623\u064e\u0646\u0651\u064e \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064e \u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0641\u06e1\u0631\u064e\u0637\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062c\u06e1\u0639\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "And they assign", "word_transliteration": "wayaj'aluna"}, {"word_arabic_uthmani": "\u0644\u0650\u0644\u0651\u064e\u0647\u0650", "word_translation": "to Allah", "word_transliteration": "lillahi"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0643\u06e1\u0631\u064e\u0647\u064f\u0648\u0646\u064e\u06da", "word_translation": "they dislike", "word_transliteration": "yakrahuna"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0635\u0650\u0641\u064f", "word_translation": "and assert", "word_transliteration": "watasifu"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u06e1\u0633\u0650\u0646\u064e\u062a\u064f\u0647\u064f\u0645\u064f", "word_translation": "their tongues", "word_transliteration": "alsinatuhumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0630\u0650\u0628\u064e", "word_translation": "the lie", "word_transliteration": "al-kadhiba"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e", "word_translation": "that", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "for them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064f\u0633\u06e1\u0646\u064e\u0649\u0670\u06da", "word_translation": "(is) the best", "word_transliteration": "al-hus'na"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "No", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u062c\u064e\u0631\u064e\u0645\u064e", "word_translation": "doubt", "word_transliteration": "jarama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e", "word_translation": "that", "word_transliteration": "anna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "for them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0646\u0651\u064e\u0627\u0631\u064e", "word_translation": "(is) the Fire", "word_transliteration": "al-nara"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0651\u064e\u0647\u064f\u0645", "word_translation": "and that they", "word_transliteration": "wa-annahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0641\u06e1\u0631\u064e\u0637\u064f\u0648\u0646\u064e", "word_translation": "(will) be abandoned", "word_transliteration": "muf'ratuna"}]}