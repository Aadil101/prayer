{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0630\u064e\u0643\u0651\u0650\u0631\u06e1 \u0641\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064e \u0628\u0650\u0646\u0650\u0639\u06e1\u0645\u064e\u062a\u0650 \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0628\u0650\u0643\u064e\u0627\u0647\u0650\u0646\u0656 \u0648\u064e\u0644\u064e\u0627 \u0645\u064e\u062c\u06e1\u0646\u064f\u0648\u0646\u064d"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0630\u064e\u0643\u0651\u0650\u0631\u06e1", "word_translation": "Therefore remind", "word_transliteration": "fadhakkir"}, {"word_arabic_uthmani": "\u0641\u064e\u0645\u064e\u0627\u0653", "word_translation": "for not", "word_transliteration": "fama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "you", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0628\u0650\u0646\u0650\u0639\u06e1\u0645\u064e\u062a\u0650", "word_translation": "(are) by (the) grace", "word_transliteration": "bini'mati"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "(of) your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064e\u0627\u0647\u0650\u0646\u0656", "word_translation": "a soothsayer", "word_transliteration": "bikahinin"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0645\u064e\u062c\u06e1\u0646\u064f\u0648\u0646\u064d", "word_translation": "a madman", "word_transliteration": "majnunin"}]}