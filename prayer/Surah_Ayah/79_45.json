{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064e \u0645\u064f\u0646\u0630\u0650\u0631\u064f \u0645\u064e\u0646 \u064a\u064e\u062e\u06e1\u0634\u064e\u0649\u0670\u0647\u064e\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627\u0653", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064e", "word_translation": "you", "word_transliteration": "anta"}, {"word_arabic_uthmani": "\u0645\u064f\u0646\u0630\u0650\u0631\u064f", "word_translation": "(are) a warner", "word_transliteration": "mundhiru"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "(for him) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0634\u064e\u0649\u0670\u0647\u064e\u0627", "word_translation": "fears it", "word_transliteration": "yakhshaha"}]}