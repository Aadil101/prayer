{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0670\u0643\u0650\u0646\u0650 \u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064f \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652 \u0645\u064e\u0639\u064e\u0647\u064f\u06e5 \u062c\u064e\u0670\u0647\u064e\u062f\u064f\u0648\u0627\u0652 \u0628\u0650\u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1\u06da \u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0644\u064e\u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u062e\u064e\u064a\u06e1\u0631\u064e\u0670\u062a\u064f\u06d6 \u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0644\u0650\u062d\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0670\u0643\u0650\u0646\u0650", "word_translation": "But", "word_transliteration": "lakini"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0633\u064f\u0648\u0644\u064f", "word_translation": "the Messenger", "word_transliteration": "al-rasulu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "and those who", "word_transliteration": "wa-alladhina"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0645\u064e\u0646\u064f\u0648\u0627\u0652", "word_translation": "believed", "word_transliteration": "amanu"}, {"word_arabic_uthmani": "\u0645\u064e\u0639\u064e\u0647\u064f\u06e5", "word_translation": "with him", "word_transliteration": "ma'ahu"}, {"word_arabic_uthmani": "\u062c\u064e\u0670\u0647\u064e\u062f\u064f\u0648\u0627\u0652", "word_translation": "strove", "word_transliteration": "jahadu"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u0650\u0647\u0650\u0645\u06e1", "word_translation": "with their wealth", "word_transliteration": "bi-amwalihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1\u06da", "word_translation": "and their lives", "word_transliteration": "wa-anfusihim"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "And those", "word_transliteration": "wa-ulaika"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u064f", "word_translation": "for them", "word_transliteration": "lahumu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u064a\u06e1\u0631\u064e\u0670\u062a\u064f\u06d6", "word_translation": "(are) the good things", "word_transliteration": "al-khayratu"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e", "word_translation": "and those", "word_transliteration": "wa-ulaika"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "they", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0641\u06e1\u0644\u0650\u062d\u064f\u0648\u0646\u064e", "word_translation": "(are) the successful ones", "word_transliteration": "al-muf'lihuna"}]}