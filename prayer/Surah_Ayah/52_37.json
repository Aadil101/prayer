{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0645\u06e1 \u0639\u0650\u0646\u062f\u064e\u0647\u064f\u0645\u06e1 \u062e\u064e\u0632\u064e\u0627\u0653\u0626\u0650\u0646\u064f \u0631\u064e\u0628\u0651\u0650\u0643\u064e \u0623\u064e\u0645\u06e1 \u0647\u064f\u0645\u064f \u0671\u0644\u06e1\u0645\u064f\u0635\u06dc\u064e\u064a\u06e1\u0637\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "Or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0639\u0650\u0646\u062f\u064e\u0647\u064f\u0645\u06e1", "word_translation": "with them", "word_transliteration": "indahum"}, {"word_arabic_uthmani": "\u062e\u064e\u0632\u064e\u0627\u0653\u0626\u0650\u0646\u064f", "word_translation": "(are the) treasures", "word_transliteration": "khazainu"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u0650\u0643\u064e", "word_translation": "(of) your Lord", "word_transliteration": "rabbika"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1", "word_translation": "or", "word_transliteration": "am"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u064f", "word_translation": "(are) they", "word_transliteration": "humu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0635\u06dc\u064e\u064a\u06e1\u0637\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "the controllers", "word_transliteration": "al-musaytiruna"}]}