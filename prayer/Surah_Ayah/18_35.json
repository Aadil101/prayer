{"ayahs": {"ayah_uthmani": "\u0648\u064e\u062f\u064e\u062e\u064e\u0644\u064e \u062c\u064e\u0646\u0651\u064e\u062a\u064e\u0647\u064f\u06e5 \u0648\u064e\u0647\u064f\u0648\u064e \u0638\u064e\u0627\u0644\u0650\u0645\u065e \u0644\u0651\u0650\u0646\u064e\u0641\u06e1\u0633\u0650\u0647\u0650\u06e6 \u0642\u064e\u0627\u0644\u064e \u0645\u064e\u0627\u0653 \u0623\u064e\u0638\u064f\u0646\u0651\u064f \u0623\u064e\u0646 \u062a\u064e\u0628\u0650\u064a\u062f\u064e \u0647\u064e\u0670\u0630\u0650\u0647\u0650\u06e6\u0653 \u0623\u064e\u0628\u064e\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u062f\u064e\u062e\u064e\u0644\u064e", "word_translation": "And he entered", "word_transliteration": "wadakhala"}, {"word_arabic_uthmani": "\u062c\u064e\u0646\u0651\u064e\u062a\u064e\u0647\u064f\u06e5", "word_translation": "his garden", "word_transliteration": "jannatahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "while he", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0638\u064e\u0627\u0644\u0650\u0645\u065e", "word_translation": "(was) unjust", "word_transliteration": "zalimun"}, {"word_arabic_uthmani": "\u0644\u0651\u0650\u0646\u064e\u0641\u06e1\u0633\u0650\u0647\u0650\u06e6", "word_translation": "to himself", "word_transliteration": "linafsihi"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "He said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0623\u064e\u0638\u064f\u0646\u0651\u064f", "word_translation": "I think", "word_transliteration": "azunnu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u0628\u0650\u064a\u062f\u064e", "word_translation": "will perish", "word_transliteration": "tabida"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0630\u0650\u0647\u0650