{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064e \u0644\u064e\u0647\u064f\u06e5 \u062b\u064e\u0645\u064e\u0631\u065e \u0641\u064e\u0642\u064e\u0627\u0644\u064e \u0644\u0650\u0635\u064e\u0670\u062d\u0650\u0628\u0650\u0647\u0650\u06e6 \u0648\u064e\u0647\u064f\u0648\u064e \u064a\u064f\u062d\u064e\u0627\u0648\u0650\u0631\u064f\u0647\u064f\u06e5\u0653 \u0623\u064e\u0646\u064e\u0627\u06e0 \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f \u0645\u0650\u0646\u0643\u064e \u0645\u064e\u0627\u0644\u0657\u0627 \u0648\u064e\u0623\u064e\u0639\u064e\u0632\u0651\u064f \u0646\u064e\u0641\u064e\u0631\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "And was", "word_transliteration": "wakana"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u062b\u064e\u0645\u064e\u0631\u065e", "word_translation": "fruit", "word_transliteration": "thamarun"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064e\u0627\u0644\u064e", "word_translation": "so he said", "word_transliteration": "faqala"}, {"word_arabic_uthmani": "\u0644\u0650\u0635\u064e\u0670\u062d\u0650\u0628\u0650\u0647\u0650\u06e6", "word_translation": "to his companion", "word_transliteration": "lisahibihi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "while he", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u064e\u0627\u0648\u0650\u0631\u064f\u0647\u064f\u06e5\u0653", "word_translation": "(was) talking with him", "word_transliteration": "yuhawiruhu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u064e\u0627\u06e0", "word_translation": "I am", "word_transliteration": "ana"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f", "word_translation": "greater", "word_transliteration": "aktharu"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u0643\u064e", "word_translation": "than you", "word_transliteration": "minka"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0644\u0657\u0627", "word_translation": "(in) wealth", "word_transliteration": "malan"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0639\u064e\u0632\u0651\u064f", "word_translation": "and stronger", "word_transliteration": "wa-a'azzu"}, {"word_arabic_uthmani": "\u0646\u064e\u0641\u064e\u0631\u0657\u0627", "word_translation": "(in) men", "word_transliteration": "nafaran"}]}