{"ayahs": {"ayah_uthmani": "\u06de\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u064a\u064e\u0633\u06e1\u062a\u064e\u062c\u0650\u064a\u0628\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u064a\u064e\u0633\u06e1\u0645\u064e\u0639\u064f\u0648\u0646\u064e\u06d8 \u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e\u0649\u0670 \u064a\u064e\u0628\u06e1\u0639\u064e\u062b\u064f\u0647\u064f\u0645\u064f \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u062b\u064f\u0645\u0651\u064e \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u064a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "Only", "word_transliteration": "innama"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u062a\u064e\u062c\u0650\u064a\u0628\u064f", "word_translation": "respond", "word_transliteration": "yastajibu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u064a\u064e\u0633\u06e1\u0645\u064e\u0639\u064f\u0648\u0646\u064e\u06d8", "word_translation": "listen", "word_transliteration": "yasma'una"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0645\u064e\u0648\u06e1\u062a\u064e\u0649\u0670", "word_translation": "But the dead", "word_transliteration": "wal-mawta"}, {"word_arabic_uthmani": "\u064a\u064e\u0628\u06e1\u0639\u064e\u062b\u064f\u0647\u064f\u0645\u064f", "word_translation": "will resurrect them", "word_transliteration": "yab'athuhumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650", "word_translation": "to Him", "word_transliteration": "ilayhi"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u06e1\u062c\u064e\u0639\u064f\u0648\u0646\u064e", "word_translation": "they will be returned", "word_transliteration": "yur'ja'una"}]}