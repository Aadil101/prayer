{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0627 \u064a\u064e\u062a\u064f\u0648\u0628\u064f\u0648\u0646\u064e \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0648\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f\u0648\u0646\u064e\u0647\u064f\u06e5\u06da \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u063a\u064e\u0641\u064f\u0648\u0631\u065e \u0631\u0651\u064e\u062d\u0650\u064a\u0645\u065e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u0644\u064e\u0627", "word_translation": "So will not", "word_transliteration": "afala"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064f\u0648\u0628\u064f\u0648\u0646\u064e", "word_translation": "they repent", "word_transliteration": "yatubuna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u063a\u06e1\u0641\u0650\u0631\u064f\u0648\u0646\u064e\u0647\u064f\u06e5\u06da", "word_translation": "and seek His forgiveness", "word_transliteration": "wayastaghfirunahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u063a\u064e\u0641\u064f\u0648\u0631\u065e", "word_translation": "(is) Oft-Forgiving", "word_transliteration": "ghafurun"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u065e", "word_translation": "Most Merciful", "word_transliteration": "rahimun"}]}