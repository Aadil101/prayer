{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064e \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0623\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6 \u0644\u0650\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u0633\u06e1\u062a\u064f\u0636\u06e1\u0639\u0650\u0641\u064f\u0648\u0627\u0652 \u0644\u0650\u0645\u064e\u0646\u06e1 \u0621\u064e\u0627\u0645\u064e\u0646\u064e \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0623\u064e\u062a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e \u0623\u064e\u0646\u0651\u064e \u0635\u064e\u0670\u0644\u0650\u062d\u0657\u0627 \u0645\u0651\u064f\u0631\u06e1\u0633\u064e\u0644\u065e \u0645\u0651\u0650\u0646 \u0631\u0651\u064e\u0628\u0651\u0650\u0647\u0650\u06e6\u06da \u0642\u064e\u0627\u0644\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0646\u0651\u064e\u0627 \u0628\u0650\u0645\u064e\u0627\u0653 \u0623\u064f\u0631\u06e1\u0633\u0650\u0644\u064e \u0628\u0650\u0647\u0650\u06e6 \u0645\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e", "word_translation": "Said", "word_transliteration": "qala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0623\u064f", "word_translation": "the chiefs", "word_transliteration": "al-mala-u"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0671\u0633\u06e1\u062a\u064e\u0643\u06e1\u0628\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "were arrogant", "word_transliteration": "is'takbaru"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "among", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u0650\u0647\u0650\u06e6", 