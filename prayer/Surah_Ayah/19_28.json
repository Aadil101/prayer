{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064f\u062e\u06e1\u062a\u064e \u0647\u064e\u0670\u0631\u064f\u0648\u0646\u064e \u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e \u0623\u064e\u0628\u064f\u0648\u0643\u0650 \u0671\u0645\u06e1\u0631\u064e\u0623\u064e \u0633\u064e\u0648\u06e1\u0621\u0656 \u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064e\u062a\u06e1 \u0623\u064f\u0645\u0651\u064f\u0643\u0650 \u0628\u064e\u063a\u0650\u064a\u0651\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0640\u0670\u0653\u0623\u064f\u062e\u06e1\u062a\u064e", "word_translation": "O sister", "word_transliteration": "yaukh'ta"}, {"word_arabic_uthmani": "\u0647\u064e\u0670\u0631\u064f\u0648\u0646\u064e", "word_translation": "(of) Harun", "word_transliteration": "haruna"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "Not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "was", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u064f\u0648\u0643\u0650", "word_translation": "your father", "word_transliteration": "abuki"}, {"word_arabic_uthmani": "\u0671\u0645\u06e1\u0631\u064e\u0623\u064e", "word_translation": "an evil man", "word_transliteration": "im'ra-a"}, {"word_arabic_uthmani": "\u0633\u064e\u0648\u06e1\u0621\u0656", "word_translation": "an evil man", "word_transliteration": "sawin"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "and not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e\u062a\u06e1", "word_translation": "was", "word_transliteration": "kanat"}, {"word_arabic_uthmani": "\u0623\u064f\u0645\u0651\u064f\u0643\u0650", "word_translation": "your mother", "word_transliteration": "ummuki"}, {"word_arabic_uthmani": "\u0628\u064e\u063a\u0650\u064a\u0651\u0657\u0627", "word_translation": "unchaste", "word_transliteration": "baghiyyan"}]}