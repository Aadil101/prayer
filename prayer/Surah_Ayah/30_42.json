{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u0633\u0650\u064a\u0631\u064f\u0648\u0627\u0652 \u0641\u0650\u064a \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0641\u064e\u0671\u0646\u0638\u064f\u0631\u064f\u0648\u0627\u0652 \u0643\u064e\u064a\u06e1\u0641\u064e \u0643\u064e\u0627\u0646\u064e \u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u064f\u06da \u0643\u064e\u0627\u0646\u064e \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645 \u0645\u0651\u064f\u0634\u06e1\u0631\u0650\u0643\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0633\u0650\u064a\u0631\u064f\u0648\u0627\u0652", "word_translation": "Travel", "word_transliteration": "siru"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0646\u0638\u064f\u0631\u064f\u0648\u0627\u0652", "word_translation": "and see", "word_transliteration": "fa-unzuru"}, {"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "how", "word_transliteration": "kayfa"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "was", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0642\u0650\u0628\u064e\u0629\u064f", "word_translation": "(the) end", "word_transliteration": "aqibatu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "(were) before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u064f\u06da", "word_translation": "(were) before", "word_transliteration": "qablu"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u0646\u064e", "word_translation": "Most of them were", "word_transliteration": "kana"}, {"word_arabic_uthmani": "\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645", "word_translation": "Most of them were", "word_transliteration": "aktharuhum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0634\u06e1\u0631\u0650\u0643\u0650\u064a\u0646\u064e", "word_translation": "polytheists", "word_transliteration": "mush'rikina"}]}