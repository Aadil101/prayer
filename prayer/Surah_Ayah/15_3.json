{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0631\u06e1\u0647\u064f\u0645\u06e1 \u064a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0627\u0652 \u0648\u064e\u064a\u064e\u062a\u064e\u0645\u064e\u062a\u0651\u064e\u0639\u064f\u0648\u0627\u0652 \u0648\u064e\u064a\u064f\u0644\u06e1\u0647\u0650\u0647\u0650\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0645\u064e\u0644\u064f\u06d6 \u0641\u064e\u0633\u064e\u0648\u06e1\u0641\u064e \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0631\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "Leave them", "word_transliteration": "dharhum"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u0643\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "(to) eat", "word_transliteration": "yakulu"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062a\u064e\u0645\u064e\u062a\u0651\u064e\u0639\u064f\u0648\u0627\u0652", "word_translation": "and enjoy", "word_transliteration": "wayatamatta'u"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0644\u06e1\u0647\u0650\u0647\u0650\u0645\u064f", "word_translation": "and diverted them", "word_transliteration": "wayul'hihimu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0645\u064e\u0644\u064f\u06d6", "word_translation": "the hope", "word_transliteration": "al-amalu"}, {"word_arabic_uthmani": "\u0641\u064e\u0633\u064e\u0648\u06e1\u0641\u064e", "word_translation": "then soon", "word_transliteration": "fasawfa"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "they will come to know", "word_transliteration": "ya'lamuna"}]}