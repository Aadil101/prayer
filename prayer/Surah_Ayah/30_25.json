{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1 \u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0647\u0650\u06e6\u0653 \u0623\u064e\u0646 \u062a\u064e\u0642\u064f\u0648\u0645\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064f \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064f \u0628\u0650\u0623\u064e\u0645\u06e1\u0631\u0650\u0647\u0650\u06e6\u06da \u062b\u064f\u0645\u0651\u064e \u0625\u0650\u0630\u064e\u0627 \u062f\u064e\u0639\u064e\u0627\u0643\u064f\u0645\u06e1 \u062f\u064e\u0639\u06e1\u0648\u064e\u0629\u0657 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0625\u0650\u0630\u064e\u0627\u0653 \u0623\u064e\u0646\u062a\u064f\u0645\u06e1 \u062a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u0650\u0646\u06e1", "word_translation": "And among", "word_transliteration": "wamin"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u0647\u0650\u06e6\u0653", "word_translation": "His Signs", "word_transliteration": "ayatihi"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "(is) that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u064f\u0648\u0645\u064e", "word_translation": "stands", "word_transliteration": "taquma"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064f", "word_translation": "the heavens", "word_transliteration": "al-samau"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064f", "word_translation": "and the earth", "word_transliteration": "wal-ardu"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0645\u06e1\u0631\u0650\u0647\u0650\u06e6\u06da", "word_translation": "by His Command", "word_transliteration": "bi-amrihi"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "Then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627", "word_translation": "when", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u062f\u064e\u0639\u064e\u0627\u0643\u064f\u0645\u06e1", "word_translation": "He calls you", "word_transliteration": "da'akum"}, {"word_arabic_uthmani": "\u062f\u064e\u0639\u06e1\u0648\u064e\u0629\u0657", "word_translation": "(with) a call", "word_transliteration": "da'watan"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the earth", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0625\u0650\u0630\u064e\u0627\u0653", "word_translation": "behold!", "word_transliteration": "idha"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "You", "word_transliteration": "antum"}, {"word_arabic_uthmani": "\u062a\u064e\u062e\u06e1\u0631\u064f\u062c\u064f\u0648\u0646\u064e", "word_translation": "will come forth", "word_transliteration": "takhrujuna"}]}