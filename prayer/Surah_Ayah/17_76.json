{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0646 \u0643\u064e\u0627\u062f\u064f\u0648\u0627\u0652 \u0644\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u0641\u0650\u0632\u0651\u064f\u0648\u0646\u064e\u0643\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0644\u0650\u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064f\u0648\u0643\u064e \u0645\u0650\u0646\u06e1\u0647\u064e\u0627\u06d6 \u0648\u064e\u0625\u0650\u0630\u0657\u0627 \u0644\u0651\u064e\u0627 \u064a\u064e\u0644\u06e1\u0628\u064e\u062b\u064f\u0648\u0646\u064e \u062e\u0650\u0644\u064e\u0670\u0641\u064e\u0643\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "And indeed", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u0643\u064e\u0627\u062f\u064f\u0648\u0627\u0652", "word_translation": "they were about", "word_transliteration": "kadu"}, {"word_arabic_uthmani": "\u0644\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u0641\u0650\u0632\u0651\u064f\u0648\u0646\u064e\u0643\u064e", "word_translation": "(to) scare you", "word_transliteration": "layastafizzunaka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "the land", "word_transliteration": "al-ardi"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064f\u0648\u0643\u064e", "word_translation": "that they evict you", "word_transliteration": "liyukh'rijuka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064e\u0627\u06d6", "word_translation": "from it", "word_transliteration": "min'ha"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u0657\u0627", "word_translation": "But then", "word_transliteration": "wa-idhan"}, {"word_arabic_uthmani": "\u0644\u0651\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0644\u06e1\u0628\u064e\u062b\u064f\u0648\u0646\u064e", "word_translation": "they (would) have stayed", "word_transliteration": "yalbathuna"}, {"word_arabic_uthmani": "\u062e\u0650\u0644\u064e\u0670\u0641\u064e\u0643\u064e", "word_translation": "after you", "word_transliteration": "khilafaka"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0642\u064e\u0644\u0650\u064a\u0644\u0657\u0627", "word_translation": "a little", "word_transliteration": "qalilan"}]}