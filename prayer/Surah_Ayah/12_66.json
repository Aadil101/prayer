rd_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0646\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "we say", "word_transliteration": "naqulu"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u0650\u064a\u0644\u065e", "word_translation": "(is) a Guardian", "word_transliteration": "wakilun"}]}