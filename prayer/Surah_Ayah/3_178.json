{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0646\u0651\u064e\u0645\u064e\u0627 \u0646\u064f\u0645\u06e1\u0644\u0650\u064a \u0644\u064e\u0647\u064f\u0645\u06e1 \u062e\u064e\u064a\u06e1\u0631\u065e \u0644\u0651\u0650\u0623\u064e\u0646\u0641\u064f\u0633\u0650\u0647\u0650\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e\u0645\u064e\u0627 \u0646\u064f\u0645\u06e1\u0644\u0650\u064a \u0644\u064e\u0647\u064f\u0645\u06e1 \u0644\u0650\u064a\u064e\u0632\u06e1\u062f\u064e\u0627\u062f\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u062b\u06e1\u0645\u0657\u0627\u06d6 \u0648\u064e\u0644\u064e\u0647\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u065e \u0645\u0651\u064f\u0647\u0650\u064a\u0646\u065e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (let) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0633\u064e\u0628\u064e\u0646\u0651\u064e", "word_translation": "think", "word_transliteration": "yahsabanna"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0653\u0627\u0652", "word_translation": "disbelieved", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0651\u064e\u0645\u064e\u0627", "word_translation": "that", "word_transliteration": "annama"}, {"word_arabic_uthmani": "\u0646\u064f\u0645\u06e1\u0644\u0650\u064a", "word_translation": "We give respite", "word_transliteration": "num'li"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "to them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u065e", "