{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u064a\u064e\u0670\u0634\u064f\u0639\u064e\u064a\u06e1\u0628\u064f \u0623\u064e\u0635\u064e\u0644\u064e\u0648\u0670\u062a\u064f\u0643\u064e \u062a\u064e\u0623\u06e1\u0645\u064f\u0631\u064f\u0643\u064e \u0623\u064e\u0646 \u0646\u0651\u064e\u062a\u06e1\u0631\u064f\u0643\u064e \u0645\u064e\u0627 \u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f \u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0624\u064f\u0646\u064e\u0627\u0653 \u0623\u064e\u0648\u06e1 \u0623\u064e\u0646 \u0646\u0651\u064e\u0641\u06e1\u0639\u064e\u0644\u064e \u0641\u0650\u064a\u0653 \u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u0650\u0646\u064e\u0627 \u0645\u064e\u0627 \u0646\u064e\u0634\u064e\u0640\u0670\u0653\u0624\u064f\u0627\u0652\u06d6 \u0625\u0650\u0646\u0651\u064e\u0643\u064e \u0644\u064e\u0623\u064e\u0646\u062a\u064e \u0671\u0644\u06e1\u062d\u064e\u0644\u0650\u064a\u0645\u064f \u0671\u0644\u0631\u0651\u064e\u0634\u0650\u064a\u062f\u064f"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0634\u064f\u0639\u064e\u064a\u06e1\u0628\u064f", "word_translation": "O Shuaib!", "word_transliteration": "yashu'aybu"}, {"word_arabic_uthmani": "\u0623\u064e\u0635\u064e\u0644\u064e\u0648\u0670\u062a\u064f\u0643\u064e", "word_translation": "Does your prayer", "word_transliteration": "asalatuka"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u0645\u064f\u0631\u064f\u0643\u064e", "word_translation": "command you", "word_transliteration": "tamuruka"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u062a\u06e1\u0631\u064f\u0643\u064e", "word_translation": "we leave", "word_transliteration": "natruka"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0628\u064f\u062f\u064f", "word_translation": "worship", "word_transliteration": "ya'budu"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u0628\u064e\u0627\u0653\u0624\u064f\u0646\u064e\u0627\u0653", "word_translation": "our forefathers", "word_transliteration": "abauna"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "that", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u0646\u0651\u064e\u0641\u06e1\u0639\u064e\u0644\u064e", "word_translation": "we do", "word_transliteration": "naf 'ala"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0653", "word_translation": "concerning", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u06e1\u0648\u064e\u0670\u0644\u0650\u0646\u064e\u0627", "word_translation": "our wealth", "word_transliteration": "amwalina"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0646\u064e\u0634\u064e\u0640\u0670\u0653\u0624\u064f\u0627\u0652\u06d6", "word_translation": "we will", "word_transliteration": "nashau"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0643\u064e", "word_translation": "Indeed you", "word_transliteration": "innaka"}, {"word_arabic_uthmani": "\u0644\u064e\u0623\u064e\u0646\u062a\u064e", "word_translation": "surely you", "word_transliteration": "la-anta"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "(are) the forbearing", "word_transliteration": "al-halimu"}, {"word_arabic_uthmani": "\u0671\u0644\u0631\u0651\u064e\u0634\u0650\u064a\u062f\u064f", "word_translation": "the right-minded", "word_transliteration": "al-rashidu"}]}