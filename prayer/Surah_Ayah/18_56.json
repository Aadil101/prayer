{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u0627 \u0646\u064f\u0631\u06e1\u0633\u0650\u0644\u064f \u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u0650\u064a\u0646\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064f\u0628\u064e\u0634\u0651\u0650\u0631\u0650\u064a\u0646\u064e \u0648\u064e\u0645\u064f\u0646\u0630\u0650\u0631\u0650\u064a\u0646\u064e\u06da \u0648\u064e\u064a\u064f\u062c\u064e\u0670\u062f\u0650\u0644\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u0670\u0637\u0650\u0644\u0650 \u0644\u0650\u064a\u064f\u062f\u06e1\u062d\u0650\u0636\u064f\u0648\u0627\u0652 \u0628\u0650\u0647\u0650 \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064e\u06d6 \u0648\u064e\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0653\u0627\u0652 \u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u064a \u0648\u064e\u0645\u064e\u0627\u0653 \u0623\u064f\u0646\u0630\u0650\u0631\u064f\u0648\u0627\u0652 \u0647\u064f\u0632\u064f\u0648\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627", "word_translation": "And not", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0646\u064f\u0631\u06e1\u0633\u0650\u0644\u064f", "word_translation": "We send", "word_transliteration": "nur'silu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u0650\u064a\u0646\u064e", "word_translation": "the Messengers", "word_transliteration": "al-mur'salina"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "except", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u0645\u064f\u0628\u064e\u0634\u0651\u0650\u0631\u0650\u064a\u0646\u064e", "word_translation": "(as) bearers of glad tidings", "word_transliteration": "mubashirina"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064f\u0646\u0630\u0650\u0631\u0650\u064a\u0646\u064e\u06da", "word_translation": "and (as) warners", "word_transliteration": "wamundhirina"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u062c\u064e\u0670\u062f\u0650\u0644\u064f", "word_translation": "And dispute", "word_transliteration": "wayujadilu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "disbelieve", "word_transliteration": "kafaru"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0628\u064e\u0670\u0637\u0650\u0644\u0650", "word_translation": "with falsehood", "word_transliteration": "bil-batili"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064f\u062f\u06e1\u062d\u0650\u0636\u064f\u0648\u0627\u0652", "word_translation": "to refute", "word_transliteration": "liyud'hidu"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650", "word_translation": "thereby", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064e\u06d6", "word_translation": "the truth", "word_transliteration": "al-haqa"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u062a\u0651\u064e\u062e\u064e\u0630\u064f\u0648\u0653\u0627\u0652", "word_translation": "And they take", "word_transliteration": "wa-ittakhadhu"}, {"word_arabic_uthmani": "\u0621\u064e\u0627\u064a\u064e\u0670\u062a\u0650\u064a", "word_translation": "My Verses", "word_transliteration": "ayati"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0627\u0653", "word_translation": "and what", "word_transliteration": "wama"}, {"word_arabic_uthmani": "\u0623\u064f\u0646\u0630\u0650\u0631\u064f\u0648\u0627\u0652", "word_translation": "they are warned", "word_transliteration": "undhiru"}, {"word_arabic_uthmani": "\u0647\u064f\u0632\u064f\u0648\u0657\u0627", "word_translation": "(in) ridicule", "word_transliteration": "huzuwan"}]}