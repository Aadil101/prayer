{"ayahs": {"ayah_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e\u06d6 \u0648\u064e\u0645\u064e\u0646 \u064a\u064f\u0639\u064e\u0638\u0651\u0650\u0645\u06e1 \u0634\u064e\u0639\u064e\u0640\u0670\u0653\u0626\u0650\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064e\u0627 \u0645\u0650\u0646 \u062a\u064e\u0642\u06e1\u0648\u064e\u0649 \u0671\u0644\u06e1\u0642\u064f\u0644\u064f\u0648\u0628\u0650"}, "words": [{"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e\u06d6", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u0646", "word_translation": "and whoever", "word_transliteration": "waman"}, {"word_arabic_uthmani": "\u064a\u064f\u0639\u064e\u0638\u0651\u0650\u0645\u06e1", "word_translation": "honors", "word_transliteration": "yu'azzim"}, {"word_arabic_uthmani": "\u0634\u064e\u0639\u064e\u0640\u0670\u0653\u0626\u0650\u0631\u064e", "word_translation": "(the) Symbols", "word_transliteration": "sha'aira"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064e\u0627", "word_translation": "then indeed it", "word_transliteration": "fa-innaha"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "(is) from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u062a\u064e\u0642\u06e1\u0648\u064e\u0649", "word_translation": "(the) piety", "word_transliteration": "taqwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064f\u0644\u064f\u0648\u0628\u0650", "word_translation": "(of) the hearts", "word_transliteration": "al-qulubi"}]}