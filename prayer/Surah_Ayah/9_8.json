{"ayahs": {"ayah_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e \u0648\u064e\u0625\u0650\u0646 \u064a\u064e\u0638\u06e1\u0647\u064e\u0631\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0631\u06e1\u0642\u064f\u0628\u064f\u0648\u0627\u0652 \u0641\u0650\u064a\u0643\u064f\u0645\u06e1 \u0625\u0650\u0644\u0651\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u0630\u0650\u0645\u0651\u064e\u0629\u0657\u06da \u064a\u064f\u0631\u06e1\u0636\u064f\u0648\u0646\u064e\u0643\u064f\u0645 \u0628\u0650\u0623\u064e\u0641\u06e1\u0648\u064e\u0670\u0647\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u062a\u064e\u0623\u06e1\u0628\u064e\u0649\u0670 \u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645\u06e1 \u0648\u064e\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645\u06e1 \u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0643\u064e\u064a\u06e1\u0641\u064e", "word_translation": "How", "word_transliteration": "kayfa"}, {"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0646", "word_translation": "while, if", "word_transliteration": "wa-in"}, {"word_arabic_uthmani": "\u064a\u064e\u0638\u06e1\u0647\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "they gain dominance", "word_transliteration": "yazharu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "over you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "they do not regard (the ties)", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u0642\u064f\u0628\u064f\u0648\u0627\u0652", "word_translation": "they do not regard (the ties)", "word_transliteration": "yarqubu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0643\u064f\u0645\u06e1", "word_translation": "with you", "word_transliteration": "fikum"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u0657\u0627", "word_translation": "(of) kinship", "word_transliteration": "illan"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0630\u0650\u0645\u0651\u064e\u0629\u0657\u06da", "word_translation": "covenant of protection", "word_transliteration": "dhimmatan"}, {"word_arabic_uthmani": "\u064a\u064f\u0631\u06e1\u0636\u064f\u0648\u0646\u064e\u0643\u064f\u0645", "word_translation": "They satisfy you", "word_transliteration": "yur'dunakum"}, {"word_arabic_uthmani": "\u0628\u0650\u0623\u064e\u0641\u06e1\u0648\u064e\u0670\u0647\u0650\u0647\u0650\u0645\u06e1", "word_translation": "with their mouths", "word_transliteration": "bi-afwahihim"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0623\u06e1\u0628\u064e\u0649\u0670", "word_translation": "but refuse", "word_transliteration": "wataba"}, {"word_arabic_uthmani": "\u0642\u064f\u0644\u064f\u0648\u0628\u064f\u0647\u064f\u0645\u06e1", "word_translation": "their hearts", "word_transliteration": "qulubuhum"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064f\u0647\u064f\u0645\u06e1", "word_translation": "and most of them", "word_transliteration": "wa-aktharuhum"}, {"word_arabic_uthmani": "\u0641\u064e\u0670\u0633\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "(are) defiantly disobedient", "word_transliteration": "fasiquna"}]}