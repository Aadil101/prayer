{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627 \u0645\u064e\u0646\u06e1 \u0623\u064f\u0648\u062a\u0650\u064a\u064e \u0643\u0650\u062a\u064e\u0670\u0628\u064e\u0647\u064f\u06e5 \u0648\u064e\u0631\u064e\u0627\u0653\u0621\u064e \u0638\u064e\u0647\u06e1\u0631\u0650\u0647\u0650\u06e6"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0623\u064e\u0645\u0651\u064e\u0627", "word_translation": "But as for", "word_transliteration": "wa-amma"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u06e1", "word_translation": "(him) who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u062a\u0650\u064a\u064e", "word_translation": "is given", "word_transliteration": "utiya"}, {"word_arabic_uthmani": "\u0643\u0650\u062a\u064e\u0670\u0628\u064e\u0647\u064f\u06e5", "word_translation": "his record", "word_transliteration": "kitabahu"}, {"word_arabic_uthmani": "\u0648\u064e\u0631\u064e\u0627\u0653\u0621\u064e", "word_translation": "behind", "word_transliteration": "waraa"}, {"word_arabic_uthmani": "\u0638\u064e\u0647\u06e1\u0631\u0650\u0647\u0650\u06e6", "word_translation": "his back", "word_transliteration": "zahrihi"}]}