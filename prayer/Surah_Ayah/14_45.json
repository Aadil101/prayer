{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0633\u064e\u0643\u064e\u0646\u062a\u064f\u0645\u06e1 \u0641\u0650\u064a \u0645\u064e\u0633\u064e\u0670\u0643\u0650\u0646\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0638\u064e\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652 \u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0647\u064f\u0645\u06e1 \u0648\u064e\u062a\u064e\u0628\u064e\u064a\u0651\u064e\u0646\u064e \u0644\u064e\u0643\u064f\u0645\u06e1 \u0643\u064e\u064a\u06e1\u0641\u064e \u0641\u064e\u0639\u064e\u0644\u06e1\u0646\u064e\u0627 \u0628\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0636\u064e\u0631\u064e\u0628\u06e1\u0646\u064e\u0627 \u0644\u064e\u0643\u064f\u0645\u064f \u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u062b\u064e\u0627\u0644\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0633\u064e\u0643\u064e\u0646\u062a\u064f\u0645\u06e1", "word_translation": "And you dwelt", "word_transliteration": "wasakantum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0645\u064e\u0633\u064e\u0670\u0643\u0650\u0646\u0650", "word_translation": "the dwellings", "word_transliteration": "masakini"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "(of) those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0638\u064e\u0644\u064e\u0645\u064f\u0648\u0653\u0627\u0652", "word_translation": "wronged", "word_transliteration": "zalamu"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0641\u064f\u0633\u064e\u0647\u064f\u0645\u06e1", "word_translation": "themselves", "word_transliteration": "anfusahum"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u0628\u064e\u064a\u0651\u064e\u0646\u064e", "word_translation": "and it had become clear", "word_transliteration": "watabayyana"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "to you", "word_transliteration": "lakum"}, {"word_ar