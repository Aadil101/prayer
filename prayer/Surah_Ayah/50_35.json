{"ayahs": {"ayah_uthmani": "\u0644\u064e\u0647\u064f\u0645 \u0645\u0651\u064e\u0627 \u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u0648\u0646\u064e \u0641\u0650\u064a\u0647\u064e\u0627 \u0648\u064e\u0644\u064e\u062f\u064e\u064a\u06e1\u0646\u064e\u0627 \u0645\u064e\u0632\u0650\u064a\u062f\u065e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645", "word_translation": "For them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "whatever", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u064a\u064e\u0634\u064e\u0627\u0653\u0621\u064f\u0648\u0646\u064e", "word_translation": "they wish", "word_transliteration": "yashauna"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "therein", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u062f\u064e\u064a\u06e1\u0646\u064e\u0627", "word_translation": "and with Us", "word_transliteration": "waladayna"}, {"word_arabic_uthmani": "\u0645\u064e\u0632\u0650\u064a\u062f\u065e", "word_translation": "(is) more", "word_transliteration": "mazidun"}]}