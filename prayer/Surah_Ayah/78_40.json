{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653 \u0623\u064e\u0646\u0630\u064e\u0631\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u0657\u0627 \u0642\u064e\u0631\u0650\u064a\u0628\u0657\u0627 \u064a\u064e\u0648\u06e1\u0645\u064e \u064a\u064e\u0646\u0638\u064f\u0631\u064f \u0671\u0644\u06e1\u0645\u064e\u0631\u06e1\u0621\u064f \u0645\u064e\u0627 \u0642\u064e\u062f\u0651\u064e\u0645\u064e\u062a\u06e1 \u064a\u064e\u062f\u064e\u0627\u0647\u064f \u0648\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f \u0671\u0644\u06e1\u0643\u064e\u0627\u0641\u0650\u0631\u064f \u064a\u064e\u0670\u0644\u064e\u064a\u06e1\u062a\u064e\u0646\u0650\u064a \u0643\u064f\u0646\u062a\u064f \u062a\u064f\u0631\u064e\u0670\u0628\u064e\u06e2\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0627\u0653", "word_translation": "Indeed We", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0630\u064e\u0631\u06e1\u0646\u064e\u0670\u0643\u064f\u0645\u06e1", "word_translation": "[We] have warned you", "word_transliteration": "andharnakum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u0657\u0627", "word_translation": "(of) a punishment", "word_transliteration": "adhaban"}, {"word_arabic_uthmani": "\u0642\u064e\u0631\u0650\u064a\u0628\u0657\u0627", "word_translation": "near", "word_transliteration": "qariban"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064e", "word_translation": "(the) Day", "word_transliteration": "yawma"}, {"word_arabic_uthmani": "\u064a\u064e\u0646\u0638\u064f\u0631\u064f", "word_translation": "will see", "word_transliteration": "yanzuru"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0631\u06e1\u0621\u064f", "word_translation": "the man", "word_transliteration": "al-maru"}, {"word_arabic_uthmani": "\u0645\u064e\u0627", "word_translation": "what", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0651\u064e\u0645\u064e\u062a\u06e1", "word_translation": "have sent forth", "word_transliteration": "qaddamat"}, {"word_arabic_uthmani": "\u064a\u064e\u062f\u064e\u0627\u0647\u064f", "word_translation": "his hands", "word_transliteration": "yadahu"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f", "word_translation": "and will say", "word_transliteration": "wayaqulu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0627\u0641\u0650\u0631\u064f", "word_translation": "the disbeliever", "word_transliteration": "al-kafiru"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0644\u064e\u064a\u06e1\u062a\u064e\u0646\u0650\u064a", "word_translation": "O I wish", "word_transliteration": "yalaytani"}, {"word_arabic_uthmani": "\u0643\u064f\u0646\u062a\u064f", "word_translation": "I were", "word_transliteration": "kuntu"}, {"word_arabic_uthmani": "\u062a\u064f\u0631\u064e\u0670\u0628\u064e\u06e2\u0627", "word_translation": "dust!", "word_transliteration": "turaban"}]}