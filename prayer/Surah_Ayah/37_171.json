{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0633\u064e\u0628\u064e\u0642\u064e\u062a\u06e1 \u0643\u064e\u0644\u0650\u0645\u064e\u062a\u064f\u0646\u064e\u0627 \u0644\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0646\u064e\u0627 \u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1", "word_translation": "And verily", "word_transliteration": "walaqad"}, {"word_arabic_uthmani": "\u0633\u064e\u0628\u064e\u0642\u064e\u062a\u06e1", "word_translation": "has preceded", "word_transliteration": "sabaqat"}, {"word_arabic_uthmani": "\u0643\u064e\u0644\u0650\u0645\u064e\u062a\u064f\u0646\u064e\u0627", "word_translation": "Our Word", "word_transliteration": "kalimatuna"}, {"word_arabic_uthmani": "\u0644\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0646\u064e\u0627", "word_translation": "for Our slaves", "word_transliteration": "li'ibadina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064f\u0631\u06e1\u0633\u064e\u0644\u0650\u064a\u0646\u064e", "word_translation": "the Messengers", "word_transliteration": "al-mur'salina"}]}