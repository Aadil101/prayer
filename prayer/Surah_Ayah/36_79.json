{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u064a\u064f\u062d\u06e1\u064a\u0650\u064a\u0647\u064e\u0627 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653 \u0623\u064e\u0646\u0634\u064e\u0623\u064e\u0647\u064e\u0627\u0653 \u0623\u064e\u0648\u0651\u064e\u0644\u064e \u0645\u064e\u0631\u0651\u064e\u0629\u0656\u06d6 \u0648\u064e\u0647\u064f\u0648\u064e \u0628\u0650\u0643\u064f\u0644\u0651\u0650 \u062e\u064e\u0644\u06e1\u0642\u064d \u0639\u064e\u0644\u0650\u064a\u0645\u064c"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u064a\u064f\u062d\u06e1\u064a\u0650\u064a\u0647\u064e\u0627", "word_translation": "He will give them life", "word_transliteration": "yuh'yiha"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0653", "word_translation": "Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u0623\u064e\u0646\u0634\u064e\u0623\u064e\u0647\u064e\u0627\u0653", "word_translation": "produced them", "word_transliteration": "ansha-aha"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u0651\u064e\u0644\u064e", "word_translation": "(the) first", "word_transliteration": "awwala"}, {"word_arabic_uthmani": "\u0645\u064e\u0631\u0651\u064e\u0629\u0656\u06d6", "word_translation": "time", "word_transliteration": "marratin"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064f\u0644\u0651\u0650", "word_translation": "(is) of every", "word_transliteration": "bikulli"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u06e1\u0642\u064d", "word_translation": "creation", "word_transliteration": "khalqin"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u0650\u064a\u0645\u064c", "word_translation": "All-Knower", "word_transliteration": "alimun"}]}