{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0642\u064f\u0644 \u0631\u0651\u064e\u0628\u0651\u0650 \u0623\u064e\u0639\u064f\u0648\u0630\u064f \u0628\u0650\u0643\u064e \u0645\u0650\u0646\u06e1 \u0647\u064e\u0645\u064e\u0632\u064e\u0670\u062a\u0650 \u0671\u0644\u0634\u0651\u064e\u064a\u064e\u0670\u0637\u0650\u064a\u0646\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0642\u064f\u0644", "word_translation": "And say", "word_transliteration": "waqul"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650", "word_translation": "My Lord!", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u064f\u0648\u0630\u064f", "word_translation": "I seek refuge", "word_transliteration": "a'udhu"}, {"word_arabic_uthmani": "\u0628\u0650\u0643\u064e", "word_translation": "in You", "word_transliteration": "bika"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1", "word_translation": "from", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0647\u064e\u0645\u064e\u0632\u064e\u0670\u062a\u0650", "word_translation": "(the) suggestions", "word_transliteration": "hamazati"}, {"word_arabic_uthmani": "\u0671\u0644\u0634\u0651\u064e\u064a\u064e\u0670\u0637\u0650\u064a\u0646\u0650", "word_translation": "(of) the evil ones", "word_transliteration": "al-shayatini"}]}