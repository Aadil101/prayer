{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0646\u064f\u0641\u0650\u062e\u064e \u0641\u0650\u064a \u0671\u0644\u0635\u0651\u064f\u0648\u0631\u0650\u06da \u0630\u064e\u0670\u0644\u0650\u0643\u064e \u064a\u064e\u0648\u06e1\u0645\u064f \u0671\u0644\u06e1\u0648\u064e\u0639\u0650\u064a\u062f\u0650"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0646\u064f\u0641\u0650\u062e\u064e", "word_translation": "And will be blown", "word_transliteration": "wanufikha"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "[in]", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0671\u0644\u0635\u0651\u064f\u0648\u0631\u0650\u06da", "word_translation": "the trumpet", "word_transliteration": "al-suri"}, {"word_arabic_uthmani": "\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "That", "word_transliteration": "dhalika"}, {"word_arabic_uthmani": "\u064a\u064e\u0648\u06e1\u0645\u064f", "word_translation": "(is the) Day", "word_transliteration": "yawmu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0648\u064e\u0639\u0650\u064a\u062f\u0650", "word_translation": "(of) the Warning", "word_transliteration": "al-wa'idi"}]}