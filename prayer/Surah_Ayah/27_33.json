{"ayahs": {"ayah_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652 \u0646\u064e\u062d\u06e1\u0646\u064f \u0623\u064f\u0648\u0652\u0644\u064f\u0648\u0627\u0652 \u0642\u064f\u0648\u0651\u064e\u0629\u0656 \u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064f\u0648\u0627\u0652 \u0628\u064e\u0623\u06e1\u0633\u0656 \u0634\u064e\u062f\u0650\u064a\u062f\u0656 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0631\u064f \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u0650 \u0641\u064e\u0671\u0646\u0638\u064f\u0631\u0650\u064a \u0645\u064e\u0627\u0630\u064e\u0627 \u062a\u064e\u0623\u06e1\u0645\u064f\u0631\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064f\u0648\u0627\u0652", "word_translation": "They said", "word_transliteration": "qalu"}, {"word_arabic_uthmani": "\u0646\u064e\u062d\u06e1\u0646\u064f", "word_translation": "We", "word_transliteration": "nahnu"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u0652\u0644\u064f\u0648\u0627\u0652", "word_translation": "(are) possessors", "word_transliteration": "ulu"}, {"word_arabic_uthmani": "\u0642\u064f\u0648\u0651\u064e\u0629\u0656", "word_translation": "(of) strength", "word_transliteration": "quwwatin"}, {"word_arabic_uthmani": "\u0648\u064e\u0623\u064f\u0648\u0652\u0644\u064f\u0648\u0627\u0652", "word_translation": "and possessors", "word_transliteration": "wa-ulu"}, {"word_arabic_uthmani": "\u0628\u064e\u0623\u06e1\u0633\u0656", "word_translation": "(of) might", "word_transliteration": "basin"}, {"word_arabic_uthmani": "\u0634\u064e\u062f\u0650\u064a\u062f\u0656", "word_translation": "great", "word_transliteration": "shadidin"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0631\u064f", "word_translation": "and the command", "word_transliteration": "wal-amru"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u0650", "word_translation": "(is) up to you", "word_transliteration": "ilayki"}, {"word_arabic_uthmani": "\u0641\u064e\u0671\u0646\u0638\u064f\u0631\u0650\u064a", "word_translation": "so look", "word_transliteration": "fa-unzuri"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0630\u064e\u0627", "word_translation": "what", "word_transliteration": "madha"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u0645\u064f\u0631\u0650\u064a\u0646\u064e", "word_translation": "you will command", "word_transliteration": "tamurina"}]}