{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0627 \u064a\u064f\u0624\u06e1\u0630\u064e\u0646\u064f \u0644\u064e\u0647\u064f\u0645\u06e1 \u0641\u064e\u064a\u064e\u0639\u06e1\u062a\u064e\u0630\u0650\u0631\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u064a\u064f\u0624\u06e1\u0630\u064e\u0646\u064f", "word_translation": "will it be permitted", "word_transliteration": "yu'dhanu"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for them", "word_transliteration": "lahum"}, {"word_arabic_uthmani": "\u0641\u064e\u064a\u064e\u0639\u06e1\u062a\u064e\u0630\u0650\u0631\u064f\u0648\u0646\u064e", "word_translation": "to make excuses", "word_transliteration": "faya'tadhiruna"}]}