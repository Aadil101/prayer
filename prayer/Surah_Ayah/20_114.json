{"ayahs": {"ayah_uthmani": "\u0641\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u06e1\u0645\u064e\u0644\u0650\u0643\u064f \u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f\u06d7 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0639\u06e1\u062c\u064e\u0644\u06e1 \u0628\u0650\u0671\u0644\u06e1\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u0650 \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650 \u0623\u064e\u0646 \u064a\u064f\u0642\u06e1\u0636\u064e\u0649\u0670\u0653 \u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e \u0648\u064e\u062d\u06e1\u064a\u064f\u0647\u064f\u06e5\u06d6 \u0648\u064e\u0642\u064f\u0644 \u0631\u0651\u064e\u0628\u0651\u0650 \u0632\u0650\u062f\u06e1\u0646\u0650\u064a \u0639\u0650\u0644\u06e1\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u062a\u064e\u0639\u064e\u0670\u0644\u064e\u0649", "word_translation": "So high (above all)", "word_transliteration": "fata'ala"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(is) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u0650\u0643\u064f", "word_translation": "the King", "word_transliteration": "al-maliku"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u064f\u06d7", "word_translation": "the True", "word_transliteration": "al-haqu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "And (do) not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u062c\u064e\u0644\u06e1", "word_translation": "hasten", "word_transliteration": "ta'jal"}, {"word_arabic_uthmani": "\u0628\u0650\u0671\u0644\u06e1\u0642\u064f\u0631\u06e1\u0621\u064e\u0627\u0646\u0650", "word_translation": "with the Quran", "word_transliteration": "bil-qur'ani"}, {"word_arabic_uthmani": "\u0645\u0650\u0646", "word_translation": "before", "word_transliteration": "min"}, {"word_arabic_uthmani": "\u0642\u064e\u0628\u06e1\u0644\u0650", "word_translation": "before", "word_transliteration": "qabli"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "[that]", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064f\u0642\u06e1\u0636\u064e\u0649\u0670\u0653", "word_translation": "is completed", "word_transliteration": "yuq'da"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u064a\u06e1\u0643\u064e", "word_translation": "to you", "word_transliteration": "ilayka"}, {"word_arabic_uthmani": "\u0648\u064e\u062d\u06e1\u064a\u064f\u0647\u064f\u06e5\u06d6", "word_translation": "its revelation", "word_transliteration": "wahyuhu"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064f\u0644", "word_translation": "and say", "word_transliteration": "waqul"}, {"word_arabic_uthmani": "\u0631\u0651\u064e\u0628\u0651\u0650", "word_translation": "My Lord", "word_transliteration": "rabbi"}, {"word_arabic_uthmani": "\u0632\u0650\u062f\u06e1\u0646\u0650\u064a", "word_translation": "Increase me", "word_transliteration": "zid'ni"}, {"word_arabic_uthmani": "\u0639\u0650\u0644\u06e1\u0645\u0657\u0627", "word_translation": "(in) knowledge", "word_transliteration": "il'man"}]}