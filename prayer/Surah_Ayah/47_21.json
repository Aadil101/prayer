{"ayahs": {"ayah_uthmani": "\u0637\u064e\u0627\u0639\u064e\u0629\u065e \u0648\u064e\u0642\u064e\u0648\u06e1\u0644\u065e \u0645\u0651\u064e\u0639\u06e1\u0631\u064f\u0648\u0641\u065e\u06da \u0641\u064e\u0625\u0650\u0630\u064e\u0627 \u0639\u064e\u0632\u064e\u0645\u064e \u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0631\u064f \u0641\u064e\u0644\u064e\u0648\u06e1 \u0635\u064e\u062f\u064e\u0642\u064f\u0648\u0627\u0652 \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0644\u064e\u0643\u064e\u0627\u0646\u064e \u062e\u064e\u064a\u06e1\u0631\u0657\u0627 \u0644\u0651\u064e\u0647\u064f\u0645\u06e1"}, "words": [{"word_arabic_uthmani": "\u0637\u064e\u0627\u0639\u064e\u0629\u065e", "word_translation": "(Is) obedience", "word_transliteration": "ta'atun"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0648\u06e1\u0644\u065e", "word_translation": "and a word", "word_transliteration": "waqawlun"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0639\u06e1\u0631\u064f\u0648\u0641\u065e\u06da", "word_translation": "kind", "word_transliteration": "ma'rufun"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0630\u064e\u0627", "word_translation": "And when", "word_transliteration": "fa-idha"}, {"word_arabic_uthmani": "\u0639\u064e\u0632\u064e\u0645\u064e", "word_translation": "(is) determined", "word_transliteration": "azama"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0631\u064f", "word_translation": "the matter", "word_transliteration": "al-amru"}, {"word_arabic_uthmani": "\u0641\u064e\u0644\u064e\u0648\u06e1", "word_translation": "then if", "word_transliteration": "falaw"}, {"word_arabic_uthmani": "\u0635\u064e\u062f\u064e\u0642\u064f\u0648\u0627\u0652", "word_translation": "they had been true", "word_transliteration": "sadaqu"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "(to) Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "surel