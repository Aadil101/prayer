{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1 \u062a\u064e\u0631\u064e \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0623\u064f\u0648\u062a\u064f\u0648\u0627\u0652 \u0646\u064e\u0635\u0650\u064a\u0628\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u064a\u064f\u062f\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e \u0625\u0650\u0644\u064e\u0649\u0670 \u0643\u0650\u062a\u064e\u0670\u0628\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u0644\u0650\u064a\u064e\u062d\u06e1\u0643\u064f\u0645\u064e \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1 \u062b\u064f\u0645\u0651\u064e \u064a\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0649\u0670 \u0641\u064e\u0631\u0650\u064a\u0642\u065e \u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0648\u064e\u0647\u064f\u0645 \u0645\u0651\u064f\u0639\u06e1\u0631\u0650\u0636\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Have not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e", "word_translation": "you seen", "word_transliteration": "tara"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "[to]", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0623\u064f\u0648\u062a\u064f\u0648\u0627\u0652", "word_translation": "were given", "word_transliteration": "utu"}, {"word_arabic_uthmani": "\u0646\u064e\u0635\u0650\u064a\u0628\u0657\u0627", "word_translation": "a portion", "word_transliteration": "nasiban"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "the Scripture", "word_transliteration": "al-kitabi"}, {"word_arabic_uthmani": "\u064a\u064f\u062f\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e", "word_translation": "They are invited", "word_transliteration": "yud'awna"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649\u0670", "word_translation": "to", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0643\u0650\u062a\u064e\u0670\u0628\u0650", "word_translation": "(the) Book", "word_transliteration": "kitabi"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "(of) Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u0644\u0650\u064a\u064e\u062d\u06e1\u0643\u064f\u0645\u064e", "word_translation": "that (it should) arbitrate", "word_transliteration": "liyahkuma"}, {"word_arabic_uthmani": "\u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u06e1", "word_translation": "between them", "word_transliteration": "baynahum"}, {"word_arabic_uthmani": "\u062b\u064f\u0645\u0651\u064e", "word_translation": "then", "word_transliteration": "thumma"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0649\u0670", "word_translation": "turns away", "word_transliteration": "yatawalla"}, {"word_arabic_uthmani": "\u0641\u064e\u0631\u0650\u064a\u0642\u065e", "word_translation": "a party", "word_transliteration": "fariqun"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "of them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645", "word_translation": "and they (are)", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u0639\u06e1\u0631\u0650\u0636\u064f\u0648\u0646\u064e", "word_translation": "those who are averse", "word_transliteration": "mu'riduna"}]}