{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u0645\u0651\u064e \u0641\u064e\u0627\u0637\u0650\u0631\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650 \u0639\u064e\u0670\u0644\u0650\u0645\u064e \u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650 \u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0647\u064e\u0670\u062f\u064e\u0629\u0650 \u0623\u064e\u0646\u062a\u064e \u062a\u064e\u062d\u06e1\u0643\u064f\u0645\u064f \u0628\u064e\u064a\u06e1\u0646\u064e \u0639\u0650\u0628\u064e\u0627\u062f\u0650\u0643\u064e \u0641\u0650\u064a \u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0641\u0650\u064a\u0647\u0650 \u064a\u064e\u062e\u06e1\u062a\u064e\u0644\u0650\u0641\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u0650", "word_translation": "Say", "word_transliteration": "quli"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f\u0645\u0651\u064e", "word_translation": "O Allah!", "word_transliteration": "al-lahuma"}, {"word_arabic_uthmani": "\u0641\u064e\u0627\u0637\u0650\u0631\u064e", "word_translation": "Creator", "word_transliteration": "fatira"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "(of) the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u0650", "word_translation": "and the earth", "word_transliteration": "wal-ardi"}, {"word_arabic_uthmani": "\u0639\u064e\u0670\u0644\u0650\u0645\u064e", "word_translation": "Knower", "word_transliteration": "alima"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u063a\u064e\u064a\u06e1\u0628\u0650", "word_translation": "(of) the unseen", "word_transliteration": "al-ghaybi"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0634\u0651\u064e\u0647\u064e