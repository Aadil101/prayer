{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0645\u064e\u0646 \u064a\u064e\u0623\u06e1\u062a\u0650 \u0631\u064e\u0628\u0651\u064e\u0647\u064f\u06e5 \u0645\u064f\u062c\u06e1\u0631\u0650\u0645\u0657\u0627 \u0641\u064e\u0625\u0650\u0646\u0651\u064e \u0644\u064e\u0647\u064f\u06e5 \u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e \u0644\u064e\u0627 \u064a\u064e\u0645\u064f\u0648\u062a\u064f \u0641\u0650\u064a\u0647\u064e\u0627 \u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u062d\u06e1\u064a\u064e\u0649\u0670"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5", "word_translation": "Indeed he", "word_transliteration": "innahu"}, {"word_arabic_uthmani": "\u0645\u064e\u0646", "word_translation": "who", "word_transliteration": "man"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062a\u0650", "word_translation": "comes", "word_transliteration": "yati"}, {"word_arabic_uthmani": "\u0631\u064e\u0628\u0651\u064e\u0647\u064f\u06e5", "word_translation": "(to) his Lord", "word_transliteration": "rabbahu"}, {"word_arabic_uthmani": "\u0645\u064f\u062c\u06e1\u0631\u0650\u0645\u0657\u0627", "word_translation": "(as) a criminal", "word_transliteration": "muj'riman"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e", "word_translation": "then indeed", "word_transliteration": "fa-inna"}, {"word_arabic_uthmani": "\u0644\u064e\u0647\u064f\u06e5", "word_translation": "for him", "word_transliteration": "lahu"}, {"word_arabic_uthmani": "\u062c\u064e\u0647\u064e\u0646\u0651\u064e\u0645\u064e", "word_translation": "(is) Hell", "word_transliteration": "jahannama"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "Not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u064f\u0648\u062a\u064f", "word_translation": "he will die", "word_transliteration": "yamutu"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "in it", "word_