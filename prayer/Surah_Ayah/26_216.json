{"ayahs": {"ayah_uthmani": "\u0641\u064e\u0625\u0650\u0646\u06e1 \u0639\u064e\u0635\u064e\u0648\u06e1\u0643\u064e \u0641\u064e\u0642\u064f\u0644\u06e1 \u0625\u0650\u0646\u0651\u0650\u064a \u0628\u064e\u0631\u0650\u064a\u0653\u0621\u065e \u0645\u0651\u0650\u0645\u0651\u064e\u0627 \u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u06e1", "word_translation": "Then if", "word_transliteration": "fa-in"}, {"word_arabic_uthmani": "\u0639\u064e\u0635\u064e\u0648\u06e1\u0643\u064e", "word_translation": "they disobey you", "word_transliteration": "asawka"}, {"word_arabic_uthmani": "\u0641\u064e\u0642\u064f\u0644\u06e1", "word_translation": "then say", "word_transliteration": "faqul"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u0650\u064a", "word_translation": "Indeed I am", "word_transliteration": "inni"}, {"word_arabic_uthmani": "\u0628\u064e\u0631\u0650\u064a\u0653\u0621\u065e", "word_translation": "innocent", "word_transliteration": "barion"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0645\u0651\u064e\u0627", "word_translation": "of what", "word_transliteration": "mimma"}, {"word_arabic_uthmani": "\u062a\u064e\u0639\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e", "word_translation": "you do", "word_transliteration": "ta'maluna"}]}