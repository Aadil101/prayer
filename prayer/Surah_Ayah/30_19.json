{"ayahs": {"ayah_uthmani": "\u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064f \u0671\u0644\u06e1\u062d\u064e\u064a\u0651\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064e\u064a\u0651\u0650\u062a\u0650 \u0648\u064e\u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064f \u0671\u0644\u06e1\u0645\u064e\u064a\u0651\u0650\u062a\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u062d\u064e\u064a\u0651\u0650 \u0648\u064e\u064a\u064f\u062d\u06e1\u064a\u0650 \u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0628\u064e\u0639\u06e1\u062f\u064e \u0645\u064e\u0648\u06e1\u062a\u0650\u0647\u064e\u0627\u06da \u0648\u064e\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u062a\u064f\u062e\u06e1\u0631\u064e\u062c\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064f", "word_translation": "He brings forth", "word_transliteration": "yukh'riju"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u064a\u0651\u064e", "word_translation": "the living", "word_transliteration": "al-haya"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u064a\u0651\u0650\u062a\u0650", "word_translation": "the dead", "word_transliteration": "al-mayiti"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u062e\u06e1\u0631\u0650\u062c\u064f", "word_translation": "and He brings forth", "word_transliteration": "wayukh'riju"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u064a\u0651\u0650\u062a\u064e", "word_translation": "the dead", "word_transliteration": "al-mayita"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062d\u064e\u064a\u0651\u0650", "word_translation": "the living", "word_transliteration": "al-hayi"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u062d\u06e1\u064a\u0650", "word_translation": "and He gives life", "word_transliteration": "wayuh'yi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "(to) the earth", "word_transliteration": "al-arda"}, {"word_arabic_uthmani": "\u0628\u064e\u0639\u06e1\u062f\u064e", "word_translation": "after", "word_transliteration": "ba'da"}, {"word_arabic_uthmani": "\u0645\u064e\u0648\u06e1\u062a\u0650\u0647\u064e\u0627\u06da", "word_translation": "its death", "word_transliteration": "mawtiha"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "and thus", "word_transliteration": "wakadhalika"}, {"word_arabic_uthmani": "\u062a\u064f\u062e\u06e1\u0631\u064e\u062c\u064f\u0648\u0646\u064e", "word_translation": "you will be brought forth", "word_transliteration": "tukh'rajuna"}]}