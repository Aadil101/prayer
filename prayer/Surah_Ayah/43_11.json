{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a \u0646\u064e\u0632\u0651\u064e\u0644\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650 \u0645\u064e\u0627\u0653\u0621\u064e\u06e2 \u0628\u0650\u0642\u064e\u062f\u064e\u0631\u0656 \u0641\u064e\u0623\u064e\u0646\u0634\u064e\u0631\u06e1\u0646\u064e\u0627 \u0628\u0650\u0647\u0650\u06e6 \u0628\u064e\u0644\u06e1\u062f\u064e\u0629\u0657 \u0645\u0651\u064e\u064a\u06e1\u062a\u0657\u0627\u06da \u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e \u062a\u064f\u062e\u06e1\u0631\u064e\u062c\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "And the One Who", "word_transliteration": "wa-alladhi"}, {"word_arabic_uthmani": "\u0646\u064e\u0632\u0651\u064e\u0644\u064e", "word_translation": "sends down", "word_transliteration": "nazzala"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "from", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u0650", "word_translation": "the sky", "word_transliteration": "al-samai"}, {"word_arabic_uthmani": "\u0645\u064e\u0627\u0653\u0621\u064e\u06e2", "word_translation": "water", "word_transliteration": "maan"}, {"word_arabic_uthmani": "\u0628\u0650\u0642\u064e\u062f\u064e\u0631\u0656", "word_translation": "in (due) measure", "word_transliteration": "biqadarin"}, {"word_arabic_uthmani": "\u0641\u064e\u0623\u064e\u0646\u0634\u064e\u0631\u06e1\u0646\u064e\u0627", "word_translation": "then We revive", "word_transliteration": "fa-ansharna"}, {"word_arabic_uthmani": "\u0628\u0650\u0647\u0650\u06e6", "word_translation": "with it", "word_transliteration": "bihi"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u06e1\u062f\u064e\u0629\u0657", "word_translation": "a land", "word_transliteration": "baldatan"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u064a\u06e1\u062a\u0657\u0627\u06da", "word_translation": "dead", "word_transliteration": "maytan"}, {"word_arabic_uthmani": "\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e", "word_translation": "thus", "word_transliteration": "kadhalika"}, {"word_arabic_uthmani": "\u062a\u064f\u062e\u06e1\u0631\u064e\u062c\u064f\u0648\u0646\u064e", "word_translation": "you will be brought forth", "word_transliteration": "tukh'rajuna"}]}