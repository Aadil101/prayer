{"ayahs": {"ayah_uthmani": "\u0644\u064e\u064a\u06e1\u0633\u064e \u0644\u064e\u0643\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0631\u0650 \u0634\u064e\u064a\u06e1\u0621\u064c \u0623\u064e\u0648\u06e1 \u064a\u064e\u062a\u064f\u0648\u0628\u064e \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1 \u0623\u064e\u0648\u06e1 \u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064e\u0647\u064f\u0645\u06e1 \u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0638\u064e\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "Not", "word_transliteration": "laysa"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064e", "word_translation": "for you", "word_transliteration": "laka"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u064e", "word_translation": "of", "word_transliteration": "mina"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0623\u064e\u0645\u06e1\u0631\u0650", "word_translation": "the decision", "word_transliteration": "al-amri"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u064c", "word_translation": "(of) anything", "word_transliteration": "shayon"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "whether", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u064a\u064e\u062a\u064f\u0648\u0628\u064e", "word_translation": "He turns", "word_transliteration": "yatuba"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u06e1", "word_translation": "to them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u064e\u0647\u064f\u0645\u06e1", "word_translation": "punishes them", "word_transliteration": "yu'adhibahum"}, {"word_arabic_uthmani": "\u0641\u064e\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "for indeed they", "word_transliteration": "fa-innahum"}, {"word_arabic_uthmani": "\u0638\u064e\u0670\u0644\u0650\u0645\u064f\u0648\u0646\u064e", "word_translation": "(are) wrongdoers", "word_transliteration": "zalimuna"}]}