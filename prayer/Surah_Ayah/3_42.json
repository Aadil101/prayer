{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1 \u0642\u064e\u0627\u0644\u064e\u062a\u0650 \u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f \u064a\u064e\u0670\u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064f \u0625\u0650\u0646\u0651\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064e \u0671\u0635\u06e1\u0637\u064e\u0641\u064e\u0649\u0670\u0643\u0650 \u0648\u064e\u0637\u064e\u0647\u0651\u064e\u0631\u064e\u0643\u0650 \u0648\u064e\u0671\u0635\u06e1\u0637\u064e\u0641\u064e\u0649\u0670\u0643\u0650 \u0639\u064e\u0644\u064e\u0649\u0670 \u0646\u0650\u0633\u064e\u0627\u0653\u0621\u0650 \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0625\u0650\u0630\u06e1", "word_translation": "And when", "word_transliteration": "wa-idh"}, {"word_arabic_uthmani": "\u0642\u064e\u0627\u0644\u064e\u062a\u0650", "word_translation": "said", "word_transliteration": "qalati"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0645\u064e\u0644\u064e\u0640\u0670\u0653\u0626\u0650\u0643\u064e\u0629\u064f", "word_translation": "the Angels", "word_transliteration": "al-malaikatu"}, {"word_arabic_uthmani": "\u064a\u064e\u0670\u0645\u064e\u0631\u06e1\u064a\u064e\u0645\u064f", "word_translation": "O Maryam!", "word_transliteration": "yamaryamu"}, {"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e", "word_translation": "Indeed", "word_transliteration": "inna"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064e", "word_translation": "Allah", "word_transliteration": "al-laha"}, {"word_arabic_uthmani": "\u0671\u0635\u06e1\u0637\u064e\u0641\u064e\u0649\u0670\u0643\u0650", "word_translation": "(has) chosen you", "word_transliteration": "is'tafaki"}, {"word_arabic_uthmani": "\u0648\u064e\u0637\u064e\u0647\u0651\u064e\u0631\u064e\u0643\u0650", "word_translation": "and purified you", "word_transliteration": "watahharaki"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0635\u06e1\u0637\u064e\u0641\u064e\u0649\u0670\u0643\u0650", "word_translation": "and chosen you", "word_transliteration": "wa-is'tafaki"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "over", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0646\u0650\u0633\u064e\u0627\u0653\u0621\u0650", "word_translation": "(the) women", "word_transliteration": "nisai"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e", "word_translation": "(of) the worlds", "word_transliteration": "al-'alamina"}]}