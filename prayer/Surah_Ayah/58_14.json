{"ayahs": {"ayah_uthmani": "\u06de\u0623\u064e\u0644\u064e\u0645\u06e1 \u062a\u064e\u0631\u064e \u0625\u0650\u0644\u064e\u0649 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0648\u06e1\u0627\u0652 \u0642\u064e\u0648\u06e1\u0645\u064b\u0627 \u063a\u064e\u0636\u0650\u0628\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645 \u0645\u0651\u064e\u0627 \u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1 \u0648\u064e\u064a\u064e\u062d\u06e1\u0644\u0650\u0641\u064f\u0648\u0646\u064e \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u06e1\u0643\u064e\u0630\u0650\u0628\u0650 \u0648\u064e\u0647\u064f\u0645\u06e1 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u06de\u0623\u064e\u0644\u064e\u0645\u06e1", "word_translation": "Do not", "word_transliteration": "alam"}, {"word_arabic_uthmani": "\u062a\u064e\u0631\u064e", "word_translation": "you see", "word_transliteration": "tara"}, {"word_arabic_uthmani": "\u0625\u0650\u0644\u064e\u0649", "word_translation": "[to]", "word_transliteration": "ila"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u062a\u064e\u0648\u064e\u0644\u0651\u064e\u0648\u06e1\u0627\u0652", "word_translation": "take as allies", "word_transliteration": "tawallaw"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064b\u0627", "word_translation": "a people", "word_transliteration": "qawman"}, {"word_arabic_uthmani": "\u063a\u064e\u0636\u0650\u0628\u064e", "word_translation": "wrath", "word_transliteration": "ghadiba"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "(of) Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645", "word_translation": "(is) upon them", "word_transliteration": "alayhim"}, {"word_arabic_uthmani": "\u0645\u0651\u064e\u0627", "word_translation": "They (are) not", "word_transliteration": "ma"}, {"word_arabic_uthmani": "\u0647\u064f\u0645", "word_translation": "They (are) not", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u0645\u0651\u0650\u0646\u0643\u064f\u0645\u06e1", "word_translation": "of you", "word_transliteration": "minkum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0645\u0650\u0646\u06e1\u0647\u064f\u0645\u06e1", "word_translation": "of them", "word_transliteration": "min'hum"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u062d\u06e1\u0644\u0650\u0641\u064f\u0648\u0646\u064e", "word_translation": "and they swear", "word_transliteration": "wayahlifuna"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649", "word_translation": "to", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0643\u064e\u0630\u0650\u0628\u0650", "word_translation": "the lie", "word_transliteration": "al-kadhibi"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0645\u06e1", "word_translation": "while they", "word_transliteration": "wahum"}, {"word_arabic_uthmani": "\u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e", "word_translation": "know", "word_transliteration": "ya'lamuna"}]}