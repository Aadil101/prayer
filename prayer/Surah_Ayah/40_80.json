{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0644\u064e\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a\u0647\u064e\u0627 \u0645\u064e\u0646\u064e\u0670\u0641\u0650\u0639\u064f \u0648\u064e\u0644\u0650\u062a\u064e\u0628\u06e1\u0644\u064f\u063a\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u064e\u0627 \u062d\u064e\u0627\u062c\u064e\u0629\u0657 \u0641\u0650\u064a \u0635\u064f\u062f\u064f\u0648\u0631\u0650\u0643\u064f\u0645\u06e1 \u0648\u064e\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u064e\u0627 \u0648\u064e\u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u06e1\u0641\u064f\u0644\u06e1\u0643\u0650 \u062a\u064f\u062d\u06e1\u0645\u064e\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "And for you", "word_transliteration": "walakum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a\u0647\u064e\u0627", "word_translation": "in them", "word_transliteration": "fiha"}, {"word_arabic_uthmani": "\u0645\u064e\u0646\u064e\u0670\u0641\u0650\u0639\u064f", "word_translation": "(are) benefits", "word_transliteration": "manafi'u"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u0650\u062a\u064e\u0628\u06e1\u0644\u064f\u063a\u064f\u0648\u0627\u0652", "word_translation": "and that you may reach", "word_transliteration": "walitablughu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u064e\u0627", "word_translation": "through them", "word_transliteration": "alayha"}, {"word_arabic_uthmani": "\u062d\u064e\u0627\u062c\u064e\u0629\u0657", "word_translation": "a need", "word_transliteration": "hajatan"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "(that is) in", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0635\u064f\u062f\u064f\u0648\u0631\u0650\u0643\u064f\u0645\u06e1", "word_translation": "your breasts", "word_transliteration": "sudurikum"}, {"word_arabic_uthmani": "\u0648\u064e\u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u064e\u0627", "word_tran