{"ayahs": {"ayah_uthmani": "\u0648\u064e\u0645\u064e\u063a\u064e\u0627\u0646\u0650\u0645\u064e \u0643\u064e\u062b\u0650\u064a\u0631\u064e\u0629\u0657 \u064a\u064e\u0623\u06e1\u062e\u064f\u0630\u064f\u0648\u0646\u064e\u0647\u064e\u0627\u06d7 \u0648\u064e\u0643\u064e\u0627\u0646\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0632\u0650\u064a\u0632\u064b\u0627 \u062d\u064e\u0643\u0650\u064a\u0645\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u0645\u064e\u063a\u064e\u0627\u0646\u0650\u0645\u064e", "word_translation": "And spoils of war", "word_transliteration": "wamaghanima"}, {"word_arabic_uthmani": "\u0643\u064e\u062b\u0650\u064a\u0631\u064e\u0629\u0657", "word_translation": "much", "word_transliteration": "kathiratan"}, {"word_arabic_uthmani": "\u064a\u064e\u0623\u06e1\u062e\u064f\u0630\u064f\u0648\u0646\u064e\u0647\u064e\u0627\u06d7", "word_translation": "that they will take", "word_transliteration": "yakhudhunaha"}, {"word_arabic_uthmani": "\u0648\u064e\u0643\u064e\u0627\u0646\u064e", "word_translation": "and is", "word_transliteration": "wakana"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0632\u0650\u064a\u0632\u064b\u0627", "word_translation": "All-Mighty", "word_transliteration": "azizan"}, {"word_arabic_uthmani": "\u062d\u064e\u0643\u0650\u064a\u0645\u0657\u0627", "word_translation": "All-Wise", "word_transliteration": "hakiman"}]}