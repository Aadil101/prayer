{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627 \u062a\u064e\u0646\u0641\u0650\u0631\u064f\u0648\u0627\u0652 \u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u06e1\u0643\u064f\u0645\u06e1 \u0639\u064e\u0630\u064e\u0627\u0628\u064b\u0627 \u0623\u064e\u0644\u0650\u064a\u0645\u0657\u0627 \u0648\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u0628\u06e1\u062f\u0650\u0644\u06e1 \u0642\u064e\u0648\u06e1\u0645\u064b\u0627 \u063a\u064e\u064a\u06e1\u0631\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u0644\u064e\u0627 \u062a\u064e\u0636\u064f\u0631\u0651\u064f\u0648\u0647\u064f \u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627\u06d7 \u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0639\u064e\u0644\u064e\u0649\u0670 \u0643\u064f\u0644\u0651\u0650 \u0634\u064e\u064a\u06e1\u0621\u0656 \u0642\u064e\u062f\u0650\u064a\u0631\u064c"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0644\u0651\u064e\u0627", "word_translation": "If not", "word_transliteration": "illa"}, {"word_arabic_uthmani": "\u062a\u064e\u0646\u0641\u0650\u0631\u064f\u0648\u0627\u0652", "word_translation": "you go forth", "word_transliteration": "tanfiru"}, {"word_arabic_uthmani": "\u064a\u064f\u0639\u064e\u0630\u0651\u0650\u0628\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "He will punish you", "word_transliteration": "yu'adhib'kum"}, {"word_arabic_uthmani": "\u0639\u064e\u0630\u064e\u0627\u0628\u064b\u0627", "word_translation": "(with) a painful punishment", "word_transliteration": "adhaban"}, {"word_arabic_uthmani": "\u0623\u064e\u0644\u0650\u064a\u0645\u0657\u0627", "word_translation": "(with) a painful punishment", "word_transliteration": "aliman"}, {"word_arabic_uthmani": "\u0648\u064e\u064a\u064e\u0633\u06e1\u062a\u064e\u0628\u06e1\u062f\u0650\u0644\u06e1", "word_translation": "and will replace you", "word_transliteration": "wayastabdil"}, {"word_arabic_uthmani": "\u0642\u064e\u0648\u06e1\u0645\u064b\u0627", "word_translation": "(with) a people", "word_transliteration": "qawman"}, {"word_arabic_uthmani": "\u063a\u064e\u064a\u06e1\u0631\u064e\u0643\u064f\u0645\u06e1", "word_translation": "other than you", "word_transliteration": "ghayrakum"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u062a\u064e\u0636\u064f\u0631\u0651\u064f\u0648\u0647\u064f", "word_translation": "you can harm Him", "word_transliteration": "tadurruhu"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627\u06d7", "word_translation": "(in) anything", "word_transliteration": "shayan"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah", "word_transliteration": "wal-lahu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670", "word_translation": "(is) on", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0643\u064f\u0644\u0651\u0650", "word_translation": "every", "word_transliteration": "kulli"}, {"word_arabic_uthmani": "\u0634\u064e\u064a\u06e1\u0621\u0656", "word_translation": "thing", "word_transliteration": "shayin"}, {"word_arabic_uthmani": "\u0642\u064e\u062f\u0650\u064a\u0631\u064c", "word_translation": "All-Powerful", "word_transliteration": "qadirun"}]}