{"ayahs": {"ayah_uthmani": "\u0671\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0627\u0652 \u064a\u064f\u0648\u0633\u064f\u0641\u064e \u0623\u064e\u0648\u0650 \u0671\u0637\u06e1\u0631\u064e\u062d\u064f\u0648\u0647\u064f \u0623\u064e\u0631\u06e1\u0636\u0657\u0627 \u064a\u064e\u062e\u06e1\u0644\u064f \u0644\u064e\u0643\u064f\u0645\u06e1 \u0648\u064e\u062c\u06e1\u0647\u064f \u0623\u064e\u0628\u0650\u064a\u0643\u064f\u0645\u06e1 \u0648\u064e\u062a\u064e\u0643\u064f\u0648\u0646\u064f\u0648\u0627\u0652 \u0645\u0650\u0646\u06e2 \u0628\u064e\u0639\u06e1\u062f\u0650\u0647\u0650\u06e6 \u0642\u064e\u0648\u06e1\u0645\u0657\u0627 \u0635\u064e\u0670\u0644\u0650\u062d\u0650\u064a\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0671\u0642\u06e1\u062a\u064f\u0644\u064f\u0648\u0627\u0652", "word_translation": "Kill", "word_transliteration": "uq'tulu"}, {"word_arabic_uthmani": "\u064a\u064f\u0648\u0633\u064f\u0641\u064e", "word_translation": "Yusuf", "word_transliteration": "yusufa"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u0650", "word_translation": "or", "word_transliteration": "awi"}, {"word_arabic_uthmani": "\u0671\u0637\u06e1\u0631\u064e\u062d\u064f\u0648\u0647\u064f", "word_translation": "cast him", "word_transliteration": "it'rahuhu"}, {"word_arabic_uthmani": "\u0623\u064e\u0631\u06e1\u0636\u0657\u0627", "word_translation": "(to) a land", "word_transliteration": "ardan"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0644\u064f", "word_translation": "so will be free", "word_transliteration": "yakhlu"}, {"word_arabic_uthmani": "\u0644\u064e\u0643\u064f\u0645\u06e1", "word_translation": "for you", "word_transliteration": "lakum"}, {"word_arabic_uthmani": "\u0648\u064e\u062c\u06e1\u0647\u064f", "word_translation": "(the) face", "word_transliteration": "wajhu"}, {"word_arabic_uthmani": "\u0623\u064e\u0628\u0650\u064a\u0643\u064f\u0645\u06e1", "word_translation": "(of) your father", "word_transliteration": "abikum"}, {"word_arabic_uthmani": "\u0648\u064e\u062a\u064e\u064