{"ayahs": {"ayah_uthmani": "\u063a\u064e\u0627\u0641\u0650\u0631\u0650 \u0671\u0644\u0630\u0651\u064e\u0646\u06e2\u0628\u0650 \u0648\u064e\u0642\u064e\u0627\u0628\u0650\u0644\u0650 \u0671\u0644\u062a\u0651\u064e\u0648\u06e1\u0628\u0650 \u0634\u064e\u062f\u0650\u064a\u062f\u0650 \u0671\u0644\u06e1\u0639\u0650\u0642\u064e\u0627\u0628\u0650 \u0630\u0650\u064a \u0671\u0644\u0637\u0651\u064e\u0648\u06e1\u0644\u0650\u06d6 \u0644\u064e\u0627\u0653 \u0625\u0650\u0644\u064e\u0670\u0647\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0647\u064f\u0648\u064e\u06d6 \u0625\u0650\u0644\u064e\u064a\u06e1\u0647\u0650 \u0671\u0644\u06e1\u0645\u064e\u0635\u0650\u064a\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u063a\u064e\u0627\u0641\u0650\u0631\u0650", "word_translation": "(The) Forgiver", "word_transliteration": "ghafiri"}, {"word_arabic_uthmani": "\u0671\u0644\u0630\u0651\u064e\u0646\u06e2\u0628\u0650", "word_translation": "(of) the sin", "word_transliteration": "al-dhanbi"}, {"word_arabic_uthmani": "\u0648\u064e\u0642\u064e\u0627\u0628\u0650\u0644\u0650", "word_translation": "and (the) Acceptor", "word_transliteration": "waqabili"}, {"word_arabic_uthmani": "\u0671\u0644\u062a\u0651\u064e\u0648\u06e1\u0628\u0650", "word_translation": "(of) [the] repentance", "word_transliteration": "al-tawbi"}, {"word_arabic_uthmani": "\u0634\u064e\u062f\u0650\u064a\u062f\u0650", "word_translation": "severe", "word_transliteration": "shadidi"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u0650\u0642\u064e\u0627\u0628\u0650", "word_translation": "(in) the punishment", "word_transliteration": "al-'iqabi"}, {"word_arabic_uthmani": "\u0630\u0650\u064a", "word_translation": "Owner of", "word_transliteration": "dhi"}, {"word_arabic_uthmani": "\u0671\u0644\u0637\u0651\u064e\u0648\u06e1\u0644\u0650\u06d6", "word_translation": "the abundance", "word_transliteration": "al-tawli"}, {"word_arabic_uthmani": "\u0644\u064e\u0627\u0653", "word_translation": "(There is) no", "word_transliteration": "la"},