{"ayahs": {"ayah_uthmani": "\u064a\u064e\u0670\u0635\u064e\u0670\u062d\u0650\u0628\u064e\u064a\u0650 \u0671\u0644\u0633\u0651\u0650\u062c\u06e1\u0646\u0650 \u0621\u064e\u0623\u064e\u0631\u06e1\u0628\u064e\u0627\u0628\u065e \u0645\u0651\u064f\u062a\u064e\u0641\u064e\u0631\u0651\u0650\u0642\u064f\u0648\u0646\u064e \u062e\u064e\u064a\u06e1\u0631\u064c \u0623\u064e\u0645\u0650 \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u06e1\u0648\u064e\u0670\u062d\u0650\u062f\u064f \u0671\u0644\u06e1\u0642\u064e\u0647\u0651\u064e\u0627\u0631\u064f"}, "words": [{"word_arabic_uthmani": "\u064a\u064e\u0670\u0635\u064e\u0670\u062d\u0650\u0628\u064e\u064a\u0650", "word_translation": "O my two companions", "word_transliteration": "yasahibayi"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u0650\u062c\u06e1\u0646\u0650", "word_translation": "(of) the prison!", "word_transliteration": "al-sij'ni"}, {"word_arabic_uthmani": "\u0621\u064e\u0623\u064e\u0631\u06e1\u0628\u064e\u0627\u0628\u065e", "word_translation": "Are lords", "word_transliteration": "a-arbabun"}, {"word_arabic_uthmani": "\u0645\u0651\u064f\u062a\u064e\u0641\u064e\u0631\u0651\u0650\u0642\u064f\u0648\u0646\u064e", "word_translation": "separate", "word_transliteration": "mutafarriquna"}, {"word_arabic_uthmani": "\u062e\u064e\u064a\u06e1\u0631\u064c", "word_translation": "better", "word_transliteration": "khayrun"}, {"word_arabic_uthmani": "\u0623\u064e\u0645\u0650", "word_translation": "or", "word_transliteration": "ami"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "Allah", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0648\u064e\u0670\u062d\u0650\u062f\u064f", "word_translation": "the One", "word_transliteration": "al-wahidu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0642\u064e\u0647\u0651\u064e\u0627\u0631\u064f", "word_translation": "the Irresistible", "word_transliteration": "al-qaharu"}]}