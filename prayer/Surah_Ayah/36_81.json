{"ayahs": {"ayah_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u064a\u06e1\u0633\u064e \u0671\u0644\u0651\u064e\u0630\u0650\u064a \u062e\u064e\u0644\u064e\u0642\u064e \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0628\u0650\u0642\u064e\u0670\u062f\u0650\u0631\u064d \u0639\u064e\u0644\u064e\u0649\u0670\u0653 \u0623\u064e\u0646 \u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064e \u0645\u0650\u062b\u06e1\u0644\u064e\u0647\u064f\u0645\u06da \u0628\u064e\u0644\u064e\u0649\u0670 \u0648\u064e\u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u062e\u064e\u0644\u0651\u064e\u0640\u0670\u0642\u064f \u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f"}, "words": [{"word_arabic_uthmani": "\u0623\u064e\u0648\u064e\u0644\u064e\u064a\u06e1\u0633\u064e", "word_translation": "Is it not", "word_transliteration": "awalaysa"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a", "word_translation": "(He) Who", "word_transliteration": "alladhi"}, {"word_arabic_uthmani": "\u062e\u064e\u0644\u064e\u0642\u064e", "word_translation": "created", "word_transliteration": "khalaqa"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650", "word_translation": "the heavens", "word_transliteration": "al-samawati"}, {"word_arabic_uthmani": "\u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e", "word_translation": "and the earth", "word_transliteration": "wal-arda"}, {"word_arabic_uthmani": "\u0628\u0650\u0642\u064e\u0670\u062f\u0650\u0631\u064d", "word_translation": "Able", "word_transliteration": "biqadirin"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u0649\u0670\u0653", "word_translation": "to", "word_transliteration": "ala"}, {"word_arabic_uthmani": "\u0623\u064e\u0646", "word_translation": "[that]", "word_transliteration": "an"}, {"word_arabic_uthmani": "\u064a\u064e\u062e\u06e1\u0644\u064f\u0642\u064e", "word_translation": "create", "word_transliteration": "yakhluqa"}, {"word_arabic_uthmani": "\u0645\u0650\u062b\u06e1\u0644\u064e\u0647\u064f\u0645\u06da", "word_translation": "(the) like of them", "word_transliteration": "mith'lahum"}, {"word_arabic_uthmani": "\u0628\u064e\u0644\u064e\u0649\u0670", "word_translation": "Yes indeed!", "word_transliteration": "bala"}, {"word_arabic_uthmani": "\u0648\u064e\u0647\u064f\u0648\u064e", "word_translation": "and He", "word_transliteration": "wahuwa"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062e\u064e\u0644\u0651\u064e\u0640\u0670\u0642\u064f", "word_translation": "(is) the Supreme Creator", "word_transliteration": "al-khalaqu"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u0639\u064e\u0644\u0650\u064a\u0645\u064f", "word_translation": "the All-Knower", "word_transliteration": "al-'alimu"}]}