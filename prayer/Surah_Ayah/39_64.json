{"ayahs": {"ayah_uthmani": "\u0642\u064f\u0644\u06e1 \u0623\u064e\u0641\u064e\u063a\u064e\u064a\u06e1\u0631\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u0650 \u062a\u064e\u0623\u06e1\u0645\u064f\u0631\u064f\u0648\u0653\u0646\u0651\u0650\u064a\u0653 \u0623\u064e\u0639\u06e1\u0628\u064f\u062f\u064f \u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627 \u0671\u0644\u06e1\u062c\u064e\u0670\u0647\u0650\u0644\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0642\u064f\u0644\u06e1", "word_translation": "Say", "word_transliteration": "qul"}, {"word_arabic_uthmani": "\u0623\u064e\u0641\u064e\u063a\u064e\u064a\u06e1\u0631\u064e", "word_translation": "Is (it) other than", "word_transliteration": "afaghayra"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u0650", "word_translation": "Allah", "word_transliteration": "al-lahi"}, {"word_arabic_uthmani": "\u062a\u064e\u0623\u06e1\u0645\u064f\u0631\u064f\u0648\u0653\u0646\u0651\u0650\u064a\u0653", "word_translation": "you order me", "word_transliteration": "tamurunni"}, {"word_arabic_uthmani": "\u0623\u064e\u0639\u06e1\u0628\u064f\u062f\u064f", "word_translation": "(to) worship", "word_transliteration": "a'budu"}, {"word_arabic_uthmani": "\u0623\u064e\u064a\u0651\u064f\u0647\u064e\u0627", "word_translation": "O", "word_transliteration": "ayyuha"}, {"word_arabic_uthmani": "\u0671\u0644\u06e1\u062c\u064e\u0670\u0647\u0650\u0644\u064f\u0648\u0646\u064e", "word_translation": "ignorant ones", "word_transliteration": "al-jahiluna"}]}