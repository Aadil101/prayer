{"ayahs": {"ayah_uthmani": "\u0648\u064e\u064a\u064f\u0646\u064e\u062c\u0651\u0650\u064a \u0671\u0644\u0644\u0651\u064e\u0647\u064f \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0671\u062a\u0651\u064e\u0642\u064e\u0648\u06e1\u0627\u0652 \u0628\u0650\u0645\u064e\u0641\u064e\u0627\u0632\u064e\u062a\u0650\u0647\u0650\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0645\u064e\u0633\u0651\u064f\u0647\u064f\u0645\u064f \u0671\u0644\u0633\u0651\u064f\u0648\u0653\u0621\u064f \u0648\u064e\u0644\u064e\u0627 \u0647\u064f\u0645\u06e1 \u064a\u064e\u062d\u06e1\u0632\u064e\u0646\u064f\u0648\u0646\u064e"}, "words": [{"word_arabic_uthmani": "\u0648\u064e\u064a\u064f\u0646\u064e\u062c\u0651\u0650\u064a", "word_translation": "And Allah will deliver", "word_transliteration": "wayunajji"}, {"word_arabic_uthmani": "\u0671\u0644\u0644\u0651\u064e\u0647\u064f", "word_translation": "And Allah will deliver", "word_transliteration": "al-lahu"}, {"word_arabic_uthmani": "\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e", "word_translation": "those who", "word_transliteration": "alladhina"}, {"word_arabic_uthmani": "\u0671\u062a\u0651\u064e\u0642\u064e\u0648\u06e1\u0627\u0652", "word_translation": "feared (Him)", "word_transliteration": "ittaqaw"}, {"word_arabic_uthmani": "\u0628\u0650\u0645\u064e\u0641\u064e\u0627\u0632\u064e\u062a\u0650\u0647\u0650\u0645\u06e1", "word_translation": "to their place of salvation", "word_transliteration": "bimafazatihim"}, {"word_arabic_uthmani": "\u0644\u064e\u0627", "word_translation": "not", "word_transliteration": "la"}, {"word_arabic_uthmani": "\u064a\u064e\u0645\u064e\u0633\u0651\u064f\u0647\u064f\u0645\u064f", "word_translation": "will touch them", "word_transliteration": "yamassuhumu"}, {"word_arabic_uthmani": "\u0671\u0644\u0633\u0651\u064f\u0648\u0653\u0621\u064f", "word_translation": "the evil", "word_transliteration": "al-suu"}, {"word_arabic_uthmani": "\u0648\u064e\u0644\u064e\u0627", "word_translation": "and not", "word_transliteration": "wala"}, {"word_arabic_uthmani": "\u0647\u064f\u0645\u06e1", "word_translation": "they", "word_transliteration": "hum"}, {"word_arabic_uthmani": "\u064a\u064e\u062d\u06e1\u0632\u064e\u0646\u064f\u0648\u0646\u064e", "word_translation": "will grieve", "word_transliteration": "yahzanuna"}]}