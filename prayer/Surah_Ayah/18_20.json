{"ayahs": {"ayah_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0625\u0650\u0646 \u064a\u064e\u0638\u06e1\u0647\u064e\u0631\u064f\u0648\u0627\u0652 \u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1 \u064a\u064e\u0631\u06e1\u062c\u064f\u0645\u064f\u0648\u0643\u064f\u0645\u06e1 \u0623\u064e\u0648\u06e1 \u064a\u064f\u0639\u0650\u064a\u062f\u064f\u0648\u0643\u064f\u0645\u06e1 \u0641\u0650\u064a \u0645\u0650\u0644\u0651\u064e\u062a\u0650\u0647\u0650\u0645\u06e1 \u0648\u064e\u0644\u064e\u0646 \u062a\u064f\u0641\u06e1\u0644\u0650\u062d\u064f\u0648\u0653\u0627\u0652 \u0625\u0650\u0630\u064b\u0627 \u0623\u064e\u0628\u064e\u062f\u0657\u0627"}, "words": [{"word_arabic_uthmani": "\u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1", "word_translation": "Indeed [they]", "word_transliteration": "innahum"}, {"word_arabic_uthmani": "\u0625\u0650\u0646", "word_translation": "if", "word_transliteration": "in"}, {"word_arabic_uthmani": "\u064a\u064e\u0638\u06e1\u0647\u064e\u0631\u064f\u0648\u0627\u0652", "word_translation": "they come to know", "word_transliteration": "yazharu"}, {"word_arabic_uthmani": "\u0639\u064e\u0644\u064e\u064a\u06e1\u0643\u064f\u0645\u06e1", "word_translation": "about you", "word_transliteration": "alaykum"}, {"word_arabic_uthmani": "\u064a\u064e\u0631\u06e1\u062c\u064f\u0645\u064f\u0648\u0643\u064f\u0645\u06e1", "word_translation": "they will stone you", "word_transliteration": "yarjumukum"}, {"word_arabic_uthmani": "\u0623\u064e\u0648\u06e1", "word_translation": "or", "word_transliteration": "aw"}, {"word_arabic_uthmani": "\u064a\u064f\u0639\u0650\u064a\u062f\u064f\u0648\u0643\u064f\u0645\u06e1", "word_translation": "return you", "word_transliteration": "yu'idukum"}, {"word_arabic_uthmani": "\u0641\u0650\u064a", "word_translation": "to", "word_transliteration": "fi"}, {"word_arabic_uthmani": "\u0645\u0650\u0644\u0651\u064e\u062a\u0650\u0647\u0650\u0645\u06e1", "word_translation": "their religion", "word_transliteration": "m